tr '[A-Z]' '[a-z]' |
sed -n '/function/s,^      [ 	]*\([a-z\*0-9]*\)[ 	]*function[ 	]*\([a-z0-9_ ]*\).*$,      external \2      \1 \2,p' |
tr '' '\012'

#
# that is all
#
