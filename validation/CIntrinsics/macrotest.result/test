
Parsed printed file for "macrotest"

int main()
{
  int a, b, c;

  a = b = 1;
  c = 0;
  if (a^c)
    printf("a=%d  xor  c=%d = True", a, c);

  if (!c)
    printf("\nc=False \n");
}

