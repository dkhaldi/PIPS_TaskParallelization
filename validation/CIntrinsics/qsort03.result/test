#
# Initial code for module main
#
int main()
{
   int b[10];
   s_int a[10];
   int i;
   for(i = 0; i <= 9; i += 1) {
      b[i] = 10-i-1;
      a[i].n = &b[i];
   }
   for(i = 0; i <= 9; i += 1)
      printf("%d\n", *a[i].n);
   qsort(a, 10, sizeof(s_int), &foo);
   for(i = 0; i <= 9; i += 1)
      printf("%d\n", *a[i].n);
   return 0;
}
#
# Display print_code_proper_effects for module main
#
int main()
{
//             <    is declared>: b
   int b[10];
//             <    is declared>: a
   s_int a[10];
//             <    is declared>: i
   int i;
//             <    is written>: i
//             <    is referenced>: i
   for(i = 0; i <= 9; i += 1) {
//             <    is read   >: i
//             <    is written>: b[i]
//             <    is referenced>: b i
      b[i] = 10-i-1;
//             <    is read   >: i
//             <    is written>: a[i].n
//             <    is referenced>: a b i
      a[i].n = &b[i];
   }
//             <    is written>: i
//             <    is referenced>: i
   for(i = 0; i <= 9; i += 1)
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: a[i].n a[i].n[0] i stdout 
//                               __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
//             <    is referenced>: a i stdout __IO_EFFECTS:LUNS
      printf("%d\n", *a[i].n);
//             <may be read   >: a[*].n a[*].n[*]
//             <may be written>: a[*].n
//             <    is referenced>: a foo s_int
   qsort(a, 10, sizeof(s_int), &foo);
//             <    is written>: i
//             <    is referenced>: i
   for(i = 0; i <= 9; i += 1)
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: a[i].n a[i].n[0] i stdout 
//                               __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
//             <    is referenced>: a i stdout __IO_EFFECTS:LUNS
      printf("%d\n", *a[i].n);
   return 0;
}
