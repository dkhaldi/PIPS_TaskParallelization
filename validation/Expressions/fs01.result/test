! forward substitution example 01
!
      PROGRAM FS01

      REAL X, Y, Z, W, U, V

      X = 0.2226473                                                     0008
      Y = X+X*X                                                         0009
      Z = Y+X                                                           0010
      W = Z*Z*Z+Y*X                                                     0011

      PRINT *, 'w = ', W                                                0013

      U = 0.3236433                                                     0015
      V = U+2*U                                                         0016

      END
!! After forward substitution
