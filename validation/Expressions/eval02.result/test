
//  P() {}

void eval02()
{

//  P() {}

   int i = 1;

//  P(i) {i==1}

   int j = 2;

//  P(i,j) {i==1, j==2}


   i = i+j;

//  P(i,j) {i==3, j==2}


   i += j;

//  P(i,j) {i==5, j==2}


   i = 1+1;
}

//  P() {}

void eval02()
{

//  P() {}

   int i = 1;

//  P(i) {i==1}

   int j = 2;

//  P(i,j) {i==1, j==2}


   i = i+2;

//  P(i,j) {i==3, j==2}


   i += 2;

//  P(i,j) {i==5, j==2}


   i = 2;
}

