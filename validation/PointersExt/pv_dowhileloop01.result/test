
PROPER  EFFECTS

int main()
{
//             <    is written>: i
   int i = 0;
   int a[10];
//             <    is read   >: i
   do {
//             <    is read   >: i
//             <    is written>: a[i]
      a[i] = i;
//             <    is read   >: i
//             <    is written>: i
      i++;
   }
   while (i<10);
   return 0;
}

POINTS-TO


// Points To: none

int main()
{

// Points To: none

   int i = 0;

// Points To: none

   int a[10];

// Points To: none

   do {

// Points To: none

      a[i] = i;

// Points To: none

      i++;
   }
   while (i<10);

// Points To: none

   return 0;
}
