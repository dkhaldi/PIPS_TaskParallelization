
PROPER  EFFECTS

int main()
{
   float t;
   float a;
//             <    is written>: t

   t = 1.0;
//             <    is read   >: t
   while (t<2.0) {
//             <    is read   >: a t
//             <    is written>: a
      a = a+t*0, 5;
//             <    is read   >: t
//             <    is written>: t
      t += 0, 01;
   }
   return 0;
}

POINTS-TO


// Points To: none

int main()
{

// Points To: none

   float t;

// Points To: none

   float a;

// Points To: none


   t = 1.0;

// Points To: none

   while (t<2.0) {

// Points To: none

      a = a+t*0, 5;

// Points To: none

      t += 0, 01;
   }

// Points To: none

   return 0;
}
