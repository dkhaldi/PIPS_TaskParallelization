int a(int i)
{
   int j[2] = {2, 2}, k[2], l;
   j[0] = 0;
   l = 0;
   if (i)
      l = i;
   else
      k[0] = 1;
   load(k, j);
   return k[0]+k[1];
}
int a(int i)
{
   int j[2] = {2, 2}, k[2], l;
   j[0] = 0;
   l = 0;
   if (i) {
      load(k, j);
      l = i;
   }
   else {
      k[0] = 1;
      load(k, j);
   }
   return k[0]+k[1];
}

