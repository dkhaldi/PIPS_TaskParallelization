
// IN Pointer values:
// stderr == &__IO_EFFECTS:__stderr__ (exact);
// stdin == &__IO_EFFECTS:__stdin__ (exact);
// stdout == &__IO_EFFECTS:__stdout__ (exact);


// OUT Pointer values:
// stderr == &__IO_EFFECTS:__stderr__ (exact);
// stdin == &__IO_EFFECTS:__stdin__ (exact);
// stdout == &__IO_EFFECTS:__stdout__ (exact);

int main()
{

// Pointer values:
// stderr == &__IO_EFFECTS:__stderr__ (exact);
// stdin == &__IO_EFFECTS:__stdin__ (exact);
// stdout == &__IO_EFFECTS:__stdout__ (exact);

   int **a;

// Pointer values:
// a == *ANY_MODULE*:*UNDEFINED* (exact);
// stderr == &__IO_EFFECTS:__stderr__ (exact);
// stdin == &__IO_EFFECTS:__stdin__ (exact);
// stdout == &__IO_EFFECTS:__stdout__ (exact);

   int b = 1;

// Pointer values:
// a == *ANY_MODULE*:*UNDEFINED* (exact);
// stderr == &__IO_EFFECTS:__stderr__ (exact);
// stdin == &__IO_EFFECTS:__stdin__ (exact);
// stdout == &__IO_EFFECTS:__stdout__ (exact);

   a[0] = &b;

// Pointer values:
// a == *ANY_MODULE*:*UNDEFINED* (exact);
// a[0] == &b (exact);
// stderr == &__IO_EFFECTS:__stderr__ (exact);
// stdin == &__IO_EFFECTS:__stdin__ (exact);
// stdout == &__IO_EFFECTS:__stdout__ (exact);

   printf("\nb= %d, a[0] = %d\n", b, a[0][0]);

// Pointer values:
// a == *ANY_MODULE*:*UNDEFINED* (exact);
// a[0] == &b (exact);
// stderr == &__IO_EFFECTS:__stderr__ (exact);
// stdin == &__IO_EFFECTS:__stdin__ (exact);
// stdout == &__IO_EFFECTS:__stdout__ (exact);

   return 0;
}
