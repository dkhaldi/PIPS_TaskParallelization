
// IN Pointer values: none


// OUT Pointer values: none

int main()
{

// Pointer values: none

   int a[10];

// Pointer values: none

   int *p;

// Pointer values:
// p == *ANY_MODULE*:*UNDEFINED* (exact);

   p = &a[0];

// Pointer values:
// p == &a[0] (exact);

   p++;

// Pointer values:
// p == &a[1] (exact);

   ++p;

// Pointer values:
// p == &a[2] (exact);

   p--;

// Pointer values:
// p == &a[1] (exact);

   --p;

// Pointer values:
// p == &a[0] (exact);

   return 0;
}
