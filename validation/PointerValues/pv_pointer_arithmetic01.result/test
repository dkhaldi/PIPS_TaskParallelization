
// IN Pointer values: none


// OUT Pointer values: none

int main()
{

// Pointer values: none

   int a[10];

// Pointer values: none

   int *p;

// Pointer values:
// p == *ANY_MODULE*:*UNDEFINED* (exact);

   p = &a[0];

// Pointer values:
// p == &a[0] (exact);

   p = p+1;

// Pointer values:
// p == &a[1] (exact);

   p = p+3;

// Pointer values:
// p == &a[4] (exact);

   p = p-1;

// Pointer values:
// p == &a[3] (exact);

   p = p-2;

// Pointer values:
// p == &a[1] (exact);

   return 0;
}
