
// IN Pointer values: none


// OUT Pointer values: none

int foo(int a)
{

// Pointer values: none

   return a;
}

// IN Pointer values: none


// OUT Pointer values: none

int main()
{

// Pointer values: none

   int a, *p;

// Pointer values:
// p == *ANY_MODULE*:*UNDEFINED* (exact);

   p = &a;

// Pointer values:
// p == &a (exact);

   a = foo(2);

// Pointer values:
// p == &a (exact);

   return 0;
}
