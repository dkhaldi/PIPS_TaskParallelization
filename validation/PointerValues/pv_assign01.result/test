
// IN Pointer values: none


// OUT Pointer values: none

int main()
{

// Pointer values: none

   int *a;

// Pointer values:
// a == *ANY_MODULE*:*UNDEFINED* (exact);

   int b = 0;

// Pointer values:
// a == *ANY_MODULE*:*UNDEFINED* (exact);

   a = &b;

// Pointer values:
// a == &b (exact);

   return 0;
}
