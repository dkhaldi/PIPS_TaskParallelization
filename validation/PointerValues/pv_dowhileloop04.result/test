
// IN Pointer values: none


// OUT Pointer values: none

int main()
{

// Pointer values: none

   int i = 0;

// Pointer values: none

   int *p[10];

// Pointer values:
// p[*] == *ANY_MODULE*:*UNDEFINED* (exact);

   int a[10];

// Pointer values:
// p[*] == *ANY_MODULE*:*UNDEFINED* (exact);

   do {

// Pointer values:
// p[*] == *ANY_MODULE*:*UNDEFINED* (may);
// p[*] == &a[*] (may);

      p[i] = &a[i];

// Pointer values:
// p[*] == *ANY_MODULE*:*UNDEFINED* (may);
// p[*] == &a[*] (may);

      i++;
   }
   while (i<10);

// Pointer values:
// p[*] == *ANY_MODULE*:*UNDEFINED* (may);
// p[*] == &a[*] (may);


   return 0;
}
