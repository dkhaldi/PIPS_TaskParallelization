
Preconditions


//  P() {}

int main()
{
   
   

//  P() {}

   struct Liste *Nouveau;


//  P() {}

   struct Liste *Tete;

//  P() {}


   Tete = (void *) 0;

//  P() {}

   
   // problem of sizeof instrinsic with structure 
   // Nouveau = (struct Liste *)malloc(sizeof(struct Liste));
   
   Nouveau->pSuivant = Tete;

//  P() {}


   Tete = Nouveau;
}

Complexities

//                                                                    8 (SUMMARY)
int main()
{
   
   
//                                                                    0 (STMT)
   struct Liste *Nouveau;

//                                                                    0 (STMT)
   struct Liste *Tete;
//                                                                    2 (STMT)

   Tete = (void *) 0;
//                                                                    4 (STMT)
   
   // problem of sizeof instrinsic with structure 
   // Nouveau = (struct Liste *)malloc(sizeof(struct Liste));
   
   Nouveau->pSuivant = Tete;
//                                                                    2 (STMT)

   Tete = Nouveau;
}
