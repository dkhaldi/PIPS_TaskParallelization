      SUBROUTINE NODE
      implicit none
      include "global_parameters.h"
      include "hpfc_commons.h"
      include "hpfc_includes.h"
      include "HPFTEST04_parameters.h"
      INTEGER I_1, I_10, I_2, I_3, I_4, I_5, I_6, I_7, I_8, I_9, I, 
     &I_0
      REAL*4 F_1, F_0, A(1:5), B(1:5)
      CALL HPFC INIT NODE
      CALL HPFC LOOP BOUNDS(I_2, I_3, I_4, 1, 10, 2, 1)
C     
C
Commencement du code
      I = I_4
      DO I_1 = I_2, I_3
         I = I+1
         A(I_1) = I
      ENDDO
      DO I = 1, 10
         I_0 = 11-I
         CALL HPFC CMPCOMPUTER(1, I_0, 0, 0, 0, 0, 0, 0)
         IF (HPFC COMPUTERP()) THEN
            CALL HPFC CMPOWNERS(2, I, 0, 0, 0, 0, 0, 0)
            IF (HPFC OWNERP()) THEN
               I_5 = MOD(I-1, 5)+1
               F_0 = A(I_5)
            ELSE
               CALL HPFC RCVFR S(HPFC REAL4, F_0)
            ENDIF
            I_7 = MOD(I_0-1, 5)+1
            B(I_7) = F_0
         ELSE
            CALL HPFC CMPOWNERS(2, I, 0, 0, 0, 0, 0, 0)
            IF (HPFC SENDERP()) THEN
               I_6 = MOD(I-1, 5)+1
               CALL HPFC SNDTO C(HPFC REAL4, A(I_6))
            ENDIF
         ENDIF
      ENDDO
      DO I = 1, 10
         CALL HPFC CMPCOMPUTER(2, I, 0, 0, 0, 0, 0, 0)
         IF (HPFC COMPUTERP()) THEN
            CALL HPFC CMPOWNERS(1, I, 0, 0, 0, 0, 0, 0)
            IF (HPFC OWNERP()) THEN
               I_8 = MOD(I-1, 5)+1
               F_1 = B(I_8)
            ELSE
               CALL HPFC RCVFR S(HPFC REAL4, F_1)
            ENDIF
            I_10 = MOD(I-1, 5)+1
            A(I_10) = F_1
         ELSE
            CALL HPFC CMPOWNERS(1, I, 0, 0, 0, 0, 0, 0)
            IF (HPFC SENDERP()) THEN
               I_9 = MOD(I-1, 5)+1
               CALL HPFC SNDTO C(HPFC REAL4, B(I_9))
            ENDIF
         ENDIF
      ENDDO
      CALL HPFC NODE END
      END
