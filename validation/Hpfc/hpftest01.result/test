! File HPFTEST01_node.f
! This file has been automatically generated by the HPF compiler
!
      SUBROUTINE NODE
      implicit none
      include "global_parameters.h"
      include "hpfc_commons.h"
      include "hpfc_includes.h"
      include "HPFTEST01_parameters.h"
      INTEGER I_0, I_1, I_2, I_3, I
      REAL*4 A(1:5)
      CALL HPFC INIT NODE
      CALL HPFC LOOP BOUNDS(I_1, I_2, I_3, 1, 10, 1, 1)
      I = 0
      I = I_3
      DO I_0 = I_1, I_2
         I = I+1
         A(I_0) = I
      ENDDO
      CALL HPFC NODE END
      END
!
! That is all for HPFTEST01_node.f
