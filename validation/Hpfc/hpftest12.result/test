!
! File HPFTEST12_node.f
! This file has been automatically generated by the HPF compiler
!
      SUBROUTINE NODE
      implicit none
      include "global_parameters.h"
      include "hpfc_commons.h"
      include "hpfc_includes.h"
      include "HPFTEST12_parameters.h"
      INTEGER I_0, I_1, I, J, K
      REAL*4 A(1:25, 1:100, 1:100)
      CALL HPFC INIT NODE
      DO I = 1, 100
         DO J = 1, 100
            DO K = 1, 100
               CALL HPFC CMPCOMPUTER(1, I, J, K, 0, 0, 0, 0)
               IF (HPFC COMPUTERP()) THEN
                  I_0 = MOD(I-1, 25)+1
                  A(I_0,J,K) = I+J+K
                  CALL HPFC CMPOWNERS(1, I, J, K, 0, 0, 0, 0)
                  CALL HPFC SNDTO OOS(HPFC REAL4, A(I_0,J,K))
               ELSE
                  CALL HPFC CMPOWNERS(1, I, J, K, 0, 0, 0, 0)
                  IF (HPFC OWNERP()) THEN
                     I_1 = MOD(I-1, 25)+1
                     CALL HPFC RCVFR C(HPFC REAL4, A(I_1,J,K))
                  ENDIF
               ENDIF
            ENDDO
         ENDDO
      ENDDO
      CALL HPFC NODE END
      END
!
! That is all for HPFTEST12_node.f
