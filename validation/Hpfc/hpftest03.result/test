      SUBROUTINE NODE
      implicit none
      include "global_parameters.h"
      include "hpfc_commons.h"
      include "hpfc_includes.h"
      include "HPFTEST03_parameters.h"
      INTEGER I_0, I
      REAL*4 A1(1:6)
      CALL HPFC INIT NODE
C     
C
Commencement du code
      DO I = 1, 10
         CALL HPFC CMPCOMPUTER(4, I, 0, 0, 0, 0, 0, 0)
         IF (HPFC COMPUTERP()) THEN
            I_0 = HPFC LOCALINDGAMMA(4, 1, I)
            A1(I_0) = I
         ENDIF
      ENDDO
      CALL HPFC NODE END
      END
!
! That is all for HPFTEST03_node.f
