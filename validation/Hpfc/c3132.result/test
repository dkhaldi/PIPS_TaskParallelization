      SUBROUTINE NODE
      implicit none
      include "global_parameters.h"
      include "hpfc_commons.h"
      include "hpfc_includes.h"
      include "C3132_parameters.h"
      INTEGER*4 NF
      PARAMETER (NF = 320)
      INTEGER*4 NI
      PARAMETER (NI = 8)
      INTEGER*4 NR
      PARAMETER (NR = 160)
      INTEGER ALPHA1, ALPHA2, ALPHA3, LALPHA1, LALPHA2, LALPHA3, PSI1
     &, I_3, LALPHAp1, LALPHAp2, LALPHAp3, PSIp1, T LID
      REAL*4 T31P(1:20, 1:NR, 1:NI), T31P_1(1:NF, 1:16, 1:NI)
      CALL HPFC INIT NODE
! all livenesss for T31P
      LIVE MAPPING(3) = .FALSE.
      LIVE MAPPING(1) = .FALSE.
! direct remapping for T31P
      LIVE MAPPING(3) = .TRUE.
      MSTATUS(3) = 3

      CALL HPFC CMPCOMPUTER(3, 1, 1, 1, 0, 0, 0, 0)
      IF (HPFC COMPUTERP()) THEN
         I_3 = MOD(1-1, 20)+1
         T31P(I_3,1,1) = 2
      ENDIF
! begin remappings
      include 'C3132_T31P_3_1_node.h'
! liveness management
! clean live set for T31P
      LIVE MAPPING(3) = .FALSE.
! end of liveness management
! end remappings
! collecting distributed variable T31P_1
      PSI1 = MY POS(1,1)
      IF (PSI1.EQ.1) THEN
         BUF INDEX = 0
         SEND NOT INITIALIZED = .TRUE.
         ALPHA3 = 1
         ALPHA2 = 1
         ALPHA1 = 1
         LALPHA1 = 1
         LALPHA2 = 1
         LALPHA3 = 1
         BUF INDEX = BUF INDEX+1
         HPFC REAL4 BUFF(BUF INDEX) = T31P_1(LALPHA1,LALPHA2,LALPHA3)
         CALL HPFC REAL4 BUFPCK
         CALL HPFC SND TO HOST
      ENDIF
! end of collect

      CALL HPFC NODE END
      END
!
! That is all for C3132_node.f
