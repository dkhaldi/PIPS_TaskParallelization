!
! File C3132_T31P_3_1_node.h
! This file has been automatically generated by the HPF compiler
!
      IF (MSTATUS(3).EQ.3) THEN
         IF (.NOT.LIVE MAPPING(1)) THEN
! send part
! remapping T31P[3]: T31P[3] -> T31P_1[1]
            PSI1 = MY POS(1,2)
            IF (MY LID.LE.16) THEN
               DO PSIp1 = 1, 10
                  T LID = PSIp1
                  IF (MY LID.NE.T LID) THEN
! - lazy sending
                     BUF INDEX = 0
                     SEND NOT INITIALIZED = .TRUE.
                     DO LALPHA3 = 1, 8
                        DO LALPHA2 = 16*PSIp1-15, 16*PSIp1
                           DO LALPHA1 = 1, 20
                              LALPHAp3 = LALPHA3
                              LALPHAp2 = LALPHA2-16*PSIp1+16
                              LALPHAp1 = LALPHA1+20*PSI1-20
                              BUF INDEX = BUF INDEX+1
                              HPFC REAL4 BUFF(BUF INDEX) = T31P(
     &                        LALPHA1,LALPHA2,LALPHA3)
                              IF (BUF INDEX.EQ.HPFC REAL4 BUFF SIZE
     &                        ) THEN
                                 CALL HPFC REAL4 BUFPCK
                                 CALL HPFC BROADCAST 0(T LID)
                                 BUF INDEX = 0
                                 SEND NOT INITIALIZED = .TRUE.
                              ENDIF
                           ENDDO
                        ENDDO
                     ENDDO
                     IF (BUF INDEX.NE.0) THEN
                        CALL HPFC REAL4 BUFPCK
                        CALL HPFC BROADCAST 0(T LID)
                     ENDIF
                  ENDIF
               ENDDO
            ENDIF
! receive part
            PSIp1 = MY POS(1,1)
            IF (MY LID.LE.10) THEN
               DO PSI1 = 1, 16
                  T LID = PSI1
                  IF (MY LID.NE.T LID) THEN
! - lazy receiving
                     BUF INDEX = 0
                     RECEIVED NOT PERFORMED = .TRUE.
                     SIZE OF RECEIVED BUFFER = 0
                     DO LALPHA3 = 1, 8
                        DO LALPHA2 = 16*PSIp1-15, 16*PSIp1
                           DO LALPHA1 = 1, 20
                              LALPHAp3 = LALPHA3
                              LALPHAp2 = LALPHA2-16*PSIp1+16
                              LALPHAp1 = LALPHA1+20*PSI1-20
                              IF (BUF INDEX.EQ.
     &                        SIZE OF RECEIVED BUFFER) THEN
                                 RECEIVED NOT PERFORMED = .TRUE.
                                 CALL HPFC REAL4 BUFUPK(T LID)
                                 BUF INDEX = 0
                              ENDIF
                              BUF INDEX = BUF INDEX+1
                              T31P_1(LALPHAp1,LALPHAp2,LALPHAp3) = 
     &                        HPFC REAL4 BUFF(BUF INDEX)
                           ENDDO
                        ENDDO
                     ENDDO
                  ELSE
! - lazy copying
                     DO LALPHA3 = 1, 8
                        DO LALPHA2 = 16*PSIp1-15, 16*PSIp1
                           DO LALPHA1 = 1, 20
                              LALPHAp3 = LALPHA3
                              LALPHAp2 = LALPHA2-16*PSIp1+16
                              LALPHAp1 = LALPHA1+20*PSI1-20
                              T31P_1(LALPHAp1,LALPHAp2,LALPHAp3) = 
     &                        T31P(LALPHA1,LALPHA2,LALPHA3)
                           ENDDO
                        ENDDO
                     ENDDO
                  ENDIF
               ENDDO
            ENDIF
! end of remapping
         ENDIF
         LIVE MAPPING(1) = .TRUE.
         MSTATUS(3) = 1
      ENDIF
!
! That is all for C3132_T31P_3_1_node.h
