      SUBROUTINE HOST
      implicit none
      include "global_parameters.h"
      include "hpfc_commons.h"
      include "hpfc_includes.h"
      include "C3132_parameters.h"
      INTEGER*4 NF
      PARAMETER (NF = 320)
      INTEGER*4 NI
      PARAMETER (NI = 8)
      INTEGER*4 NR
      PARAMETER (NR = 160)
      INTEGER ALPHA1, ALPHA2, ALPHA3, LALPHA1, LALPHA2, LALPHA3, PSI1
     &, T LID
      REAL*4 T31P(1:NF, 1:NR, 1:NI)
      CALL HPFC INIT HOST

! collecting distributed variable T31P_1
      PSI1 = 1
      BUF INDEX = 0
      RECEIVED NOT PERFORMED = .FALSE.
      SIZE OF RECEIVED BUFFER = 0
      T LID = PSI1
      CALL HPFC RCV FROM NODE(T LID)
      CALL HPFC REAL4 BUFUPK
      ALPHA3 = 1
      ALPHA2 = 1
      ALPHA1 = 1
      LALPHA1 = 1
      LALPHA2 = 1
      LALPHA3 = 1
      BUF INDEX = BUF INDEX+1
      T31P(ALPHA1,ALPHA2,ALPHA3) = HPFC REAL4 BUFF(BUF INDEX)
! end of collect

      PRINT *, T31P(1,1,1)

      CALL HPFC HOST END
      END
