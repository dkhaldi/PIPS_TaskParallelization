!
! File HPFTEST35_node.f
! This file has been automatically generated by the HPF compiler
!
      SUBROUTINE NODE
      implicit none
      include "global_parameters.h"
      include "hpfc_commons.h"
      include "hpfc_includes.h"
      include "HPFTEST35_parameters.h"
      INTEGER ALPHA1, ALPHA2, LALPHA1, LALPHA2, PSI1, PSI2, I_10, 
     &I_11, I_12, I_13, I_14, I_15, I_16, I_17, I_18, I_19, I_20, 
     &I_21, I_22, I_23, I_24, I_25, I_26, I_27, I_28, I_29, I_30, 
     &I_31, I_32, I_33, I_34, I_35, I_4, I_5, I_6, I_7, I_8, I_9, I, 
     &K
      REAL*4 TC(0:126, 0:126), TS(1:125, 1:125), NORTH(1:125)
      CALL HPFC INIT NODE
      CALL HPFC LOOP BOUNDS(I_33, I_34, I_35, 2, 249, 3, 2)
      CALL HPFC LOOP BOUNDS(I_29, I_30, I_31, 2, 249, 3, 1)
      CALL HPFC LOOP BOUNDS(I_25, I_26, I_27, 2, 249, 2, 2)
      CALL HPFC LOOP BOUNDS(I_21, I_22, I_23, 2, 249, 2, 1)
      CALL HPFC LOOP BOUNDS(I_17, I_18, I_19, 1, 250, 3, 2)
      CALL HPFC LOOP BOUNDS(I_13, I_14, I_15, 2, 250, 3, 1)
      CALL HPFC LOOP BOUNDS(I_9, I_10, I_11, 1, 250, 3, 2)
      CALL HPFC LOOP BOUNDS(I_5, I_6, I_7, 1, 250, 1, 2)
      IF (MY POS(1,1).EQ.1) THEN
         DO I_4 = I_5, I_6
            NORTH(I_4) = 250.0
         ENDDO
      ENDIF
      IF (MY POS(1,1).EQ.1) THEN
         DO I_8 = I_9, I_10
            TC(1,I_8) = NORTH(I_8)
            TS(1,I_8) = NORTH(I_8)
         ENDDO
      ENDIF
      DO I_12 = I_13, I_14
         DO I_16 = I_17, I_18
            TC(I_12,I_16) = 10.0
            TS(I_12,I_16) = 10.0
         ENDDO
      ENDDO
      DO K = 1, 500
c
c computation and copy back (dataparallel semantic)
! P(1:2, 2) send TC(1:125, 1) to (-1)
         IF (MY POS(2,1).EQ.2) THEN
            CALL HPFC CMPNEIGHBOUR(-1)
            CALL HPFC REAL4 PACK 2(TC, HPFTEST35 TC LO1, 
     &      HPFTEST35 TC UP1, HPFTEST35 TC LO2, HPFTEST35 TC UP2, 1, 
     &      125, 1, 1, 1, 1)
            CALL HPFC SNDTO N
         ENDIF
! P(1:2, 1) send TC(1:125, 125) to (+1)
         IF (MY POS(2,1).EQ.1) THEN
            CALL HPFC CMPNEIGHBOUR(1)
            CALL HPFC REAL4 PACK 2(TC, HPFTEST35 TC LO1, 
     &      HPFTEST35 TC UP1, HPFTEST35 TC LO2, HPFTEST35 TC UP2, 1, 
     &      125, 1, 125, 125, 1)
            CALL HPFC SNDTO N
         ENDIF
! P(2, 1:2) send TC(1, 1:125) to (-2)
         IF (MY POS(1,1).EQ.2) THEN
            CALL HPFC CMPNEIGHBOUR(-2)
            CALL HPFC REAL4 PACK 2(TC, HPFTEST35 TC LO1, 
     &      HPFTEST35 TC UP1, HPFTEST35 TC LO2, HPFTEST35 TC UP2, 1, 
     &      1, 1, 1, 125, 1)
            CALL HPFC SNDTO N
         ENDIF
! P(1, 1:2) send TC(125, 1:125) to (+2)
         IF (MY POS(1,1).EQ.1) THEN
            CALL HPFC CMPNEIGHBOUR(2)
            CALL HPFC REAL4 PACK 2(TC, HPFTEST35 TC LO1, 
     &      HPFTEST35 TC UP1, HPFTEST35 TC LO2, HPFTEST35 TC UP2, 125
     &      , 125, 1, 1, 125, 1)
            CALL HPFC SNDTO N
         ENDIF
! P(1:2, 1) receive TC(1:125, 126) from (+1)
         IF (MY POS(2,1).EQ.1) THEN
            CALL HPFC CMPNEIGHBOUR(1)
            CALL HPFC RCVFR N
            CALL HPFC REAL4 UNPACK 2(TC, HPFTEST35 TC LO1, 
     &      HPFTEST35 TC UP1, HPFTEST35 TC LO2, HPFTEST35 TC UP2, 1, 
     &      125, 1, 126, 126, 1)
         ENDIF
! P(1:2, 2) receive TC(1:125, 0) from (-1)
         IF (MY POS(2,1).EQ.2) THEN
            CALL HPFC CMPNEIGHBOUR(-1)
            CALL HPFC RCVFR N
            CALL HPFC REAL4 UNPACK 2(TC, HPFTEST35 TC LO1, 
     &      HPFTEST35 TC UP1, HPFTEST35 TC LO2, HPFTEST35 TC UP2, 1, 
     &      125, 1, 0, 0, 1)
         ENDIF
! P(1, 1:2) receive TC(126, 1:125) from (+2)
         IF (MY POS(1,1).EQ.1) THEN
            CALL HPFC CMPNEIGHBOUR(2)
            CALL HPFC RCVFR N
            CALL HPFC REAL4 UNPACK 2(TC, HPFTEST35 TC LO1, 
     &      HPFTEST35 TC UP1, HPFTEST35 TC LO2, HPFTEST35 TC UP2, 126
     &      , 126, 1, 1, 125, 1)
         ENDIF
! P(2, 1:2) receive TC(0, 1:125) from (-2)
         IF (MY POS(1,1).EQ.2) THEN
            CALL HPFC CMPNEIGHBOUR(-2)
            CALL HPFC RCVFR N
            CALL HPFC REAL4 UNPACK 2(TC, HPFTEST35 TC LO1, 
     &      HPFTEST35 TC UP1, HPFTEST35 TC LO2, HPFTEST35 TC UP2, 0, 
     &      0, 1, 1, 125, 1)
         ENDIF
         DO I_20 = I_21, I_22
            DO I_24 = I_25, I_26
               TS(I_20,I_24) = 0.25*(TC(I_20-1,I_24)+TC(I_20+1,I_24)+
     &         TC(I_20,I_24-1)+TC(I_20,I_24+1))
            ENDDO
         ENDDO
         DO I_28 = I_29, I_30
            DO I_32 = I_33, I_34
               TC(I_28,I_32) = TS(I_28,I_32)
            ENDDO
         ENDDO
      ENDDO
! collecting distributed variable TC
      PSI1 = MY POS(1,1)
      PSI2 = MY POS(2,1)
      BUF INDEX = 0
      SEND NOT INITIALIZED = .TRUE.
      DO ALPHA2 = MAX(125*PSI2-124, 50), MIN(225, 125*PSI2)
         DO ALPHA1 = MAX(25, 125*PSI1-124), MIN(200, 125*PSI1)
            LALPHA1 = ALPHA1-125*PSI1+125
            LALPHA2 = ALPHA2-125*PSI2+125
            BUF INDEX = BUF INDEX+1
            HPFC REAL4 BUFF(BUF INDEX) = TC(LALPHA1,LALPHA2)
         ENDDO
      ENDDO
      CALL HPFC REAL4 BUFPCK
      CALL HPFC SND TO HOST
! end of collect
! updating shared variable I
      BUF INDEX = 0
      RECEIVED NOT PERFORMED = .FALSE.
      SIZE OF RECEIVED BUFFER = 0
      CALL HPFC NCAST
      CALL HPFC INTEGER4 BUFUPK
      BUF INDEX = BUF INDEX+1
      I = HPFC INTEGER4 BUFF(BUF INDEX)
! end of update
      CALL HPFC NODE END
      END
!
! That is all for HPFTEST35_node.f
!
