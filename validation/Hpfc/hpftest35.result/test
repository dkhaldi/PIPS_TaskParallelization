      integer 
     $    HPFTEST35 TC LO1,
     $    HPFTEST35 TC UP1
      parameter(HPFTEST35 TC LO1 = 0)
      parameter(HPFTEST35 TC UP1 = 126)
      integer 
     $    HPFTEST35 TC LO2,
     $    HPFTEST35 TC UP2
      parameter(HPFTEST35 TC LO2 = 0)
      parameter(HPFTEST35 TC UP2 = 126)
      integer 
     $    HPFTEST35 TS LO1,
     $    HPFTEST35 TS UP1
      parameter(HPFTEST35 TS LO1 = 1)
      parameter(HPFTEST35 TS UP1 = 125)
      integer 
     $    HPFTEST35 TS LO2,
     $    HPFTEST35 TS UP2
      parameter(HPFTEST35 TS LO2 = 1)
      parameter(HPFTEST35 TS UP2 = 125)
      integer 
     $    HPFTEST35 NORTH LO1,
     $    HPFTEST35 NORTH UP1
      parameter(HPFTEST35 NORTH LO1 = 1)
      parameter(HPFTEST35 NORTH UP1 = 125)
!
