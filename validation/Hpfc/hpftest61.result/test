!
! File HPFTEST61_node.f
! This file has been automatically generated by the HPF compiler
!
      SUBROUTINE NODE
      implicit none
      include "global_parameters.h"
      include "hpfc_commons.h"
      include "hpfc_includes.h"
      include "HPFTEST61_parameters.h"
      INTEGER*4 N
      PARAMETER (N = 10)
      INTEGER ALPHA1, ALPHA2, LALPHA1, LALPHA2, PSI1, PSI2, I_0, I_1
     &, I_2, I_3, I_4, I_5, I_6, I_7, M, I, J
      INTEGER A(1:5, 1:5)
      CALL HPFC INIT NODE
      CALL HPFC LOOP BOUNDS(I_5, I_6, I_7, 1, N, 1, 1)
      CALL HPFC LOOP BOUNDS(I_1, I_2, I_3, 1, N, 1, 2)
! updating shared variable M
      BUF INDEX = 0
      RECEIVED NOT PERFORMED = .FALSE.
      SIZE OF RECEIVED BUFFER = 0
      CALL HPFC NCAST
      CALL HPFC INTEGER4 BUFUPK
      BUF INDEX = BUF INDEX+1
      M = HPFC INTEGER4 BUFF(BUF INDEX)
! end of update
      I = I_3
      DO I_0 = I_1, I_2
         I = I+1
         J = I_7
         DO I_4 = I_5, I_6
            J = J+1
            A(I_4,I_0) = M+I+J
         ENDDO
      ENDDO
! collecting distributed variable A
      PSI1 = MY POS(1,1)
      PSI2 = MY POS(2,1)
      BUF INDEX = 0
      SEND NOT INITIALIZED = .TRUE.
      DO ALPHA2 = 5*PSI2-4, MIN(10, 5*PSI2)
         DO ALPHA1 = 5*PSI1-4, MIN(10, 5*PSI1)
            LALPHA1 = ALPHA1-5*PSI1+5
            LALPHA2 = ALPHA2-5*PSI2+5
            BUF INDEX = BUF INDEX+1
            HPFC INTEGER4 BUFF(BUF INDEX) = A(LALPHA1,LALPHA2)
         ENDDO
      ENDDO
      CALL HPFC INTEGER4 BUFPCK
      CALL HPFC SND TO HOST
! end of collect
! updating shared variable I
      BUF INDEX = 0
      RECEIVED NOT PERFORMED = .FALSE.
      SIZE OF RECEIVED BUFFER = 0
      CALL HPFC NCAST
      CALL HPFC INTEGER4 BUFUPK
      BUF INDEX = BUF INDEX+1
      I = HPFC INTEGER4 BUFF(BUF INDEX)
! end of update
! updating shared variable J
      BUF INDEX = 0
      RECEIVED NOT PERFORMED = .FALSE.
      SIZE OF RECEIVED BUFFER = 0
      CALL HPFC NCAST
      CALL HPFC INTEGER4 BUFUPK
      BUF INDEX = BUF INDEX+1
      J = HPFC INTEGER4 BUFF(BUF INDEX)
! end of update
      CALL HPFC NODE END
      END
!
! That is all for HPFTEST61_node.f
!
