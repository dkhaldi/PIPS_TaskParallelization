      SUBROUTINE NODE
      implicit none
      include "global_parameters.h"
      include "hpfc_commons.h"
      include "hpfc_includes.h"
      include "HPFTEST07_parameters.h"
      INTEGER I_10, I_12, I_14, I_16, I_18, I_20, I_21, I_5, I_7, I_8
     &, I_19, I_17, I_15, I_13, I_11, I_9, I_6, J, I, I_0, I_1, I_2, 
     &I_3, I_4
      INTEGER G(1:5)
      LOGICAL L_0
      CALL HPFC INIT NODE
      J = 2
      CALL HPFC CMPCOMPUTER(1, 1, 0, 0, 0, 0, 0, 0)
      IF (HPFC COMPUTERP()) THEN
         I_5 = MOD(1-1, 5)+1
         G(I_5) = 1
      ENDIF
      DO I = J, 10
         I_1 = I_6
         I_0 = I-1
         CALL HPFC CMPCOMPUTER(1, I_1, 0, 0, 0, 0, 0, 0)
         IF (HPFC COMPUTERP()) THEN
            CALL HPFC CMPOWNERS(1, I_0, 0, 0, 0, 0, 0, 0)
            IF (HPFC OWNERP()) THEN
               I_7 = MOD(I_0-1, 5)+1
               I_6 = G(I_7)
            ELSE
               CALL HPFC RCVFR S(HPFC INTEGER4, I_6)
            ENDIF
            I_9 = I
            I_10 = MOD(I_1-1, 5)+1
            G(I_10) = I_9
         ELSE
            CALL HPFC CMPOWNERS(1, I_0, 0, 0, 0, 0, 0, 0)
            IF (HPFC SENDERP()) THEN
               I_8 = MOD(I_0-1, 5)+1
               CALL HPFC SNDTO C(HPFC INTEGER4, G(I_8))
            ENDIF
         ENDIF
      ENDDO
      CALL HPFC CMPOWNERS(1, 1, 0, 0, 0, 0, 0, 0)
      IF (HPFC OWNERP()) THEN
         I_12 = MOD(1-1, 5)+1
         I_11 = G(I_12)
         CALL HPFC NSNDTO HA(HPFC INTEGER4, I_11)
      ELSE
         CALL HPFC RCVFR HNBCAST S(HPFC INTEGER4, I_11)
      ENDIF
      I_2 = I_11+1
      CALL HPFC CMPOWNERS(1, I_2, 0, 0, 0, 0, 0, 0)
      IF (HPFC OWNERP()) THEN
         I_14 = MOD(I_2-1, 5)+1
         I_13 = G(I_14)
         CALL HPFC NSNDTO HA(HPFC INTEGER4, I_13)
      ELSE
         CALL HPFC RCVFR HNBCAST S(HPFC INTEGER4, I_13)
      ENDIF
      I_3 = I_13-1
      CALL HPFC CMPOWNERS(1, 1, 0, 0, 0, 0, 0, 0)
      IF (HPFC OWNERP()) THEN
         I_16 = MOD(1-1, 5)+1
         I_15 = G(I_16)
         CALL HPFC NSNDTO HA(HPFC INTEGER4, I_15)
      ELSE
         CALL HPFC RCVFR HNBCAST S(HPFC INTEGER4, I_15)
      ENDIF
      CALL HPFC CMPOWNERS(1, I_3, 0, 0, 0, 0, 0, 0)
      IF (HPFC OWNERP()) THEN
         I_18 = MOD(I_3-1, 5)+1
         I_17 = G(I_18)
         CALL HPFC NSNDTO HA(HPFC INTEGER4, I_17)
      ELSE
         CALL HPFC RCVFR HNBCAST S(HPFC INTEGER4, I_17)
      ENDIF
      L_0 = I_17.EQ.I_15
      IF (L_0) THEN
         CALL HPFC CMPOWNERS(1, 1, 0, 0, 0, 0, 0, 0)
         IF (HPFC OWNERP()) THEN
            I_20 = MOD(1-1, 5)+1
            I_19 = G(I_20)
            CALL HPFC NSNDTO HA(HPFC INTEGER4, I_19)
         ELSE
            CALL HPFC RCVFR HNBCAST S(HPFC INTEGER4, I_19)
         ENDIF
         I_4 = I_19
         CALL HPFC CMPCOMPUTER(1, I_4, 0, 0, 0, 0, 0, 0)
         IF (HPFC COMPUTERP()) THEN
            I_21 = MOD(I_4-1, 5)+1
            G(I_21) = 1000
         ENDIF
      ENDIF
      IF (J.EQ.1) J = 1000
      CALL HPFC NODE END
      END
