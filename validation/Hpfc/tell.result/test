      SUBROUTINE HOST
      implicit none
      include "global_parameters.h"
      include "hpfc_commons.h"
      include "hpfc_includes.h"
      include "TELL_parameters.h"
      CALL HPFC INIT HOST

      CALL HPFC HTELL('tell one')
      CALL HPFC HTELL('tell two')
      CALL HPFC HOST END
      END
!
! That is all for TELL_host.f
