!
! File TELL_node.f
! This file has been automatically generated by the HPF compiler
!
      SUBROUTINE NODE
      implicit none
      include "global_parameters.h"
      include "hpfc_commons.h"
      include "hpfc_includes.h"
      include "TELL_parameters.h"
      INTEGER I_0, I
      REAL*4 A(1:5)
      CALL HPFC INIT NODE

      CALL HPFC NTELL('tell one')
      DO I = 1, 10
         CALL HPFC CMPCOMPUTER(1, I, 0, 0, 0, 0, 0, 0)
         IF (HPFC COMPUTERP()) THEN
            I_0 = MOD(I-1, 5)+1
            A(I_0) = I*0.333
         ENDIF
      ENDDO
      CALL HPFC NTELL('tell two')
      CALL HPFC NODE END
      END
!
! That is all for TELL_node.f
!
