      SUBROUTINE HOST
      implicit none
      include "global_parameters.h"
      include "hpfc_commons.h"
      include "hpfc_includes.h"
      include "HPFTEST19_parameters.h"
      INTEGER ALPHA1, LALPHA1, PSI1, T LID, D, I
      INTEGER J(1:10)
      CALL HPFC INIT HOST
      PRINT *, 'hpftest19 running'
1000  FORMAT(I6)
      PRINT *, 'please enter base'
      READ (5, 1000) D
! updating shared variable D
      BUF INDEX = 0
      SEND NOT INITIALIZED = .TRUE.
      BUF INDEX = BUF INDEX+1
      HPFC INTEGER4 BUFF(BUF INDEX) = D
      CALL HPFC INTEGER4 BUFPCK
      CALL HPFC HCAST
! end of update
! collecting distributed variable J
      DO PSI1 = 1, 3
         BUF INDEX = 0
         RECEIVED NOT PERFORMED = .FALSE.
         SIZE OF RECEIVED BUFFER = 0
         T LID = PSI1
         CALL HPFC RCV FROM NODE(T LID)
         CALL HPFC INTEGER4 BUFUPK
         DO ALPHA1 = 4*PSI1-3, MIN(10, 4*PSI1)
            LALPHA1 = ALPHA1-4*PSI1+4
            BUF INDEX = BUF INDEX+1
            J(ALPHA1) = HPFC INTEGER4 BUFF(BUF INDEX)
         ENDDO
      ENDDO
! end of collect
      DO I = 1, 10
         PRINT *, 'j(', I, ') = ', J(I)
      ENDDO
! updating shared variable I
      BUF INDEX = 0
      SEND NOT INITIALIZED = .TRUE.
      BUF INDEX = BUF INDEX+1
      HPFC INTEGER4 BUFF(BUF INDEX) = I
      CALL HPFC INTEGER4 BUFPCK
      CALL HPFC HCAST
! end of update
      PRINT *, 'hpftest19 ended'
      CALL HPFC HOST END
      END
!
! That is all for HPFTEST19_host.f
