!
! File HPFTEST46_node.f
! This file has been automatically generated by the HPF compiler
!
      SUBROUTINE NODE
      implicit none
      include "global_parameters.h"
      include "hpfc_commons.h"
      include "hpfc_includes.h"
      include "HPFTEST46_parameters.h"
      INTEGER*4 N
      PARAMETER (N = 100)
      INTEGER I_1, I_10, I_11, I_12, I_13, I_14, I_15, I_16, I_2, I_3
     &, I_4, I_5, I_6, I_7, I_8, I_9, J
      REAL*4 A(-1:99, 1:50), B(1:99, 1:50)
      CALL HPFC INIT NODE
      CALL HPFC LOOP BOUNDS(I_14, I_15, I_16, N-2, 2*N-2, 1, 1)
      CALL HPFC LOOP BOUNDS(I_10, I_11, I_12, 1, N, 1, 2)
      CALL HPFC LOOP BOUNDS(I_6, I_7, I_8, 1, N, 2, 1)
      CALL HPFC LOOP BOUNDS(I_2, I_3, I_4, 1, N, 2, 2)
      J = 0
      DO I_1 = I_2, I_3
         DO I_5 = I_6, I_7
            A(I_5,I_1) = 2.0
         ENDDO
      ENDDO

! P(1, 1:2) send A(98:99, 1:50) to (+2)
      IF (MY POS(1,1).EQ.1) THEN
         CALL HPFC CMPNEIGHBOUR(2)
         CALL HPFC REAL4 PACK 2(A, HPFTEST46 A LO1, HPFTEST46 A UP1, 
     &   HPFTEST46 A LO2, HPFTEST46 A UP2, 98, 99, 1, 1, 50, 1)
         CALL HPFC SNDTO N
      ENDIF
! P(2, 1:2) receive A(-1:0, 1:50) from (-2)
      IF (MY POS(1,1).EQ.2) THEN
         CALL HPFC CMPNEIGHBOUR(-2)
         CALL HPFC RCVFR N
         CALL HPFC REAL4 UNPACK 2(A, HPFTEST46 A LO1, HPFTEST46 A UP1
     &   , HPFTEST46 A LO2, HPFTEST46 A UP2, -1, 0, 1, 1, 50, 1)
      ENDIF
      DO I_9 = I_10, I_11
         DO I_13 = I_14, I_15
            B(I_13,I_9) = A(I_13-2,I_9)
         ENDDO
      ENDDO

      CALL HPFC NODE END
      END
!
! That is all for HPFTEST46_node.f
!
