      SUBROUTINE NODE
      implicit none
      include "global_parameters.h"
      include "hpfc_commons.h"
      include "hpfc_includes.h"
      include "CC_parameters.h"
      INTEGER ALPHA1, ALPHA2, LALPHA1, LALPHA2, PSI1, PSI2, I_2, I_3
     &, LALPHAp1, LALPHAp2, T LID, PSIp2, PSIp1
      REAL*4 A(1:50, 1:50), A_1(1:50, 1:50)
      CALL HPFC INIT NODE
! all livenesss for A
      LIVE MAPPING(2) = .FALSE.
      LIVE MAPPING(1) = .FALSE.
! direct remapping for A
      LIVE MAPPING(2) = .TRUE.
      MSTATUS(2) = 2

      CALL HPFC CMPCOMPUTER(2, 1, 1, 0, 0, 0, 0, 0)
      IF (HPFC COMPUTERP()) THEN
         I_2 = HPFC LOCALINDGAMMA(2, 1, 1)
         I_3 = HPFC LOCALINDGAMMA(2, 2, 1)
         A(I_2,I_3) = 1.0
      ENDIF
! begin remappings
      include 'CC_A_2_1_node.h'
! liveness management
! clean live set for A
      LIVE MAPPING(2) = .FALSE.
! end of liveness management
! end remappings
! collecting distributed variable A_1
      PSI1 = MY POS(1,1)
      PSI2 = MY POS(2,1)
      BUF INDEX = 0
      SEND NOT INITIALIZED = .TRUE.
      DO LALPHA1 = 1, -PSI2+2
         DO LALPHA2 = 1, (-PSI1+3)/2
            ALPHA2 = 1
            ALPHA1 = 1
            BUF INDEX = BUF INDEX+1
            HPFC REAL4 BUFF(BUF INDEX) = A_1(LALPHA1,LALPHA2)
         ENDDO
      ENDDO
      CALL HPFC REAL4 BUFPCK
      CALL HPFC SND TO HOST
! end of collect

      CALL HPFC NODE END
      END
!
! That is all for CC_node.f
!
