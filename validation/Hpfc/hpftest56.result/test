! File COMPUTEDATA_node.f
! This file has been automatically generated by the HPF compiler
!
      SUBROUTINE COMPUTEDATA_NODE
      implicit none
      include "global_parameters.h"
      include "hpfc_commons.h"
      include "hpfc_includes.h"
      include "COMPUTEDATA_parameters.h"
      INTEGER*4 N
      PARAMETER (N = 20)
      INTEGER I_2, I_3, I_4, I_5, I
      INTEGER A
      include "DATA_NODE_parameters.h"
      include "DATA_NODE_node.h"
      CALL HPFC LOOP BOUNDS(I_3, I_4, I_5, 2, N-1, 1, 1)
c end of the common declarations
! P(2:4) send A(1, 1) to (-1)
      IF (MY POS(1,3).GE.2) THEN
         CALL HPFC CMPNEIGHBOUR(-1)
         CALL HPFC INTEGER4 PACK 2(A, DATA A LO1, DATA A UP1, 
     &   DATA A LO2, DATA A UP2, 1, 1, 1, 1, 1, 1)
         CALL HPFC SNDTO N
      ENDIF
! P(1:3) send A(5, 1) to (+1)
      IF (MY POS(1,3).LE.3) THEN
         CALL HPFC CMPNEIGHBOUR(1)
         CALL HPFC INTEGER4 PACK 2(A, DATA A LO1, DATA A UP1, 
     &   DATA A LO2, DATA A UP2, 5, 5, 1, 1, 1, 1)
         CALL HPFC SNDTO N
      ENDIF
! P(1:3) receive A(6, 1) from (+1)
      IF (MY POS(1,3).LE.3) THEN
         CALL HPFC CMPNEIGHBOUR(1)
         CALL HPFC RCVFR N
         CALL HPFC INTEGER4 UNPACK 2(A, DATA A LO1, DATA A UP1, 
     &   DATA A LO2, DATA A UP2, 6, 6, 1, 1, 1, 1)
      ENDIF
! P(2:4) receive A(0, 1) from (-1)
      IF (MY POS(1,3).GE.2) THEN
         CALL HPFC CMPNEIGHBOUR(-1)
         CALL HPFC RCVFR N
         CALL HPFC INTEGER4 UNPACK 2(A, DATA A LO1, DATA A UP1, 
     &   DATA A LO2, DATA A UP2, 0, 0, 1, 1, 1, 1)
      ENDIF
      I = I_5
      DO I_2 = I_3, I_4
         I = I+1
         A(I_2,2) = A(I_2-1,1)+A(I_2,1)+A(I_2+1,1)-2*I
      ENDDO
      END
!
! That is all for COMPUTEDATA_node.f
!
!
! File COMPUTEDATA_parameters.h
! This file has been automatically generated by the HPF compiler
!
!
! parameters generated for COMPUTEDATA
!
!
! That is all for COMPUTEDATA_parameters.h
!
!
! File DATA_host.h
! This file has been automatically generated by the HPF compiler
