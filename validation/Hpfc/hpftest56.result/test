      COMMON /DATA_HOST/ A(1:N, 1:2), B
!
! That is all for DATA_host.h
!
!
! File DATA_init.h
! This file has been automatically generated by the HPF compiler
!
!
! Arrays Initializations for DATA
!
!
! initializing array A, number 1
!
      NODIMA(1) = 2
      ATOT(1) = 3

      RANGEA(1, 1, 1) = 1
      RANGEA(1, 1, 2) = 20
      RANGEA(1, 1, 3) = 20
!
      RANGEA(1, 1, 4) = 2
      RANGEA(1, 1, 5) = 5
      RANGEA(1, 1, 6) = 1
      RANGEA(1, 1, 7) = -1

      RANGEA(1, 2, 1) = 1
      RANGEA(1, 2, 2) = 2
      RANGEA(1, 2, 3) = 2
!
      RANGEA(1, 2, 4) = 0

      ALIGN(1, 1, 1) = 1
      ALIGN(1, 1, 2) = 1
      ALIGN(1, 1, 3) = 0
!
! That is all for DATA_init.h
!
