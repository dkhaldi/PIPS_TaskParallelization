!
! File INITDATA_node.f
! This file has been automatically generated by the HPF compiler
!
      SUBROUTINE INITDATA_NODE
      implicit none
      include "global_parameters.h"
      include "hpfc_commons.h"
      include "hpfc_includes.h"
      include "INITDATA_parameters.h"
      INTEGER*4 N
      PARAMETER (N = 20)
      INTEGER I_0, I_1, I_2, I_3, I_4, I_5, I_6, I_7, I
      INTEGER A
      include "DATA_NODE_parameters.h"
      include "DATA_NODE_node.h"
      CALL HPFC LOOP BOUNDS(I_5, I_6, I_7, 1, N, 1, 1)
      CALL HPFC LOOP BOUNDS(I_1, I_2, I_3, 1, N, 1, 1)
c end of the common declarations
      I = I_3
      DO I_0 = I_1, I_2
         I = I+1
         A(I_0,1) = I
      ENDDO
      DO I_4 = I_5, I_6
         A(I_4,2) = -1
      ENDDO
      END
!
! That is all for INITDATA_node.f
!
