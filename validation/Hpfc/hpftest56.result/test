     $    DATA A LO1,
     $    DATA A UP1
      parameter(DATA A LO1 = 0)
      parameter(DATA A UP1 = 6)
      integer 
     $    DATA A LO2,
     $    DATA A UP2
      parameter(DATA A LO2 = 1)
      parameter(DATA A UP2 = 2)
!
