! File HPFTEST29_node.f
! This file has been automatically generated by the HPF compiler
!
      SUBROUTINE NODE
      implicit none
      include "global_parameters.h"
      include "hpfc_commons.h"
      include "hpfc_includes.h"
      include "HPFTEST29_parameters.h"
      INTEGER ALPHA1, LALPHA1, PSI1, I_0, I_1, I_2, I_3, I_4, I_5, 
     &I_6, I_7, I
      REAL*4 A(1:5), B(1:5), D
      CALL HPFC INIT NODE
      CALL HPFC LOOP BOUNDS(I_5, I_6, I_7, 1, 10, 1, 1)
      CALL HPFC LOOP BOUNDS(I_1, I_2, I_3, 1, 10, 2, 1)
! updating shared variable D
      BUF INDEX = 0
      RECEIVED NOT PERFORMED = .FALSE.
      SIZE OF RECEIVED BUFFER = 0
      CALL HPFC NCAST
      CALL HPFC REAL4 BUFUPK
      BUF INDEX = BUF INDEX+1
      D = HPFC REAL4 BUFF(BUF INDEX)
! end of update
      I = I_3
      DO I_0 = I_1, I_2
         I = I+1
         A(I_0) = I
      ENDDO
      DO I_4 = I_5, I_6
         B(I_4) = A(I_4)+D
      ENDDO
! collecting distributed variable B
      PSI1 = MY POS(1,1)
      BUF INDEX = 0
      SEND NOT INITIALIZED = .TRUE.
      DO ALPHA1 = 5*PSI1-4, MIN(10, 5*PSI1)
         LALPHA1 = ALPHA1-5*PSI1+5
         BUF INDEX = BUF INDEX+1
         HPFC REAL4 BUFF(BUF INDEX) = B(LALPHA1)
      ENDDO
      CALL HPFC REAL4 BUFPCK
      CALL HPFC SND TO HOST
! end of collect
! updating shared variable I
      BUF INDEX = 0
      RECEIVED NOT PERFORMED = .FALSE.
      SIZE OF RECEIVED BUFFER = 0
      CALL HPFC NCAST
      CALL HPFC INTEGER4 BUFUPK
      BUF INDEX = BUF INDEX+1
      I = HPFC INTEGER4 BUFF(BUF INDEX)
! end of update
      CALL HPFC NODE END
      END
!
! That is all for HPFTEST29_node.f
