      SUBROUTINE HOST
      implicit none
      include "global_parameters.h"
      include "hpfc_commons.h"
      include "hpfc_includes.h"
      include "HPFTEST29_parameters.h"
      INTEGER ALPHA1, LALPHA1, PSI1, T LID, I
      REAL*4 B(1:10), D
      CALL HPFC INIT HOST
      PRINT *, 'hpftest29 running'
      PRINT *, 'enter real d'
      READ *, D
! updating shared variable D
      BUF INDEX = 0
      SEND NOT INITIALIZED = .TRUE.
      BUF INDEX = BUF INDEX+1
      HPFC REAL4 BUFF(BUF INDEX) = D
      CALL HPFC REAL4 BUFPCK
      CALL HPFC HCAST
! end of update
! collecting distributed variable B
      DO PSI1 = 1, 2
         BUF INDEX = 0
         RECEIVED NOT PERFORMED = .FALSE.
         SIZE OF RECEIVED BUFFER = 0
         T LID = PSI1
         CALL HPFC RCV FROM NODE(T LID)
         CALL HPFC REAL4 BUFUPK
         DO ALPHA1 = 5*PSI1-4, MIN(10, 5*PSI1)
            LALPHA1 = ALPHA1-5*PSI1+5
            BUF INDEX = BUF INDEX+1
            B(ALPHA1) = HPFC REAL4 BUFF(BUF INDEX)
         ENDDO
      ENDDO
! end of collect
      DO I = 1, 10
         PRINT *, 'b(', I, ')=', B(I)
      ENDDO
! updating shared variable I
      BUF INDEX = 0
      SEND NOT INITIALIZED = .TRUE.
      BUF INDEX = BUF INDEX+1
      HPFC INTEGER4 BUFF(BUF INDEX) = I
      CALL HPFC INTEGER4 BUFPCK
      CALL HPFC HCAST
! end of update
      PRINT *, 'hpftest29 ended'
      CALL HPFC HOST END
      END
!
! That is all for HPFTEST29_host.f
