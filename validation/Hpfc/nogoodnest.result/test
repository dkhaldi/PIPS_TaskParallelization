! File NOGOODNEST_node.f
! This file has been automatically generated by the HPF compiler
!
      SUBROUTINE NODE
      implicit none
      include "global_parameters.h"
      include "hpfc_commons.h"
      include "hpfc_includes.h"
      include "NOGOODNEST_parameters.h"
      INTEGER*4 N
      PARAMETER (N = 100)
      INTEGER*4 N
      PARAMETER (N = 100)
      INTEGER I_10, I_11, I_12, I_13, I_14, I_15, I_16, I_17, I_18, 
     &I_19, I_20, I_21, I_22, I_23, I_24, I_25, I_26, I_27, I_28, 
     &I_29, I_6, I_7, I_8, I_9, J, I_LOCAL, I_0, I_1, I_2, I_3, I_4, 
     &I_5
      REAL*4 F_3, F_2, F_1, F_0, U(1:50, 1:N), UH(1:50, 1:N)
      CALL HPFC INIT NODE
      CALL HPFC LOOP BOUNDS(I_24, I_25, I_26, 2, N-1, 1, 1)
      CALL HPFC LOOP BOUNDS(I_17, I_18, I_19, 2, N-1, 1, 1)
      CALL HPFC LOOP BOUNDS(I_10, I_11, I_12, 2, N-1, 1, 1)

      DO J = 2, N-1
         CALL HPFC CMPCOMPUTER(1, 1, J, 0, 0, 0, 0, 0)
         IF (HPFC COMPUTERP()) THEN
            CALL HPFC CMPOWNERS(2, 1, J, 0, 0, 0, 0, 0)
            IF (HPFC OWNERP()) THEN
               I_6 = MOD(1-1, 50)+1
               F_0 = U(I_6,J)
            ELSE
               CALL HPFC RCVFR S(HPFC REAL4, F_0)
            ENDIF
            I_8 = MOD(1-1, 50)+1
            UH(I_8,J) = F_0
         ELSE
            CALL HPFC CMPOWNERS(2, 1, J, 0, 0, 0, 0, 0)
            IF (HPFC SENDERP()) THEN
               I_7 = MOD(1-1, 50)+1
               CALL HPFC SNDTO C(HPFC REAL4, U(I_7,J))
            ENDIF
         ENDIF
         DO I_9 = I_10, I_11
            I_0 = J+1
            I_1 = J-1
            UH(I_9,J) = U(I_9,J)+U(I_9,I_0)+U(I_9,I_1)
         ENDDO
         CALL HPFC CMPCOMPUTER(1, N, J, 0, 0, 0, 0, 0)
         IF (HPFC COMPUTERP()) THEN
            CALL HPFC CMPOWNERS(2, N, J, 0, 0, 0, 0, 0)
            IF (HPFC OWNERP()) THEN
               I_13 = MOD(N-1, 50)+1
               F_1 = U(I_13,J)
            ELSE
               CALL HPFC RCVFR S(HPFC REAL4, F_1)
            ENDIF
            I_15 = MOD(N-1, 50)+1
            UH(I_15,J) = F_1
         ELSE
            CALL HPFC CMPOWNERS(2, N, J, 0, 0, 0, 0, 0)
            IF (HPFC SENDERP()) THEN
               I_14 = MOD(N-1, 50)+1
               CALL HPFC SNDTO C(HPFC REAL4, U(I_14,J))
            ENDIF
         ENDIF
      ENDDO

      DO J = 2, N-1
         IF (MY POS(1,1).EQ.1) THEN
            I_LOCAL = 0
            UH(1,J) = U(1,J)
         ENDIF
         DO I_16 = I_17, I_18
            I_2 = J+1
            I_3 = J-1
            UH(I_16,J) = U(I_16,J)+U(I_16,I_2)+U(I_16,I_3)
         ENDDO
         IF (MY POS(1,1).EQ.2) THEN
            I_LOCAL = 0
            UH(50,J) = U(50,J)
         ENDIF
      ENDDO

      DO J = 2, N-1
         CALL HPFC CMPCOMPUTER(1, 1, J, 0, 0, 0, 0, 0)
         IF (HPFC COMPUTERP()) THEN
            CALL HPFC CMPOWNERS(2, 1, J, 0, 0, 0, 0, 0)
            IF (HPFC OWNERP()) THEN
               I_20 = MOD(1-1, 50)+1
               F_2 = U(I_20,J)
            ELSE
               CALL HPFC RCVFR S(HPFC REAL4, F_2)
            ENDIF
            I_22 = MOD(1-1, 50)+1
            UH(I_22,J) = F_2
         ELSE
            CALL HPFC CMPOWNERS(2, 1, J, 0, 0, 0, 0, 0)
            IF (HPFC SENDERP()) THEN
               I_21 = MOD(1-1, 50)+1
               CALL HPFC SNDTO C(HPFC REAL4, U(I_21,J))
            ENDIF
         ENDIF
         DO I_23 = I_24, I_25
            I_4 = J+1
            I_5 = J-1
            UH(I_23,J) = U(I_23,J)+U(I_23,I_4)+U(I_23,I_5)
         ENDDO
         CALL HPFC CMPCOMPUTER(1, N, J, 0, 0, 0, 0, 0)
         IF (HPFC COMPUTERP()) THEN
            CALL HPFC CMPOWNERS(2, N, J, 0, 0, 0, 0, 0)
            IF (HPFC OWNERP()) THEN
               I_27 = MOD(N-1, 50)+1
               F_3 = U(I_27,J)
            ELSE
               CALL HPFC RCVFR S(HPFC REAL4, F_3)
            ENDIF
            I_29 = MOD(N-1, 50)+1
            UH(I_29,J) = F_3
         ELSE
            CALL HPFC CMPOWNERS(2, N, J, 0, 0, 0, 0, 0)
            IF (HPFC SENDERP()) THEN
               I_28 = MOD(N-1, 50)+1
               CALL HPFC SNDTO C(HPFC REAL4, U(I_28,J))
            ENDIF
         ENDIF
      ENDDO

      CALL HPFC NODE END
      END
!
! That is all for NOGOODNEST_node.f
