int main(int argc, char **argv)
{
   int i, j;
   int n = atoi(argv[1]);
   int a[n][n];
   {
      int i;

      for(i = 1; i <= n-1; i += 1) {
         int j;
         for(j = i; j <= n-1; j += 1)
            a[i][j] = a[i-1][j-1]+a[i][j-1];
      }
   }
}
