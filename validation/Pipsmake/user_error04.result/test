C             <    is read   >: X Y __IO_EFFECTS:LUNS(6)
C             <    is written>: __IO_EFFECTS:LUNS(6)

      SUBROUTINE B(X, Y)
C             <    is read   >: X Y __IO_EFFECTS:LUNS(6)
C             <    is written>: __IO_EFFECTS:LUNS(6)

      PRINT *, X+Y                                                      0004

      END
