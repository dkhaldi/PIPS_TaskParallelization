int find_do_while(int i)
{
begin:   i = 8*i;                                                             /*0004*/
   i++;                                                                 /*0005*/
   if (i<9) {                                                           /*0006*/
   }
   else {
      goto begin;
   }

the_end:   return i;
}
int find_do_while(int i)
{
   do {                                                                 /*0006*/
begin:      i = 8*i;                                                          /*0004*/
      i++;                                                              /*0005*/
   }
   while (!(i<9));                                                      /*0006*/

the_end:   return i;
}
