
Points-to for "foo2"

int foo2(float b[5][3])
{
   float c;
   (*b)[3] = 2.0;
   c = (*b)[3];
   b[1][3] = 2.0;
   c = b[1][3];

   (*b)[3]++;
   (*b)[3] += 5.0;
   b[1][3]++;
   b[1][3] += 5.0;

   return 1;
}

Proper effects for "foo2"

int foo2(float b[5][3])
{
   float c;
//             <    is written>: b[0][3]
   (*b)[3] = 2.0;
//             <    is read   >: b[0][3]
//             <    is written>: c
   c = (*b)[3];
//             <    is written>: b[1][3]
   b[1][3] = 2.0;
//             <    is read   >: b[1][3]
//             <    is written>: c
   c = b[1][3];
//             <    is read   >: b[0][3]
//             <    is written>: b[0][3]

   (*b)[3]++;
//             <    is read   >: b[0][3]
//             <    is written>: b[0][3]
   (*b)[3] += 5.0;
//             <    is read   >: b[1][3]
//             <    is written>: b[1][3]
   b[1][3]++;
//             <    is read   >: b[1][3]
//             <    is written>: b[1][3]
   b[1][3] += 5.0;

   return 1;
}

