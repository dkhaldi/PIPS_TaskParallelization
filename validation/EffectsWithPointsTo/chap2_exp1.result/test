
PROPER  EFFECTS 

extern void main();
void main()
{
   int **a, *b, c;
//             <    is written>: b
   b = &c;
//             <    is written>: a
   a = &b;
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*_b0
//             <may be written>: *ANY_MODULE*:*ANYWHERE*_b0
//             <    is read   >: a
   **a = 7;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none


// Points To: none

extern void main();

// Points To IN: none


// Points To OUT: none

void main()
{

// Points To: none

   int **a, *b, c;

// Points To:
// a -> undefined , EXACT
// b -> undefined , EXACT

   b = &c;

// Points To:
// a -> undefined , EXACT
// b -> c , EXACT

   a = &b;

// Points To:
// a -> b , EXACT
// b -> c , EXACT

   **a = 7;
}

PROPER  EFFECTS WITH POINTS TO

extern void main();
void main()
{
   int **a, *b, c;
//             <    is written>: b
   b = &c;
//             <    is written>: a
   a = &b;
//             <    is read   >: a b
//             <    is written>: c
   **a = 7;
}

 *********************** Use-Def Chains *********************

	03 --> 05 with conflicts
		from <b-W-EXACT> to <b-R-EXACT>
	04 --> 05 with conflicts
		from <a-W-EXACT> to <a-R-EXACT>

****************** End of Dependence Graph ******************
