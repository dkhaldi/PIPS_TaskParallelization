
int struct04()
{
   struct one {
      int first;
      int second;
   } x;
   int y[10];
//             <    is read   >: x.second
//             <    is written>: x.first
   
   //  x.first = 1;
   //x.second = 2;
   
   //y[0] = 1;
   
   x.first = x.second+1;
}

