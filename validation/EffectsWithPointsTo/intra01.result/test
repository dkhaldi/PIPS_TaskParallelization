
PROPER  EFFECTS 

void bar(int **fpp, int **fqq)
{
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*_b0
//             <may be written>: *ANY_MODULE*:*ANYWHERE*_b0
//             <    is read   >: fpp fqq
   *fpp = *fqq;
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   printf("pointers copied");
   return;
}
void foo()
{
//             <    is written>: i j p pp q qq
   int i = 0, j = 1, *p = &i, *q = &j, **pp = &p, **qq = &q;
//             <may be read   >: stdout[0] *ANY_MODULE*:*ANYWHERE*_b0
//             <may be written>: stdout[0] *ANY_MODULE*:*ANYWHERE*_b0
//             <    is read   >: pp qq stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]

   bar(pp, qq);
   return;
}

CUMULATED  EFFECTS 

//             <may be read   >: stdout[0] *ANY_MODULE*:*ANYWHERE*_b0
//             <may be written>: stdout[0] *ANY_MODULE*:*ANYWHERE*_b0
//             <    is read   >: fpp fqq stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
void bar(int **fpp, int **fqq)
{
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*_b0
//             <may be written>: *ANY_MODULE*:*ANYWHERE*_b0
//             <    is read   >: fpp fqq
   *fpp = *fqq;
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   printf("pointers copied");
   return;
}
//             <may be read   >: stdout[0] *ANY_MODULE*:*ANYWHERE*_b0
//             <may be written>: stdout[0] *ANY_MODULE*:*ANYWHERE*_b0
//             <    is read   >: stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
void foo()
{
//             <    is written>: i j p pp q qq
   int i = 0, j = 1, *p = &i, *q = &j, **pp = &p, **qq = &q;
//             <may be read   >: stdout[0] *ANY_MODULE*:*ANYWHERE*_b0
//             <may be written>: stdout[0] *ANY_MODULE*:*ANYWHERE*_b0
//             <    is read   >: pp qq stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]

   bar(pp, qq);
   return;
}

POINTS-TO


// Points To IN:
// _fqq_2 -> *NULL* , MAY
// _fqq_2 -> _fqq_2_2 , MAY
// fpp -> *NULL* , MAY
// fpp -> _fpp_1 , MAY
// fqq -> *NULL* , MAY
// fqq -> _fqq_2 , MAY


// Points To OUT:
// _fpp_1 -> *NULL* , MAY
// _fpp_1 -> _fqq_2_2 , MAY
// _fqq_2 -> *NULL* , MAY
// _fqq_2 -> _fqq_2_2 , MAY
// fpp -> _fpp_1 , EXACT
// fqq -> _fqq_2 , EXACT

void bar(int **fpp, int **fqq)
{

// Points To:
// _fqq_2 -> *NULL* , MAY
// _fqq_2 -> _fqq_2_2 , MAY
// fpp -> *NULL* , MAY
// fpp -> _fpp_1 , MAY
// fqq -> *NULL* , MAY
// fqq -> _fqq_2 , MAY

   *fpp = *fqq;

// Points To:
// _fpp_1 -> *NULL* , MAY
// _fpp_1 -> _fqq_2_2 , MAY
// _fqq_2 -> *NULL* , MAY
// _fqq_2 -> _fqq_2_2 , MAY
// fpp -> _fpp_1 , EXACT
// fqq -> _fqq_2 , EXACT

   printf("pointers copied");

// Points To:
// _fpp_1 -> *NULL* , MAY
// _fpp_1 -> _fqq_2_2 , MAY
// _fqq_2 -> *NULL* , MAY
// _fqq_2 -> _fqq_2_2 , MAY
// fpp -> _fpp_1 , EXACT
// fqq -> _fqq_2 , EXACT

   return;
}

// Points To IN: none


// Points To OUT: none

void foo()
{

// Points To: none

   int i = 0, j = 1, *p = &i, *q = &j, **pp = &p, **qq = &q;

// Points To:
// p -> i , EXACT
// pp -> p , EXACT
// q -> j , EXACT
// qq -> q , EXACT


   bar(pp, qq);

// Points To:
// p -> *NULL* , MAY
// p -> j , MAY
// pp -> p , EXACT
// q -> j , EXACT
// qq -> q , EXACT

   return;
}
