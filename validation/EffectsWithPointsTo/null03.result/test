
PROPER  EFFECTS 

int main()
{
//             <    is written>: p
   int **p = (void *) 0;
   
   
   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int **p = (void *) 0;

// Points To:
// p -> *NULL* , EXACT

   
   
   return 0;
}

PROPER EFFECTS WITH POINTS TO

int main()
{
//             <    is written>: p
   int **p = (void *) 0;
   
   
   return 0;
}
