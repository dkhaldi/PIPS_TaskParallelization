
int main()
{
   int i;
   int *p, *q;
   int a[10];
   int b[10];
//             <    is written>: p

   p = &a[0];
//             <    is written>: q
   q = &b[0];
//             <    is written>: i
   for(i = 0; i <= 9; i += 1)
//             <may be read   >: b[0] b[1] b[2] b[3] b[*]
//             <may be written>: a[0] a[1] a[2] a[3] a[*]
//             <    is read   >: p q
//             <    is written>: p q
      *p++ = 2**q++;

   return 0;
}

