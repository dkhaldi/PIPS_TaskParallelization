		from <p-W-EXACT> to <p-W-EXACT>
		from <a[3]-W-MAY> to <a[3]-W-MAY>
		from <a[3]-W-MAY> to <a[*]-W-MAY>
		from <a[2]-W-MAY> to <a[2]-W-MAY>
		from <a[2]-W-MAY> to <a[*]-W-MAY>
		from <a[1]-W-MAY> to <a[1]-W-MAY>
		from <a[1]-W-MAY> to <a[*]-W-MAY>
