      PROGRAM UNROLL07
C
      PARAMETER (N=4)
      REAL T(N,N)
Cxxx
      DO 10 I = 1, N                                                    0006
Cxxx
         DO 10 J = 1, N                                                 0008
            T(I,J) = -N*I+J                                             0009
10          CONTINUE                                                    0010
C
      END
      PROGRAM UNROLL07
C
      PARAMETER (N=4)
      REAL T(N,N)
Cxxx
      T(1,1) = -N*1+1
      T(1,2) = -N*1+2
      T(1,3) = -N*1+3
      T(1,4) = -N*1+4
      T(2,1) = -N*2+1
      T(2,2) = -N*2+2
      T(2,3) = -N*2+3
      T(2,4) = -N*2+4
      T(3,1) = -N*3+1
      T(3,2) = -N*3+2
      T(3,3) = -N*3+3
      T(3,4) = -N*3+4
      T(4,1) = -N*4+1
      T(4,2) = -N*4+2
      T(4,3) = -N*4+3
      T(4,4) = -N*4+4
10    CONTINUE
C
      END
