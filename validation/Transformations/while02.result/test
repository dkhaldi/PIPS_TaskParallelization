### initial code
void while02()
{
   int i = 1;
   while (i<=2)
      i++;
   printf("%d", i);
}
### Preconditions

//  P() {}

void while02()
{

//  P() {}

   int i = 1;

//  P(i) {i==1}

   while (i<=2)

//  P(i) {1<=i, i<=2}

      i++;

//  P(i) {i==3}

   printf("%d", i);
}
### suppress dead code
void while02()
{
   int i = 1;
   while (i<=2)
      i++;
   printf("%d", i);
}

