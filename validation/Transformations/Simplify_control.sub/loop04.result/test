#
# Parsed printed file
#
void loop04()
{
   int i = 0;

   for (i = 3;i<1;i++)
      /* This statement is never executed */
      printf("%d\n", i);

   return;
}
#
# Printed file
#
void loop04()
{
   int i = 0;

   for(i = 3; i <= 0; i += 1)
      /* This statement is never executed */
      printf("%d\n", i);

   return;
}
#
# Cumulated effects
#
void loop04()
{
//             <    is written>: i
   int i = 0;
//             <    is written>: i

   for(i = 3; i <= 0; i += 1)
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: i stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
      /* This statement is never executed */
      printf("%d\n", i);

   return;
}
#
# simplify control
#
void loop04()
{
   int i = 0;

   i = 3;

   return;
}
