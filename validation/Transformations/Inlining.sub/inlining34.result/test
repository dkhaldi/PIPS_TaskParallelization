   int *b = &a[0];
   //PIPS generated variable
   int _return0, _return1, _return2, _return3;
   printf("%d.", 0);
   _return0 = 0;
   _return0;
   {
      //PIPS generated variable
      int I_0;
      I_0 = sizeof(char);
      printf("%d.", I_0);
      _return1 = I_0;
   }
   _return1;
   {
      //PIPS generated variable
      int I_1;
      I_1 = (b+0)[0];
      printf("%d.", I_1);
      _return2 = I_1;
   }
   _return2;
   {
      //PIPS generated variable
