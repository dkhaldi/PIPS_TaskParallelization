char * foo(void)
{
   const char *foo_fun = __FUNCTION__;
   return (char *) foo_fun;
}
int main(void)
{
   const char *fun, *fun2, *fun3;
   fun = foo();
   fun2 = __FUNCTION__;
   fun3 = __func__;
   return 0;
}
extern char *foo(void);

extern int main(void);
int main(void)
{
   const char *fun, *fun2, *fun3;
   //PIPS generated variable
   char *_return0;
   {
      const char *foo_fun;
      foo_fun = __FUNCTION__;
      _return0 = (char *) foo_fun;
   }
   fun = _return0;
   fun2 = __FUNCTION__;
   fun3 = __func__;
   return 0;
}
