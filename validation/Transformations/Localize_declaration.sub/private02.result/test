int main()
{
   int i;
   int a[10][10];
   
   // tmp must be declared as private in the two loops
   for(i = 0; i <= 9; i += 1) {
      //PIPS generated variable
      int j, tmp;
      for(j = 0; j <= 9; j += 1) {
         tmp = a[i][j];
         a[i][j] = tmp+j;
      }
      tmp = i*2;
      a[i][i] = a[i][i]+tmp;
   }
   return 0;
}
