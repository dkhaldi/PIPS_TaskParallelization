
"foo" file

int foo(int i)
{
   int j;

   j = 3*i-2*i-i;
   j = 3*i-2*i-i+1;
   j = 1+3*i-2*i-i+1;
   j = 1+i;
   j = i+1;
   return j;
}

"foo" file with preconditions


//  P() {}

int foo(int i)
{

//  P() {}

   int j;

//  P() {}


   j = 3*i-2*i-i;

//  P(j) {j==0}

   j = 3*i-2*i-i+1;

//  P(j) {j==1}

   j = 1+3*i-2*i-i+1;

//  P(j) {j==2}

   j = 1+i;

//  P(j) {i==j-1}

   j = i+1;

//  P(j) {i==j-1}

   return j;
}

"foo" file after partial evaluation

int foo(int i)
{
   int j;

   j = 0;
   j = 1;
   j = 2;
   j = i+1;
   j = i+1;
   return j;
}
