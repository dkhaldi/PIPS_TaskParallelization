
Initial source code

void loop_interchange02(int n, int ni, int nj, int nk)
{
   float x[n][n][n];
   int i, j, k;

l1:
   for(i = 0; i <= ni-1; i += 1)
l2:
      for(j = 0; j <= nj-1; j += 1)
l3:
         for(k = 0; k <= nk-1; k += 1)
            x[i][j][k] = 0.;
}

Source code after loop interchange

void loop_interchange02(int n, int ni, int nj, int nk)
{
   float x[n][n][n];
   int i, j, k;
   //PIPS generated variable
   int jp, kp;

l1:
   for(i = 0; i <= ni-1; i += 1)
l2:
      for(jp = 0; jp <= nk-1; jp += 1)
         for(kp = 0; kp <= nj-1; kp += 1)
            x[i][kp][jp] = 0.;
}

