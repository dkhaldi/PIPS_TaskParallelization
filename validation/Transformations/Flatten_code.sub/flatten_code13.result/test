int flatten_code13(void)
{
   int i1, i2;
   //PIPS generated variable
   int _return0;
   //PIPS generated variable
   int _return1;
   {
      extern int bla(void);
      _return0 = bla();
   }
   i1 = _return0;
   {
      extern int bla(void);
      _return1 = bla();
   }
   i2 = _return1;
   return i1+i2;
}
