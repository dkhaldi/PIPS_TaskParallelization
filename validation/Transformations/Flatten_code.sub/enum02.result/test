
"Initial Code"

void enum02(void)
{
   int i;
   {
      int i;
      enum foo {zero, un, deux};
      enum foo j;
      j = un;
   }
   {
      int i;
      enum foo {zero, one, two};
      enum foo j;
      j = one;
   }
   return;
}

"FLATTEN_CODE"

void enum02(void)
{
   int i;
   //PIPS generated variable
   int i_0;
   //PIPS generated variable
   enum foo {zero, un, deux};
   //PIPS generated variable
   enum foo j_0;
   //PIPS generated variable
   int i_1;
   //PIPS generated variable
   enum foo_0 {zero, one, two};
   //PIPS generated variable
   enum foo_0 j_1;
   j_0 = un;
   j_1 = one;
   return;
}

