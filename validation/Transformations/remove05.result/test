int main(int argc, char *argv[])
{
   int j;
   {
      int i;
      for(i = 1; i <= argc-1; i += 1) {
         int n = atoi(argv[i]);
         int m = 0;
rof:
         for(j = 0; j <= n-1; j += 1)
            m += j;
         printf("%d\n", m);
      }
   }
   return 0;
}
