setenv WSPACE=for20
delete $WSPACE
create $WSPACE $WSPACE.c 

setproperty ABORT_ON_USER_ERROR TRUE

setproperty MEMORY_EFFECTS_ONLY FALSE

setproperty PRETTYPRINT_SCALAR_REGIONS TRUE
# setenv LOOP_BOUND_MINIMIZATION_DEBUG_LEVEL 5

activate MUST_REGIONS

echo //
echo // Init Code
echo //
display PRINTED_FILE[main]

echo //
echo // OUT regions for "$WSPACE"
echo //
activate PRINT_CODE_OUT_REGIONS
display PRINTED_FILE[main]

echo //
echo // Source code for "$WSPACE" after LOOP_BOUND_MINIMIZATION_WITH_OUT_REGIONS
echo //
apply LOOP_BOUND_MINIMIZATION_WITH_OUT_REGIONS[main]
activate PRINT_CODE
display PRINTED_FILE[main]

close
echo
echo Warnings
echo
shell cat $WSPACE.database/Warnings

delete $WSPACE
quit
