int foo()
{
   int n, i;
   scanf("%d", &n);
   int a[100];
   //if (n<=100)
   for(i = 1; i <= n-1; i += 1)
      a[i] = 0;
   return 0;
}

ARRAY_RESIZING_FULL_BOTTOM_UP

int foo()
{
   int n, i;
   scanf("%d", &n);
   int a[99];
   //if (n<=100)
   for(i = 1; i <= n-1; i += 1)
      a[i-1] = 0;
   return 0;
}

