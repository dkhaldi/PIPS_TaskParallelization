ARRAY_RESIZING_FULL_BOTTOM_UP

int foo(int n, int m)
{
   int i;
   int a[-m+98+1];
   for(i = n; i <= 99; i += 1)
      if (i>n&&i>m)
         a[i-m-1] = 0;
   
   //   for(i=0; i<n; i++)
   //     if (i<m)
   //         a[i] = 0;
   
