
Initial code

void foo(int bar[256][256])
{
   int i, j;
   for(i = 0; i <= 255; i += 1)
      for(j = 0; j <= 255; j += 1)
kernel:         bar[i][j] = i*j;
}
extern void foo(int bar[256][256]);

Code after outlining

void foo(int bar[256][256])
{
   int i, j;
   for(i = 0; i <= 255; i += 1)
      for(j = 0; j <= 255; j += 1)
kernel:         kernel(i, j, bar);
}
void kernel(int i, int j, int bar[256][256])
{
kernel:   bar[i][j] = i*j;
}
extern void foo(int bar[256][256]);
//PIPS generated variable
void kernel(int i, int j, int bar[256][256]);
