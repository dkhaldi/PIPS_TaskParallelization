void * safe_malloc(size_t n)
{
   void *ptr = malloc(n*sizeof(*ptr));                                  /*0006*/
   int i;                                                               /*0007*/

   if (!ptr) {                                                          /*0009*/
      exit(1);                                                          /*0010*/
      exit(1);                                                          /*0011*/
   }
   else
      return ptr;                                                       /*0014*/
}
