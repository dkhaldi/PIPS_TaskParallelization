int main(void)
{
   printf("on line 14\n");                                              /*0014*/
#pragma BLA
   printf("BLA: on line 17\n");                                         /*0017*/
   return 0;                                                            /*0018*/
}
int test(void)
{
   printf("on line 6\n");                                               /*0006*/
#pragma FOO
   printf("FOO: on line 8\n");                                          /*0008*/
   return 0;                                                            /*0009*/
}

