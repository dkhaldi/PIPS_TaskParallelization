
Printed file for main

int main()
{
   char c1;
   unsigned char c2;
   signed char c3;
   int i1, i2, i3, r;

   c1 = '\\';
   c2 = '\'';
   c3 = '\012';

   i1 = c1;
   i2 = c2;
   i3 = c3;

   r = i1+i2+i3;
}

Preconditions for main


//  P() {}

int main()
{

//  P() {}

   char c1;

//  P() {}

   unsigned char c2;

//  P() {}

   signed char c3;

//  P() {}

   int i1, i2, i3, r;

//  P() {}


   c1 = '\\';

//  P(c1) {c1==92}

   c2 = '\'';

//  P(c1,c2) {c1==92, c2==39}

   c3 = '\012';

//  P(c1,c2,c3) {c1==92, c2==39, c3==10}


   i1 = c1;

//  P(c1,c2,c3,i1) {c1==92, c2==39, c3==10, i1==92}

   i2 = c2;

//  P(c1,c2,c3,i1,i2) {c1==92, c2==39, c3==10, i1==92, i2==39}

   i3 = c3;

//  P(c1,c2,c3,i1,i2,i3) {c1==92, c2==39, c3==10, i1==92, i2==39,
//    i3==10}


   r = i1+i2+i3;
}
