
Parser output

int main()
{
   int i, j, k, l;
   i = 015;
   // octal constant
   j = 0x1ae;
   // hexadecimal 
   k = 890L;
   // long constant
   l = i+j+k;
}

Controlizer output

int main()
{
   int i, j, k, l;
   i = 015;
   // octal constant
   j = 0x1ae;
   // hexadecimal 
   k = 890L;
   // long constant
   l = i+j+k;
}

Preconditions


//  P() {}

int main()
{

//  P() {}

   int i, j, k, l;

//  P() {}

   i = 015;

//  P(i) {i==13}

   // octal constant
   j = 0x1ae;

//  P(i,j) {i==13, j==430}

   // hexadecimal 
   k = 890L;

//  P(i,j,k) {i==13, j==430, k==890}

   // long constant
   l = i+j+k;
}

