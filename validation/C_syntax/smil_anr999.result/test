extern long double __coshl(long double __x);                            /*0149*/
/* Hyperbolic sine of X.  */
extern long double sinhl(long double __x);                              /*0149*/
extern long double __sinhl(long double __x);                            /*0149*/
/* Hyperbolic tangent of X.  */
extern long double tanhl(long double __x);                              /*0149*/
extern long double __tanhl(long double __x);                            /*0149*/



