/* The offsetof() macro calculates the offset of a structure member
   in its structure.  Unfortunately this cannot be written down
   portably, hence it is provided by a Standard C header file.
   For pre-Standard C compilers, here is a version that usually works
   (but watch out!): */





/* An array of memberlist structures defines the name, type and offset
   of selected members of a C structure.  These can be read by
   PyMember_Get() and set by PyMember_Set() (except if their READONLY flag
   is set).  The array must be terminated with an entry whose name
   pointer is NULL. */

/* Obsolete version, for binary backwards compatibility */
struct memberlist {                                                     /*0650*/
   char *name;
   int type;
   int offset;
   int flags;
};

/* Current version, use this */
typedef struct PyMemberDef PyMemberDef;                                 /*0650*/

/* Types */







/* XXX the ordering here is weird for binary compatibility */


/* unsigned variants: */





/* Added by Jack: strings contained in the structure */


/* Added by Lillo: bools contained in the structure (assumed char) */
/* Flags */







/* Obsolete API, for binary backwards compatibility */
PyObject *PyMember_Get(const char *, struct memberlist *, const char *);               /*0650*/
int PyMember_Set(char *, struct memberlist *, const char *, PyObject *);               /*0650*/

/* Current API, use this */
PyObject *PyMember_GetOne(const char *, struct PyMemberDef *);          /*0650*/
int PyMember_SetOne(char *, struct PyMemberDef *, PyObject *);          /*0650*/
typedef struct {PyCFunctionObject func; PyObject *func_weakreflist; PyObject *func_dict; PyObject *func_name; PyObject *func_qualname; PyObject *func_doc; PyObject *func_globals; PyObject *func_code; PyObject *func_closure; PyObject *func_classobj; void *defaults; int defaults_pyobjects; int flags; PyObject *defaults_tuple; PyObject *defaults_kwdict; PyObject *(*defaults_getter)(PyObject *); PyObject *func_annotations;} __pyx_CyFunctionObject;               /*0691*/
static PyTypeObject *__pyx_CyFunctionType = 0;                          /*0692*/


static PyObject *__Pyx_CyFunction_New(PyTypeObject *type, PyMethodDef *ml, int flags, PyObject *qualname, PyObject *closure, PyObject *module, PyObject *globals, PyObject *code);              /*0699*/
static void *__Pyx_CyFunction_InitDefaults(PyObject *func, size_t size, int pyobjects);               /*0702*/
static void __Pyx_CyFunction_SetDefaultsTuple(PyObject *func, PyObject *tuple);        /*0704*/
static void __Pyx_CyFunction_SetDefaultsKwDict(PyObject *func, PyObject *dict);        /*0706*/
static void __Pyx_CyFunction_SetAnnotationsDict(PyObject *func, PyObject *dict);       /*0708*/
static int __Pyx_CyFunction_init(void);                                 /*0709*/

