/*
 * ISO C Standard:  7.15  Variable arguments  <stdarg.h>
 */

/* Type PyByteArrayObject represents a mutable array of bytes.
 * The Python API is that of a sequence;
 * the bytes are mapped to ints in [0, 256).
 * Bytes are not characters; they may be used to encode characters.
 * The only way to go between bytes and str/unicode is via encoding
 * and decoding.
 * For the convenience of C programmers, the bytes type is considered
 * to contain a char pointer, not an unsigned char pointer.
 */

/* Object layout */
/* XXX(nnorwitz): should ob_exports be Py_ssize_t? */
/* how many buffer exports */
/* How many bytes allocated */
typedef struct {Py_ssize_t ob_refcnt; struct _typeobject *ob_type; Py_ssize_t ob_size; int ob_exports; Py_ssize_t ob_alloc; char *ob_bytes;} PyByteArrayObject;   /*0149*/

/* Type object */
extern PyTypeObject PyByteArray_Type;                                   /*0149*/
extern PyTypeObject PyByteArrayIter_Type;                               /*0149*/

/* Type check macros */



/* Direct API functions */
PyObject *PyByteArray_FromObject(PyObject *);                           /*0149*/
PyObject *PyByteArray_Concat(PyObject *, PyObject *);                   /*0149*/
PyObject *PyByteArray_FromStringAndSize(const char *, Py_ssize_t);      /*0149*/
Py_ssize_t PyByteArray_Size(PyObject *);                                /*0149*/
char *PyByteArray_AsString(PyObject *);                                 /*0149*/
int PyByteArray_Resize(PyObject *, Py_ssize_t);                         /*0149*/

/* Macros, trading safety for speed */





extern char _PyByteArray_empty_string[];                                /*0149*/

/* Tuple object interface */







/*
Another generally useful object type is a tuple of object pointers.
For Python, this is an immutable type.  C code can change the tuple items
(but not their number), and even use tuples are general-purpose arrays of
object references, but in general only brand new tuples should be mutated,
not ones that might already have been exposed to Python code.

*** WARNING *** PyTuple_SetItem does not increment the new item's reference
count, but does decrement the reference count of the item it replaces,
if not nil.  It does *decrement* the reference count if it is *not*
inserted in the tuple.  Similarly, PyTuple_GetItem does not increment the
returned item's reference count.
*/

/* ob_item contains space for 'ob_size' elements.
     * Items must normally not be NULL, except during construction when
     * the tuple is not yet visible outside the function that builds it.
     */
typedef struct {Py_ssize_t ob_refcnt; struct _typeobject *ob_type; Py_ssize_t ob_size; PyObject *ob_item[1];} PyTupleObject;        /*0149*/

extern PyTypeObject PyTuple_Type;                                       /*0149*/





PyObject *PyTuple_New(Py_ssize_t size);                                 /*0149*/
Py_ssize_t PyTuple_Size(PyObject *);                                    /*0149*/
PyObject *PyTuple_GetItem(PyObject *, Py_ssize_t);                      /*0149*/
int PyTuple_SetItem(PyObject *, Py_ssize_t, PyObject *);                /*0149*/
PyObject *PyTuple_GetSlice(PyObject *, Py_ssize_t, Py_ssize_t);         /*0149*/
int _PyTuple_Resize(PyObject **, Py_ssize_t);                           /*0149*/
PyObject *PyTuple_Pack(Py_ssize_t, ...);                                /*0149*/
void _PyTuple_MaybeUntrack(PyObject *);                                 /*0149*/

/* Macro, trading safety for speed */



/* Macro, *only* to be used to fill in brand new tuples */


int PyTuple_ClearFreeList(void);                                        /*0149*/

/* List object interface */

/*
Another generally useful object type is an list of object pointers.
This is a mutable type: the list items can be changed, and items can be
added or removed.  Out-of-range indices or non-list objects are ignored.

*** WARNING *** PyList_SetItem does not increment the new item's reference
count, but does decrement the reference count of the item it replaces,
if not nil.  It does *decrement* the reference count if it is *not*
inserted in the list.  Similarly, PyList_GetItem does not increment the
returned item's reference count.
*/







/* Vector of pointers to list elements.  list[0] is ob_item[0], etc. */
/* ob_item contains space for 'allocated' elements.  The number
     * currently in use is ob_size.
     * Invariants:
     *     0 <= ob_size <= allocated
     *     len(list) == ob_size
     *     ob_item == NULL implies ob_size == allocated == 0
     * list.sort() temporarily sets allocated to -1 to detect mutations.
     *
     * Items must normally not be NULL, except during construction when
     * the list is not yet visible outside the function that builds it.
     */
typedef struct {Py_ssize_t ob_refcnt; struct _typeobject *ob_type; Py_ssize_t ob_size; PyObject **ob_item; Py_ssize_t allocated;} PyListObject;    /*0149*/

extern PyTypeObject PyList_Type;                                        /*0149*/





PyObject *PyList_New(Py_ssize_t size);                                  /*0149*/
Py_ssize_t PyList_Size(PyObject *);                                     /*0149*/
PyObject *PyList_GetItem(PyObject *, Py_ssize_t);                       /*0149*/
int PyList_SetItem(PyObject *, Py_ssize_t, PyObject *);                 /*0149*/
int PyList_Insert(PyObject *, Py_ssize_t, PyObject *);                  /*0149*/
int PyList_Append(PyObject *, PyObject *);                              /*0149*/
PyObject *PyList_GetSlice(PyObject *, Py_ssize_t, Py_ssize_t);          /*0149*/
int PyList_SetSlice(PyObject *, Py_ssize_t, Py_ssize_t, PyObject *);    /*0149*/
int PyList_Sort(PyObject *);                                            /*0149*/
int PyList_Reverse(PyObject *);                                         /*0149*/
PyObject *PyList_AsTuple(PyObject *);                                   /*0149*/
PyObject *_PyList_Extend(PyListObject *, PyObject *);                   /*0149*/

/* Macro, trading safety for speed */







/* Dictionary object type -- mapping from hashable object to object */

/* The distribution includes a separate file, Objects/dictnotes.txt,
   describing explorations into dictionary design and optimization.
   It covers typical dictionary use patterns, the parameters for
   tuning dictionaries, and several ideas for possible optimizations.
*/

/*
There are three kinds of slots in the table:

1. Unused.  me_key == me_value == NULL
   Does not hold an active (key, value) pair now and never did.  Unused can
   transition to Active upon key insertion.  This is the only case in which
   me_key is NULL, and is each slot's initial state.

2. Active.  me_key != NULL and me_key != dummy and me_value != NULL
   Holds an active (key, value) pair.  Active can transition to Dummy upon
   key deletion.  This is the only case in which me_value != NULL.

3. Dummy.  me_key == dummy and me_value == NULL
   Previously held an active (key, value) pair, but that was deleted and an
   active pair has not yet overwritten the slot.  Dummy can transition to
   Active upon key insertion.  Dummy slots cannot be made Unused again
   (cannot have me_key set to NULL), else the probe sequence in case of
   collision would have no way to know they were once active.

Note: .popitem() abuses the me_hash field of an Unused or Dummy slot to
hold a search finger.  The me_hash field of Unused or Dummy slots has no
meaning otherwise.
*/

/* PyDict_MINSIZE is the minimum size of a dictionary.  This many slots are
 * allocated directly in the dict object (in the ma_smalltable member).
 * It must be a power of 2, and at least 4.  8 allows dicts with no more
 * than 5 active entries to live in ma_smalltable (and so avoid an
 * additional malloc); instrumentation suggested this suffices for the
 * majority of dicts (consisting mostly of usually-small instance dicts and
 * usually-small dicts created to pass keyword arguments).
 */


/* Cached hash code of me_key.  Note that hash codes are C longs.
     * We have to use Py_ssize_t instead because dict_popitem() abuses
     * me_hash to hold a search finger.
     */
typedef struct {Py_ssize_t me_hash; PyObject *me_key; PyObject *me_value;} PyDictEntry;               /*0149*/

/*
To ensure the lookup algorithm terminates, there must be at least one Unused
slot (NULL key) in the table.
The value ma_fill is the number of non-NULL keys (sum of Active and Dummy);
ma_used is the number of non-NULL, non-dummy keys (== the number of non-NULL
values == the number of Active items).
To avoid slowing down lookups on a near-full table, we resize the table when
it's two-thirds full.
*/
typedef struct _dictobject PyDictObject;                                /*0149*/
/* # Active + # Dummy */
/* # Active */
/* The table contains ma_mask + 1 slots, and that's a power of 2.
     * We store the mask instead of the size because the mask is more
     * frequently needed.
     */
/* ma_table points to ma_smalltable for small tables, else to
     * additional malloc'ed memory.  ma_table is never NULL!  This rule
     * saves repeated runtime null-tests in the workhorse getitem and
     * setitem calls.
     */
struct _dictobject;                                                     /*0149*/

extern PyTypeObject PyDict_Type;                                        /*0149*/
extern PyTypeObject PyDictIterKey_Type;                                 /*0149*/
extern PyTypeObject PyDictIterValue_Type;                               /*0149*/
extern PyTypeObject PyDictIterItem_Type;                                /*0149*/
extern PyTypeObject PyDictKeys_Type;                                    /*0149*/
extern PyTypeObject PyDictItems_Type;                                   /*0149*/
extern PyTypeObject PyDictValues_Type;                                  /*0149*/







/* This excludes Values, since they are not sets. */



PyObject *PyDict_New(void);                                             /*0149*/
PyObject *PyDict_GetItem(PyObject *mp, PyObject *key);                  /*0149*/
int PyDict_SetItem(PyObject *mp, PyObject *key, PyObject *item);        /*0149*/
int PyDict_DelItem(PyObject *mp, PyObject *key);                        /*0149*/
void PyDict_Clear(PyObject *mp);                                        /*0149*/
int PyDict_Next(PyObject *mp, Py_ssize_t *pos, PyObject **key, PyObject **value);      /*0149*/
int _PyDict_Next(PyObject *mp, Py_ssize_t *pos, PyObject **key, PyObject **value, long int *hash);    /*0149*/
PyObject *PyDict_Keys(PyObject *mp);                                    /*0149*/
PyObject *PyDict_Values(PyObject *mp);                                  /*0149*/
PyObject *PyDict_Items(PyObject *mp);                                   /*0149*/
Py_ssize_t PyDict_Size(PyObject *mp);                                   /*0149*/
PyObject *PyDict_Copy(PyObject *mp);                                    /*0149*/
int PyDict_Contains(PyObject *mp, PyObject *key);                       /*0149*/
int _PyDict_Contains(PyObject *mp, PyObject *key, long int hash);       /*0149*/
PyObject *_PyDict_NewPresized(Py_ssize_t minused);                      /*0149*/
void _PyDict_MaybeUntrack(PyObject *mp);                                /*0149*/

/* PyDict_Update(mp, other) is equivalent to PyDict_Merge(mp, other, 1). */
int PyDict_Update(PyObject *mp, PyObject *other);                       /*0149*/

/* PyDict_Merge updates/merges from a mapping object (an object that
   supports PyMapping_Keys() and PyObject_GetItem()).  If override is true,
   the last occurrence of a key wins, else the first.  The Python
   dict.update(other) is equivalent to PyDict_Merge(dict, other, 1).
*/
int PyDict_Merge(PyObject *mp, PyObject *other, int override);          /*0149*/

/* PyDict_MergeFromSeq2 updates/merges from an iterable object producing
   iterable objects of length 2.  If override is true, the last occurrence
   of a key wins, else the first.  The Python dict constructor dict(seq2)
   is equivalent to dict={}; PyDict_MergeFromSeq(dict, seq2, 1).
*/
int PyDict_MergeFromSeq2(PyObject *d, PyObject *seq2, int override);    /*0149*/

PyObject *PyDict_GetItemString(PyObject *dp, const char *key);          /*0149*/
int PyDict_SetItemString(PyObject *dp, const char *key, PyObject *item);               /*0149*/
int PyDict_DelItemString(PyObject *dp, const char *key);                /*0149*/



/* Enumerate Object */





extern PyTypeObject PyEnum_Type;                                        /*0149*/
extern PyTypeObject PyReversed_Type;                                    /*0149*/
/* Set object interface */
/*
There are three kinds of slots in the table:

1. Unused:  key == NULL
2. Active:  key != NULL and key != dummy
3. Dummy:   key == dummy

Note: .pop() abuses the hash field of an Unused or Dummy slot to
hold a search finger.  The hash field of Unused or Dummy slots has
no meaning otherwise.
*/



/* cached hash code for the entry key */
typedef struct {long int hash; PyObject *key;} setentry;                /*0149*/


/*
This data structure is shared by set and frozenset objects.
*/

typedef struct _setobject PySetObject;                                  /*0149*/
/* # Active + # Dummy */
/* # Active */
/* The table contains mask + 1 slots, and that's a power of 2.
     * We store the mask instead of the size because the mask is more
     * frequently needed.
     */
/* table points to smalltable for small tables, else to
     * additional malloc'ed memory.  table is never NULL!  This rule
     * saves repeated runtime null-tests.
     */
/* only used by frozenset objects */
/* List of weak references */
struct _setobject;                                                      /*0149*/

extern PyTypeObject PySet_Type;                                         /*0149*/
extern PyTypeObject PyFrozenSet_Type;                                   /*0149*/

/* Invariants for frozensets:
 *     data is immutable.
 *     hash is the hash of the frozenset or -1 if not computed yet.
 * Invariants for sets:
 *     hash is -1
 */
PyObject *PySet_New(PyObject *);                                        /*0149*/
PyObject *PyFrozenSet_New(PyObject *);                                  /*0149*/
Py_ssize_t PySet_Size(PyObject *anyset);                                /*0149*/

int PySet_Clear(PyObject *set);                                         /*0149*/
int PySet_Contains(PyObject *anyset, PyObject *key);                    /*0149*/
int PySet_Discard(PyObject *set, PyObject *key);                        /*0149*/
int PySet_Add(PyObject *set, PyObject *key);                            /*0149*/
int _PySet_Next(PyObject *set, Py_ssize_t *pos, PyObject **key);        /*0149*/
int _PySet_NextEntry(PyObject *set, Py_ssize_t *pos, PyObject **key, long int *hash);  /*0149*/
PyObject *PySet_Pop(PyObject *set);                                     /*0149*/
int _PySet_Update(PyObject *set, PyObject *iterable);                   /*0149*/

/* Method object interface */







/* This is about the type 'builtin_function_or_method',
   not Python methods in user-defined classes.  See classobject.h
   for the latter. */

extern PyTypeObject PyCFunction_Type;                                   /*0149*/



typedef PyObject *(*PyCFunction)(PyObject *, PyObject *);               /*0149*/
typedef PyObject *(*PyCFunctionWithKeywords)(PyObject *, PyObject *, PyObject *);      /*0149*/
typedef PyObject *(*PyNoArgsFunction)(PyObject *);                      /*0149*/

PyCFunction PyCFunction_GetFunction(PyObject *);                        /*0149*/
PyObject *PyCFunction_GetSelf(PyObject *);                              /*0149*/
