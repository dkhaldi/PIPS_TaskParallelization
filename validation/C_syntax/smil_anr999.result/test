   <http://www.gnu.org/licenses/>.  */

/* NOTE: Because of the special way this file is used by <math.h>, this
   file must NOT be protected from multiple inclusion as header files
   usually are.

   This file provides prototype declarations for the math functions.
   Most functions are declared using the macro:

   __MATHCALL (NAME,[_r], (ARGS...));

   This means there is a function `NAME' returning `double' and a function
   `NAMEf' returning `float'.  Each place `_Mdouble_' appears in the
   prototype, that is actually `double' in the prototype for `NAME' and
   `float' in the prototype for `NAMEf'.  Reentrant variant functions are
   called `NAME_r' and `NAMEf_r'.

   Functions returning other types like `int' are declared using the macro:

   __MATHDECL (TYPE, NAME,[_r], (ARGS...));

   This is just like __MATHCALL but for a function returning `TYPE'
   instead of `_Mdouble_'.  In all of these cases, there is still
   both a `NAME' and a `NAMEf' that takes `float' arguments.

   Note that there must be no whitespace before the argument passed for
   NAME, to make token pasting work with -traditional.  */






/* Trigonometric functions.  */


/* Arc cosine of X.  */
extern float acosf(float __x);                                          /*0149*/
extern float __acosf(float __x);                                        /*0149*/
/* Arc sine of X.  */
extern float asinf(float __x);                                          /*0149*/
extern float __asinf(float __x);                                        /*0149*/
/* Arc tangent of X.  */
extern float atanf(float __x);                                          /*0149*/
extern float __atanf(float __x);                                        /*0149*/
/* Arc tangent of Y/X.  */
extern float atan2f(float __y, float __x);                              /*0149*/
extern float __atan2f(float __y, float __x);                            /*0149*/

/* Cosine of X.  */
extern float cosf(float __x);                                           /*0149*/
extern float __cosf(float __x);                                         /*0149*/
/* Sine of X.  */
extern float sinf(float __x);                                           /*0149*/
extern float __sinf(float __x);                                         /*0149*/
/* Tangent of X.  */
extern float tanf(float __x);                                           /*0149*/
extern float __tanf(float __x);                                         /*0149*/

/* Hyperbolic functions.  */

/* Hyperbolic cosine of X.  */
extern float coshf(float __x);                                          /*0149*/
extern float __coshf(float __x);                                        /*0149*/
/* Hyperbolic sine of X.  */
extern float sinhf(float __x);                                          /*0149*/
extern float __sinhf(float __x);                                        /*0149*/
/* Hyperbolic tangent of X.  */
extern float tanhf(float __x);                                          /*0149*/
extern float __tanhf(float __x);                                        /*0149*/



/* Cosine and sine of X.  */
extern void sincosf(float __x, float *__sinx, float *__cosx);           /*0149*/
extern void __sincosf(float __x, float *__sinx, float *__cosx);         /*0149*/




/* Hyperbolic arc cosine of X.  */
extern float acoshf(float __x);                                         /*0149*/
extern float __acoshf(float __x);                                       /*0149*/
/* Hyperbolic arc sine of X.  */
extern float asinhf(float __x);                                         /*0149*/
extern float __asinhf(float __x);                                       /*0149*/
/* Hyperbolic arc tangent of X.  */
extern float atanhf(float __x);                                         /*0149*/
extern float __atanhf(float __x);                                       /*0149*/



/* Exponential and logarithmic functions.  */


/* Exponential function of X.  */
extern float expf(float __x);                                           /*0149*/
extern float __expf(float __x);                                         /*0149*/

/* Break VALUE into a normalized fraction and an integral power of 2.  */
extern float frexpf(float __x, int *__exponent);                        /*0149*/
extern float __frexpf(float __x, int *__exponent);                      /*0149*/

/* X times (two to the EXP power).  */
extern float ldexpf(float __x, int __exponent);                         /*0149*/
extern float __ldexpf(float __x, int __exponent);                       /*0149*/

/* Natural logarithm of X.  */
extern float logf(float __x);                                           /*0149*/
extern float __logf(float __x);                                         /*0149*/

/* Base-ten logarithm of X.  */
extern float log10f(float __x);                                         /*0149*/
extern float __log10f(float __x);                                       /*0149*/

/* Break VALUE into integral and fractional parts.  */
extern float modff(float __x, float *__iptr);                           /*0149*/
extern float __modff(float __x, float *__iptr);                         /*0149*/



/* A function missing in all standards: compute exponent to base ten.  */
extern float exp10f(float __x);                                         /*0149*/
extern float __exp10f(float __x);                                       /*0149*/
/* Another name occasionally used.  */
extern float pow10f(float __x);                                         /*0149*/
extern float __pow10f(float __x);                                       /*0149*/




/* Return exp(X) - 1.  */
extern float expm1f(float __x);                                         /*0149*/
extern float __expm1f(float __x);                                       /*0149*/

/* Return log(1 + X).  */
extern float log1pf(float __x);                                         /*0149*/
extern float __log1pf(float __x);                                       /*0149*/

/* Return the base 2 signed integral exponent of X.  */
extern float logbf(float __x);                                          /*0149*/
extern float __logbf(float __x);                                        /*0149*/





/* Compute base-2 exponential of X.  */
extern float exp2f(float __x);                                          /*0149*/
extern float __exp2f(float __x);                                        /*0149*/

/* Compute base-2 logarithm of X.  */
extern float log2f(float __x);                                          /*0149*/
extern float __log2f(float __x);                                        /*0149*/




/* Power functions.  */


/* Return X to the Y power.  */
extern float powf(float __x, float __y);                                /*0149*/
extern float __powf(float __x, float __y);                              /*0149*/

/* Return the square root of X.  */
extern float sqrtf(float __x);                                          /*0149*/
extern float __sqrtf(float __x);                                        /*0149*/




/* Return `sqrt(X*X + Y*Y)'.  */
extern float hypotf(float __x, float __y);                              /*0149*/
extern float __hypotf(float __x, float __y);                            /*0149*/





/* Return the cube root of X.  */
extern float cbrtf(float __x);                                          /*0149*/
extern float __cbrtf(float __x);                                        /*0149*/




/* Nearest integer, absolute value, and remainder functions.  */


/* Smallest integral value not less than X.  */
extern float ceilf(float __x);                                          /*0149*/
extern float __ceilf(float __x);                                        /*0149*/

/* Absolute value of X.  */
extern float fabsf(float __x);                                          /*0149*/
extern float __fabsf(float __x);                                        /*0149*/

/* Largest integer not greater than X.  */
extern float floorf(float __x);                                         /*0149*/
extern float __floorf(float __x);                                       /*0149*/

/* Floating-point modulo remainder of X/Y.  */
extern float fmodf(float __x, float __y);                               /*0149*/
extern float __fmodf(float __x, float __y);                             /*0149*/


/* Return 0 if VALUE is finite or NaN, +1 if it
   is +Infinity, -1 if it is -Infinity.  */
extern int __isinff(float __value);                                     /*0149*/

/* Return nonzero if VALUE is finite and not NaN.  */
extern int __finitef(float __value);                                    /*0149*/



/* Return 0 if VALUE is finite or NaN, +1 if it
   is +Infinity, -1 if it is -Infinity.  */
extern int isinff(float __value);                                       /*0149*/

/* Return nonzero if VALUE is finite and not NaN.  */
extern int finitef(float __value);                                      /*0149*/

/* Return the remainder of X/Y.  */
extern float dremf(float __x, float __y);                               /*0149*/
extern float __dremf(float __x, float __y);                             /*0149*/


/* Return the fractional part of X after dividing out `ilogb (X)'.  */
extern float significandf(float __x);                                   /*0149*/
extern float __significandf(float __x);                                 /*0149*/




/* Return X with its signed changed to Y's.  */
extern float copysignf(float __x, float __y);                           /*0149*/
extern float __copysignf(float __x, float __y);                         /*0149*/





/* Return representation of qNaN for double type.  */
extern float nanf(const char *__tagb);                                  /*0149*/
extern float __nanf(const char *__tagb);                                /*0149*/




/* Return nonzero if VALUE is not a number.  */
extern int isnanf(float __value);                                       /*0149*/


/* Return nonzero if VALUE is not a number.  */
extern int isnanf(float __value);                                       /*0149*/

/* Bessel functions.  */
extern float j0f(float);                                                /*0149*/
extern float __j0f(float);                                              /*0149*/
extern float j1f(float);                                                /*0149*/
extern float __j1f(float);                                              /*0149*/
extern float jnf(int, float);                                           /*0149*/
extern float __jnf(int, float);                                         /*0149*/
