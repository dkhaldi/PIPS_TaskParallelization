extern unsigned int gnu_dev_major(unsigned long long int __dev);        /*0149*/

extern unsigned int gnu_dev_minor(unsigned long long int __dev);        /*0149*/

extern unsigned long long int gnu_dev_makedev(unsigned int __major, unsigned int __minor);            /*0149*/


/* Access the functions with their traditional names.  */





typedef __blksize_t blksize_t;                                          /*0149*/



/* Types from the Large File Support interface.  */
typedef __blkcnt64_t blkcnt_t;                                          /*0149*/
/* Type to count number of disk blocks.  */



typedef __fsblkcnt64_t fsblkcnt_t;                                      /*0149*/
/* Type to count file system blocks.  */



typedef __fsfilcnt64_t fsfilcnt_t;                                      /*0149*/
/* Type to count file system inodes.  */





typedef __blkcnt64_t blkcnt64_t;                                        /*0149*/
/* Type to count number of disk blocks. */
typedef __fsblkcnt64_t fsblkcnt64_t;                                    /*0149*/
/* Type to count file system blocks.  */
typedef __fsfilcnt64_t fsfilcnt64_t;                                    /*0149*/
/* Type to count file system inodes.  */



/* Now add the thread types.  */

/* Copyright (C) 2002-2014 Free Software Foundation, Inc.
   This file is part of the GNU C Library.

   The GNU C Library is free software; you can redistribute it and/or
   modify it under the terms of the GNU Lesser General Public
   License as published by the Free Software Foundation; either
   version 2.1 of the License, or (at your option) any later version.

   The GNU C Library is distributed in the hope that it will be useful,
   but WITHOUT ANY WARRANTY; without even the implied warranty of
   MERCHANTABILITY or FITNESS FOR A PARTICULAR PURPOSE.  See the GNU
   Lesser General Public License for more details.

   You should have received a copy of the GNU Lesser General Public
   License along with the GNU C Library; if not, see
   <http://www.gnu.org/licenses/>.  */




/* Determine the wordsize from the preprocessor defines.  */
/* Both x86-64 and x32 use the 64-bit system call interface.  */
/* Thread identifiers.  The structure of the attribute type is not
   exposed on purpose.  */
typedef unsigned long int pthread_t;                                    /*0149*/


union pthread_attr_t {                                                  /*0149*/
   char __size[56];
   long int __align;
};

typedef union pthread_attr_t pthread_attr_t;                            /*0149*/





typedef struct __pthread_internal_list {                                /*0149*/
   struct __pthread_internal_list *__prev;
   struct __pthread_internal_list *__next;
} __pthread_list_t;
/* Data structures for mutex handling.  The structure of the attribute
   type is not exposed on purpose.  */
/* KIND must stay at this position in the structure to maintain
       binary compatibility.  */
typedef union {struct __pthread_mutex_s {int __lock; unsigned int __count; int __owner; unsigned int __nusers; int __kind; short __spins; short __elision; __pthread_list_t __list;} __data; char __size[40]; long int __align;} pthread_mutex_t;           /*0149*/

typedef union {char __size[4]; int __align;} pthread_mutexattr_t;       /*0149*/


/* Data structure for conditional variable handling.  The structure of
   the attribute type is not exposed on purpose.  */
typedef union {struct {int __lock; unsigned int __futex; unsigned long long int __total_seq; unsigned long long int __wakeup_seq; unsigned long long int __woken_seq; void *__mutex; unsigned int __nwaiters; unsigned int __broadcast_seq;} __data; char __size[48]; long long int __align;} pthread_cond_t;           /*0149*/

typedef union {char __size[4]; int __align;} pthread_condattr_t;        /*0149*/


/* Keys for thread-specific data */
typedef unsigned int pthread_key_t;                                     /*0149*/


/* Once-only execution */
typedef int pthread_once_t;                                             /*0149*/



/* Data structure for read-write lock variable handling.  The
   structure of the attribute type is not exposed on purpose.  */
/* FLAGS must stay at this position in the structure to maintain
       binary compatibility.  */
typedef union {struct {int __lock; unsigned int __nr_readers; unsigned int __readers_wakeup; unsigned int __writer_wakeup; unsigned int __nr_readers_queued; unsigned int __nr_writers_queued; int __writer; int __shared; unsigned long int __pad1; unsigned long int __pad2; unsigned int __flags;} __data; char __size[56]; long int __align;} pthread_rwlock_t;  /*0149*/

typedef union {char __size[8]; long int __align;} pthread_rwlockattr_t;                /*0149*/




/* POSIX spinlock data type.  */
typedef volatile int pthread_spinlock_t;                                /*0149*/


/* POSIX barriers data type.  The structure of the type is
   deliberately not exposed.  */
typedef union {char __size[32]; long int __align;} pthread_barrier_t;   /*0149*/

typedef union {char __size[4]; int __align;} pthread_barrierattr_t;     /*0149*/




/* These are the functions that actually do things.  The `random', `srandom',
   `initstate' and `setstate' functions are those from BSD Unices.
   The `rand' and `srand' functions are required by the ANSI standard.
   We provide both interfaces to the same random number generator.  */
/* Return a random long integer between 0 and RAND_MAX inclusive.  */
extern long int random(void);                                           /*0149*/

/* Seed the random number generator with the given number.  */
extern void srandom(unsigned int __seed);                               /*0149*/

/* Initialize the random number generator to use state buffer STATEBUF,
   of length STATELEN, and seed it with SEED.  Optimal lengths are 8, 16,
   32, 64, 128 and 256, the bigger the better; values less than 8 will
   cause an error and values greater than 256 will be rounded down.  */
extern char *initstate(unsigned int __seed, char *__statebuf, size_t __statelen);      /*0149*/

/* Switch the random number generator to state buffer STATEBUF,
   which should have been previously initialized by `initstate'.  */
extern char *setstate(char *__statebuf);                                /*0149*/



/* Reentrant versions of the `random' family of functions.
   These functions all use the following data structure to contain
   state, rather than global state variables.  */

/* Front pointer.  */
/* Rear pointer.  */
/* Array of state values.  */
/* Type of random number generator.  */
/* Degree of random number generator.  */
/* Distance between front and rear.  */
/* Pointer behind state table.  */
struct random_data {                                                    /*0149*/
   int32_t *fptr;
   int32_t *rptr;
   int32_t *state;
   int rand_type;
   int rand_deg;
   int rand_sep;
   int32_t *end_ptr;
};

extern int random_r(struct random_data *__buf, int32_t *__result);      /*0149*/

extern int srandom_r(unsigned int __seed, struct random_data *__buf);   /*0149*/

extern int initstate_r(unsigned int __seed, char *__statebuf, size_t __statelen, struct random_data *__buf);         /*0149*/

extern int setstate_r(char *__statebuf, struct random_data *__buf);     /*0149*/





/* Return a random integer between 0 and RAND_MAX inclusive.  */
extern int rand(void);                                                  /*0149*/
/* Seed the random number generator with the given number.  */
extern void srand(unsigned int __seed);                                 /*0149*/



/* Reentrant interface according to POSIX.1.  */
extern int rand_r(unsigned int *__seed);                                /*0149*/




/* System V style 48-bit random number generator functions.  */

/* Return non-negative, double-precision floating-point value in [0.0,1.0).  */
extern double drand48(void);                                            /*0149*/
extern double erand48(unsigned short __xsubi[3]);                       /*0149*/

/* Return non-negative, long integer in [0,2^31).  */
extern long int lrand48(void);                                          /*0149*/
extern long int nrand48(unsigned short __xsubi[3]);                     /*0149*/

/* Return signed, long integers in [-2^31,2^31).  */
extern long int mrand48(void);                                          /*0149*/
extern long int jrand48(unsigned short __xsubi[3]);                     /*0149*/

/* Seed random number generator.  */
extern void srand48(long int __seedval);                                /*0149*/
extern unsigned short *seed48(unsigned short __seed16v[3]);             /*0149*/
extern void lcong48(unsigned short __param[7]);                         /*0149*/


/* Data structure for communication with thread safe versions.  This
   type is to be regarded as opaque.  It's only exported because users
   have to allocate objects of this type.  */
/* Current state.  */
/* Old state.  */
/* Additive const. in congruential formula.  */
/* Flag for initializing.  */
/* Factor in congruential
						   formula.  */
struct drand48_data {                                                   /*0149*/
   unsigned short __x[3];
   unsigned short __old_x[3];
   unsigned short __c;
   unsigned short __init;
   unsigned long long int __a;
};

/* Return non-negative, double-precision floating-point value in [0.0,1.0).  */
extern int drand48_r(struct drand48_data *__buffer, double *__result);  /*0149*/
extern int erand48_r(unsigned short __xsubi[3], struct drand48_data *__buffer, double *__result);     /*0149*/

/* Return non-negative, long integer in [0,2^31).  */
extern int lrand48_r(struct drand48_data *__buffer, long int *__result);               /*0149*/
extern int nrand48_r(unsigned short __xsubi[3], struct drand48_data *__buffer, long int *__result);   /*0149*/

/* Return signed, long integers in [-2^31,2^31).  */
extern int mrand48_r(struct drand48_data *__buffer, long int *__result);               /*0149*/
extern int jrand48_r(unsigned short __xsubi[3], struct drand48_data *__buffer, long int *__result);   /*0149*/

/* Seed random number generator.  */
extern int srand48_r(long int __seedval, struct drand48_data *__buffer);               /*0149*/

extern int seed48_r(unsigned short __seed16v[3], struct drand48_data *__buffer);       /*0149*/

extern int lcong48_r(unsigned short __param[7], struct drand48_data *__buffer);        /*0149*/








/* Allocate SIZE bytes of memory.  */
extern void *malloc(size_t __size);                                     /*0149*/
/* Allocate NMEMB elements of SIZE bytes each, all initialized to 0.  */
extern void *calloc(size_t __nmemb, size_t __size);                     /*0149*/





/* Re-allocate the previously allocated block
   in PTR, making the new block SIZE bytes long.  */
/* __attribute_malloc__ is not used, because if realloc returns
   the same pointer that was passed to it, aliasing needs to be allowed
   between objects pointed by the old and new pointers.  */
extern void *realloc(void *__ptr, size_t __size);                       /*0149*/
/* Free a block allocated by `malloc', `realloc' or `calloc'.  */
extern void free(void *__ptr);                                          /*0149*/



/* Free a block.  An alias for `free'.	(Sun Unices).  */
extern void cfree(void *__ptr);                                         /*0149*/



