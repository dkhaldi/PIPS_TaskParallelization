/* void assert (int expression);

   If NDEBUG is defined, do nothing.
   If not, and EXPRESSION is zero, print an error message and abort.  */


/* This prints an "Assertion failed" message and aborts.  */
extern void __assert_fail(const char *__assertion, const char *__file, unsigned int __line, const char *__function);                /*0149*/

/* Likewise, but prints the error text for ERRNUM.  */
extern void __assert_perror_fail(int __errnum, const char *__file, unsigned int __line, const char *__function);     /*0149*/


/* The following is not at all used here but needed for standard
   compliance.  */
extern void __assert(const char *__assertion, const char *__file, int __line);         /*0149*/



/* Version 2.4 and later of GCC define a magical variable `__PRETTY_FUNCTION__'
