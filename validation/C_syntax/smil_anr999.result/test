int _PyUnicodeUCS4_IsLowercase(Py_UNICODE ch);                          /*0149*/

/* Unicode character */
int _PyUnicodeUCS4_IsUppercase(Py_UNICODE ch);                          /*0149*/

/* Unicode character */
int _PyUnicodeUCS4_IsTitlecase(Py_UNICODE ch);                          /*0149*/

/* Unicode character */
int _PyUnicodeUCS4_IsWhitespace(const Py_UNICODE ch);                   /*0149*/

/* Unicode character */
int _PyUnicodeUCS4_IsLinebreak(const Py_UNICODE ch);                    /*0149*/

/* Unicode character */
Py_UNICODE _PyUnicodeUCS4_ToLowercase(Py_UNICODE ch);                   /*0149*/

/* Unicode character */
Py_UNICODE _PyUnicodeUCS4_ToUppercase(Py_UNICODE ch);                   /*0149*/

/* Unicode character */
Py_UNICODE _PyUnicodeUCS4_ToTitlecase(Py_UNICODE ch);                   /*0149*/

