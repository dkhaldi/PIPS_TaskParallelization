   `signgam'.  The reentrant version instead takes a pointer and stores
   the value through it.  */
extern float lgammaf_r(float, int *__signgamp);                         /*0149*/
extern float __lgammaf_r(float, int *__signgamp);                       /*0149*/





/* Return the integer nearest X in the direction of the
   prevailing rounding mode.  */
extern float rintf(float __x);                                          /*0149*/
extern float __rintf(float __x);                                        /*0149*/

/* Return X + epsilon if X < Y, X - epsilon if X > Y.  */
extern float nextafterf(float __x, float __y);                          /*0149*/
extern float __nextafterf(float __x, float __y);                        /*0149*/

extern float nexttowardf(float __x, long double __y);                   /*0149*/
extern float __nexttowardf(float __x, long double __y);                 /*0149*/


/* Return the remainder of integer divison X / Y with infinite precision.  */
extern float remainderf(float __x, float __y);                          /*0149*/
extern float __remainderf(float __x, float __y);                        /*0149*/


/* Return X times (2 to the Nth power).  */
extern float scalbnf(float __x, int __n);                               /*0149*/
extern float __scalbnf(float __x, int __n);                             /*0149*/


/* Return the binary exponent of X, which must be nonzero.  */
extern int ilogbf(float __x);                                           /*0149*/
extern int __ilogbf(float __x);                                         /*0149*/



/* Return X times (2 to the Nth power).  */
extern float scalblnf(float __x, long int __n);                         /*0149*/
extern float __scalblnf(float __x, long int __n);                       /*0149*/

/* Round X to integral value in floating-point format using current
   rounding direction, but do not raise inexact exception.  */
extern float nearbyintf(float __x);                                     /*0149*/
extern float __nearbyintf(float __x);                                   /*0149*/

/* Round X to nearest integral value, rounding halfway cases away from
   zero.  */
extern float roundf(float __x);                                         /*0149*/
extern float __roundf(float __x);                                       /*0149*/

/* Round X to the integral value in floating-point format nearest but
   not larger in magnitude.  */
extern float truncf(float __x);                                         /*0149*/
extern float __truncf(float __x);                                       /*0149*/

