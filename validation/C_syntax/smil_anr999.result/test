/* Determine the wordsize from the preprocessor defines.  */
/* Both x86-64 and x32 use the 64-bit system call interface.  */

/* Convenience types.  */
typedef unsigned char __u_char;                                         /*0016*/
typedef unsigned short __u_short;                                       /*0016*/
typedef unsigned int __u_int;                                           /*0016*/
typedef unsigned long int __u_long;                                     /*0016*/

/* Fixed-size types, underlying types depend on word size and compiler.  */
typedef signed char __int8_t;                                           /*0016*/
typedef unsigned char __uint8_t;                                        /*0016*/
typedef signed short __int16_t;                                         /*0016*/
typedef unsigned short __uint16_t;                                      /*0016*/
typedef signed int __int32_t;                                           /*0016*/
typedef unsigned int __uint32_t;                                        /*0016*/

typedef signed long int __int64_t;                                      /*0016*/
typedef unsigned long int __uint64_t;                                   /*0016*/





/* quad_t is also 64 bits.  */

typedef long int __quad_t;                                              /*0016*/
typedef unsigned long int __u_quad_t;                                   /*0016*/






/* The machine-dependent file <bits/typesizes.h> defines __*_T_TYPE
   macros for each of the OS types we define below.  The definitions
   of those macros must use the following macros for underlying types.
   We define __S<SIZE>_TYPE and __U<SIZE>_TYPE for the signed and unsigned
   variants of each of the following integer types on this machine.

	16		-- "natural" 16-bit type (always short)
	32		-- "natural" 32-bit type (always int)
	64		-- "natural" 64-bit type (long or long long)
	LONG32		-- 32-bit type, traditionally long
	QUAD		-- 64-bit type, always long long
	WORD		-- natural type of __WORDSIZE bits (int or long)
	LONGWORD	-- type of __WORDSIZE bits, traditionally long

   We distinguish WORD/LONGWORD, 32/LONG32, and 64/QUAD so that the
   conventional uses of `long' or `long long' type modifiers match the
   types we define, even when a less-adorned type would be the same size.
   This matters for (somewhat) portably writing printf/scanf formats for
   these types, where using the appropriate l or ll format modifiers can
   make the typedefs and the formats match up across all GNU platforms.  If
   we used `long' when it's 64 bits where `long long' is expected, then the
   compiler would warn about the formats not matching the argument types,
   and the programmer changing them to shut up the compiler would break the
   program's portability.

   Here we assume what is presently the case in all the GCC configurations
   we support: long long is always 64 bits, long is always word/address size,
   and int is always 32 bits.  */
/* No need to mark the typedef with __extension__.   */




/* bits/typesizes.h -- underlying types for *_t.  Linux/x86-64 version.
   Copyright (C) 2012-2014 Free Software Foundation, Inc.
   This file is part of the GNU C Library.

   The GNU C Library is free software; you can redistribute it and/or
   modify it under the terms of the GNU Lesser General Public
   License as published by the Free Software Foundation; either
   version 2.1 of the License, or (at your option) any later version.

   The GNU C Library is distributed in the hope that it will be useful,
   but WITHOUT ANY WARRANTY; without even the implied warranty of
   MERCHANTABILITY or FITNESS FOR A PARTICULAR PURPOSE.  See the GNU
   Lesser General Public License for more details.

   You should have received a copy of the GNU Lesser General Public
   License along with the GNU C Library; if not, see
   <http://www.gnu.org/licenses/>.  */
/* See <bits/types.h> for the meaning of these macros.  This file exists so
   that <bits/types.h> need not vary across different GNU platforms.  */

/* X32 kernel interface is 64-bit.  */
/* Tell the libc code that off_t and off64_t are actually the same type
   for all ABI purposes, even if possibly expressed as different base types
   for C type-checking purposes.  */


/* Same for ino_t and ino64_t.  */



/* Number of descriptors that can fit in an `fd_set'.  */


typedef unsigned long int __dev_t;                                      /*0016*/
/* Type of device numbers.  */
typedef unsigned int __uid_t;                                           /*0016*/
/* Type of user identifications.  */
typedef unsigned int __gid_t;                                           /*0016*/
/* Type of group identifications.  */
typedef unsigned long int __ino_t;                                      /*0016*/
/* Type of file serial numbers.  */
typedef unsigned long int __ino64_t;                                    /*0016*/
/* Type of file serial numbers (LFS).*/
typedef unsigned int __mode_t;                                          /*0016*/
/* Type of file attribute bitmasks.  */
typedef unsigned long int __nlink_t;                                    /*0016*/
/* Type of file link counts.  */
typedef long int __off_t;                                               /*0016*/
/* Type of file sizes and offsets.  */
typedef long int __off64_t;                                             /*0016*/
/* Type of file sizes and offsets (LFS).  */
typedef int __pid_t;                                                    /*0016*/
/* Type of process identifications.  */
typedef struct {int __val[2];} __fsid_t;                                /*0016*/
/* Type of file system IDs.  */
typedef long int __clock_t;                                             /*0016*/
/* Type of CPU usage counts.  */
typedef unsigned long int __rlim_t;                                     /*0016*/
/* Type for resource measurement.  */
typedef unsigned long int __rlim64_t;                                   /*0016*/
/* Type for resource measurement (LFS).  */
typedef unsigned int __id_t;                                            /*0016*/
/* General type for IDs.  */
typedef long int __time_t;                                              /*0016*/
/* Seconds since the Epoch.  */
typedef unsigned int __useconds_t;                                      /*0016*/
/* Count of microseconds.  */
typedef long int __suseconds_t;                                         /*0016*/
/* Signed count of microseconds.  */

typedef int __daddr_t;                                                  /*0016*/
/* The type of a disk address.  */
typedef int __key_t;                                                    /*0016*/
/* Type of an IPC key.  */

/* Clock ID used in clock and timer functions.  */
typedef int __clockid_t;                                                /*0016*/

/* Timer ID returned by `timer_create'.  */
typedef void *__timer_t;                                                /*0016*/

/* Type to represent block size.  */
typedef long int __blksize_t;                                           /*0016*/

/* Types from the Large File Support interface.  */

/* Type to count number of disk blocks.  */
typedef long int __blkcnt_t;                                            /*0016*/
typedef long int __blkcnt64_t;                                          /*0016*/

/* Type to count file system blocks.  */
typedef unsigned long int __fsblkcnt_t;                                 /*0016*/
typedef unsigned long int __fsblkcnt64_t;                               /*0016*/

/* Type to count file system nodes.  */
typedef unsigned long int __fsfilcnt_t;                                 /*0016*/
typedef unsigned long int __fsfilcnt64_t;                               /*0016*/

/* Type of miscellaneous file system fields.  */
typedef long int __fsword_t;                                            /*0016*/

typedef long int __ssize_t;                                             /*0016*/
/* Type of a byte count, or error.  */

/* Signed long type used in system calls.  */
typedef long int __syscall_slong_t;                                     /*0016*/
/* Unsigned long type used in system calls.  */
typedef unsigned long int __syscall_ulong_t;                            /*0016*/

/* These few don't really vary by system, they always correspond
   to one of the other defined types.  */
typedef __off64_t __loff_t;                                             /*0016*/
/* Type of file sizes and offsets (LFS).  */
typedef __quad_t *__qaddr_t;                                            /*0016*/
typedef char *__caddr_t;                                                /*0016*/

/* Duplicates info from stdint.h but this is used in unistd.h.  */
typedef long int __intptr_t;                                            /*0016*/

/* Duplicate info from sys/socket.h.  */
typedef unsigned int __socklen_t;                                       /*0016*/







/* Define outside of namespace so the C++ is happy.  */
struct _IO_FILE;                                                        /*0016*/


/* The opaque type of streams.  This is the definition used elsewhere.  */
typedef struct _IO_FILE FILE;                                           /*0016*/





/* The opaque type of streams.  This is the definition used elsewhere.  */
typedef struct _IO_FILE __FILE;                                         /*0016*/
