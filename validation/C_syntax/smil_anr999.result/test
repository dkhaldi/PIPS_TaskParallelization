/*
 *	ISO C99 Standard: 7.14 Signal handling <signal.h>
 */

/* We need the signal context definitions even if they are not used
   included in <signal.h>.  */
/* Copyright (C) 2002-2014 Free Software Foundation, Inc.
   This file is part of the GNU C Library.

   The GNU C Library is free software; you can redistribute it and/or
   modify it under the terms of the GNU Lesser General Public
   License as published by the Free Software Foundation; either
   version 2.1 of the License, or (at your option) any later version.

   The GNU C Library is distributed in the hope that it will be useful,
   but WITHOUT ANY WARRANTY; without even the implied warranty of
   MERCHANTABILITY or FITNESS FOR A PARTICULAR PURPOSE.	 See the GNU
   Lesser General Public License for more details.

   You should have received a copy of the GNU Lesser General Public
   License along with the GNU C Library; if not, see
   <http://www.gnu.org/licenses/>.  */



/* Type for general register.  */
typedef long long int greg_t;                                           /*0149*/

/* Number of general registers.  */


/* Container for all general registers.  */
typedef greg_t gregset_t[23];                                           /*0149*/


/* Number of each register in the `gregset_t' array.  */
/* Actually short cs, gs, fs, __pad0.  */
enum {REG_R8, REG_R9, REG_R10, REG_R11, REG_R12, REG_R13, REG_R14, REG_R15, REG_RDI, REG_RSI, REG_RBP, REG_RBX, REG_RDX, REG_RAX, REG_RCX, REG_RSP, REG_RIP, REG_EFL, REG_CSGSFS, REG_ERR, REG_TRAPNO, REG_OLDMASK, REG_CR2};                /*0149*/


struct _libc_fpxreg {                                                   /*0149*/
   unsigned short significand[4];
   unsigned short exponent;
   unsigned short padding[3];
};

struct _libc_xmmreg {                                                   /*0149*/
   __uint32_t element[4];
};

/* 64-bit FXSAVE format.  */
struct _libc_fpstate {                                                  /*0149*/
   __uint16_t cwd;
   __uint16_t swd;
   __uint16_t ftw;
   __uint16_t fop;
   __uint64_t rip;
   __uint64_t rdp;
   __uint32_t mxcsr;
   __uint32_t mxcr_mask;
   struct _libc_fpxreg _st[8];
   struct _libc_xmmreg _xmm[16];
   __uint32_t padding[24];
};

/* Structure to describe FPU registers.  */
typedef struct _libc_fpstate *fpregset_t;                               /*0149*/

/* Context to describe whole processor state.  */
/* Note that fpregs is a pointer.  */
typedef struct {gregset_t gregs; fpregset_t fpregs; unsigned long long int __reserved1[8];} mcontext_t;              /*0149*/

/* Userlevel context.  */
typedef struct ucontext {                                               /*0149*/
   unsigned long int uc_flags;
   struct ucontext *uc_link;
   stack_t uc_stack;
   mcontext_t uc_mcontext;
   __sigset_t uc_sigmask;
   struct _libc_fpstate __fpregs_mem;
} ucontext_t;


/* Run signals handlers on the stack specified by SS (if not NULL).
   If OSS is not NULL, it is filled in with the old signal stack status.
   This interface is obsolete and on many platform not implemented.  */
extern int sigstack(struct sigstack *__ss, struct sigstack *__oss);     /*0149*/

/* Alternate signal handler stack interface.
   This interface should always be preferred over `sigstack'.  */
extern int sigaltstack(const struct sigaltstack *__ss, struct sigaltstack *__oss);     /*0149*/




/* Simplified interface for signal management.  */

/* Add SIG to the calling process' signal mask.  */
extern int sighold(int __sig);                                          /*0149*/

/* Remove SIG from the calling process' signal mask.  */
extern int sigrelse(int __sig);                                         /*0149*/

/* Set the disposition of SIG to SIG_IGN.  */
extern int sigignore(int __sig);                                        /*0149*/

/* Set the disposition of SIG.  */
extern __sighandler_t sigset(int __sig, __sighandler_t __disp);         /*0149*/



/* Some of the functions for handling signals in threaded programs must
   be defined here.  */
/* Copyright (C) 2002-2014 Free Software Foundation, Inc.
   This file is part of the GNU C Library.

   The GNU C Library is free software; you can redistribute it and/or
   modify it under the terms of the GNU Lesser General Public
