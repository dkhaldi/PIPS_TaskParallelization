/* IEEE Not A Number.  */
/* Copyright (C) 1992-2014 Free Software Foundation, Inc.
   This file is part of the GNU C Library.

   The GNU C Library is free software; you can redistribute it and/or
   modify it under the terms of the GNU Lesser General Public
   License as published by the Free Software Foundation; either
   version 2.1 of the License, or (at your option) any later version.

   The GNU C Library is distributed in the hope that it will be useful,
   but WITHOUT ANY WARRANTY; without even the implied warranty of
   MERCHANTABILITY or FITNESS FOR A PARTICULAR PURPOSE.  See the GNU
   Lesser General Public License for more details.

   You should have received a copy of the GNU Lesser General Public
   License along with the GNU C Library; if not, see
   <http://www.gnu.org/licenses/>.  */
static union {unsigned char __c[4]; float __d;} __qnan_union = {{0, 0, 0xc0, 0x7f}};   /*0149*/


/* Get general and ISO C99 specific information.  */
/* Copyright (C) 2001-2014 Free Software Foundation, Inc.
   This file is part of the GNU C Library.

   The GNU C Library is free software; you can redistribute it and/or
   modify it under the terms of the GNU Lesser General Public
   License as published by the Free Software Foundation; either
   version 2.1 of the License, or (at your option) any later version.

   The GNU C Library is distributed in the hope that it will be useful,
   but WITHOUT ANY WARRANTY; without even the implied warranty of
   MERCHANTABILITY or FITNESS FOR A PARTICULAR PURPOSE.  See the GNU
   Lesser General Public License for more details.

   You should have received a copy of the GNU Lesser General Public
   License along with the GNU C Library; if not, see
   <http://www.gnu.org/licenses/>.  */
/* The x86-64 architecture computes values with the precission of the
   used type.  Similarly for -m32 -mfpmath=sse.  */
typedef float float_t;                                                  /*0149*/
/* `float' expressions are evaluated as `float'.  */
typedef double double_t;                                                /*0149*/
/* `double' expressions are evaluated
				   as `double'.  */
/* The values returned by `ilogb' for 0 and NaN respectively.  */



/* The GCC 4.6 compiler will define __FP_FAST_FMA{,F,L} if the fma{,f,l}
   builtins are supported.  */

/* The file <bits/mathcalls.h> contains the prototypes for all the
   actual math functions.  These macros are used for those prototypes,
   so we can easily declare each function as both `name' and `__name',
   and can declare the float versions `namef' and `__namef'.  */
/* Prototype declarations for math functions; helper file for <math.h>.
   Copyright (C) 1996-2014 Free Software Foundation, Inc.
   This file is part of the GNU C Library.

   The GNU C Library is free software; you can redistribute it and/or
   modify it under the terms of the GNU Lesser General Public
   License as published by the Free Software Foundation; either
   version 2.1 of the License, or (at your option) any later version.

   The GNU C Library is distributed in the hope that it will be useful,
   but WITHOUT ANY WARRANTY; without even the implied warranty of
   MERCHANTABILITY or FITNESS FOR A PARTICULAR PURPOSE.  See the GNU
   Lesser General Public License for more details.

   You should have received a copy of the GNU Lesser General Public
   License along with the GNU C Library; if not, see
   <http://www.gnu.org/licenses/>.  */

/* NOTE: Because of the special way this file is used by <math.h>, this
   file must NOT be protected from multiple inclusion as header files
   usually are.

   This file provides prototype declarations for the math functions.
   Most functions are declared using the macro:

   __MATHCALL (NAME,[_r], (ARGS...));

   This means there is a function `NAME' returning `double' and a function
   `NAMEf' returning `float'.  Each place `_Mdouble_' appears in the
   prototype, that is actually `double' in the prototype for `NAME' and
   `float' in the prototype for `NAMEf'.  Reentrant variant functions are
   called `NAME_r' and `NAMEf_r'.

   Functions returning other types like `int' are declared using the macro:

   __MATHDECL (TYPE, NAME,[_r], (ARGS...));

   This is just like __MATHCALL but for a function returning `TYPE'
   instead of `_Mdouble_'.  In all of these cases, there is still
   both a `NAME' and a `NAMEf' that takes `float' arguments.

   Note that there must be no whitespace before the argument passed for
   NAME, to make token pasting work with -traditional.  */






/* Trigonometric functions.  */


/* Arc cosine of X.  */
extern double acos(double __x);                                         /*0149*/
extern double __acos(double __x);                                       /*0149*/
/* Arc sine of X.  */
extern double asin(double __x);                                         /*0149*/
extern double __asin(double __x);                                       /*0149*/
/* Arc tangent of X.  */
extern double atan(double __x);                                         /*0149*/
extern double __atan(double __x);                                       /*0149*/
/* Arc tangent of Y/X.  */
extern double atan2(double __y, double __x);                            /*0149*/
extern double __atan2(double __y, double __x);                          /*0149*/

/* Cosine of X.  */
extern double cos(double __x);                                          /*0149*/
extern double __cos(double __x);                                        /*0149*/
/* Sine of X.  */
extern double sin(double __x);                                          /*0149*/
extern double __sin(double __x);                                        /*0149*/
/* Tangent of X.  */
extern double tan(double __x);                                          /*0149*/
extern double __tan(double __x);                                        /*0149*/

/* Hyperbolic functions.  */

/* Hyperbolic cosine of X.  */
extern double cosh(double __x);                                         /*0149*/
extern double __cosh(double __x);                                       /*0149*/
/* Hyperbolic sine of X.  */
extern double sinh(double __x);                                         /*0149*/
extern double __sinh(double __x);                                       /*0149*/
/* Hyperbolic tangent of X.  */
extern double tanh(double __x);                                         /*0149*/
extern double __tanh(double __x);                                       /*0149*/



/* Cosine and sine of X.  */
extern void sincos(double __x, double *__sinx, double *__cosx);         /*0149*/
extern void __sincos(double __x, double *__sinx, double *__cosx);       /*0149*/




/* Hyperbolic arc cosine of X.  */
extern double acosh(double __x);                                        /*0149*/
extern double __acosh(double __x);                                      /*0149*/
/* Hyperbolic arc sine of X.  */
extern double asinh(double __x);                                        /*0149*/
extern double __asinh(double __x);                                      /*0149*/
/* Hyperbolic arc tangent of X.  */
extern double atanh(double __x);                                        /*0149*/
extern double __atanh(double __x);                                      /*0149*/



/* Exponential and logarithmic functions.  */


/* Exponential function of X.  */
extern double exp(double __x);                                          /*0149*/
extern double __exp(double __x);                                        /*0149*/

/* Break VALUE into a normalized fraction and an integral power of 2.  */
extern double frexp(double __x, int *__exponent);                       /*0149*/
extern double __frexp(double __x, int *__exponent);                     /*0149*/

/* X times (two to the EXP power).  */
extern double ldexp(double __x, int __exponent);                        /*0149*/
extern double __ldexp(double __x, int __exponent);                      /*0149*/

/* Natural logarithm of X.  */
extern double log(double __x);                                          /*0149*/
extern double __log(double __x);                                        /*0149*/

/* Base-ten logarithm of X.  */
extern double log10(double __x);                                        /*0149*/
extern double __log10(double __x);                                      /*0149*/

/* Break VALUE into integral and fractional parts.  */
extern double modf(double __x, double *__iptr);                         /*0149*/
extern double __modf(double __x, double *__iptr);                       /*0149*/



/* A function missing in all standards: compute exponent to base ten.  */
extern double exp10(double __x);                                        /*0149*/
extern double __exp10(double __x);                                      /*0149*/
/* Another name occasionally used.  */
extern double pow10(double __x);                                        /*0149*/
extern double __pow10(double __x);                                      /*0149*/




/* Return exp(X) - 1.  */
extern double expm1(double __x);                                        /*0149*/
extern double __expm1(double __x);                                      /*0149*/

/* Return log(1 + X).  */
extern double log1p(double __x);                                        /*0149*/
extern double __log1p(double __x);                                      /*0149*/

/* Return the base 2 signed integral exponent of X.  */
extern double logb(double __x);                                         /*0149*/
extern double __logb(double __x);                                       /*0149*/





/* Compute base-2 exponential of X.  */
extern double exp2(double __x);                                         /*0149*/
extern double __exp2(double __x);                                       /*0149*/

/* Compute base-2 logarithm of X.  */
extern double log2(double __x);                                         /*0149*/
extern double __log2(double __x);                                       /*0149*/




/* Power functions.  */


/* Return X to the Y power.  */
extern double pow(double __x, double __y);                              /*0149*/
extern double __pow(double __x, double __y);                            /*0149*/

/* Return the square root of X.  */
extern double sqrt(double __x);                                         /*0149*/
extern double __sqrt(double __x);                                       /*0149*/




/* Return `sqrt(X*X + Y*Y)'.  */
extern double hypot(double __x, double __y);                            /*0149*/
extern double __hypot(double __x, double __y);                          /*0149*/





/* Return the cube root of X.  */
extern double cbrt(double __x);                                         /*0149*/
extern double __cbrt(double __x);                                       /*0149*/




/* Nearest integer, absolute value, and remainder functions.  */


/* Smallest integral value not less than X.  */
extern double ceil(double __x);                                         /*0149*/
extern double __ceil(double __x);                                       /*0149*/

/* Absolute value of X.  */
extern double fabs(double __x);                                         /*0149*/
extern double __fabs(double __x);                                       /*0149*/

/* Largest integer not greater than X.  */
extern double floor(double __x);                                        /*0149*/
extern double __floor(double __x);                                      /*0149*/

/* Floating-point modulo remainder of X/Y.  */
extern double fmod(double __x, double __y);                             /*0149*/
extern double __fmod(double __x, double __y);                           /*0149*/


/* Return 0 if VALUE is finite or NaN, +1 if it
   is +Infinity, -1 if it is -Infinity.  */
extern int __isinf(double __value);                                     /*0149*/

/* Return nonzero if VALUE is finite and not NaN.  */
extern int __finite(double __value);                                    /*0149*/



/* Return 0 if VALUE is finite or NaN, +1 if it
   is +Infinity, -1 if it is -Infinity.  */
extern int isinf(double __value);                                       /*0149*/

/* Return nonzero if VALUE is finite and not NaN.  */
extern int finite(double __value);                                      /*0149*/

/* Return the remainder of X/Y.  */
extern double drem(double __x, double __y);                             /*0149*/
extern double __drem(double __x, double __y);                           /*0149*/


/* Return the fractional part of X after dividing out `ilogb (X)'.  */
extern double significand(double __x);                                  /*0149*/
extern double __significand(double __x);                                /*0149*/




/* Return X with its signed changed to Y's.  */
extern double copysign(double __x, double __y);                         /*0149*/
extern double __copysign(double __x, double __y);                       /*0149*/





/* Return representation of qNaN for double type.  */
extern double nan(const char *__tagb);                                  /*0149*/
extern double __nan(const char *__tagb);                                /*0149*/




/* Return nonzero if VALUE is not a number.  */
extern int isnan(double __value);                                       /*0149*/


/* Return nonzero if VALUE is not a number.  */
extern int isnan(double __value);                                       /*0149*/

/* Bessel functions.  */
extern double j0(double);                                               /*0149*/
extern double __j0(double);                                             /*0149*/
extern double j1(double);                                               /*0149*/
extern double __j1(double);                                             /*0149*/
extern double jn(int, double);                                          /*0149*/
extern double __jn(int, double);                                        /*0149*/
extern double y0(double);                                               /*0149*/
extern double __y0(double);                                             /*0149*/
extern double y1(double);                                               /*0149*/
extern double __y1(double);                                             /*0149*/
extern double yn(int, double);                                          /*0149*/
extern double __yn(int, double);                                        /*0149*/





/* Error and gamma functions.  */
extern double erf(double);                                              /*0149*/
extern double __erf(double);                                            /*0149*/
extern double erfc(double);                                             /*0149*/
extern double __erfc(double);                                           /*0149*/
extern double lgamma(double);                                           /*0149*/
extern double __lgamma(double);                                         /*0149*/





/* True gamma function.  */
extern double tgamma(double);                                           /*0149*/
extern double __tgamma(double);                                         /*0149*/




/* Obsolete alias for `lgamma'.  */
extern double gamma(double);                                            /*0149*/
extern double __gamma(double);                                          /*0149*/



/* Reentrant version of lgamma.  This function uses the global variable
   `signgam'.  The reentrant version instead takes a pointer and stores
   the value through it.  */
extern double lgamma_r(double, int *__signgamp);                        /*0149*/
extern double __lgamma_r(double, int *__signgamp);                      /*0149*/





/* Return the integer nearest X in the direction of the
   prevailing rounding mode.  */
extern double rint(double __x);                                         /*0149*/
extern double __rint(double __x);                                       /*0149*/

/* Return X + epsilon if X < Y, X - epsilon if X > Y.  */
extern double nextafter(double __x, double __y);                        /*0149*/
extern double __nextafter(double __x, double __y);                      /*0149*/

extern double nexttoward(double __x, long double __y);                  /*0149*/
extern double __nexttoward(double __x, long double __y);                /*0149*/


/* Return the remainder of integer divison X / Y with infinite precision.  */
extern double remainder(double __x, double __y);                        /*0149*/
extern double __remainder(double __x, double __y);                      /*0149*/


/* Return X times (2 to the Nth power).  */
extern double scalbn(double __x, int __n);                              /*0149*/
extern double __scalbn(double __x, int __n);                            /*0149*/


/* Return the binary exponent of X, which must be nonzero.  */
extern int ilogb(double __x);                                           /*0149*/
extern int __ilogb(double __x);                                         /*0149*/



/* Return X times (2 to the Nth power).  */
extern double scalbln(double __x, long int __n);                        /*0149*/
extern double __scalbln(double __x, long int __n);                      /*0149*/

/* Round X to integral value in floating-point format using current
   rounding direction, but do not raise inexact exception.  */
extern double nearbyint(double __x);                                    /*0149*/
extern double __nearbyint(double __x);                                  /*0149*/

/* Round X to nearest integral value, rounding halfway cases away from
   zero.  */
extern double round(double __x);                                        /*0149*/
extern double __round(double __x);                                      /*0149*/

/* Round X to the integral value in floating-point format nearest but
   not larger in magnitude.  */
extern double trunc(double __x);                                        /*0149*/
extern double __trunc(double __x);                                      /*0149*/

/* Compute remainder of X and Y and put in *QUO a value with sign of x/y
   and magnitude congruent `mod 2^n' to the magnitude of the integral
   quotient x/y, with n >= 3.  */
extern double remquo(double __x, double __y, int *__quo);               /*0149*/
extern double __remquo(double __x, double __y, int *__quo);             /*0149*/


/* Conversion functions.  */

/* Round X to nearest integral value according to current rounding
   direction.  */
extern long int lrint(double __x);                                      /*0149*/
extern long int __lrint(double __x);                                    /*0149*/

extern long long int llrint(double __x);                                /*0149*/
extern long long int __llrint(double __x);                              /*0149*/

/* Round X to nearest integral value, rounding halfway cases away from
   zero.  */
extern long int lround(double __x);                                     /*0149*/
extern long int __lround(double __x);                                   /*0149*/

extern long long int llround(double __x);                               /*0149*/
extern long long int __llround(double __x);                             /*0149*/


/* Return positive difference between X and Y.  */
extern double fdim(double __x, double __y);                             /*0149*/
extern double __fdim(double __x, double __y);                           /*0149*/

/* Return maximum numeric value from X and Y.  */
extern double fmax(double __x, double __y);                             /*0149*/
extern double __fmax(double __x, double __y);                           /*0149*/

/* Return minimum numeric value from X and Y.  */
extern double fmin(double __x, double __y);                             /*0149*/
extern double __fmin(double __x, double __y);                           /*0149*/


/* Classify given number.  */
extern int __fpclassify(double __value);                                /*0149*/

/* Test for negative number.  */
extern int __signbit(double __value);                                   /*0149*/


/* Multiply-add function computed as a ternary operation.  */
extern double fma(double __x, double __y, double __z);                  /*0149*/
extern double __fma(double __x, double __y, double __z);                /*0149*/







/* Test for signaling NaN.  */
extern int __issignaling(double __value);                               /*0149*/



/* Return X times (2 to the Nth power).  */
extern double scalb(double __x, double __n);                            /*0149*/
extern double __scalb(double __x, double __n);                          /*0149*/
/* Include the file of declarations again, this time using `float'
   instead of `double' and appending f to each function name.  */
