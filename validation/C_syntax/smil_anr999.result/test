/*!
    \brief Pick a specific uint8_t value in a freia_data2d
    
    You must take care to the bounds of coordinates. No specific
    protection is set up and a core dump could happen with bad
    coordinates

    \param[in] data pointer to a valid instance of freia_data2d
    \param[in] x x coordinate of the value to pick
    \param[in] y y coordinate of the value to pick
    \return value picked
  */

static uint8_t freia_common_get_uint8_t(freia_data2d *data, uint32_t x, uint32_t y);   /*0056*/

/*!
    \brief Pick a specific int16_t value in a freia_data2d
    
    You must take care to the bounds of coordinates. No specific
    protection is set up and a core dump could happen with bad
    coordinates

    \param[in] data pointer to a valid instance of freia_data2d
    \param[in] x x coordinate of the value to pick
    \param[in] y y coordinate of the value to pick
    \return value picked
  */

static int16_t freia_common_get_int16_t(freia_data2d *data, uint32_t x, uint32_t y);   /*0081*/

/*!
    \brief Pick a specific value in a freia_data2d
    
    You must take care to the bounds of coordinates. No specific
    protection is set up and a core dump could happen with bad
    coordinates

    \param[in] data pointer to a valid instance of freia_data2d
    \param[in] x x coordinate of the value to pick
    \param[in] y y coordinate of the value to pick
    \return value picked
  */

static int32_t freia_common_get(freia_data2d *data, uint32_t x, uint32_t y);           /*0106*/

/*!
    \brief Set a specific uint8_t value in a freia_data2d
    
    You must take care to the bounds of coordinates. No specific
    protection is set up and a core dump could happen with bad
    coordinates

    \param[in,out] data: pointer to a valid instance of freia_data2d
    \param[in] x x coordinate of the value to pick
    \param[in] y y coordinate of the value to pick
    \param[in] value uint8_t value to set in the data
    \return void
  */

static void freia_common_set_uint8_t(freia_data2d *data, uint32_t x, uint32_t y, uint8_t value);      /*0124*/

/*!
    \brief Set a specific int16_t value in a freia_data2d

    You must take care to the bounds of coordinates. No specific
    protection is set up and a core dump could happen with bad
    coordinates

    \param[in,out] data: pointer to a valid instance of freia_data2d
    \param[in] x x coordinate of the value to pick
    \param[in] y y coordinate of the value to pick
    \param[in] value int16_t value to set in the data
    \return void
  */

static void freia_common_set_int16_t(freia_data2d *data, uint32_t x, uint32_t y, int16_t value);      /*0148*/


/*!
    \brief Set a specific value in a freia_data2d
    
    You must take care to the bounds of coordinates. No specific
    protection is set up and a core dump could happen with bad
    coordinates

    \param[in,out] data: pointer to a valid instance of freia_data2d
    \param[in] x x coordinate of the value to pick
    \param[in] y y coordinate of the value to pick
    \param[in] value uint8_t value to set in the data
    \return void
  */

static void freia_common_set(freia_data2d *data, uint32_t x, uint32_t y, int32_t value);              /*0173*/

/*!
    \brief Set a specific uint8_t value in a freia_data2d
    
    If coordinates are out of the current working area, the function
    will not write any values

    \param[in,out] data: pointer to a valid instance of freia_data2d
    \param[in] x x coordinate of the value to pick
    \param[in] y y coordinate of the value to pick
    \param[in] value uint8_t value to set in the data
    \return void
  */

static void freia_common_setb_uint8_t(freia_data2d *data, int32_t x, int32_t y, uint8_t value);       /*0190*/

/*!
    \brief Set a specific int16_t value in a freia_data2d
    
    The access out of bounds of coordinates is managed by simulating an
    unfolding of the data using some symmetry properties
 
    \param[in,out] data: pointer to a valid instance of freia_data2d
    \param[in] x x coordinate of the value to pick
    \param[in] y y coordinate of the value to pick
    \param[in] value int16_t value to set in the data
    \return void
  */

static void freia_common_setb_int16_t(freia_data2d *data, int32_t x, int32_t y, int16_t value);       /*0210*/


/*!
    \brief Set a specific value in a freia_data2d
    
    The access out of bounds of coordinates is managed by simulating an
    unfolding of the data using some symmetry properties
 
    \param[in,out] data: pointer to a valid instance of freia_data2d
    \param[in] x x coordinate of the value to pick
    \param[in] y y coordinate of the value to pick
    \param[in] value int16_t value to set in the data
    \return void
  */

static void freia_common_setb(freia_data2d *data, int32_t x, int32_t y, int16_t value);               /*0231*/





/*!
    \brief Pick a specific uint8_t value in a freia_data2d
    
    The access out of bounds of coordinates is managed by simulating an
    unfolding of the data using some symmetry properties

    \param[in] data pointer to a valid instance of freia_data2d
    \param[in] x x coordinate of the value to pick
    \param[in] y y coordinate of the value to pick
    \return value picked
  */
extern uint8_t freia_common_getu_uint8_t(freia_data2d *data, int32_t x, int32_t y);    /*0248*/

