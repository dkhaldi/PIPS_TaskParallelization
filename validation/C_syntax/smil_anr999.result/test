typedef struct {readbufferproc bf_getreadbuffer; writebufferproc bf_getwritebuffer; segcountproc bf_getsegcount; charbufferproc bf_getcharbuffer; getbufferproc bf_getbuffer; releasebufferproc bf_releasebuffer;} PyBufferProcs;            /*0149*/


typedef void (*freefunc)(void *);                                       /*0149*/
typedef void (*destructor)(PyObject *);                                 /*0149*/
