typedef union sigval {                                                  /*0149*/
   int sival_int;
   void *sival_ptr;
} sigval_t;
typedef __clock_t __sigchld_clock_t;                                    /*0149*/



/* Signal number.  */
/* If non-zero, an errno value associated with
				   this signal, as defined in <errno.h>.  */
/* Signal code.  */
/* kill().  */
/* Sending process ID.  */
/* Real user ID of sending process.  */
/* POSIX.1b timers.  */
/* Timer ID.  */
/* Overrun count.  */
/* Signal value.  */
/* POSIX.1b signals.  */
/* Sending process ID.  */
/* Real user ID of sending process.  */
/* Signal value.  */
/* SIGCHLD.  */
/* Which child.  */
/* Real user ID of sending process.  */
/* Exit value or signal.  */
/* SIGILL, SIGFPE, SIGSEGV, SIGBUS.  */
/* Faulting insn/memory ref.  */
/* Valid LSB of the reported address.  */
/* SIGPOLL.  */
/* Band event for SIGPOLL.  */
/* SIGSYS.  */
/* Calling user insn.  */
/* Triggering system call number.  */
/* AUDIT_ARCH_* of syscall.  */
typedef struct {int si_signo; int si_errno; int si_code; union {int _pad[128/sizeof(int)-4]; struct {__pid_t si_pid; __uid_t si_uid;} _kill; struct {int si_tid; int si_overrun; sigval_t si_sigval;} _timer; struct {__pid_t si_pid; __uid_t si_uid; sigval_t si_sigval;} _rt; struct {__pid_t si_pid; __uid_t si_uid; int si_status; __sigchld_clock_t si_utime; __sigchld_clock_t si_stime;} _sigchld; struct {void *si_addr; short si_addr_lsb;} _sigfault; struct {long int si_band; int si_fd;} _sigpoll; struct {void *_call_addr; int _syscall; unsigned int _arch;} _sigsys;} _sifields;} siginfo_t;        /*0149*/


/* X/Open requires some more fields with fixed names.  */
/* Values for `si_code'.  Positive values are reserved for kernel-generated
   signals.  */
/* Sent by asynch name lookup completion.  */
/* Sent by tkill.  */
/* Sent by queued SIGIO. */
/* Sent by AIO completion.  */
/* Sent by real time mesq state change.  */
/* Sent by timer expiration.  */
/* Sent by sigqueue.  */
/* Sent by kill, sigsend.  */
/* Send by kernel.  */
enum {SI_ASYNCNL=-60, SI_TKILL=-6, SI_SIGIO, SI_ASYNCIO, SI_MESGQ, SI_TIMER, SI_QUEUE, SI_USER, SI_KERNEL=0x80};     /*0149*/


/* `si_code' values for SIGILL signal.  */
/* Illegal opcode.  */
/* Illegal operand.  */
/* Illegal addressing mode.  */
/* Illegal trap. */
/* Privileged opcode.  */
/* Privileged register.  */
/* Coprocessor error.  */
/* Internal stack error.  */
enum {ILL_ILLOPC=1, ILL_ILLOPN, ILL_ILLADR, ILL_ILLTRP, ILL_PRVOPC, ILL_PRVREG, ILL_COPROC, ILL_BADSTK};             /*0149*/

/* `si_code' values for SIGFPE signal.  */
/* Integer divide by zero.  */
/* Integer overflow.  */
/* Floating point divide by zero.  */
/* Floating point overflow.  */
/* Floating point underflow.  */
/* Floating point inexact result.  */
/* Floating point invalid operation.  */
/* Subscript out of range.  */
enum {FPE_INTDIV=1, FPE_INTOVF, FPE_FLTDIV, FPE_FLTOVF, FPE_FLTUND, FPE_FLTRES, FPE_FLTINV, FPE_FLTSUB};             /*0149*/

/* `si_code' values for SIGSEGV signal.  */
/* Address not mapped to object.  */
/* Invalid permissions for mapped object.  */
enum {SEGV_MAPERR=1, SEGV_ACCERR};                                      /*0149*/

/* `si_code' values for SIGBUS signal.  */
/* Invalid address alignment.  */
/* Non-existant physical address.  */
/* Object specific hardware error.  */
/* Hardware memory error: action required.  */
/* Hardware memory error: action optional.  */
enum {BUS_ADRALN=1, BUS_ADRERR, BUS_OBJERR, BUS_MCEERR_AR, BUS_MCEERR_AO};             /*0149*/

/* `si_code' values for SIGTRAP signal.  */
/* Process breakpoint.  */
/* Process trace trap.  */
enum {TRAP_BRKPT=1, TRAP_TRACE};                                        /*0149*/

/* `si_code' values for SIGCHLD signal.  */
/* Child has exited.  */
/* Child was killed.  */
/* Child terminated abnormally.  */
/* Traced child has trapped.  */
/* Child has stopped.  */
/* Stopped child has continued.  */
enum {CLD_EXITED=1, CLD_KILLED, CLD_DUMPED, CLD_TRAPPED, CLD_STOPPED, CLD_CONTINUED};  /*0149*/

/* `si_code' values for SIGPOLL signal.  */
/* Data input available.  */
/* Output buffers available.  */
/* Input message available.   */
/* I/O error.  */
/* High priority input available.  */
/* Device disconnected.  */
enum {POLL_IN=1, POLL_OUT, POLL_MSG, POLL_ERR, POLL_PRI, POLL_HUP};     /*0149*/
/* Structure to transport application-defined values with signals.  */







/* Forward declaration.  */





/* When SIGEV_SIGNAL and SIGEV_THREAD_ID set, LWP ID of the
	   thread to receive the signal.  */
/* Function to start.  */
/* Thread attributes.  */
typedef struct sigevent {                                               /*0149*/
   sigval_t sigev_value;
   int sigev_signo;
   int sigev_notify;
   union {int _pad[64/sizeof(int)-4]; __pid_t _tid; struct {void (*_function)(sigval_t); pthread_attr_t *_attribute;} _sigev_thread;} _sigev_un;
} sigevent_t;

/* POSIX names to access some of the members.  */



/* `sigev_notify' values.  */
/* Notify via signal.  */
/* Other notification: meaningless.  */
/* Deliver via thread creation.  */
/* Send signal to specific thread.  */
enum {SIGEV_SIGNAL, SIGEV_NONE, SIGEV_THREAD, SIGEV_THREAD_ID=4};       /*0149*/



/* Type of a signal handler.  */
typedef void (*__sighandler_t)(int);                                    /*0149*/

/* The X/Open definition of `signal' specifies the SVID semantic.  Use
   the additional function `sysv_signal' when X/Open compatibility is
   requested.  */
extern __sighandler_t __sysv_signal(int __sig, __sighandler_t __handler);              /*0149*/

extern __sighandler_t sysv_signal(int __sig, __sighandler_t __handler);                /*0149*/


/* Set the handler for the signal SIG to HANDLER, returning the old
   handler, or SIG_ERR on error.
   By default `signal' has the BSD semantic.  */


extern __sighandler_t signal(int __sig, __sighandler_t __handler);      /*0149*/



/* The X/Open definition of `signal' conflicts with the BSD version.
   So they defined another function `bsd_signal'.  */
extern __sighandler_t bsd_signal(int __sig, __sighandler_t __handler);  /*0149*/


/* Send signal SIG to process number PID.  If PID is zero,
   send SIG to all processes in the current process's process group.
   If PID is < -1, send SIG to all processes in process group - PID.  */

extern int kill(__pid_t __pid, int __sig);                              /*0149*/



/* Send SIG to all processes in process group PGRP.
   If PGRP is zero, send SIG to all processes in
   the current process's process group.  */
extern int killpg(__pid_t __pgrp, int __sig);                           /*0149*/



/* Raise signal SIG, i.e., send SIG to yourself.  */
extern int raise(int __sig);                                            /*0149*/



/* SVID names for the same things.  */
extern __sighandler_t ssignal(int __sig, __sighandler_t __handler);     /*0149*/
extern int gsignal(int __sig);                                          /*0149*/



/* Print a message describing the meaning of the given signal number.  */
extern void psignal(int __sig, const char *__s);                        /*0149*/



/* Print a message describing the meaning of the given signal information.  */
extern void psiginfo(const siginfo_t *__pinfo, const char *__s);        /*0149*/




/* The `sigpause' function in X/Open defines the argument as the
   signal number.  This requires redirecting to another function
   because the default version in glibc uses an old BSD interface.

   This function is a cancellation point and therefore not marked with
   __THROW.  */
extern int __sigpause(int __sig_or_mask, int __is_sig);                 /*0149*/





/* Remove a signal from the signal mask and suspend the process.  */






/* None of the following functions should be used anymore.  They are here
   only for compatibility.  A single word (`int') is not guaranteed to be
   enough to hold a complete signal mask and therefore these functions
   simply do not work in many situations.  Use `sigprocmask' instead.  */

/* Compute mask for signal SIG.  */


/* Block signals in MASK, returning the old mask.  */
extern int sigblock(int __mask);                                        /*0149*/

/* Set the mask of blocked signals to MASK, returning the old mask.  */
extern int sigsetmask(int __mask);                                      /*0149*/

/* Return currently selected signal mask.  */
extern int siggetmask(void);                                            /*0149*/
typedef __sighandler_t sighandler_t;                                    /*0149*/


/* 4.4 BSD uses the name `sig_t' for this.  */

typedef __sighandler_t sig_t;                                           /*0149*/




/* Clear all signals from SET.  */
extern int sigemptyset(sigset_t *__set);                                /*0149*/

/* Set all signals in SET.  */
extern int sigfillset(sigset_t *__set);                                 /*0149*/

/* Add SIGNO to SET.  */
extern int sigaddset(sigset_t *__set, int __signo);                     /*0149*/

/* Remove SIGNO from SET.  */
extern int sigdelset(sigset_t *__set, int __signo);                     /*0149*/

/* Return 1 if SIGNO is in SET, 0 if not.  */
extern int sigismember(const sigset_t *__set, int __signo);             /*0149*/


/* Return non-empty value is SET is not empty.  */
extern int sigisemptyset(const sigset_t *__set);                        /*0149*/

/* Build new signal set by combining the two inputs set using logical AND.  */
extern int sigandset(sigset_t *__set, const sigset_t *__left, const sigset_t *__right);               /*0149*/

/* Build new signal set by combining the two inputs set using logical OR.  */
extern int sigorset(sigset_t *__set, const sigset_t *__left, const sigset_t *__right);                /*0149*/


/* Get the system-specific definitions of `struct sigaction'
   and the `SA_*' and `SIG_*'. constants.  */
/* The proper definitions for Linux's sigaction.
   Copyright (C) 1993-2014 Free Software Foundation, Inc.
   This file is part of the GNU C Library.

   The GNU C Library is free software; you can redistribute it and/or
   modify it under the terms of the GNU Lesser General Public
   License as published by the Free Software Foundation; either
   version 2.1 of the License, or (at your option) any later version.

   The GNU C Library is distributed in the hope that it will be useful,
   but WITHOUT ANY WARRANTY; without even the implied warranty of
   MERCHANTABILITY or FITNESS FOR A PARTICULAR PURPOSE.  See the GNU
   Lesser General Public License for more details.

   You should have received a copy of the GNU Lesser General Public
   License along with the GNU C Library; if not, see
   <http://www.gnu.org/licenses/>.  */





/* Structure describing the action to be taken when a signal arrives.  */
/* Signal handler.  */
/* Used if SA_SIGINFO is not set.  */
/* Used if SA_SIGINFO is set.  */
/* Additional set of signals to be blocked.  */
/* Special flags.  */
/* Restore handler.  */
struct sigaction {                                                      /*0149*/
   union {__sighandler_t sa_handler; void (*sa_sigaction)(int, siginfo_t *, void *);} __sigaction_handler;
   __sigset_t sa_mask;
   int sa_flags;
   void (*sa_restorer)(void);
};

/* Bits in `sa_flags'.  */
/* Some aliases for the SA_ constants.  */





/* Values for the HOW argument to `sigprocmask'.  */

/* Get and/or change the set of blocked signals.  */
extern int sigprocmask(int __how, const sigset_t *__set, sigset_t *__oset);            /*0149*/

/* Change the set of blocked signals to SET,
   wait until a signal arrives, and restore the set of blocked signals.

   This function is a cancellation point and therefore not marked with
   __THROW.  */
extern int sigsuspend(const sigset_t *__set);                           /*0149*/

/* Get and/or set the action for signal SIG.  */
extern int sigaction(int __sig, const struct sigaction *__act, struct sigaction *__oact);             /*0149*/

/* Put in SET all signals that are blocked and waiting to be delivered.  */
extern int sigpending(sigset_t *__set);                                 /*0149*/


/* Select any of pending signals from SET or wait for any to arrive.

   This function is a cancellation point and therefore not marked with
   __THROW.  */
extern int sigwait(const sigset_t *__set, int *__sig);                  /*0149*/


/* Select any of pending signals from SET and place information in INFO.

   This function is a cancellation point and therefore not marked with
   __THROW.  */
extern int sigwaitinfo(const sigset_t *__set, siginfo_t *__info);       /*0149*/

/* Select any of pending signals from SET and place information in INFO.
   Wait the time specified by TIMEOUT if no signal is pending.

   This function is a cancellation point and therefore not marked with
   __THROW.  */
extern int sigtimedwait(const sigset_t *__set, siginfo_t *__info, const struct timespec *__timeout);  /*0149*/

/* Send signal SIG to the process PID.  Associate data in VAL with the
   signal.  */
extern int sigqueue(__pid_t __pid, int __sig, const union sigval __val);               /*0149*/






/* Names of the signals.  This variable exists only for compatibility.
   Use `strsignal' instead (see <string.h>).  */
extern const char *_sys_siglist[65];                                    /*0149*/
extern const char *sys_siglist[65];                                     /*0149*/

/* Structure passed to `sigvec'.  */
/* Signal handler.  */
/* Mask of signals to be blocked.  */
/* Flags (see below).  */
struct sigvec {                                                         /*0149*/
   __sighandler_t sv_handler;
   int sv_mask;
   int sv_flags;
};

/* Bits in `sv_flags'.  */





/* If VEC is non-NULL, set the handler for SIG to the `sv_handler' member
   of VEC.  The signals in `sv_mask' will be blocked while the handler runs.
   If the SV_RESETHAND bit is set in `sv_flags', the handler for SIG will be
   reset to SIG_DFL before `sv_handler' is entered.  If OVEC is non-NULL,
   it is filled in with the old information for SIG.  */
extern int sigvec(int __sig, const struct sigvec *__vec, struct sigvec *__ovec);       /*0149*/


/* Get machine-dependent `struct sigcontext' and signal subcodes.  */
/* Copyright (C) 2002-2014 Free Software Foundation, Inc.
   This file is part of the GNU C Library.

   The GNU C Library is free software; you can redistribute it and/or
   modify it under the terms of the GNU Lesser General Public
   License as published by the Free Software Foundation; either
   version 2.1 of the License, or (at your option) any later version.

   The GNU C Library is distributed in the hope that it will be useful,
   but WITHOUT ANY WARRANTY; without even the implied warranty of
   MERCHANTABILITY or FITNESS FOR A PARTICULAR PURPOSE.	 See the GNU
   Lesser General Public License for more details.

   You should have received a copy of the GNU Lesser General Public
   License along with the GNU C Library; if not, see
   <http://www.gnu.org/licenses/>.  */
struct _fpx_sw_bytes {                                                  /*0149*/
   __uint32_t magic1;
   __uint32_t extended_size;
   __uint64_t xstate_bv;
   __uint32_t xstate_size;
   __uint32_t padding[7];
};

struct _fpreg {                                                         /*0149*/
   unsigned short significand[4];
   unsigned short exponent;
};

struct _fpxreg {                                                        /*0149*/
   unsigned short significand[4];
   unsigned short exponent;
   unsigned short padding[3];
};

struct _xmmreg {                                                        /*0149*/
   __uint32_t element[4];
};
/* FPU environment matching the 64-bit FXSAVE layout.  */
struct _fpstate {                                                       /*0149*/
   __uint16_t cwd;
   __uint16_t swd;
   __uint16_t ftw;
   __uint16_t fop;
   __uint64_t rip;
   __uint64_t rdp;
   __uint32_t mxcsr;
   __uint32_t mxcr_mask;
   struct _fpxreg _st[8];
   struct _xmmreg _xmm[16];
   __uint32_t padding[24];
};

struct sigcontext {                                                     /*0149*/
   __uint64_t r8;
   __uint64_t r9;
   __uint64_t r10;
   __uint64_t r11;
   __uint64_t r12;
   __uint64_t r13;
   __uint64_t r14;
   __uint64_t r15;
   __uint64_t rdi;
   __uint64_t rsi;
   __uint64_t rbp;
   __uint64_t rbx;
   __uint64_t rdx;
   __uint64_t rax;
   __uint64_t rcx;
   __uint64_t rsp;
   __uint64_t rip;
   __uint64_t eflags;
   unsigned short cs;
   unsigned short gs;
   unsigned short fs;
   unsigned short __pad0;
   __uint64_t err;
   __uint64_t trapno;
   __uint64_t oldmask;
   __uint64_t cr2;
   union {struct _fpstate *fpstate; __uint64_t __fpstate_word;} PIPS_MEMBER_114;
   __uint64_t __reserved1[8];
};



struct _xsave_hdr {                                                     /*0149*/
   __uint64_t xstate_bv;
   __uint64_t reserved1[2];
   __uint64_t reserved2[5];
};

struct _ymmh_state {                                                    /*0149*/
   __uint32_t ymmh_space[64];
};

struct _xstate {                                                        /*0149*/
   struct _fpstate fpstate;
   struct _xsave_hdr xstate_hdr;
   struct _ymmh_state ymmh;
};

/* Restore the state saved in SCP.  */
extern int sigreturn(struct sigcontext *__scp);                         /*0149*/






