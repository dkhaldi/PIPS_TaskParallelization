PyObject *PyBuffer_FromReadWriteObject(PyObject *base, Py_ssize_t offset, Py_ssize_t size);           /*0149*/

PyObject *PyBuffer_FromMemory(void *ptr, Py_ssize_t size);              /*0149*/
PyObject *PyBuffer_FromReadWriteMemory(void *ptr, Py_ssize_t size);     /*0149*/

