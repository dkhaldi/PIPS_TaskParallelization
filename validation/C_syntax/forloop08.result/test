int main()
{
   int i, j = 0;                                                        /*0012*/
   
   // This is an overcommented for loop :-)
   /* before init */
   /* after init */
   /* before cond */
   /* after cond */
   /* before inc */
   /* after inc */
   for (i = 0;i<5;i++)                                                  /*0015*/
      /* loop body */
      j++;                                                              /*0017*/
   /* after the loop */
   
   return 0;                                                            /*0022*/
}
