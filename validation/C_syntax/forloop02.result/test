
PARSED PRINTED FILE for main

int main()
{
   int i, j;
   int a[5];
   for (i = 1, j = 2;i<5;i++, j += 2)
      a[i] = i;
}

PRINTED FILE for main

int main()
{
   int i, j;
   int a[5];
   i = 1, j = 2;
   while (i<5) {
      a[i] = i;
      i++, j += 2;
   }
}

PRINTED FILE with effects for main

int main()
{
   int i, j;
   int a[5];
//             <    is written>: i j
   i = 1, j = 2;
//             <    is read   >: i
   while (i<5) {
//             <    is read   >: i
//             <    is written>: a[i]
      a[i] = i;
//             <    is read   >: i j
//             <    is written>: i j
      i++, j += 2;
   }
}

PRINTED FILE with transformers for main


//  T() {}

int main()
{

//  T() {}

   int i, j;

//  T() {}

   int a[5];

//  T(i,j) {i==1, j==2}

   i = 1, j = 2;

//  T(i,j) {i<=4, i#init<=4}

   while (i<5) {

//  T() {}

      a[i] = i;

//  T(i,j) {i==i#init+1, j==j#init+2}

      i++, j += 2;
   }
}
