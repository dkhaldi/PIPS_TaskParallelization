void bar()
{
   int bar;
   foobar = 10;
}
//Checks the memory allocation of extern variables in 3 files
//bar.c: 2 external declarations and one local variable
extern int i;
extern int foobar;
extern void bar();
//foo.c: global variable declaration
int i;
//foobar.c: global variables and extern variable declarations
int foobar;
int j;
extern int main();
int main()
{
   extern int i;
   i = 10;
   return 0;
}
