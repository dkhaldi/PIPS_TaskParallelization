
Parser output

int main()
{
   unsigned int i = 4294967295U;
   unsigned int j;
   int k;
   int t;
   int u;

   j = i;
   k = j;
   t = i-j+k;
   i = u;
   j = u;
   k = u;
}

Controlizer output

int main()
{
   unsigned int i = 4294967295U;
   unsigned int j;
   int k;
   int t;
   int u;

   j = i;
   k = j;
   t = i-j+k;
   i = u;
   j = u;
   k = u;
}

Preconditions


//  P() {}

int main()
{

//  P() {}

   unsigned int i = 4294967295U;

//  P(i) {i==-1}

   unsigned int j;

//  P(i) {i==-1}

   int k;

//  P(i) {i==-1}

   int t;

//  P(i) {i==-1}

   int u;

//  P(i) {i==-1}


   j = i;

//  P(i,j) {i==-1, j==-1}

   k = j;

//  P(i,j,k) {i==-1, j==-1, k==-1}

   t = i-j+k;

//  P(i,j,k,t) {i==-1, j==-1, k==-1, t==-1}

   i = u;

//  P(i,j,k,t) {i==u, j==-1, k==-1, t==-1}

   j = u;

//  P(i,j,k,t) {i==j, i==u, k==-1, t==-1}

   k = u;
}

