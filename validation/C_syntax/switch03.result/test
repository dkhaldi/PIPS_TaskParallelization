int switch03(int predicate)
{
   int x = 0;

   if (predicate==0)
      goto _switch_1_case_0;
   if (predicate==1)
      goto _switch_1_case_1;
   if (predicate==2)
      goto _switch_1_case_2;
   if (predicate==3)
      goto _switch_1_case_3;
   goto _break_1;
_switch_1_case_0:   ;
   return 111;
_switch_1_case_1:   ;
   x = x+1;
_switch_1_case_2:   ;
   return x+3;
_switch_1_case_3:   ;
   goto _break_1;
_break_1:   ;
   return 333;
}

