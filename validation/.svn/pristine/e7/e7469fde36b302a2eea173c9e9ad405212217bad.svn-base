int freia_while_01(void)
{
   freia_dataio fdin, fdout;                                            /*0004*/
   freia_data2d *in, *out;                                              /*0005*/

   freia_common_open_input(&fdin, 0);                                   /*0007*/
   freia_common_open_output(&fdout, 0, fdin.framewidth, fdin.frameheight, fdin.framebpp);             /*0009*/

   in = freia_common_create_data(fdin.framebpp, fdin.framewidth, fdin.frameheight);    /*0012*/
   out = freia_common_create_data(fdin.framebpp, fdin.framewidth, fdin.frameheight);   /*0014*/
   
   // input
   freia_common_rx_image(in, &fdin);                                    /*0017*/

   int i = 0;                                                           /*0019*/
   while (i++<10) {                                                     /*0020*/
      freia_data2d *w1 = freia_common_create_data(in->bpp, in->widthWa, in->heightWa);                /*0022*/

      freia_mppa_launch(0, 1, 1, 1, 0, in, out, w1->bpp==16?32767:255);                /*0027*/

      freia_common_destruct_data(w1);                                   /*0029*/
   }
   
   // output
   freia_common_tx_image(in, &fdout);                                   /*0033*/
   freia_common_tx_image(out, &fdout);                                  /*0034*/
   
   // cleanup
   freia_common_destruct_data(in);                                      /*0037*/
   freia_common_destruct_data(out);                                     /*0038*/

   freia_common_close_input(&fdin);                                     /*0040*/
   freia_common_close_output(&fdout);                                   /*0041*/

   return 0;                                                            /*0043*/
}

