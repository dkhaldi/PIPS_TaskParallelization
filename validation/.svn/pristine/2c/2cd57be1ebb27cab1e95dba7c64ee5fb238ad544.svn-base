C    3<=J, NP<=J+1, 2<=N, NT+1<=N}>
C  <__IO_EFFECTS:LUNS(PHI1)-W-EXACT-{PHI1==2, K==3, KM+KP==3, 3<=I,
C    3<=J, NP<=J+1, 2<=N, NT+1<=N}>

C////////////////END   FUNCTION TIMER/////////////////

C
C     SORTIE FICHIER DU SISMOGRAMME
C     -----------------------------

      WRITE (2, *) NBTRAC                                               0604

C  <__IO_EFFECTS:LUNS(PHI1)-R-EXACT-{PHI1==2, K==3, KM+KP==3, 3<=I,
C    3<=J, NP<=J+1, 2<=N, NT+1<=N}>
C  <__IO_EFFECTS:LUNS(PHI1)-W-EXACT-{PHI1==2, K==3, KM+KP==3, 3<=I,
C    3<=J, NP<=J+1, 2<=N, NT+1<=N}>

      WRITE (2, *) NT                                                   0605

C  <SISMO(PHI1,PHI2)-R-EXACT-{1<=PHI1, PHI1<=50, 1<=PHI2, PHI2<=NT,
C    KM+KP==3, 3<=I, NT+1<=N}>
C  <__IO_EFFECTS:LUNS(PHI1)-R-EXACT-{PHI1==2, KM+KP==3, 3<=I, NT+1<=N,
C    1<=NT}>
C  <__IO_EFFECTS:LUNS(PHI1)-W-EXACT-{PHI1==2, KM+KP==3, 3<=I, NT+1<=N,
C    1<=NT}>

      DO 82 K = 1, NBTRAC                                               0606

C  <SISMO(PHI1,PHI2)-R-EXACT-{PHI1==K, 1<=PHI2, PHI2<=NT, KM+KP==3,
C    3<=I, 1<=K, K<=50, NT+1<=N}>
C  <__IO_EFFECTS:LUNS(PHI1)-R-EXACT-{PHI1==2, KM+KP==3, 3<=I, 1<=K,
C    K<=50, NT+1<=N, 1<=NT}>
C  <__IO_EFFECTS:LUNS(PHI1)-W-EXACT-{PHI1==2, KM+KP==3, 3<=I, 1<=K,
C    K<=50, NT+1<=N, 1<=NT}>

         DO 83 J = 1, NT                                                0607

C  <SISMO(PHI1,PHI2)-R-EXACT-{PHI1==K, PHI2==J, KM+KP==3, 3<=I, 1<=J,
C    J<=NT, 1<=K, K<=50, NT+1<=N}>
C  <__IO_EFFECTS:LUNS(PHI1)-R-EXACT-{PHI1==2, KM+KP==3, 3<=I, 1<=J,
C    J<=NT, 1<=K, K<=50, NT+1<=N}>
C  <__IO_EFFECTS:LUNS(PHI1)-W-EXACT-{PHI1==2, KM+KP==3, 3<=I, 1<=J,
C    J<=NT, 1<=K, K<=50, NT+1<=N}>

            WRITE (2, *) SISMO(K,J)                                     0608
83          CONTINUE                                                    0609
82       CONTINUE                                                       0610

C////////////////BEGIN FUNCTION TIMER/////////////////
      TFIN = GETIME(W)                                                  0613
      T3CPU = T3CPU+TFIN-TINIT-TMESUR                                   0614

C  <__IO_EFFECTS:LUNS(PHI1)-R-EXACT-{PHI1==1, K==51, KM+KP==3, 3<=I,
C    1<=J, J<=N, NT+1<=J, 2<=N}>
C  <__IO_EFFECTS:LUNS(PHI1)-W-EXACT-{PHI1==1, K==51, KM+KP==3, 3<=I,
C    1<=J, J<=N, NT+1<=J, 2<=N}>

      WRITE (1, 1020) T3CPU                                             0615

C  <__IO_EFFECTS:LUNS(PHI1)-R-EXACT-{PHI1==1, K==51, KM+KP==3, 3<=I,
C    1<=J, J<=N, NT+1<=J, 2<=N}>
C  <__IO_EFFECTS:LUNS(PHI1)-W-EXACT-{PHI1==1, K==51, KM+KP==3, 3<=I,
C    1<=J, J<=N, NT+1<=J, 2<=N}>

      WRITE (1, 1030) T1CPU+T2CPU+T3CPU                                 0616
C////////////////END   FUNCTION TIMER/////////////////

C
C     CALCUL DU NOMBRE DE MFLOPS
C     --------------------------

C     INITIALISATIONS
C     ---------------
      NBOP1 = 114.0+(NP-3)*72.0+(NP-4)*(NP-4)*4.0                       0625

C     RESOLUTION
C     ----------
      NBOP2 = (NT-1)*((NP-4)*(NP-4)*14.0+(NP-3)*27.0+(NP-4)*33.0+26.0   0629
     &)                                                                 0629


      MFLOPS = NBOP2/(T2CPU*1.0E06)                                     0633

C  <__IO_EFFECTS:LUNS(PHI1)-R-EXACT-{PHI1==1, K==51, KM+KP==3, 3<=I,
C    1<=J, J<=N, NT+1<=J, 2<=N}>
C  <__IO_EFFECTS:LUNS(PHI1)-W-EXACT-{PHI1==1, K==51, KM+KP==3, 3<=I,
C    1<=J, J<=N, NT+1<=J, 2<=N}>

      WRITE (1, 1060) MFLOPS                                            0634

      MFLOPS = (NBOP1+NBOP2)/((T1CPU+T2CPU)*1.0E06)                     0636

C  <__IO_EFFECTS:LUNS(PHI1)-R-EXACT-{PHI1==1, K==51, KM+KP==3, 3<=I,
C    1<=J, J<=N, NT+1<=J, 2<=N}>
C  <__IO_EFFECTS:LUNS(PHI1)-W-EXACT-{PHI1==1, K==51, KM+KP==3, 3<=I,
C    1<=J, J<=N, NT+1<=J, 2<=N}>

      WRITE (1, 1065) MFLOPS                                            0637

      MFLOPS = (NBOP1+NBOP2)/((T1CPU+T2CPU+T3CPU)*1.0E06)               0639

C  <__IO_EFFECTS:LUNS(PHI1)-R-EXACT-{PHI1==1, K==51, KM+KP==3, 3<=I,
C    1<=J, J<=N, NT+1<=J, 2<=N}>
C  <__IO_EFFECTS:LUNS(PHI1)-W-EXACT-{PHI1==1, K==51, KM+KP==3, 3<=I,
C    1<=J, J<=N, NT+1<=J, 2<=N}>

      WRITE (1, 1070) MFLOPS                                            0641

C  <U(PHI1,PHI2,PHI3)-R-EXACT-{PHI1==1, PHI2==1, PHI3==KM, K==51,
C    KM+KP==3, 3<=I, 1<=J, J<=N, NT+1<=J, 2<=N}>



C
C     CALCUL DE STATISTIQUES SUR LE RESULTAT
C     --------------------------------------
      UMAX = U(1,1,KM)                                                  0647

C  <U(PHI1,PHI2,PHI3)-R-EXACT-{PHI1==1, PHI2==1, PHI3==KM, K==51,
C    KM+KP==3, 3<=I, 1<=J, J<=N, NT+1<=J, 2<=N}>

      UMIN = U(1,1,KM)                                                  0648

C  <U(PHI1,PHI2,PHI3)-R-EXACT-{PHI3==KM, 1<=PHI1, PHI1<=NP, 1<=PHI2,
C    PHI2<=NP, K==51, KM+KP==3, 2<=N, NT+1<=N}>

      DO 110 J = 1, NP                                                  0649

C  <U(PHI1,PHI2,PHI3)-R-EXACT-{PHI2==J, PHI3==KM, 1<=PHI1, PHI1<=NP,
C    K==51, KM+KP==3, 1<=J, J<=NP, 2<=N, NT+1<=N}>

         DO 120 I = 1, NP                                               0650

C  <U(PHI1,PHI2,PHI3)-R-EXACT-{PHI1==I, PHI2==J, PHI3==KM, K==51,
C    KM+KP==3, 1<=I, I<=NP, 1<=J, J<=NP, 2<=N, NT+1<=N}>

            IF (U(I,J,KM).LT.UMIN) THEN                                 0651

C  <U(PHI1,PHI2,PHI3)-R-EXACT-{PHI1==I, PHI2==J, PHI3==KM, K==51,
C    KM+KP==3, 1<=I, I<=NP, 1<=J, J<=NP, 2<=N, NT+1<=N}>

               UMIN = U(I,J,KM)                                         0652
            ENDIF

C  <U(PHI1,PHI2,PHI3)-R-EXACT-{PHI1==I, PHI2==J, PHI3==KM, K==51,
C    KM+KP==3, 1<=I, I<=NP, 1<=J, J<=NP, 2<=N, NT+1<=N}>

            IF (U(I,J,KM).GT.UMAX) THEN                                 0654

C  <U(PHI1,PHI2,PHI3)-R-EXACT-{PHI1==I, PHI2==J, PHI3==KM, K==51,
C    KM+KP==3, 1<=I, I<=NP, 1<=J, J<=NP, 2<=N, NT+1<=N}>

               UMAX = U(I,J,KM)                                         0655
            ENDIF
120         CONTINUE                                                    0657
110      CONTINUE                                                       0658

C  <__IO_EFFECTS:LUNS(PHI1)-R-EXACT-{PHI1==1, K==51, KM+KP==3, J<=I,
C    4<=I+J, 1<=J, NP+1<=J, 2<=N, NT+1<=N}>
C  <__IO_EFFECTS:LUNS(PHI1)-W-EXACT-{PHI1==1, K==51, KM+KP==3, J<=I,
C    4<=I+J, 1<=J, NP+1<=J, 2<=N, NT+1<=N}>

      WRITE (1, 1050) UMIN, UMAX                                        0659

C  <SISMO(PHI1,PHI2)-R-EXACT-{PHI1==1, PHI2==1, K==51, KM+KP==3, J<=I,
C    4<=I+J, 1<=J, NP+1<=J, 2<=N, NT+1<=N}>



      SISMAX = SISMO(1,1)                                               0662

C  <SISMO(PHI1,PHI2)-R-EXACT-{PHI1==1, PHI2==1, K==51, KM+KP==3, J<=I,
C    4<=I+J, 1<=J, NP+1<=J, 2<=N, NT+1<=N}>

      SISMIN = SISMO(1,1)                                               0663

C  <SISMO(PHI1,PHI2)-R-EXACT-{1<=PHI1, PHI1<=50, 1<=PHI2, PHI2<=NT,
C    K==51, KM+KP==3, NT+1<=N}>

      DO 111 I = 1, NBTRAC                                              0664

C  <SISMO(PHI1,PHI2)-R-EXACT-{PHI1==I, 1<=PHI2, PHI2<=NT, K==51,
C    KM+KP==3, 1<=I, I<=50, NT+1<=N}>

         DO 121 J = 1, NT                                               0665

C  <SISMO(PHI1,PHI2)-R-EXACT-{PHI1==I, PHI2==J, K==51, KM+KP==3, 1<=I,
C    I<=50, 1<=J, J<=NT, NT+1<=N}>

            IF (SISMO(I,J).LT.SISMIN) THEN                              0666

C  <SISMO(PHI1,PHI2)-R-EXACT-{PHI1==I, PHI2==J, K==51, KM+KP==3, 1<=I,
C    I<=50, 1<=J, J<=NT, 2<=N, NT+1<=N}>

               SISMIN = SISMO(I,J)                                      0667
            ENDIF

C  <SISMO(PHI1,PHI2)-R-EXACT-{PHI1==I, PHI2==J, K==51, KM+KP==3, 1<=I,
C    I<=50, 1<=J, J<=NT, NT+1<=N}>

            IF (SISMO(I,J).GT.SISMAX) THEN                              0669

C  <SISMO(PHI1,PHI2)-R-EXACT-{PHI1==I, PHI2==J, K==51, KM+KP==3, 1<=I,
C    I<=50, 1<=J, J<=NT, 2<=N, NT+1<=N}>

               SISMAX = SISMO(I,J)                                      0670
            ENDIF
121         CONTINUE                                                    0672
111      CONTINUE                                                       0673

C  <__IO_EFFECTS:LUNS(PHI1)-R-EXACT-{PHI1==1, I==51, K==51, KM+KP==3,
C    1<=J, J<=N, NT+1<=J, 2<=N}>
C  <__IO_EFFECTS:LUNS(PHI1)-W-EXACT-{PHI1==1, I==51, K==51, KM+KP==3,
C    1<=J, J<=N, NT+1<=J, 2<=N}>

      WRITE (1, 1055) SISMIN, SISMAX                                    0674

C  <__IO_EFFECTS:LUNS(PHI1)-R-EXACT-{PHI1==1, I==51, K==51, KM+KP==3,
C    1<=J, J<=N, NT+1<=J, 2<=N}>
C  <__IO_EFFECTS:LUNS(PHI1)-W-EXACT-{PHI1==1, I==51, K==51, KM+KP==3,
C    1<=J, J<=N, NT+1<=J, 2<=N}>




      WRITE (1, 1200)                                                   0678

C  <__IO_EFFECTS:LUNS(PHI1)-R-EXACT-{PHI1==1, I==51, K==51, KM+KP==3,
C    1<=J, J<=N, NT+1<=J, 2<=N}>
C  <__IO_EFFECTS:LUNS(PHI1)-W-EXACT-{PHI1==1, I==51, K==51, KM+KP==3,
C    1<=J, J<=N, NT+1<=J, 2<=N}>


      CLOSE (1)                                                         0680

C  <__IO_EFFECTS:LUNS(PHI1)-R-EXACT-{PHI1==2, I==51, K==51, KM+KP==3,
C    1<=J, J<=N, NT+1<=J, 2<=N}>
C  <__IO_EFFECTS:LUNS(PHI1)-W-EXACT-{PHI1==2, I==51, K==51, KM+KP==3,
C    1<=J, J<=N, NT+1<=J, 2<=N}>

      CLOSE (2)                                                         0681

C  <__IO_EFFECTS:LUNS(PHI1)-R-EXACT-{PHI1==3, I==51, K==51, KM+KP==3,
C    1<=J, J<=N, NT+1<=J, 2<=N}>
C  <__IO_EFFECTS:LUNS(PHI1)-W-EXACT-{PHI1==3, I==51, K==51, KM+KP==3,
C    1<=J, J<=N, NT+1<=J, 2<=N}>

      CLOSE (3)                                                         0682

C  <__IO_EFFECTS:LUNS(PHI1)-R-EXACT-{PHI1==4, I==51, K==51, KM+KP==3,
C    1<=J, J<=N, NT+1<=J, 2<=N}>
C  <__IO_EFFECTS:LUNS(PHI1)-W-EXACT-{PHI1==4, I==51, K==51, KM+KP==3,
C    1<=J, J<=N, NT+1<=J, 2<=N}>

      CLOSE (4)                                                         0683

C  <__IO_EFFECTS:LUNS(PHI1)-R-EXACT-{PHI1==0, I==51, K==51, KM+KP==3,
C    1<=J, J<=N, NT+1<=J, 2<=N}>
C  <__IO_EFFECTS:LUNS(PHI1)-W-EXACT-{PHI1==0, I==51, K==51, KM+KP==3,
C    1<=J, J<=N, NT+1<=J, 2<=N}>


      STOP                                                              0685


