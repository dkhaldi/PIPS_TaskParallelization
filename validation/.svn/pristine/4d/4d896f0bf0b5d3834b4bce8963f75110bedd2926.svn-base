//  T() {}

   int n, x, c, s;

//  T(n) {0<=n}

   n = rand();

//  T(c,x) {c==0, x==0, 0<=n}

   x = c = 0;

//  T(s) {c==0, s==1, x==0, 0<=n}

   s = 1;

//  T() {c==0, s==1, x==0, 0<=n}

   if (!(c<=n+1))

//  T() {0==-1}

      checking_error();

//  T(c,s,x) {c==x, c#init==x#init, 0<=c, c<=n+1, s#init+x#init<=c+s,
//    x#init<=c, 0<=n, 1<=s, s<=2}

   while (rand_b())

//  T(c,s,x) {c#init+s#init==c+1, s#init+x#init==x+1, c<=n+1, 0<=n,
//    s<=2, 3<=s+s#init, s#init<=2}

      if (rand_b()) {

//  T() {s==1, 0<=n, x+1<=n}

         if (!(s==1&&x<n))

//  T() {0==-1}

            deadlock();

//  T(s) {s==2, s#init==1, 0<=n, x+1<=n}

         s = 2;

//  T() {s==2, c<=n+1, 0<=n, x+1<=n}

         if (!(c<=n+1))

//  T() {0==-1}

            checking_error();
      }
      else if (rand_b()) {

//  T() {s==2, 0<=n, x+1<=n}

         if (!(s==2&&x<n))

//  T() {0==-1}

            deadlock();

//  T(x) {s==2, x==x#init+1, 0<=n, x<=n}

         x++;

//  T(c) {c==c#init+1, s==2, 0<=n, x<=n}

         c++;

//  T() {s==2, c<=n+1, 0<=n, x<=n}

         if (!(c<=n+1))

//  T() {0==-1}

            checking_error();
      }
      else {

//  T() {s==2, 0<=n}

         if (!(s==2))

//  T() {0==-1}

            deadlock();

//  T(s) {s==1, s#init==2, 0<=n}

         s = 1;

//  T(x) {s==1, x==x#init+1, 0<=n}

         x++;

//  T(c) {c==c#init+1, s==1, 0<=n}

         c++;

//  T() {s==1, c<=n+1, 0<=n}

         if (!(c<=n+1))

//  T() {0==-1}

            checking_error();
      }
}

