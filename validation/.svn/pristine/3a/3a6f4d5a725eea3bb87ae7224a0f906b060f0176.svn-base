
Proper effects

int main()
{
   int *a, *b, c, d;
//             <    is written>: c
   c = 0;
//             <    is written>: d
   d = 1;
//             <    is written>: b
   b = &c;
//             <    is read   >: c

   while (c>0) {
//             <    is read   >: b
//             <    is written>: a
      a = b;
//             <    is written>: b
      b = &d;
   }
//             <    is written>: b

   b = &c;

   return 0;
}

