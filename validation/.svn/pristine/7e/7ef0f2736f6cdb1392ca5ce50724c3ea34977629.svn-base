delete struct09

setenv PIPS_CPP="cpp -C"
setproperty ABORT_ON_USER_ERROR TRUE

create struct09 struct09.c

activate C_PARSER

setproperty PRETTYPRINT_LANGUAGE "C"
setproperty PRETTYPRINT_STATEMENT_NUMBER FALSE
setproperty FOR_TO_DO_LOOP_IN_CONTROLIZER   TRUE
setproperty FOR_TO_WHILE_LOOP_IN_CONTROLIZER   TRUE

setproperty PRETTYPRINT_SCALAR_REGIONS TRUE

echo
echo Proper pointer effects for "main"
echo

activate PRINT_CODE_PROPER_POINTER_EFFECTS
display PRINTED_FILE[main]

echo
echo Proper effects for "struct09"
echo

activate PRINT_CODE_PROPER_EFFECTS
display PRINTED_FILE[main]

echo
echo Cumulated effects for "struct09"
echo

activate PRINT_CODE_CUMULATED_EFFECTS
display PRINTED_FILE[main]

# quit



echo
echo Printed files with regions
echo

activate MUST_REGIONS
activate PRINT_CODE_REGIONS
display PRINTED_FILE[main]

close
delete struct09
quit
