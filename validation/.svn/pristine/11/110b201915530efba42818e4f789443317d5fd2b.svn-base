
Proper effects

int main()
{
   int i;
//             <    is written>: j
   int j = 2;
//             <    is read   >: i
//             <    is written>: k
   int k = i;
//             <    is read   >: j
//             <    is written>: j l
   int l = j++;
//             <    is written>: m
   int m = 4;
//             <    is read   >: m
//             <    is written>: n
   int n = m;
//             <    is written>: i

   i = 5;
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: i j k l m n stdout 
//                               __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   printf("i = %d, j = %d, k = %d, l = %d, m = %d, n = %d \n", i, j, k, l, m, n);
   return 0;
}

