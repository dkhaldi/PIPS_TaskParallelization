setenv WS="isolate10"
setproperty ABORT_ON_USER_ERROR TRUE
delete $WS
create $WS $WS.c include/par4all.c

activate MUST_REGIONS
setproperty SEMANTICS_TRUST_ARRAY_DECLARATIONS TRUE

module alphablending

echo original code
apply PARTIAL_EVAL
display PRINTED_FILE[%ALLFUNC]

echo with r-w regions
activate PRINT_CODE_REGIONS
display PRINTED_FILE
activate PRINT_CODE

echo isolated code
apply FLAG_LOOPS
setproperty ISOLATE_STATEMENT_LABEL "l99998"
apply ISOLATE_STATEMENT
display PRINTED_FILE


apply UNSPLIT
close

#shell cc $WS.c -o $WS.database/Tmp/ref
