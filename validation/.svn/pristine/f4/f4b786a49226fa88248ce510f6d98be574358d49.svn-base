int main()
{

//  T() {}

   double x[2][10];

//  T(new,old) {new==1, old==0}

   int old = 0, new = 1, i, t;

//  T(t) {new==1, old==0, t==0}


   t = 0;

//  T(i,new,old,t) {new+old==1, new#init==1, old#init==0, t#init==0,
