delete costi
create costi costi.f

setproperty SEMANTICS_COMPUTE_TRANSFORMERS_IN_CONTEXT TRUE

setproperty SEMANTICS_FIX_POINT_OPERATOR "derivative"

setproperty SEMANTICS_K_FIX_POINT 1


setenv SEMANTICS_DEBUG_LEVEL "0"

echo
echo TRANSFORMERS
echo

activate PRINT_CODE_TRANSFORMERS
display PRINTED_FILE[costi]

# quit

echo
echo PRECONDITIONS
echo

activate PRINT_CODE_PRECONDITIONS
display PRINTED_FILE[costi]

echo
echo Dependence graph
echo

display DG_FILE

echo
echo Parallel code
echo

apply INDUCTION_SUBSTITUTION
apply PARTIAL_EVAL
activate PRINT_CODE
display PRINTED_FILE

echo
echo Parallel code
echo

apply PRIVATIZE_MODULE

display PARALLELPRINTED_FILE

quit

echo
echo TRANSFORMERS AFTER REFINEMENT
echo

apply REFINE_TRANSFORMERS[costi]
activate PRINT_CODE_TRANSFORMERS
display PRINTED_FILE[costi]

# quit

echo
echo PRECONDITIONS AFTER REFINEMENT
echo

activate PRINT_CODE_PRECONDITIONS
display PRINTED_FILE[costi]

quit
echo
echo AFTER DEAD CODE ELIMINATION
echo

apply SIMPLIFY_CONTROL[main]
activate PRINT_CODE
display PRINTED_FILE

close
# delete costi
quit
