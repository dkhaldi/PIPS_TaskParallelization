      PROGRAM DOT_PRODUCT

      INTEGER N, I
      PARAMETER (N=100)
      REAL A(N), B(N), RESULT

!     Some initializations
      DO I = 1, N                                                       0008
         A(I) = I*1.0                                                   0009
         B(I) = I*2.0                                                   0010
      ENDDO
      RESULT = 0.0                                                      0012

      DO I = 1, N                                                       0014
         RESULT = RESULT+A(I)*B(I)                                      0015
      ENDDO

      PRINT *, 'Final Result= ', RESULT                                 0018
      END
