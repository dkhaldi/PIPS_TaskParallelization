  int32_t pi0,
  int32_t pi1)
{
  spoc_instr si;
  spoc_param sp;
  freia_microcode mcode;
  freia_dynamic_param dynparam;
  freia_op_param param;
  freia_status ret;

  // init pipe to nop
  spoc_init_pipe(&si, &sp, 16);


  // STAGE 0
  // in [0 input 0] -> [0 alu] 18 add_const
  // nope
  // alu 18 add_const stage 0
  si.alu[0][0].op = SPOC_ALU_ADD_IN0_CONST;
  sp.alu[0][0].constant = pi0;


  // STAGE 1
  // in [0 input 1] -> [1 alu] 19 add_const
  si.mux[0][3].op = SPOC_MUX_IN1;
  // in [1 input 1] -> [1 alu] 19 add_const
  // nope
  // alu 19 add_const stage 1 flipped
  si.alu[1][0].op = SPOC_ALU_ADD_IN1_CONST;
  sp.alu[1][0].constant = pi1;


  // STAGE 2
  // 0`a [0 alu] -> [2 alu] 20 mul
  si.mux[0][0].op = SPOC_MUX_IN1;
  si.mux[0][2].op = SPOC_MUX_IN0;
  // 0`a [1 input 0] -> [2 alu] 20 mul
  si.mux[1][0].op = SPOC_MUX_IN0;
  // 0`a [2 input 0] -> [2 alu] 20 mul
  // nope
  // 0`b [1 alu] -> [2 alu] 20 mul
  si.mux[1][1].op = SPOC_MUX_IN1;
  si.mux[1][3].op = SPOC_MUX_IN0;
  // 0`b [2 input 1] -> [2 alu] 20 mul
  // nope
  // alu 20 mul stage 2
  si.alu[2][0].op = SPOC_ALU_MUL_IN0_IN1;


  // output image out on 0
  // out [2 alu] -> [2 output 0] 0 null
  si.mux[2][0].op = SPOC_MUX_IN1;
  si.mux[2][2].op = SPOC_MUX_IN0;

  // fill in to the end...
  // out [2 output 0] -> [7 output 0] 0 null
  // out [3 input 0] -> [7 output 0] 0 null
  si.mux[3][0].op = SPOC_MUX_IN0;
  // out [4 input 0] -> [7 output 0] 0 null
  si.mux[4][0].op = SPOC_MUX_IN0;
  // out [5 input 0] -> [7 output 0] 0 null
  si.mux[5][0].op = SPOC_MUX_IN0;
  // out [6 input 0] -> [7 output 0] 0 null
  si.mux[6][0].op = SPOC_MUX_IN0;
  // out [7 input 0] -> [7 output 0] 0 null
  si.mux[7][0].op = SPOC_MUX_IN0;

