PROPER  EFFECTS

int main()
{
   struct Mastruct toto;
   struct Mastruct *p;
//             <    is written>: p
   p = &toto;
//             <    is written>: toto.first
   toto.first = 0;
//             <    is read   >: p
//             <    is written>: toto.first
   p->first = 1;

   return 0;
}

