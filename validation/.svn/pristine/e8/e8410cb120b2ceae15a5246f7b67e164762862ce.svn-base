
Points-to


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int i0 = 0, i1 = 1, j = -1;

// Points To: none

   int *p, *q;

// Points To:
// p -> undefined , EXACT
// q -> undefined , EXACT

   p = &i1;

// Points To:
// p -> i1 , EXACT
// q -> undefined , EXACT

   q = &i0;

// Points To:
// p -> i1 , EXACT
// q -> i0 , EXACT


   j = 0?*p:*q;

// Points To:
// p -> i1 , EXACT
// q -> i0 , EXACT


   return j;
}

PROPER  EFFECTS

int main()
{
//             <    is written>: i0 i1 j
   int i0 = 0, i1 = 1, j = -1;
   int *p, *q;
//             <    is written>: p
   p = &i1;
//             <    is written>: q
   q = &i0;
//             <may be read   >: i0 i1 p q
//             <    is written>: j

   j = 0?*p:*q;
//             <    is read   >: j

   return j;
}

Transformers with point-to


//  T(main) {main==0}

int main()
{

//  T(i0,i1,j) {i0==0, i1==1, j==-1}

   int i0 = 0, i1 = 1, j = -1;

//  T() {i0==0, i1==1, j==-1}

   int *p, *q;

//  T(p) {&i1==p, i0==0, i1==1, j==-1}

   p = &i1;

//  T(q) {&i0==q, &i1==p, i0==0, i1==1, j==-1}

   q = &i0;

//  T(j) {&i0==q, &i1==p, i0==0, i1==1, j==0, j#init==-1}


   j = 0?*p:*q;

//  T(main) {&i0==q, &i1==p, i0==0, i1==1, j==0, main==0}


   return j;
}

