//  <__IO_EFFECTS:LUNS[PHI1]-R-MAY-{}>
//  <__IO_EFFECTS:LUNS[PHI1]-W-MAY-{}>

   
   /* Write the PGM header: */
   fprintf(fp, "P5\n%d %d\n255\n", 501, 501);                           /*0075*/

//  <fp-R-EXACT-{}>
//  <fp[PHI1]-R-MAY-{PHI1==0}>
