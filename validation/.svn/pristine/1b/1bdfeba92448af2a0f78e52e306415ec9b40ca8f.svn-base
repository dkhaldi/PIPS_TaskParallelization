C       Uninitialized scalar variables
	PROGRAM SCALAR03
	INTEGER X,Y
C             <    is exported>: X
      CALL SUB(X, Y)                                                    0004
C             <    is exported>: __IO_EFFECTS:LUNS(6)
      PRINT *, X, Y                                                     0005
      END
C             <may be exported>: X
        SUBROUTINE SUB(X,Y)
	INTEGER X,Y
C             <    is exported>: X
      X = Y                                                             0003
      END
