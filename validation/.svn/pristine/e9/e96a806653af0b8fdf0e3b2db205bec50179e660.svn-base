      SUBROUTINE NODE
      implicit none
      include "global_parameters.h"
      include "hpfc_commons.h"
      include "hpfc_includes.h"
      include "PI_parameters.h"
      INTEGER*4 N
      PARAMETER (N = 1000)
      INTEGER I_0, I_1, I_2, I_3, I
      REAL*8 V(1:125), X, GSUM, W
      CALL HPFC INIT NODE
      CALL HPFC LOOP BOUNDS(I_1, I_2, I_3, 1, N, 1, 1)
      W = 1.0/REAL(N)
      GSUM = 0.0
      CALL HPFC NTIMEON
      CALL NPRE SUM HPFC REAL8(GSUM, 1)
      I = I_3
      DO I_0 = I_1, I_2
         I = I+1
         X = (I-0.5)*W
         V(I_0) = 4.0/(1.0+X*X)
         GSUM = GSUM+V(I_0)
      ENDDO
      CALL NPOST SUM HPFC REAL8(GSUM, 1)
      CALL HPFC NTIMEOFF('pi computation')
      CALL HPFC NODE END
      END
