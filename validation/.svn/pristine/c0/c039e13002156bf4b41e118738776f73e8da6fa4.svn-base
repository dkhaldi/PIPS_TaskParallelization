// dag 0: 3 ops and 0 copies, optimized to 1 ops and 0+0+1 copies

// dag 1: 0 ops and 1 copies, optimized to 0 ops and 0+0+0 copies

// dag 2: 0 ops and 1 copies, optimized to 0 ops and 0+1+0 copies

// dag 3: 16 ops and 3 copies, optimized to 16 ops and 0+0+1 copies

// hold kernels for vs_core_loop_opencl_helper_3_0_0
static cl_kernel vs_core_loop_opencl_helper_3_0_0_kernel[2];

// compile kernels for vs_core_loop_opencl_helper_3_0_0
static freia_status vs_core_loop_opencl_helper_3_0_0_compile(void)
{
  // OpenCL source for vs_core_loop_opencl_helper_3_0_0
  const char * vs_core_loop_opencl_helper_3_0_0_source =
    "#include <freia_opencl_runtime.hcl>\n"
    "\n"
    "// opencl function vs_core_loop_opencl_helper_3_0_0\n"
    "KERNEL void vs_core_loop_opencl_helper_3_0_0(\n"
    "  GLOBAL PIXEL * o0,\n"
    "  int ofs_o0,\n"
    "  GLOBAL PIXEL * o1,\n"
    "  int ofs_o1,\n"
    "  GLOBAL PIXEL * i0, // const?\n"
    "  int ofs_i0,\n"
    "  GLOBAL PIXEL * i1, // const?\n"
    "  int ofs_i1,\n"
    "  GLOBAL PIXEL * i2, // const?\n"
    "  int ofs_i2,\n"
    "  int width, // of the working area, vs image pitch below\n"
    "  int height, // of the working area\n"
    "  int pitch,\n"
    "  int c0,\n"
    "  int c1,\n"
    "  int c2,\n"
    "  GLOBAL TMeasure * redX)\n"
    "{\n"
    "  // no tiling on height dimension\n"
    "  // assert(height==get_global_size(0));\n"
    "  int j = get_global_id(0);\n"
    "\n"
    "  // reduction stuff is currently hardcoded...\n"
    "  int vol = 0;\n"
    "  PIXEL minv = PIXEL_MAX;\n"
    "  int2 minpos = { 0, 0 };\n"
    "  PIXEL maxv = PIXEL_MIN;\n"
    "  int2 maxpos = { 0, 0 };\n"
    "\n"
    "  // loop i upper bound\n"
    "  int Wtile = (width+get_global_size(1)-1)/get_global_size(1);\n"
    "  int Wlast = Wtile*(get_global_id(1)+1);\n"
    "  if (Wlast>width) Wlast = width;\n"
    "\n"
    "  // get input & output image pointers\n"
    "  int shift = pitch*j;\n"
    "  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;\n"
    "  GLOBAL PIXEL * p1 = o1 + ofs_o1 + shift;\n"
    "  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;\n"
    "  GLOBAL PIXEL * j1 = i1 + ofs_i1 + shift;\n"
    "  GLOBAL PIXEL * j2 = i2 + ofs_i2 + shift;\n"
    "\n"
    "  int i;\n"
    "  for (i=Wtile*get_global_id(1); i<Wlast; i++)\n"
    "  {\n"
    "    // get input pixels\n"
    "    PIXEL in2 = j2[i];\n"
    "    PIXEL in0 = j0[i];\n"
    "    PIXEL in1 = j1[i];\n"
    "\n"
    "    // pixel computations\n"
    "    PIXEL t87 = PIXEL_ABSDIFF(in2, in0);\n"
    "    PIXEL t91 = PIXEL_MUL(in2, c0);\n"
    "    PIXEL t92 = PIXEL_MUL(in1, c1);\n"
    "    PIXEL t93 = PIXEL_ADD(t92, t91);\n"
    "    PIXEL_MAXIMUM(red98, t87);\n"
    "    PIXEL_MINIMUM(red99, t87);\n"
    "    PIXEL t94 = PIXEL_DIV(t93, c2);\n"
    "\n"
    "    // set output pixels\n"
    "    p0[i] = t87;\n"
    "    p1[i] = t94;\n"
    "  }\n"
    "\n"
    "  // reduction copy out\n"
    "  int thrid = get_global_id(0)*get_global_size(1)+get_global_id(1);\n"
    "  redX[thrid].max = maxv;\n"
    "  redX[thrid].min = minv;\n"
    "}\n";
  freia_status err = FREIA_OK;
  err |= freia_op_compile_kernel(vs_core_loop_opencl_helper_3_0_0_source, "vs_core_loop_opencl_helper_3_0_0", "-DPIXEL8", &vs_core_loop_opencl_helper_3_0_0_kernel[0]);
  err |= freia_op_compile_kernel(vs_core_loop_opencl_helper_3_0_0_source, "vs_core_loop_opencl_helper_3_0_0", "-DPIXEL16", &vs_core_loop_opencl_helper_3_0_0_kernel[1]);
