//  T(r) {a==4, 0<=b+9, b<=9, 0<=r}

      r = a%b;

//  T() {a==4, 0<=b+9, b<=9, 0<=r}

      printf("a=%d, b=%d, r=%d\n", a, b, r);
   }
   else if (b>5) {

//  T(r) {a==4, r==4, 6<=b}

      r = a%b;

//  T() {a==4, r==4, 6<=b}

      printf("a=%d, b=%d, r=%d\n", a, b, r);
   }
   else if (b<-5) {

//  T(r) {a==4, r==4, b+6<=0}

      r = a%b;

//  T() {a==4, r==4, b+6<=0}

      printf("a=%d, b=%d, r=%d\n", a, b, r);
   }
   else {

//  T(r) {a==4, 0<=b+5, b<=5, 0<=r}

      r = a%b;

//  T() {a==4, 0<=b+5, b<=5, 0<=r}

      printf("a=%d, b=%d, r=%d\n", a, b, r);
   }
}

