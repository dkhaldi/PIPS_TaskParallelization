//  T(main) {main==0}

int main()
{

//  T(i) {i==0}

   int i = 0;

//  T() {i==0}

   int *p;

//  T(j) {i==0, j==10}

   int j = 10;

//  T() {i==0, j==10}

   int *q;

//  T(p) {&i==p, i==0, j==10}


   p = &i;

//  T(q) {&i==p, &j==q, i==0, j==10}


   q = &j;

//  T(i) {&i==p, &j==q, i==10, i#init==0, j==10}

   
   //on modifie i
   i = j;

//  T(main) {&i==p, &j==q, i==10, j==10, main==0}


   return 0;
}

