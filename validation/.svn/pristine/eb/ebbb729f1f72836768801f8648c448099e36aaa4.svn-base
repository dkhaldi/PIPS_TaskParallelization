
Points-to


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int i = 0;

// Points To: none

   int *p;

// Points To:
// p -> undefined , EXACT

   int j = 10;

// Points To:
// p -> undefined , EXACT

   int *q;

// Points To:
// p -> undefined , EXACT
// q -> undefined , EXACT


   p = &i;

// Points To:
// p -> i , EXACT
// q -> undefined , EXACT


   q = &j;

// Points To:
// p -> i , EXACT
// q -> j , EXACT

   
   //on modifie i
   i = j;

// Points To:
// p -> i , EXACT
// q -> j , EXACT


   return 0;
}

PROPER  EFFECTS

int main()
{
//             <    is written>: i
   int i = 0;
   int *p;
//             <    is written>: j
   int j = 10;
   int *q;
//             <    is written>: p

   p = &i;
//             <    is written>: q

   q = &j;
//             <    is read   >: j
//             <    is written>: i
   
   //on modifie i
   i = j;

   return 0;
}

