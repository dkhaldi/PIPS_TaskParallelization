setenv WSPACE=malloc12
delete $WSPACE
create $WSPACE $WSPACE.c

setproperty ABORT_ON_USER_ERROR TRUE
setproperty ALIASING_ACROSS_TYPES FALSE
setproperty ABSTRACT_HEAP_LOCATIONS "context-sensitive"

echo 
echo Points-to for "foo"
echo
activate PRINT_CODE_POINTS_TO_LIST
display PRINTED_FILE[foo]

echo 
echo Points-to for "main"
echo
#activate PRINT_CODE_POINTS_TO_LIST
display PRINTED_FILE[main]

close

echo
echo Warnings and errors:  Check that the dangling pointer and the memory leak is detected?
echo They cannot be detected with our current heap model
echo

shell cat $WSPACE.database/Warnings

delete $WSPACE
quit
