   int i, j, k, l1, l2, m, n, *p, *q;
//             <    is written>: i
   i = 0;
//             <    is read   >: i
//             <    is written>: l1
   l1 = i;
//             <    is written>: j
   j = 1;
//             <    is read   >: j
//             <    is written>: l2
   l2 = j;
//             <    is written>: m
   m = 10;
//             <    is written>: n
   n = 11;
//             <    is read   >: _RANDOM_EFFECTS:GEN_SEED
//             <    is written>: _RANDOM_EFFECTS:GEN_SEED

   if (rand()) {
//             <    is written>: q
      q = &m;
//             <    is read   >: m
//             <    is written>: k
      k = m;
   }
   else {
//             <    is written>: q
      q = &n;
//             <    is read   >: n
//             <    is written>: k
      k = n;
   }
//             <    is read   >: _RANDOM_EFFECTS:GEN_SEED
//             <    is written>: _RANDOM_EFFECTS:GEN_SEED
   
   
   
   if (rand()) {
//             <    is written>: p
      p = &i;
//             <    is read   >: k
//             <    is written>: l1
      l1 = k;
   }
   else {
//             <    is written>: p
      p = &j;
//             <    is read   >: k
//             <    is written>: l2
      l2 = k;
   }
//             <may be read   >: m n
//             <may be written>: i j
//             <    is read   >: p q

   *p = *q;

   return 0;
