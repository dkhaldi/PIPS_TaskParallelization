//  T() {}

void ts_singlestate(void)
{

//  T() {}

   int t, d, s;

//  T(d,s,t) {d==0, s==0, t==0}

   t = d = s = 0;

//  T() {d==0, s==0, t==0}

   if (!(t>=0&&0<=s&&s<=4&&0<=d&&d<=4*t+s))

//  T() {0==-1}

      checking_error();

//  T(d,s,t) {d#init<=d, s<=4d, d<=s+4t, 0<=s, s<=4, 0<=t, t#init<=t}

   while (rand_b())

//  T(d,s,t) {d+t==d#init+t#init+1, 0<=d, d<=d#init+1, 4d#init+s<=4d,
//    d<=s+4t, 0<=s, 0<=t}

      if (rand_b()) {

//  T() {s<=3}

         if (!(s<=3))

//  T() {0==-1}

            deadlock();

//  T(s) {s==s#init+1, s<=4}

         s++;

//  T(d) {d==d#init+1, s<=4}

         d++;

//  T() {0<=d, d<=s+4t, 0<=s, s<=4, 0<=t}

         if (!(t>=0&&0<=s&&s<=4&&0<=d&&d<=4*t+s))

//  T() {0==-1}

            checking_error();
      }
      else {

//  T() {}

         if (!1)

//  T() {0==-1}

            deadlock();

//  T(t) {t==t#init+1}

         t++;

//  T(s) {s==0}

         s = 0;

//  T() {s==0, 0<=d, d<=4t, 0<=t}

         if (!(t>=0&&0<=s&&s<=4&&0<=d&&d<=4*t+s))

//  T() {0==-1}

            checking_error();
      }
}

