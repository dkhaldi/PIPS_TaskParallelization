   volprevious = volcurrent;                                            /*0009*/
   freia_aipo_dilate_8c(out, out, freia_morpho_kernel_8c);              /*0010*/
   freia_aipo_inf(out, out, in);                                        /*0011*/
   freia_aipo_global_vol(out, &volcurrent);                             /*0012*/
   while (volcurrent!=volprevious) {                                    /*0013*/
