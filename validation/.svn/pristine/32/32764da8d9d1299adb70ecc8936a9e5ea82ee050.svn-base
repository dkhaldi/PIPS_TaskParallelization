! updated function for OPENCL

int freia_while_00(freia_data2d *out, const freia_data2d *in)
{
   int volprevious, volcurrent;                                         /*0005*/
   freia_mppa_launch(0, 1, 1, 0, 1, in, out, &volcurrent);              /*0011*/
   volprevious = volcurrent;                                            /*0009*/
   freia_mppa_launch(1, 1, 0, 0, 1, out, &volcurrent);                  /*0012*/
   while (volcurrent!=volprevious) {                                    /*0013*/
      volprevious = volcurrent;                                         /*0014*/
      freia_mppa_launch(2, 2, 1, 0, 1, in, out, out, &volcurrent);      /*0017*/
   }
   return 0;                                                            /*0019*/
}

