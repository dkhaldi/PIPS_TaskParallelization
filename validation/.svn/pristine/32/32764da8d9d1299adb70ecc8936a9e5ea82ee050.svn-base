   int volprevious, volcurrent;                                         /*0005*/
   freia_aipo_dilate_8c(out, in, freia_morpho_kernel_8c);               /*0006*/
   freia_aipo_erode_8c(out, out, freia_morpho_kernel_8c);               /*0007*/
