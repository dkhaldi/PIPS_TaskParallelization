# code
      PROGRAM W03

      INTEGER I, N
      PARAMETER (N=10)
      REAL A(N)

      I = 1                                                             0007

      DO WHILE (I.LE.10)                                                0009
         A(I) = I*1.23                                                  0010
         I = I+1                                                        0011
      ENDDO

      I = 2                                                             0014

      DO WHILE (I.LT.N)                                                 0016
         PRINT *, A(I-1)                                                0017
         I = I+1                                                        0018
      ENDDO

      PRINT *, 'bye'                                                    0021

      END
# proper effects
      PROGRAM W03

      INTEGER I, N
      PARAMETER (N=10)
      REAL A(N)
C             <    is written>: I

      I = 1                                                             0007
C             <    is read   >: I

      DO WHILE (I.LE.10)                                                0009
C             <    is read   >: I
C             <    is written>: A(I)
         A(I) = I*1.23                                                  0010
C             <    is read   >: I
C             <    is written>: I
         I = I+1                                                        0011
      ENDDO
C             <    is written>: I

      I = 2                                                             0014
C             <    is read   >: I

      DO WHILE (I.LT.N)                                                 0016
C             <    is read   >: A(I-1) I __IO_EFFECTS:LUNS(6)
C             <    is written>: __IO_EFFECTS:LUNS(6)
         PRINT *, A(I-1)                                                0017
C             <    is read   >: I
C             <    is written>: I
         I = I+1                                                        0018
      ENDDO
C             <    is read   >: __IO_EFFECTS:LUNS(6)
C             <    is written>: __IO_EFFECTS:LUNS(6)

      PRINT *, 'bye'                                                    0021

      END
# cumulated effects
C             <    is read   >: __IO_EFFECTS:LUNS(6)
C             <    is written>: __IO_EFFECTS:LUNS(6)
      PROGRAM W03

      INTEGER I, N
      PARAMETER (N=10)
      REAL A(N)
C             <    is written>: I

      I = 1                                                             0007
C             <may be written>: A(*) I
C             <    is read   >: I

      DO WHILE (I.LE.10)                                                0009
C             <may be written>: A(*)
C             <    is read   >: I
         A(I) = I*1.23                                                  0010
C             <    is read   >: I
C             <    is written>: I
         I = I+1                                                        0011
      ENDDO
C             <    is written>: I

      I = 2                                                             0014
C             <may be read   >: A(*) __IO_EFFECTS:LUNS(6)
C             <may be written>: I __IO_EFFECTS:LUNS(6)
C             <    is read   >: I

      DO WHILE (I.LT.N)                                                 0016
C             <may be read   >: A(*)
C             <    is read   >: I __IO_EFFECTS:LUNS(6)
C             <    is written>: __IO_EFFECTS:LUNS(6)
         PRINT *, A(I-1)                                                0017
C             <    is read   >: I
C             <    is written>: I
         I = I+1                                                        0018
      ENDDO
C             <    is read   >: __IO_EFFECTS:LUNS(6)
C             <    is written>: __IO_EFFECTS:LUNS(6)

      PRINT *, 'bye'                                                    0021

      END
