
C  T(I,J) {I<=K, I<=L+2, 4<=J, K<=J, 0<=J+L, L<=J}

      SUBROUTINE MINMAX(I,J)
      COMMON /FOO/ K, L

C  T(I) {I<=K, I<=L+2}


      I = MIN0(K, L+2)                                                  0004

C  T(J) {4<=J, K<=J, 0<=J+L, L<=J}


      J = MAX0(K, L*L, 4)                                               0006

      END
