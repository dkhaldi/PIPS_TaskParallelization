PROPER EFFECTS WITH POINTS TO

void point_to02()
{
   struct s {
      int a;
      int b[10];
   } c, *p;
//             <    is written>: p

   p = &c;
//             <    is read   >: p
//             <    is written>: c.a

   p->a = 1;
//             <    is read   >: p
//             <    is written>: c.b[2]
   (p->b)[2] = 3;
}
