void pointer01(int n, float *p, float *q)
{
   int i;                                                               /*0007*/
//             <    is read   >: n
//             <    is written>: i

   for(i = 0; i <= n-1; i += 1)                                         /*0009*/
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*_b0
//             <may be written>: *ANY_MODULE*:*ANYWHERE*_b0
//             <    is read   >: i n p q
      p[i] = q[i];                                                      /*0010*/

   return;                                                              /*0012*/
}

