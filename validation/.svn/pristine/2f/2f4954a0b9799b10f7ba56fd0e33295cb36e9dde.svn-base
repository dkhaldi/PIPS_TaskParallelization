      program power01

C     Check the interpretation of the power operator

      read *, n, m3, m4, m5

      i = 2**3

      j = 2**n

      j1 = 1**n

      j2 = 4**n

      k = (-2)**n

      k2 = (-1)**n

      l = n**2

      l3 = n**3

      l4 = n**4

      m = 0**n

      m2 = n**0

      m3 = m3**2

      m4 = m4**3

      m5 = m5**(-1)

      m6 = n**1

      print *, i, j, j1, j2, k, k2, l, l3, l4, m, m2, m3, m4, m5, m6

      end
