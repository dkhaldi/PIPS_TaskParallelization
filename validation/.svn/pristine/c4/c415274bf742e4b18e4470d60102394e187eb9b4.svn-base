PROPER_EFFECTS_WITH_POINTER_VALUES

void dereferencing03()
{
   double *p;
   int i;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: p

   *(p+1) = 0.;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: i p
   *(p+i) = 0.;
}
int main()
{
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
   dereferencing03();
   return 1;
}

