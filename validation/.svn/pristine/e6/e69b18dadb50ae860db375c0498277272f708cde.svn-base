void getimage(char *filename)
{
   extern complex imagein[64][64], imageout[256][256];
   double z, amp, sum, squares;
   int i, j, conj, nx, ny;
   unsigned char c;
   FILE *fp;

#pragma omp parallel for private(j)
   for(i = 0; i <= 63; i += 1)
#pragma omp parallel for 
      for(j = 0; j <= 63; j += 1) {
         imagein_re[i][j] = 0.0;
         imagein_im[i][j] = 0.0;
      }

#pragma omp parallel for private(j)
   for(i = 0; i <= 255; i += 1)
#pragma omp parallel for 
      for(j = 0; j <= 255; j += 1) {
         imageout_re[i][j] = 0.0;
         imageout_im[i][j] = 0.0;
      }

   fp = fopen(filename, "r");
   fclose(fp);
}
