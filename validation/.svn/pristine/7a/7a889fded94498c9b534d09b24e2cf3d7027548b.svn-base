
Proper effects

int main()
{
   int *p, *q, i, j;
//             <    is written>: i

   i = 1;
//             <    is written>: j
   j = 0;
//             <    is written>: p
   /* Initialisation des pointeurs*/
   p = &i;
//             <    is written>: q
   q = &j;
//             <    is read   >: q
//             <    is written>: p
   
   /* Les deux pointeurs pointent vers la meme case memoire j */
   p = q;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*_b0
//             <    is read   >: p
   
   /* Dereferencement du pointeur p -> acces à la variable j */
   *p = 1;

   return 0;
}

