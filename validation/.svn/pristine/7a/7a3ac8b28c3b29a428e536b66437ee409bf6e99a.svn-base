//  T() {}

void ts_singlestate(void)
{

//  T() {}

   int s, x;

//  T(s) {s==1}

   s = 1;

//  T(x) {s==1, x==0}

   x = 0;

//  T() {s==1, x==0}

   if (s==1&&x>=11||s==2&&x<=4||s==2&&x>=12)

//  T() {0==-1}

      checking_error();

//  T(s,x) {1<=s, s<=2, x<=s+9}

   while (rand_b())

//  T(s,x) {2s#init+x==x#init+3, 1<=s, s<=2, x<=s+9, 3x#init<=7s+2x,
//    x#init<=x+1, x<=x#init+1}

      if (rand_b()) {

//  T() {s==1, x<=9}

         if (!(s==1&&x<=9))

//  T() {0==-1}

            deadlock();

//  T(x) {s==1, x==x#init+1, x<=10}

         x++;

//  T() {s==1, x<=10}

         if (s==1&&x>=11||s==2&&x<=4||s==2&&x>=12)

//  T() {0==-1}

            checking_error();
      }
      else if (rand_b()) {

//  T() {s==1, 10<=x}

         if (!(s==1&&x>=10))

//  T() {0==-1}

            deadlock();

//  T(s) {s==2, s#init==1, 10<=x}

         s = 2;

//  T(x) {s==2, x==x#init+1, 11<=x}

         x++;

//  T() {s==2, x==11}

         if (s==1&&x>=11||s==2&&x<=4||s==2&&x>=12)

//  T() {0==-1}

            checking_error();
      }
      else if (rand_b()) {

//  T() {s==2, x<=5}

         if (!(s==2&&x<=5))

//  T() {0==-1}

            deadlock();

//  T(s) {s==1, s#init==2, x<=5}

         s = 1;

//  T(x) {s==1, x==x#init-1, x<=4}

         x--;

//  T() {s==1, x<=4}

         if (s==1&&x>=11||s==2&&x<=4||s==2&&x>=12)

//  T() {0==-1}

            checking_error();
      }
      else {

//  T() {s==2, 6<=x}

         if (!(s==2&&x>=6))

//  T() {0==-1}

            deadlock();

//  T(x) {s==2, x==x#init-1, 5<=x}

         x--;

//  T() {s==2, 5<=x, x<=11}

         if (s==1&&x>=11||s==2&&x<=4||s==2&&x>=12)

//  T() {0==-1}

            checking_error();
      }
}

