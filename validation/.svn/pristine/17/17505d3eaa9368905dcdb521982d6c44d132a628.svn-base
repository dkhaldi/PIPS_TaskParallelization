
C  T() {1<=M1, M2<=N2, 1<=N1, 1<=N2}

      SUBROUTINE TRUSTED_REF14(A, N1, N2, M1, M2)

C     Check that nested loops are properly analyzed: since you are 
C     sure to enter the i loop, you can use information from the
C     j loop and you end up with everything (see trusted_ref12.f)

      REAL A(N1, N2)

C  T(I,J) {I==M1+1, 2<=I, 1<=J, M2+1<=J, J<=N2+1, 1<=N1, 1<=N2}

C (0,1)
C     BEGIN BLOCK

C  T() {1<=M1, 1<=N1, 1<=N2}

C (0,2)

      IF (M1.LT.1) STOP                                                 0009

C  T(I,J) {1<=I, I<=M1, 1<=M1, 1<=N1, 1<=N2}

C (0,5)

      DO I = 1, M1                                                      0011

C  T(J) {1<=I, I<=M1, 1<=J, M2+1<=J, J<=N2+1, 1<=M1, 1<=N1, 1<=N2}

C (0,6)
C        BEGIN BLOCK

C  T(J) {1<=I, I<=M1, 1<=J, J<=M2, 1<=M1, 1<=N1, 1<=N2}

C (0,7)
         DO J = 1, M2                                                   0012

C  T() {1<=I, I<=M1, I<=N1, 1<=J, J<=M2, J<=N2, 1<=M1, 1<=N1, 1<=N2}

C (0,8)
            A(I,J) = 0                                                  0013
         ENDDO
C     a comment to have an inner block and see the inner loop
C     transformer
C        END BLOCK
      ENDDO

C     END BLOCK
      END

