// compile kernels for freia_dup_14_opencl_helper_0_0_0
static freia_status freia_dup_14_opencl_helper_0_0_0_compile(void)
{
  // OpenCL source for freia_dup_14_opencl_helper_0_0_0
  const char * freia_dup_14_opencl_helper_0_0_0_source =
    "#include <freia_opencl_runtime.hcl>\n"
    "\n"
    "// opencl function freia_dup_14_opencl_helper_0_0_0\n"
    "KERNEL void freia_dup_14_opencl_helper_0_0_0(\n"
    "  GLOBAL PIXEL * o0,\n"
    "  int ofs_o0,\n"
    "  GLOBAL PIXEL * i0, // const?\n"
    "  int ofs_i0,\n"
    "  int width, // of the working area, vs image pitch below\n"
    "  int height, // of the working area\n"
    "  int pitch)\n"
    "{\n"
    "  // no tiling on height dimension\n"
    "  // assert(height==get_global_size(0));\n"
    "  int j = get_global_id(0);\n"
    "\n"
    "  // loop i upper bound\n"
    "  int Wtile = (width+get_global_size(1)-1)/get_global_size(1);\n"
    "  int Wlast = Wtile*(get_global_id(1)+1);\n"
    "  if (Wlast>width) Wlast = width;\n"
    "\n"
    "  // N & S boundaries, one thread on first dimension per row\n"
    "  // N not needed\n"
    "  // S not needed\n"
    "\n"
    "  // get input & output image pointers\n"
    "  int shift = pitch*j;\n"
    "  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;\n"
    "  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;\n"
    "\n"
    "  int i;\n"
    "  for (i=Wtile*get_global_id(1); i<Wlast; i++)\n"
    "  {\n"
    "    // W & E boundaries, assuming i global index\n"
    "    int is_W = (i==0);\n"
    "    int is_E = (i==(width-1));\n"
    "\n"
    "    // get input pixels\n"
    "    PIXEL t38 = PIXEL_MAX;\n"
    "    PIXEL in0W = (is_W)? 0: j0[i-1];\n"
    "    PIXEL in0 = j0[i];\n"
    "    PIXEL in0E = (is_E)? 0: j0[i+1];\n"
    "    PIXEL t45 = PIXEL_MIN;\n"
    "\n"
    "    // pixel computations\n"
    "    t38 = PIXEL_INF(t38, (is_W)? PIXEL_MAX: in0W);\n"
    "    t38 = PIXEL_INF(t38, in0);\n"
    "    t38 = PIXEL_INF(t38, (is_E)? PIXEL_MAX: in0E);\n"
    "    t45 = PIXEL_SUP(t45, (is_W)? PIXEL_MIN: in0W);\n"
    "    t45 = PIXEL_SUP(t45, in0);\n"
    "    t45 = PIXEL_SUP(t45, (is_E)? PIXEL_MIN: in0E);\n"
    "    PIXEL t49 = PIXEL_AND(t38, t45);\n"
    "\n"
    "    // set output pixels\n"
    "    p0[i] = t49;\n"
    "  }\n"
    "}\n";
  freia_status err = FREIA_OK;
  err |= freia_op_compile_kernel(freia_dup_14_opencl_helper_0_0_0_source, "freia_dup_14_opencl_helper_0_0_0", "-DPIXEL8", &freia_dup_14_opencl_helper_0_0_0_kernel[0]);
  err |= freia_op_compile_kernel(freia_dup_14_opencl_helper_0_0_0_source, "freia_dup_14_opencl_helper_0_0_0", "-DPIXEL16", &freia_dup_14_opencl_helper_0_0_0_kernel[1]);
  return err;
}

// helper function freia_dup_14_opencl_helper_0_0_0
freia_status freia_dup_14_opencl_helper_0_0_0(
  freia_data2d * o0,
  const freia_data2d * i0)
{
  freia_status err = FREIA_OK;

  // handle on the fly compilation...
  static int to_compile = 1;
  if (to_compile) {
    err |= freia_dup_14_opencl_helper_0_0_0_compile();
    // compilation may have failed
    if (err) return err;
    to_compile = 0;
  }

  // now get kernel, which must be have be compiled
  uint32_t bpp = i0->bpp>>4;
