  mcu_macro[0][2].addrStart = TERAPIX_UCODE_AND3;
  mcu_macro[1][2].addrStart = TERAPIX_UCODE_AND3;


  // inputs:
  // - imagelet 1 is i0 for imin
  tile_in[0][0].x = imagelet_io_1_0;
  tile_in[0][0].y = 0;
  tile_in[1][0].x = imagelet_io_1_1;
