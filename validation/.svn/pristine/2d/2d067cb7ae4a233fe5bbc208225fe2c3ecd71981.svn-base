"original file"
int main(void)
{
   int x, y, z;
   x = 1;
   y = z+x;
   printf("%d\n", y);
   return 0;
}

"after forward substitution"
int main(void)
{
   int x, y, z;
   x = 1;
   y = z+1;
   printf("%d\n", z+1);
   return 0;
}

"after dead code elimination"
int main(void)
{
   int z;
   printf("%d\n", z+1);
   return 0;
}

