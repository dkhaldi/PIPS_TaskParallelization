
Proper effects

int main()
{
//             <    is written>: x
   double x = 1.;
//             <    is written>: p
   double *p = &x;
//             <    is written>: q
   double *q = 0;
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: q stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   printf("q = %p", q);
//             <    is read   >: p
//             <    is written>: q
   q = p;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*_b0
//             <    is read   >: p
   *p = 2.;
   return 0;
}

Points-to


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   double x = 1.;

// Points To: none

   double *p = &x;

// Points To:
// p -> x , EXACT

   double *q = 0;

// Points To:
// p -> x , EXACT
// q -> *NULL* , EXACT

   printf("q = %p", q);

// Points To:
// p -> x , EXACT
// q -> *NULL* , EXACT

   q = p;

// Points To:
// p -> x , EXACT
// q -> x , EXACT

   *p = 2.;

// Points To:
// p -> x , EXACT
// q -> x , EXACT

   return 0;
}
