INTEGER*4 FUNCTION ADD1(A, B)
   INTEGER :: A, B
   ADD1 = A+B
   RETURN
END FUNCTION ADD1
INTEGER*4 FUNCTION ADD2(A, B)
   INTEGER :: A, B
   ADD2 = A+B
   RETURN
END FUNCTION ADD2
PROGRAM MAIN
   INTEGER*4 ADD2
   EXTERNAL ADD2
   INTEGER*4 ADD1
   EXTERNAL ADD1
   INTEGER :: A, C, B
   A = 10
   B = 20
   C = ADD1(A, B)
   C = ADD2(A, B)
END PROGRAM MAIN
