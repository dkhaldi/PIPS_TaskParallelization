      SUBROUTINE NODE
      implicit none
      include "global_parameters.h"
      include "hpfc_commons.h"
      include "hpfc_includes.h"
      include "HPFTEST47_parameters.h"
      INTEGER*4 N
      PARAMETER (N = 100)
      INTEGER I_0, I_1, I_2, I_3, I
      REAL*4 A(1:50), AMIN
      CALL HPFC INIT NODE
      CALL HPFC LOOP BOUNDS(I_1, I_2, I_3, 1, N, 1, 1)
      I = I_3
      DO I_0 = I_1, I_2
         I = I+1
         A(I_0) = REAL(MOD(I+N+30, N+50)+30)
      ENDDO
      AMIN = NRED 1 HPFC REAL4 MIN(A, 1, HPFTEST47 A LO1, 
     &HPFTEST47 A UP1, 1, N)
      CALL HPFC NODE END
      END
