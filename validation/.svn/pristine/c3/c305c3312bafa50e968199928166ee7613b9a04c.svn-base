      PROGRAM ALIAS 
      REAL A(100), B(100)
C (0,1)
C     BEGIN BLOCK
C (0,2)
      M = 10
C (0,3)
      N = 10
C (0,4)
      CALL P(A(1), B(11), M, N)
C (0,5)
      RETURN
C     END BLOCK
      END

      SUBROUTINE P(V1,V2,M1,N1)
      REAL V1(M1),V2(N1)
C (0,1)
C     BEGIN BLOCK
C (0,2)
      DO I = 1, M1
C (0,3)
         V1(I) = I
      ENDDO
C (0,4)
      DO I = 1, N1
C (0,5)
         V2(I) = I
      ENDDO
C (0,6)

      RETURN
C     END BLOCK
      END
