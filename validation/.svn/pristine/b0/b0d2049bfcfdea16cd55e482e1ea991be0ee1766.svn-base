
PROPER  EFFECTS

int main()
{
   int *x, x1, **y, *y1, y2;
//             <    is written>: x1

   x1 = 0;
//             <    is written>: y2
   y2 = 1;
//             <    is written>: x
   x = &x1;
//             <    is written>: y1
   y1 = &y2;
//             <    is written>: y
   y = &y1;
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*_b0
//             <    is read   >: y
//             <    is written>: x
   x = *y;
//             <may be read   >: stdout[0] *ANY_MODULE*:*ANYWHERE*_b0
//             <may be written>: stdout[0]
//             <    is read   >: stdout x __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   printf("*x=%d\n", *x);
//             <may be written>: *ANY_MODULE*:*ANYWHERE*_b0
//             <    is read   >: x y
   *y = x;
   return 0;
}

