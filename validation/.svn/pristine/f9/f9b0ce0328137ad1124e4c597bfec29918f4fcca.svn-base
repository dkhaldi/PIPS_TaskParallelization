
PROPER  EFFECTS 

int exemple01()
{
   int a;
   int *b;
//             <    is written>: b
   b = &a;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*_b0
//             <    is read   >: b
   *b = 2;
//             <    is read   >: a
   return a;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int exemple01()
{

// Points To: none

   int a;

// Points To: none

   int *b;

// Points To:
// b -> undefined , EXACT

   b = &a;

// Points To:
// b -> a , EXACT

   *b = 2;

// Points To:
// b -> a , EXACT

   return a;
}

proper effects with points-to

int exemple01()
{
   int a;
   int *b;
//             <    is written>: b
   b = &a;
//             <    is read   >: b
//             <    is written>: a
   *b = 2;
//             <    is read   >: a
   return a;
}
