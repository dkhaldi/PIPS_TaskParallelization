Controlizer output (return as a function)

int for_loop10()
{
   int i, j = 0;
   i = 0;
   while (i!=5) {
      if (i==3)
         printf("i=%d\n", i);
      j++;
      i++;
   }
   printf("Exit with j=%d\n", j);
   return i;
}
