
Proper effects

int main()
{
   int a[10];
   int *p;
   int i, j;
//             <    is written>: i
   i = 3;
//             <    is written>: j
   j = 2;
//             <    is written>: p
   p = &a[0];
//             <    is read   >: i p
//             <    is written>: p
   p = p+i;
//             <    is read   >: j p
//             <    is written>: p
   p = p+j;
//             <    is read   >: i p
//             <    is written>: p
   p = p-i;
//             <    is read   >: j p
//             <    is written>: p
   p = p-j;
   return 0;
}

