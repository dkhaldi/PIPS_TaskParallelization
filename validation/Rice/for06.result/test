"DISTRIBUTER"

void for06(int n)
{
   int a[n], b[n];                                                      /*0005*/
   {
      int i;                                                            /*0006*/
      for(i = 0; i <= n-1; i += 1) {                                    /*0006*/
         a[i] = i;                                                      /*0007*/
      }
      for(i = 0; i <= n-1; i += 1) {                                    /*0006*/
         int x;                                                         /*0008*/
         x = rand();                                                    /*0009*/
         b[i] = x;                                                      /*0010*/
      }
   }
}
