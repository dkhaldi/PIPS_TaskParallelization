// OMP style
int main()
{
   float a[10];                                                         /*0002*/
   int m = 0;                                                           /*0003*/
   int n = 0;                                                           /*0004*/

   for(m = 0; m <= 9; m += 1) {                                         /*0006*/
      n = n+1;                                                          /*0007*/
      a[m] = n;                                                         /*0008*/
   }
   return 0;                                                            /*0010*/
}
