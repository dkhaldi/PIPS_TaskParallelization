      SUBROUTINE TEST2(Y,N)
      INTEGER N
      REAL*8 Y(2:2*N)

      DO I = 1, N                                                       0005
         DO J = 1, N                                                    0006
            Y(I+J) = Y(I)+Y(I+J)                                        0007
         ENDDO
      ENDDO
      END
