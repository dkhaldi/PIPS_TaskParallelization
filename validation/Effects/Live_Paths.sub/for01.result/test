int main()
{
   int a, b;
   int tab[10];

   b = 10;
   for (a = 0;a<b;tab[a] += tab[a-1])
      a = a+1;
   return a+tab[a];
}
int main()
{
   int a, b;
//             <may be alive (in)>: a
   int tab[10];
//             <may be alive (in)>: a tab[*]

   b = 10;
//             <may be alive (in)>: a b tab[*]
   for (a = 0;a<b;tab[a] += tab[a-1])
//             <may be alive (in)>: b tab[*]
//             <    is alive (in)>: a
      a = a+1;
//             <may be alive (in)>: tab[*]
//             <    is alive (in)>: a
   return a+tab[a];
}
int main()
{
//             <may be alive (out)>: a
   int a, b;
//             <may be alive (out)>: a tab[*]
   int tab[10];
//             <may be alive (out)>: a b tab[*]

   b = 10;
//             <may be alive (out)>: tab[*]
//             <    is alive (out)>: a
   for (a = 0;a<b;tab[a] += tab[a-1])
//             <may be alive (out)>: a b tab[*]
      a = a+1;
   return a+tab[a];
}
