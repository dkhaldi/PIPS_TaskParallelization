setenv WSPACE=empty_arguments_00
setenv FILE=$WSPACE.c
delete $WSPACE
setproperty ABORT_ON_USER_ERROR=TRUE
create $WSPACE $FILE
# check syntax
shell pips_validation_exe.sh -c -i $WSPACE
setproperty PREPROCESSOR_MISSING_FILE_HANDLING="generate"
