
Proper effects

void foo()
{
//             <    is read   >: i
//             <    is written>: j
   int j = i;
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: j stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   printf("%d", j);
}

Cumulated effects 

//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: i stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
void foo()
{
//             <    is read   >: i
//             <    is written>: j
   int j = i;
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: j stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   printf("%d", j);
}

Proper effects

int main()
{
//             <    is written>: i
   i = 3;
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: i stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   foo();
   return 0;
}

Cumulated effects 

//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: i stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: i __IO_EFFECTS:LUNS[1]
int main()
{
//             <    is written>: i
   i = 3;
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: i stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   foo();
//             <    is read   >: __IO_EFFECTS:LUNS[1]
   return 0;
}
