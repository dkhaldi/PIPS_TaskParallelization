void call10(int i, int x[10], int j, int y[10])
{
   int k, l;
//             <    is read   >: i
//             <    is written>: k

   for(k = 0; k <= i-1; k += 1)
//             <    is read   >: i
//             <    is written>: x[i]
      x[i] = 0;
//             <    is read   >: j
//             <    is written>: l
   for(l = 0; l <= j-1; l += 1)
//             <    is read   >: i j
//             <    is written>: y[i]
      y[i] = 0;
}

