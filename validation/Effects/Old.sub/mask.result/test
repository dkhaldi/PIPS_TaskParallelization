      SUBROUTINE MASK(N)
C             <    is read   >: N

c     Check that MUST effects mask MAY effects

      IF (N.GT.2) THEN                                                  0005
C             <    is read   >: N
C             <    is written>: N
         N = N+1                                                        0006
      ENDIF

      END
C             <may be written>: N
C             <    is read   >: N
      SUBROUTINE MASK(N)
C             <may be written>: N
C             <    is read   >: N

c     Check that MUST effects mask MAY effects

      IF (N.GT.2) THEN                                                  0005
C             <    is read   >: N
C             <    is written>: N
         N = N+1                                                        0006
      ENDIF

      END
