Proper effetcs for "array02"

void array02()
{
//             <    is declared>: x
   float x[11][2];
//             <    is declared>: i
   int i;
//             <    is written>: i
//             <    is referenced>: i

   for(i = 1; i <= 9; i += 1)
//             <    is read   >: i
//             <    is written>: x[i][1]
//             <    is referenced>: i x
      x[i][1] = 0.;
}

Cumulated effetcs for "array02"

void array02()
{
//             <    is declared>: x
   float x[11][2];
//             <    is declared>: i
   int i;
//             <may be written>: x[*][1]
//             <    is read   >: i
//             <    is written>: i
//             <    is referenced>: i x

   for(i = 1; i <= 9; i += 1)
//             <may be written>: x[*][1]
//             <    is read   >: i
//             <    is referenced>: i x
      x[i][1] = 0.;
}

