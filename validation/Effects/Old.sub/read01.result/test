int main()
{
   char buf[20];
   size_t nbytes;
   ssize_t bytes_read;
   int fd;

   nbytes = sizeof(buf);
   bytes_read = read(fd, buf, nbytes);
}
//             <may be read   >: __IO_EFFECTS:LUNS[*]
//             <may be written>: __IO_EFFECTS:LUNS[*]
int main()
{
   char buf[20];
   size_t nbytes;
   ssize_t bytes_read;
   int fd;
//             <    is written>: nbytes

   nbytes = sizeof(buf);
//             <may be read   >: __IO_EFFECTS:LUNS[*]
//             <may be written>: buf[*] __IO_EFFECTS:LUNS[*]
//             <    is read   >: fd nbytes
//             <    is written>: bytes_read
   bytes_read = read(fd, buf, nbytes);
}
