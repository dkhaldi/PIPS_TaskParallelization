
Proper pointer effects

int main()
{
   FILE *s;
   char *fmt;
   char *ch;
   int tab[5];
   int r;
//             <may be read   >: ch[*] fmt[*] s[0] tab[*] 
//                               __IO_EFFECTS:LUNS[*]
//             <may be written>: ch[*] s[0] tab[*] 
//                               __IO_EFFECTS:LUNS[*]
//             <    is read   >: ch fmt s
//             <    is written>: r
   r = varargs03(s, fmt, ch, tab);
//             <    is read   >: r
   return r;
}
int varargs03(FILE *stream, const char *fmt, ...)
{
//             <    is read   >: stream
   if (stream) {
      va_list pa;
      int r;
//             <    is read   >: pa
//             <    is written>: pa

      va_start(pa, fmt);
//             <may be read   >: fmt[*] stream[0] __IO_EFFECTS:LUNS[*]
//             <may be written>: stream[0] __IO_EFFECTS:LUNS[*]
//             <    is read   >: fmt pa stream
//             <    is written>: pa r
      r = vfprintf(stream, fmt, pa);
//             <may be written>: pa
//             <    is read   >: pa
      va_end(pa);
//             <    is read   >: r

      return r;
   }
   else
      return 0;
}

Cumulated pointer effects

//             <may be read   >: __IO_EFFECTS:LUNS[*]
//             <may be written>: __IO_EFFECTS:LUNS[*]
int main()
{
   FILE *s;
   char *fmt;
   char *ch;
   int tab[5];
   int r;
//             <may be read   >: ch[*] fmt[*] s[0] tab[*] 
//                               __IO_EFFECTS:LUNS[*]
//             <may be written>: ch[*] s[0] tab[*] 
//                               __IO_EFFECTS:LUNS[*]
//             <    is read   >: ch fmt s
//             <    is written>: r
   r = varargs03(s, fmt, ch, tab);
//             <may be read   >: __IO_EFFECTS:LUNS[*]
//             <    is read   >: r
   return r;
}
//             <may be read   >: fmt fmt[*] stream[0] 
//                               __IO_EFFECTS:LUNS[*]
//             <may be written>: stream[0] __IO_EFFECTS:LUNS[*]
//             <    is read   >: stream
int varargs03(FILE *stream, const char *fmt, ...)
{
//             <may be read   >: fmt fmt[*] stream[0] 
//                               __IO_EFFECTS:LUNS[*]
//             <may be written>: stream[0] __IO_EFFECTS:LUNS[*]
//             <    is read   >: stream
   if (stream) {
      va_list pa;
      int r;
//             <    is read   >: pa
//             <    is written>: pa

      va_start(pa, fmt);
//             <may be read   >: fmt[*] stream[0] __IO_EFFECTS:LUNS[*]
//             <may be written>: stream[0] __IO_EFFECTS:LUNS[*]
//             <    is read   >: fmt pa stream
//             <    is written>: pa r
      r = vfprintf(stream, fmt, pa);
//             <may be written>: pa
//             <    is read   >: pa
      va_end(pa);
//             <    is read   >: r

      return r;
   }
   else
      return 0;
}
