
PRINT_CODE_SIMPLE_POINTER_VALUES


// IN Pointer values: none


// OUT Pointer values: none

void dereferencing03()
{

// Pointer values: none

   double *p;

// Pointer values:
// p == *ANY_MODULE*:*UNDEFINED* (exact);

   int i = 5;

// Pointer values:
// p == *ANY_MODULE*:*UNDEFINED* (exact);


   p = (double *) malloc(10*sizeof(double));

// Pointer values:
// p == &*HEAP*_l_8[0] (exact);


   *(p+1) = 0.;

// Pointer values:
// p == &*HEAP*_l_8[0] (exact);

   *(p+i) = 0.;
}

// IN Pointer values: none


// OUT Pointer values: none

int main()
{

// Pointer values: none

   dereferencing03();

// Pointer values: none

   return 1;
}

PROPER_EFFECTS_WITH_POINTER_VALUES

void dereferencing03()
{
   double *p;
//             <    is written>: i
   int i = 5;
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: p _MALLOC_EFFECTS:_MALLOC_

   p = (double *) malloc(10*sizeof(double));
//             <    is read   >: p
//             <    is written>: *HEAP*_l_8[1]

   *(p+1) = 0.;
//             <    is read   >: i p
//             <    is written>: *HEAP*_l_8[i]
   *(p+i) = 0.;
}
int main()
{
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: _MALLOC_EFFECTS:_MALLOC_
   dereferencing03();
   return 1;
}

MUST_REGIONS_WITH_POINTER_VALUES

void dereferencing03()
{
   double *p;

//  <i-W-EXACT-{}>

   int i = 5;

//  <p-W-EXACT-{}>
//  <_MALLOC_EFFECTS:_MALLOC_-R-EXACT-{}>
//  <_MALLOC_EFFECTS:_MALLOC_-W-EXACT-{}>


   p = (double *) malloc(10*sizeof(double));

//  <*HEAP*_l_8[PHI1]-W-EXACT-{PHI1==1, i==5}>
//  <p-R-EXACT-{}>


   *(p+1) = 0.;

//  <*HEAP*_l_8[PHI1]-W-EXACT-{PHI1==5, i==5}>
//  <i-R-EXACT-{}>
//  <p-R-EXACT-{}>

   *(p+i) = 0.;
}
int main()
{

//  <_MALLOC_EFFECTS:_MALLOC_-R-EXACT-{}>
//  <_MALLOC_EFFECTS:_MALLOC_-W-EXACT-{}>

   dereferencing03();
   return 1;
}

PRINT_CODE_REGIONS


//  <_MALLOC_EFFECTS:_MALLOC_-R-EXACT-{}>
//  <_MALLOC_EFFECTS:_MALLOC_-W-EXACT-{}>

void dereferencing03()
{
   double *p;

//  <i-W-EXACT-{}>

   int i = 5;

//  <p-W-EXACT-{}>
//  <_MALLOC_EFFECTS:_MALLOC_-R-EXACT-{}>
//  <_MALLOC_EFFECTS:_MALLOC_-W-EXACT-{}>


   p = (double *) malloc(10*sizeof(double));

//  <*HEAP*_l_8[PHI1]-W-EXACT-{PHI1==1, i==5}>
//  <p-R-EXACT-{}>


   *(p+1) = 0.;

//  <*HEAP*_l_8[PHI1]-W-EXACT-{PHI1==5, i==5}>
//  <i-R-EXACT-{}>
//  <p-R-EXACT-{}>

   *(p+i) = 0.;
}

//  <_MALLOC_EFFECTS:_MALLOC_-R-EXACT-{}>
//  <_MALLOC_EFFECTS:_MALLOC_-W-EXACT-{}>

int main()
{

//  <_MALLOC_EFFECTS:_MALLOC_-R-EXACT-{}>
//  <_MALLOC_EFFECTS:_MALLOC_-W-EXACT-{}>

   dereferencing03();
   return 1;
}
