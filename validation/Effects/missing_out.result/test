//             <may be exported>: array[*][*]
void init_2D(int n, int m, int array[n][m])
{
   int i, j;
//             <may be exported>: array[*][*]

   for(i = 0; i <= n-1; i += 1)
//             <may be exported>: array[*][*]
      for(j = 0; j <= m-1; j += 1)
//             <may be exported>: array[*][*]
         array[i][j] = i+j;
}
int main(void)
{
   int n, m;
//             <may be exported>: m n
//             <    is exported>: __IO_EFFECTS:LUNS[0]
   scanf("%d%d", &n, &m);

//             <    is exported>: size
   int size = n*m;
   int array_1D[size];
//             <may be exported>: array_1D[*]

   init_2D(n, m, array_1D);
//             <may be exported>: __IO_EFFECTS:LUNS[1]

   display_1D(size, array_1D);

   return 0;
}

//  <array[PHI1][PHI2]-OUT-MAY-{0<=PHI1, PHI1+1<=n, 0<=PHI2,
//    PHI2+1<=m}>

void init_2D(int n, int m, int array[n][m])
{
   int i, j;

//  <array[PHI1][PHI2]-OUT-MAY-{0<=PHI1, PHI1+1<=n, 0<=PHI2,
//    PHI2+1<=m}>


   for(i = 0; i <= n-1; i += 1)

//  <array[PHI1][PHI2]-OUT-MAY-{PHI1==i, 0<=PHI2, PHI2+1<=m, 0<=i,
//    i+1<=n}>

      for(j = 0; j <= m-1; j += 1)

//  <array[PHI1][PHI2]-OUT-MAY-{PHI1==i, PHI2==j, 0<=i, i+1<=n, 0<=j,
//    j+1<=m}>

         array[i][j] = i+j;
}
int main(void)
{
   int n, m;

//  <__IO_EFFECTS:LUNS[PHI1]-OUT-MAY-{PHI1==0}>

   scanf("%d%d", &n, &m);

   int size = n*m;
   int array_1D[size];

//  <array_1D[PHI1]-OUT-MAY-{0<=PHI1, PHI1+1<=size}>


   init_2D(n, m, array_1D);

//  <__IO_EFFECTS:LUNS[PHI1]-OUT-EXACT-{PHI1==1, 1<=size}>


   display_1D(size, array_1D);

   return 0;
}
