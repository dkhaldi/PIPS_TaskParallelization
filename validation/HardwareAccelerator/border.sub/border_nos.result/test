! acceleration function

// FREIA OpenCL includes
#include <CL/opencl.h>
#include "freia.h"
#include "private/freia_opencl_runtime.h"


// dag 0: 1 ops and 0 copies, optimized to 1 ops and 0+0+0 copies

// hold kernels for border_nos_opencl_helper_E8_438
static cl_kernel border_nos_opencl_helper_E8_438_kernel[2];

// compile kernels for border_nos_opencl_helper_E8_438
static freia_status border_nos_opencl_helper_E8_438_compile(void)
{
  // OpenCL source for border_nos_opencl_helper_E8_438
  const char * border_nos_opencl_helper_E8_438_source =
    "#include <freia_opencl_runtime.hcl>\n"
    "\n"
    "// opencl function border_nos_opencl_helper_E8_438\n"
    "KERNEL void border_nos_opencl_helper_E8_438(\n"
    "  GLOBAL PIXEL * o0,\n"
    "  int ofs_o0,\n"
    "  GLOBAL PIXEL * i0, // const?\n"
    "  int ofs_i0,\n"
    "  int width, // of the working area, vs image pitch below\n"
    "  int height, // of the working area\n"
    "  int pitch)\n"
    "{\n"
    "  // no tiling on height dimension\n"
    "  // assert(height==get_global_size(0));\n"
    "  int j = get_global_id(0);\n"
    "\n"
    "  // loop i upper bound\n"
    "  int Wtile = (width+get_global_size(1)-1)/get_global_size(1);\n"
    "  int Wlast = Wtile*(get_global_id(1)+1);\n"
    "  if (Wlast>width) Wlast = width;\n"
    "\n"
    "  // N & S boundaries, one thread on first dimension per row\n"
    "  int is_N = (get_global_id(0)==0);\n"
    "  int is_S = (get_global_id(0)==(height-1));\n"
    "\n"
    "  // get input & output image pointers\n"
    "  int shift = pitch*j;\n"
    "  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;\n"
    "  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;\n"
    "\n"
    "  int i;\n"
    "  for (i=Wtile*get_global_id(1); i<Wlast; i++)\n"
    "  {\n"
    "    // W & E boundaries, assuming i global index\n"
    "    int is_W = (i==0);\n"
    "    // E not needed\n"
    "\n"
    "    // get input pixels\n"
    "    PIXEL t8 = PIXEL_MAX;\n"
    "    PIXEL in0NW = (is_N|is_W)? 0: j0[i-pitch-1];\n"
    "    PIXEL in0N = (is_N)? 0: j0[i-pitch];\n"
    "    PIXEL in0W = (is_W)? 0: j0[i-1];\n"
    "    PIXEL in0 = j0[i];\n"
    "    PIXEL in0SW = (is_S|is_W)? 0: j0[i+pitch-1];\n"
    "    PIXEL in0S = (is_S)? 0: j0[i+pitch];\n"
    "\n"
    "    // pixel computations\n"
    "    t8 = PIXEL_INF(t8, (is_N|is_W)? PIXEL_MAX: in0NW);\n"
    "    t8 = PIXEL_INF(t8, (is_N)? PIXEL_MAX: in0N);\n"
    "    t8 = PIXEL_INF(t8, (is_W)? PIXEL_MAX: in0W);\n"
    "    t8 = PIXEL_INF(t8, in0);\n"
    "    t8 = PIXEL_INF(t8, (is_S|is_W)? PIXEL_MAX: in0SW);\n"
    "    t8 = PIXEL_INF(t8, (is_S)? PIXEL_MAX: in0S);\n"
    "\n"
    "    // set output pixels\n"
    "    p0[i] = t8;\n"
    "  }\n"
    "}\n";
  freia_status err = FREIA_OK;
  err |= freia_op_compile_kernel(border_nos_opencl_helper_E8_438_source, "border_nos_opencl_helper_E8_438", "-DPIXEL8", &border_nos_opencl_helper_E8_438_kernel[0]);
  err |= freia_op_compile_kernel(border_nos_opencl_helper_E8_438_source, "border_nos_opencl_helper_E8_438", "-DPIXEL16", &border_nos_opencl_helper_E8_438_kernel[1]);
  return err;
}

// helper function border_nos_opencl_helper_E8_438
freia_status border_nos_opencl_helper_E8_438(
  freia_data2d * o0,
  const freia_data2d * i0)
{
  freia_status err = FREIA_OK;

  // handle on the fly compilation...
  static int to_compile = 1;
  if (to_compile) {
    err |= border_nos_opencl_helper_E8_438_compile();
    // compilation may have failed
    if (err) return err;
    to_compile = 0;
  }

  // now get kernel, which must be have be compiled
  uint32_t bpp = i0->bpp>>4;
  cl_kernel kernel = border_nos_opencl_helper_E8_438_kernel[bpp];

  // call kernel border_nos_opencl_helper_E8_438
  err |= freia_op_call_kernel(kernel, 1, 1, 0, 0, o0, i0);

  return err;
}
#include <freia_opencl_runtime.hcl>
// generated OpenCL kernels for function border_nos

// opencl for dag 0

// opencl function border_nos_opencl_helper_E8_438
KERNEL void border_nos_opencl_helper_E8_438(
  GLOBAL PIXEL * o0,
  int ofs_o0,
  GLOBAL PIXEL * i0, // const?
  int ofs_i0,
  int width, // of the working area, vs image pitch below
  int height, // of the working area
  int pitch)
{
  // no tiling on height dimension
  // assert(height==get_global_size(0));
  int j = get_global_id(0);

  // loop i upper bound
  int Wtile = (width+get_global_size(1)-1)/get_global_size(1);
  int Wlast = Wtile*(get_global_id(1)+1);
  if (Wlast>width) Wlast = width;

  // N & S boundaries, one thread on first dimension per row
  int is_N = (get_global_id(0)==0);
  int is_S = (get_global_id(0)==(height-1));

  // get input & output image pointers
  int shift = pitch*j;
  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;
  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;

  int i;
  for (i=Wtile*get_global_id(1); i<Wlast; i++)
  {
    // W & E boundaries, assuming i global index
    int is_W = (i==0);
    // E not needed

    // get input pixels
    PIXEL t8 = PIXEL_MAX;
    PIXEL in0NW = (is_N|is_W)? 0: j0[i-pitch-1];
    PIXEL in0N = (is_N)? 0: j0[i-pitch];
    PIXEL in0W = (is_W)? 0: j0[i-1];
    PIXEL in0 = j0[i];
    PIXEL in0SW = (is_S|is_W)? 0: j0[i+pitch-1];
    PIXEL in0S = (is_S)? 0: j0[i+pitch];

    // pixel computations
    t8 = PIXEL_INF(t8, (is_N|is_W)? PIXEL_MAX: in0NW);
    t8 = PIXEL_INF(t8, (is_N)? PIXEL_MAX: in0N);
    t8 = PIXEL_INF(t8, (is_W)? PIXEL_MAX: in0W);
    t8 = PIXEL_INF(t8, in0);
    t8 = PIXEL_INF(t8, (is_S|is_W)? PIXEL_MAX: in0SW);
    t8 = PIXEL_INF(t8, (is_S)? PIXEL_MAX: in0S);

    // set output pixels
    p0[i] = t8;
  }
}

