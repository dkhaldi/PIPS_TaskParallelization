// DAG "dag_cleaned_0": #i=1 #o=2 #c=2 #s=0 #I=0 #B=0 #A=0
digraph "dag_cleaned_0" {
  // inputs
  "i" [shape=circle];

  // outputs
  "o0" [shape=circle];
  "o1" [shape=circle];

  // computation vertices
  "10 *_" [shape=trapezium,orientation=270];
  "10 *_" -> "o1";
  "9 thr" [shape=parallelogram];
  "9 thr" -> "o0";
  "i" -> "10 *_";
  "i" -> "10 *_";
  "i" -> "9 thr";
}

! updated function for AIPO

freia_status freia_30(freia_data2d *o0, freia_data2d *o1, freia_data2d *i)
{
   // parallel reuses (similar to freia_27)
   // o0 = f(i)
   // o1 = i + i
   freia_aipo_threshold(o0, i, 10, 20, true);                           /*0009*/
   freia_aipo_mul_const(o1, i, 2);                                      /*0010*/

   return FREIA_OK;                                                     /*0012*/
}
