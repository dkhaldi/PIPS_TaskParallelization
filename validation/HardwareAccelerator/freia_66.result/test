#
# AIPO
#
#
# compile generated freia_66
#

! initial function

int freia_66(int32_t *kernel)
{
   int n = 10;                                                          /*0005*/
   freia_dataio fdin, fdout;                                            /*0006*/
   freia_common_open_input(&fdin, 0);                                   /*0007*/
   freia_common_open_output(&fdout, 1, 1024, 720, 16);                  /*0008*/

   freia_data2d *in, *t, *out;                                          /*0010*/

   in = freia_common_create_data(16, 1024, 720);                        /*0012*/
   t = freia_common_create_data(16, 1024, 720);                         /*0013*/
   out = freia_common_create_data(16, 1024, 720);                       /*0014*/
   
   // input
   freia_common_rx_image(in, &fdin);                                    /*0017*/

   freia_aipo_copy(out, in);                                            /*0019*/
   freia_aipo_copy(t, in);                                              /*0020*/

   while (n--) {                                                        /*0022*/
      freia_aipo_erode_8c(out, out, kernel);                            /*0024*/
      freia_aipo_sup(t, out, t);                                        /*0025*/
      freia_aipo_dilate_8c(out, t, kernel);                             /*0026*/
      freia_aipo_sup(t, out, t);                                        /*0027*/
   }
   
   // output
   freia_common_tx_image(out, &fdout);                                  /*0031*/
   
   // cleanup
   freia_common_destruct_data(in);                                      /*0034*/
   freia_common_destruct_data(t);                                       /*0035*/
   freia_common_destruct_data(out);                                     /*0036*/

   freia_common_close_input(&fdin);                                     /*0038*/
   freia_common_close_output(&fdout);                                   /*0039*/

   return 0;                                                            /*0041*/
}

