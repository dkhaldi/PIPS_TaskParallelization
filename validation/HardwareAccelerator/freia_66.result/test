  // computation vertices
  "27 >" [shape=trapezium,orientation=270];
  "27 >" -> "t'";
  "26 D8" [shape=box];
  "26 D8" -> "27 >" [label="out"];
  "26 D8" -> "out'";
  "25 >" [shape=trapezium,orientation=270];
  "25 >" -> "27 >" [label="t_1"];
  "25 >" -> "26 D8" [label="t_1"];
  "t" -> "25 >";
  "24 E8" [shape=box];
  "24 E8" -> "25 >" [label="out_1"];
  "out" -> "24 E8";
}
// graph for dag "freia_66_terapix_helper_1_0_0" of module "freia_66" in dot format
// DAG "freia_66_terapix_helper_1_0_0": #i=2 #o=2 #c=4 #s=0 #I=0 #B=0 #A=0
digraph "freia_66_terapix_helper_1_0_0" {
  // inputs
