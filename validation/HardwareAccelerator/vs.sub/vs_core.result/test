! acceleration function

// FREIA OpenCL includes
#include <CL/opencl.h>
#include "freia.h"
#include "private/freia_opencl_runtime.h"


// dag 0: 16 ops and 1 copies, optimized to 16 ops and 0+0+0 copies

// hold kernels for vs_core_opencl_helper_0_0_0
static cl_kernel vs_core_opencl_helper_0_0_0_kernel[2];

// compile kernels for vs_core_opencl_helper_0_0_0
static freia_status vs_core_opencl_helper_0_0_0_compile(void)
{
  // OpenCL source for vs_core_opencl_helper_0_0_0
  const char * vs_core_opencl_helper_0_0_0_source =
    "#include <freia_opencl_runtime.hcl>\n"
    "\n"
    "// opencl function vs_core_opencl_helper_0_0_0\n"
    "KERNEL void vs_core_opencl_helper_0_0_0(\n"
    "  GLOBAL PIXEL * o0,\n"
    "  int ofs_o0,\n"
    "  GLOBAL PIXEL * o1,\n"
    "  int ofs_o1,\n"
    "  GLOBAL PIXEL * i0, // const?\n"
    "  int ofs_i0,\n"
    "  GLOBAL PIXEL * i1, // const?\n"
    "  int ofs_i1,\n"
    "  int width, // of the working area, vs image pitch below\n"
    "  int height, // of the working area\n"
    "  int pitch,\n"
    "  int c0,\n"
    "  int c1,\n"
    "  int c2,\n"
    "  GLOBAL TMeasure * redX)\n"
    "{\n"
    "  // no tiling on height dimension\n"
    "  // assert(height==get_global_size(0));\n"
    "  int j = get_global_id(0);\n"
    "\n"
    "  // reduction stuff is currently hardcoded...\n"
    "  int vol = 0;\n"
    "  PIXEL minv = PIXEL_MAX;\n"
    "  int2 minpos = { 0, 0 };\n"
    "  PIXEL maxv = PIXEL_MIN;\n"
    "  int2 maxpos = { 0, 0 };\n"
    "\n"
    "  // loop i upper bound\n"
    "  int Wtile = (width+get_global_size(1)-1)/get_global_size(1);\n"
    "  int Wlast = Wtile*(get_global_id(1)+1);\n"
    "  if (Wlast>width) Wlast = width;\n"
    "\n"
    "  // get input & output image pointers\n"
    "  int shift = pitch*j;\n"
    "  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;\n"
    "  GLOBAL PIXEL * p1 = o1 + ofs_o1 + shift;\n"
    "  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;\n"
    "  GLOBAL PIXEL * j1 = i1 + ofs_i1 + shift;\n"
    "\n"
    "  int i;\n"
    "  for (i=Wtile*get_global_id(1); i<Wlast; i++)\n"
    "  {\n"
    "    // get input pixels\n"
    "    PIXEL in1 = j1[i];\n"
    "    PIXEL in0 = j0[i];\n"
    "\n"
    "    // pixel computations\n"
    "    PIXEL t14 = PIXEL_ABSDIFF(in1, in0);\n"
    "    PIXEL t18 = PIXEL_MUL(in1, c0);\n"
    "    PIXEL t19 = PIXEL_MUL(in0, c1);\n"
    "    PIXEL t20 = PIXEL_ADD(t19, t18);\n"
    "    PIXEL_MAXIMUM(red24, t14);\n"
    "    PIXEL_MINIMUM(red25, t14);\n"
    "    PIXEL t21 = PIXEL_DIV(t20, c2);\n"
    "\n"
    "    // set output pixels\n"
    "    p0[i] = t14;\n"
    "    p1[i] = t21;\n"
    "  }\n"
    "\n"
    "  // reduction copy out\n"
    "  int thrid = get_global_id(0)*get_global_size(1)+get_global_id(1);\n"
    "  redX[thrid].max = maxv;\n"
    "  redX[thrid].min = minv;\n"
    "}\n";
  freia_status err = FREIA_OK;
  err |= freia_op_compile_kernel(vs_core_opencl_helper_0_0_0_source, "vs_core_opencl_helper_0_0_0", "-DPIXEL8", &vs_core_opencl_helper_0_0_0_kernel[0]);
  err |= freia_op_compile_kernel(vs_core_opencl_helper_0_0_0_source, "vs_core_opencl_helper_0_0_0", "-DPIXEL16", &vs_core_opencl_helper_0_0_0_kernel[1]);
  return err;
}

// helper function vs_core_opencl_helper_0_0_0
freia_status vs_core_opencl_helper_0_0_0(
  freia_data2d * o0,
  freia_data2d * o1,
  const freia_data2d * i0,
  const freia_data2d * i1,
  int32_t c0,
  int32_t c1,
  int32_t * po4,
  int32_t * po5,
  int32_t c2)
{
  freia_status err = FREIA_OK;

  // handle on the fly compilation...
  static int to_compile = 1;
  if (to_compile) {
    err |= vs_core_opencl_helper_0_0_0_compile();
    // compilation may have failed
    if (err) return err;
    to_compile = 0;
  }

  // now get kernel, which must be have be compiled
  uint32_t bpp = i0->bpp>>4;
  cl_kernel kernel = vs_core_opencl_helper_0_0_0_kernel[bpp];

  // currently only one reduction structure...
  freia_opencl_measure_status redres;

  // call kernel vs_core_opencl_helper_0_0_0
  err |= freia_op_call_kernel(kernel, 2, 2, 3, 1, o0, o1, i0, i1, c0, c1, c2, &redres);

  // return reduction results
  *po4 = redres.maximum;
  *po5 = redres.minimum;

  return err;
}

// hold kernels for vs_core_opencl_helper_D8_511
static cl_kernel vs_core_opencl_helper_D8_511_kernel[2];

// compile kernels for vs_core_opencl_helper_D8_511
static freia_status vs_core_opencl_helper_D8_511_compile(void)
{
  // OpenCL source for vs_core_opencl_helper_D8_511
  const char * vs_core_opencl_helper_D8_511_source =
    "#include <freia_opencl_runtime.hcl>\n"
    "\n"
    "// opencl function vs_core_opencl_helper_D8_511\n"
    "KERNEL void vs_core_opencl_helper_D8_511(\n"
    "  GLOBAL PIXEL * o0,\n"
    "  int ofs_o0,\n"
    "  GLOBAL PIXEL * i0, // const?\n"
    "  int ofs_i0,\n"
    "  int width, // of the working area, vs image pitch below\n"
    "  int height, // of the working area\n"
    "  int pitch)\n"
    "{\n"
    "  // no tiling on height dimension\n"
    "  // assert(height==get_global_size(0));\n"
    "  int j = get_global_id(0);\n"
    "\n"
    "  // loop i upper bound\n"
    "  int Wtile = (width+get_global_size(1)-1)/get_global_size(1);\n"
    "  int Wlast = Wtile*(get_global_id(1)+1);\n"
    "  if (Wlast>width) Wlast = width;\n"
    "\n"
    "  // N & S boundaries, one thread on first dimension per row\n"
    "  int is_N = (get_global_id(0)==0);\n"
    "  int is_S = (get_global_id(0)==(height-1));\n"
    "\n"
    "  // get input & output image pointers\n"
    "  int shift = pitch*j;\n"
    "  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;\n"
    "  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;\n"
    "\n"
    "  int i;\n"
    "  for (i=Wtile*get_global_id(1); i<Wlast; i++)\n"
    "  {\n"
    "    // W & E boundaries, assuming i global index\n"
    "    int is_W = (i==0);\n"
    "    int is_E = (i==(width-1));\n"
    "\n"
    "    // get input pixels\n"
    "    PIXEL t34 = PIXEL_MIN;\n"
    "    PIXEL in0NW = (is_N|is_W)? 0: j0[i-pitch-1];\n"
    "    PIXEL in0N = (is_N)? 0: j0[i-pitch];\n"
    "    PIXEL in0NE = (is_N|is_E)? 0: j0[i-pitch+1];\n"
    "    PIXEL in0W = (is_W)? 0: j0[i-1];\n"
    "    PIXEL in0 = j0[i];\n"
    "    PIXEL in0E = (is_E)? 0: j0[i+1];\n"
    "    PIXEL in0SW = (is_S|is_W)? 0: j0[i+pitch-1];\n"
    "    PIXEL in0S = (is_S)? 0: j0[i+pitch];\n"
    "    PIXEL in0SE = (is_S|is_E)? 0: j0[i+pitch+1];\n"
    "\n"
    "    // pixel computations\n"
    "    t34 = PIXEL_SUP(t34, (is_N|is_W)? PIXEL_MIN: in0NW);\n"
    "    t34 = PIXEL_SUP(t34, (is_N)? PIXEL_MIN: in0N);\n"
    "    t34 = PIXEL_SUP(t34, (is_N|is_E)? PIXEL_MIN: in0NE);\n"
    "    t34 = PIXEL_SUP(t34, (is_W)? PIXEL_MIN: in0W);\n"
    "    t34 = PIXEL_SUP(t34, in0);\n"
    "    t34 = PIXEL_SUP(t34, (is_E)? PIXEL_MIN: in0E);\n"
    "    t34 = PIXEL_SUP(t34, (is_S|is_W)? PIXEL_MIN: in0SW);\n"
    "    t34 = PIXEL_SUP(t34, (is_S)? PIXEL_MIN: in0S);\n"
    "    t34 = PIXEL_SUP(t34, (is_S|is_E)? PIXEL_MIN: in0SE);\n"
    "\n"
    "    // set output pixels\n"
    "    p0[i] = t34;\n"
    "  }\n"
    "}\n";
  freia_status err = FREIA_OK;
  err |= freia_op_compile_kernel(vs_core_opencl_helper_D8_511_source, "vs_core_opencl_helper_D8_511", "-DPIXEL8", &vs_core_opencl_helper_D8_511_kernel[0]);
  err |= freia_op_compile_kernel(vs_core_opencl_helper_D8_511_source, "vs_core_opencl_helper_D8_511", "-DPIXEL16", &vs_core_opencl_helper_D8_511_kernel[1]);
  return err;
}

// helper function vs_core_opencl_helper_D8_511
freia_status vs_core_opencl_helper_D8_511(
  freia_data2d * o0,
  const freia_data2d * i0)
{
  freia_status err = FREIA_OK;

  // handle on the fly compilation...
  static int to_compile = 1;
  if (to_compile) {
    err |= vs_core_opencl_helper_D8_511_compile();
    // compilation may have failed
    if (err) return err;
    to_compile = 0;
  }

  // now get kernel, which must be have be compiled
  uint32_t bpp = i0->bpp>>4;
  cl_kernel kernel = vs_core_opencl_helper_D8_511_kernel[bpp];

  // call kernel vs_core_opencl_helper_D8_511
  err |= freia_op_call_kernel(kernel, 1, 1, 0, 0, o0, i0);

  return err;
}

// hold kernels for vs_core_opencl_helper_E8_511
static cl_kernel vs_core_opencl_helper_E8_511_kernel[2];

// compile kernels for vs_core_opencl_helper_E8_511
static freia_status vs_core_opencl_helper_E8_511_compile(void)
{
  // OpenCL source for vs_core_opencl_helper_E8_511
  const char * vs_core_opencl_helper_E8_511_source =
    "#include <freia_opencl_runtime.hcl>\n"
    "\n"
    "// opencl function vs_core_opencl_helper_E8_511\n"
    "KERNEL void vs_core_opencl_helper_E8_511(\n"
    "  GLOBAL PIXEL * o0,\n"
    "  int ofs_o0,\n"
    "  GLOBAL PIXEL * i0, // const?\n"
    "  int ofs_i0,\n"
    "  int width, // of the working area, vs image pitch below\n"
    "  int height, // of the working area\n"
    "  int pitch)\n"
    "{\n"
    "  // no tiling on height dimension\n"
    "  // assert(height==get_global_size(0));\n"
    "  int j = get_global_id(0);\n"
    "\n"
    "  // loop i upper bound\n"
    "  int Wtile = (width+get_global_size(1)-1)/get_global_size(1);\n"
    "  int Wlast = Wtile*(get_global_id(1)+1);\n"
    "  if (Wlast>width) Wlast = width;\n"
    "\n"
    "  // N & S boundaries, one thread on first dimension per row\n"
    "  int is_N = (get_global_id(0)==0);\n"
    "  int is_S = (get_global_id(0)==(height-1));\n"
    "\n"
    "  // get input & output image pointers\n"
    "  int shift = pitch*j;\n"
    "  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;\n"
    "  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;\n"
    "\n"
    "  int i;\n"
    "  for (i=Wtile*get_global_id(1); i<Wlast; i++)\n"
    "  {\n"
    "    // W & E boundaries, assuming i global index\n"
    "    int is_W = (i==0);\n"
    "    int is_E = (i==(width-1));\n"
    "\n"
    "    // get input pixels\n"
    "    PIXEL t33 = PIXEL_MAX;\n"
    "    PIXEL in0NW = (is_N|is_W)? 0: j0[i-pitch-1];\n"
    "    PIXEL in0N = (is_N)? 0: j0[i-pitch];\n"
    "    PIXEL in0NE = (is_N|is_E)? 0: j0[i-pitch+1];\n"
    "    PIXEL in0W = (is_W)? 0: j0[i-1];\n"
    "    PIXEL in0 = j0[i];\n"
    "    PIXEL in0E = (is_E)? 0: j0[i+1];\n"
    "    PIXEL in0SW = (is_S|is_W)? 0: j0[i+pitch-1];\n"
    "    PIXEL in0S = (is_S)? 0: j0[i+pitch];\n"
    "    PIXEL in0SE = (is_S|is_E)? 0: j0[i+pitch+1];\n"
    "\n"
    "    // pixel computations\n"
    "    t33 = PIXEL_INF(t33, (is_N|is_W)? PIXEL_MAX: in0NW);\n"
    "    t33 = PIXEL_INF(t33, (is_N)? PIXEL_MAX: in0N);\n"
    "    t33 = PIXEL_INF(t33, (is_N|is_E)? PIXEL_MAX: in0NE);\n"
    "    t33 = PIXEL_INF(t33, (is_W)? PIXEL_MAX: in0W);\n"
    "    t33 = PIXEL_INF(t33, in0);\n"
    "    t33 = PIXEL_INF(t33, (is_E)? PIXEL_MAX: in0E);\n"
    "    t33 = PIXEL_INF(t33, (is_S|is_W)? PIXEL_MAX: in0SW);\n"
    "    t33 = PIXEL_INF(t33, (is_S)? PIXEL_MAX: in0S);\n"
    "    t33 = PIXEL_INF(t33, (is_S|is_E)? PIXEL_MAX: in0SE);\n"
    "\n"
    "    // set output pixels\n"
    "    p0[i] = t33;\n"
    "  }\n"
    "}\n";
  freia_status err = FREIA_OK;
  err |= freia_op_compile_kernel(vs_core_opencl_helper_E8_511_source, "vs_core_opencl_helper_E8_511", "-DPIXEL8", &vs_core_opencl_helper_E8_511_kernel[0]);
  err |= freia_op_compile_kernel(vs_core_opencl_helper_E8_511_source, "vs_core_opencl_helper_E8_511", "-DPIXEL16", &vs_core_opencl_helper_E8_511_kernel[1]);
  return err;
}

// helper function vs_core_opencl_helper_E8_511
freia_status vs_core_opencl_helper_E8_511(
  freia_data2d * o0,
  const freia_data2d * i0)
{
  freia_status err = FREIA_OK;

  // handle on the fly compilation...
  static int to_compile = 1;
  if (to_compile) {
    err |= vs_core_opencl_helper_E8_511_compile();
    // compilation may have failed
    if (err) return err;
    to_compile = 0;
  }

  // now get kernel, which must be have be compiled
  uint32_t bpp = i0->bpp>>4;
  cl_kernel kernel = vs_core_opencl_helper_E8_511_kernel[bpp];

  // call kernel vs_core_opencl_helper_E8_511
  err |= freia_op_call_kernel(kernel, 1, 1, 0, 0, o0, i0);

  return err;
}

// hold kernels for vs_core_opencl_helper_0_1_2
static cl_kernel vs_core_opencl_helper_0_1_2_kernel[2];

// compile kernels for vs_core_opencl_helper_0_1_2
static freia_status vs_core_opencl_helper_0_1_2_compile(void)
{
  // OpenCL source for vs_core_opencl_helper_0_1_2
  const char * vs_core_opencl_helper_0_1_2_source =
    "#include <freia_opencl_runtime.hcl>\n"
    "\n"
    "// opencl function vs_core_opencl_helper_0_1_2\n"
    "KERNEL void vs_core_opencl_helper_0_1_2(\n"
    "  GLOBAL PIXEL * o0,\n"
    "  int ofs_o0,\n"
    "  GLOBAL PIXEL * i0, // const?\n"
    "  int ofs_i0,\n"
    "  GLOBAL PIXEL * i1, // const?\n"
    "  int ofs_i1,\n"
    "  int width, // of the working area, vs image pitch below\n"
    "  int height, // of the working area\n"
    "  int pitch,\n"
    "  int c0,\n"
    "  int c1)\n"
    "{\n"
    "  // no tiling on height dimension\n"
    "  // assert(height==get_global_size(0));\n"
    "  int j = get_global_id(0);\n"
    "\n"
    "  // loop i upper bound\n"
    "  int Wtile = (width+get_global_size(1)-1)/get_global_size(1);\n"
    "  int Wlast = Wtile*(get_global_id(1)+1);\n"
    "  if (Wlast>width) Wlast = width;\n"
    "\n"
    "  // N & S boundaries, one thread on first dimension per row\n"
    "  int is_N = (get_global_id(0)==0);\n"
    "  int is_S = (get_global_id(0)==(height-1));\n"
    "\n"
    "  // get input & output image pointers\n"
    "  int shift = pitch*j;\n"
    "  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;\n"
    "  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;\n"
    "  GLOBAL PIXEL * j1 = i1 + ofs_i1 + shift;\n"
    "\n"
    "  int i;\n"
    "  for (i=Wtile*get_global_id(1); i<Wlast; i++)\n"
    "  {\n"
    "    // W & E boundaries, assuming i global index\n"
    "    int is_W = (i==0);\n"
    "    int is_E = (i==(width-1));\n"
    "\n"
    "    // get input pixels\n"
    "    PIXEL t37 = PIXEL_MIN;\n"
    "    PIXEL in0NW = (is_N|is_W)? 0: j0[i-pitch-1];\n"
    "    PIXEL in0N = (is_N)? 0: j0[i-pitch];\n"
    "    PIXEL in0NE = (is_N|is_E)? 0: j0[i-pitch+1];\n"
    "    PIXEL in0W = (is_W)? 0: j0[i-1];\n"
    "    PIXEL in0 = j0[i];\n"
    "    PIXEL in0E = (is_E)? 0: j0[i+1];\n"
    "    PIXEL in0SW = (is_S|is_W)? 0: j0[i+pitch-1];\n"
    "    PIXEL in0S = (is_S)? 0: j0[i+pitch];\n"
    "    PIXEL in0SE = (is_S|is_E)? 0: j0[i+pitch+1];\n"
    "    PIXEL t38 = PIXEL_MAX;\n"
    "    PIXEL in1 = j1[i];\n"
    "\n"
    "    // pixel computations\n"
    "    t37 = PIXEL_SUP(t37, (is_N|is_W)? PIXEL_MIN: in0NW);\n"
    "    t37 = PIXEL_SUP(t37, (is_N)? PIXEL_MIN: in0N);\n"
    "    t37 = PIXEL_SUP(t37, (is_N|is_E)? PIXEL_MIN: in0NE);\n"
    "    t37 = PIXEL_SUP(t37, (is_W)? PIXEL_MIN: in0W);\n"
    "    t37 = PIXEL_SUP(t37, in0);\n"
    "    t37 = PIXEL_SUP(t37, (is_E)? PIXEL_MIN: in0E);\n"
    "    t37 = PIXEL_SUP(t37, (is_S|is_W)? PIXEL_MIN: in0SW);\n"
    "    t37 = PIXEL_SUP(t37, (is_S)? PIXEL_MIN: in0S);\n"
    "    t37 = PIXEL_SUP(t37, (is_S|is_E)? PIXEL_MIN: in0SE);\n"
    "    t38 = PIXEL_INF(t38, (is_N|is_W)? PIXEL_MAX: in0NW);\n"
    "    t38 = PIXEL_INF(t38, (is_N)? PIXEL_MAX: in0N);\n"
    "    t38 = PIXEL_INF(t38, (is_N|is_E)? PIXEL_MAX: in0NE);\n"
    "    t38 = PIXEL_INF(t38, (is_W)? PIXEL_MAX: in0W);\n"
    "    t38 = PIXEL_INF(t38, in0);\n"
    "    t38 = PIXEL_INF(t38, (is_E)? PIXEL_MAX: in0E);\n"
    "    t38 = PIXEL_INF(t38, (is_S|is_W)? PIXEL_MAX: in0SW);\n"
    "    t38 = PIXEL_INF(t38, (is_S)? PIXEL_MAX: in0S);\n"
    "    t38 = PIXEL_INF(t38, (is_S|is_E)? PIXEL_MAX: in0SE);\n"
    "    PIXEL t39 = PIXEL_SUB(t37, t38);\n"
    "    PIXEL t41 = PIXEL_ADD(t39, c0);\n"
    "    PIXEL t42 = PIXEL_AND(t41, c1);\n"
    "    PIXEL t43 = PIXEL_MUL(in1, t42);\n"
    "\n"
    "    // set output pixels\n"
    "    p0[i] = t43;\n"
    "  }\n"
    "}\n";
  freia_status err = FREIA_OK;
  err |= freia_op_compile_kernel(vs_core_opencl_helper_0_1_2_source, "vs_core_opencl_helper_0_1_2", "-DPIXEL8", &vs_core_opencl_helper_0_1_2_kernel[0]);
  err |= freia_op_compile_kernel(vs_core_opencl_helper_0_1_2_source, "vs_core_opencl_helper_0_1_2", "-DPIXEL16", &vs_core_opencl_helper_0_1_2_kernel[1]);
  return err;
}

// helper function vs_core_opencl_helper_0_1_2
freia_status vs_core_opencl_helper_0_1_2(
  freia_data2d * o0,
  const freia_data2d * i0,
  const freia_data2d * i1,
  int32_t c0,
  int32_t c1)
{
  freia_status err = FREIA_OK;

  // handle on the fly compilation...
  static int to_compile = 1;
  if (to_compile) {
    err |= vs_core_opencl_helper_0_1_2_compile();
    // compilation may have failed
    if (err) return err;
    to_compile = 0;
  }

  // now get kernel, which must be have be compiled
  uint32_t bpp = i0->bpp>>4;
  cl_kernel kernel = vs_core_opencl_helper_0_1_2_kernel[bpp];

  // call kernel vs_core_opencl_helper_0_1_2
  err |= freia_op_call_kernel(kernel, 1, 2, 2, 0, o0, i0, i1, c0, c1);

  return err;
}
#include <freia_opencl_runtime.hcl>
// generated OpenCL kernels for function vs_core

// opencl for dag 0

// opencl function vs_core_opencl_helper_0_0_0
KERNEL void vs_core_opencl_helper_0_0_0(
  GLOBAL PIXEL * o0,
  int ofs_o0,
  GLOBAL PIXEL * o1,
  int ofs_o1,
  GLOBAL PIXEL * i0, // const?
  int ofs_i0,
  GLOBAL PIXEL * i1, // const?
  int ofs_i1,
  int width, // of the working area, vs image pitch below
  int height, // of the working area
  int pitch,
  int c0,
  int c1,
  int c2,
  GLOBAL TMeasure * redX)
{
  // no tiling on height dimension
  // assert(height==get_global_size(0));
  int j = get_global_id(0);

  // reduction stuff is currently hardcoded...
  int vol = 0;
  PIXEL minv = PIXEL_MAX;
  int2 minpos = { 0, 0 };
  PIXEL maxv = PIXEL_MIN;
  int2 maxpos = { 0, 0 };

  // loop i upper bound
  int Wtile = (width+get_global_size(1)-1)/get_global_size(1);
  int Wlast = Wtile*(get_global_id(1)+1);
  if (Wlast>width) Wlast = width;

  // get input & output image pointers
  int shift = pitch*j;
  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;
  GLOBAL PIXEL * p1 = o1 + ofs_o1 + shift;
  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;
  GLOBAL PIXEL * j1 = i1 + ofs_i1 + shift;

  int i;
  for (i=Wtile*get_global_id(1); i<Wlast; i++)
  {
    // get input pixels
    PIXEL in1 = j1[i];
    PIXEL in0 = j0[i];

    // pixel computations
    PIXEL t14 = PIXEL_ABSDIFF(in1, in0);
    PIXEL t18 = PIXEL_MUL(in1, c0);
    PIXEL t19 = PIXEL_MUL(in0, c1);
    PIXEL t20 = PIXEL_ADD(t19, t18);
    PIXEL_MAXIMUM(red24, t14);
    PIXEL_MINIMUM(red25, t14);
    PIXEL t21 = PIXEL_DIV(t20, c2);

    // set output pixels
    p0[i] = t14;
    p1[i] = t21;
  }

  // reduction copy out
  int thrid = get_global_id(0)*get_global_size(1)+get_global_id(1);
  redX[thrid].max = maxv;
  redX[thrid].min = minv;
}

// opencl function vs_core_opencl_helper_D8_511
KERNEL void vs_core_opencl_helper_D8_511(
  GLOBAL PIXEL * o0,
  int ofs_o0,
  GLOBAL PIXEL * i0, // const?
  int ofs_i0,
  int width, // of the working area, vs image pitch below
  int height, // of the working area
  int pitch)
{
  // no tiling on height dimension
  // assert(height==get_global_size(0));
  int j = get_global_id(0);

  // loop i upper bound
  int Wtile = (width+get_global_size(1)-1)/get_global_size(1);
  int Wlast = Wtile*(get_global_id(1)+1);
  if (Wlast>width) Wlast = width;

  // N & S boundaries, one thread on first dimension per row
  int is_N = (get_global_id(0)==0);
  int is_S = (get_global_id(0)==(height-1));

  // get input & output image pointers
  int shift = pitch*j;
  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;
  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;

  int i;
  for (i=Wtile*get_global_id(1); i<Wlast; i++)
  {
    // W & E boundaries, assuming i global index
    int is_W = (i==0);
    int is_E = (i==(width-1));

    // get input pixels
    PIXEL t34 = PIXEL_MIN;
    PIXEL in0NW = (is_N|is_W)? 0: j0[i-pitch-1];
    PIXEL in0N = (is_N)? 0: j0[i-pitch];
    PIXEL in0NE = (is_N|is_E)? 0: j0[i-pitch+1];
    PIXEL in0W = (is_W)? 0: j0[i-1];
    PIXEL in0 = j0[i];
    PIXEL in0E = (is_E)? 0: j0[i+1];
    PIXEL in0SW = (is_S|is_W)? 0: j0[i+pitch-1];
    PIXEL in0S = (is_S)? 0: j0[i+pitch];
    PIXEL in0SE = (is_S|is_E)? 0: j0[i+pitch+1];

    // pixel computations
    t34 = PIXEL_SUP(t34, (is_N|is_W)? PIXEL_MIN: in0NW);
    t34 = PIXEL_SUP(t34, (is_N)? PIXEL_MIN: in0N);
    t34 = PIXEL_SUP(t34, (is_N|is_E)? PIXEL_MIN: in0NE);
    t34 = PIXEL_SUP(t34, (is_W)? PIXEL_MIN: in0W);
    t34 = PIXEL_SUP(t34, in0);
    t34 = PIXEL_SUP(t34, (is_E)? PIXEL_MIN: in0E);
    t34 = PIXEL_SUP(t34, (is_S|is_W)? PIXEL_MIN: in0SW);
    t34 = PIXEL_SUP(t34, (is_S)? PIXEL_MIN: in0S);
    t34 = PIXEL_SUP(t34, (is_S|is_E)? PIXEL_MIN: in0SE);

    // set output pixels
    p0[i] = t34;
  }
}

// opencl function vs_core_opencl_helper_E8_511
KERNEL void vs_core_opencl_helper_E8_511(
  GLOBAL PIXEL * o0,
  int ofs_o0,
  GLOBAL PIXEL * i0, // const?
  int ofs_i0,
  int width, // of the working area, vs image pitch below
  int height, // of the working area
  int pitch)
{
  // no tiling on height dimension
  // assert(height==get_global_size(0));
  int j = get_global_id(0);

  // loop i upper bound
  int Wtile = (width+get_global_size(1)-1)/get_global_size(1);
  int Wlast = Wtile*(get_global_id(1)+1);
  if (Wlast>width) Wlast = width;

  // N & S boundaries, one thread on first dimension per row
  int is_N = (get_global_id(0)==0);
  int is_S = (get_global_id(0)==(height-1));

  // get input & output image pointers
  int shift = pitch*j;
  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;
  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;

  int i;
  for (i=Wtile*get_global_id(1); i<Wlast; i++)
  {
    // W & E boundaries, assuming i global index
    int is_W = (i==0);
    int is_E = (i==(width-1));

    // get input pixels
    PIXEL t33 = PIXEL_MAX;
    PIXEL in0NW = (is_N|is_W)? 0: j0[i-pitch-1];
    PIXEL in0N = (is_N)? 0: j0[i-pitch];
    PIXEL in0NE = (is_N|is_E)? 0: j0[i-pitch+1];
    PIXEL in0W = (is_W)? 0: j0[i-1];
    PIXEL in0 = j0[i];
    PIXEL in0E = (is_E)? 0: j0[i+1];
    PIXEL in0SW = (is_S|is_W)? 0: j0[i+pitch-1];
    PIXEL in0S = (is_S)? 0: j0[i+pitch];
    PIXEL in0SE = (is_S|is_E)? 0: j0[i+pitch+1];

    // pixel computations
    t33 = PIXEL_INF(t33, (is_N|is_W)? PIXEL_MAX: in0NW);
    t33 = PIXEL_INF(t33, (is_N)? PIXEL_MAX: in0N);
    t33 = PIXEL_INF(t33, (is_N|is_E)? PIXEL_MAX: in0NE);
    t33 = PIXEL_INF(t33, (is_W)? PIXEL_MAX: in0W);
    t33 = PIXEL_INF(t33, in0);
    t33 = PIXEL_INF(t33, (is_E)? PIXEL_MAX: in0E);
    t33 = PIXEL_INF(t33, (is_S|is_W)? PIXEL_MAX: in0SW);
    t33 = PIXEL_INF(t33, (is_S)? PIXEL_MAX: in0S);
    t33 = PIXEL_INF(t33, (is_S|is_E)? PIXEL_MAX: in0SE);

    // set output pixels
    p0[i] = t33;
  }
}

// opencl function vs_core_opencl_helper_0_1_2
KERNEL void vs_core_opencl_helper_0_1_2(
  GLOBAL PIXEL * o0,
  int ofs_o0,
  GLOBAL PIXEL * i0, // const?
  int ofs_i0,
  GLOBAL PIXEL * i1, // const?
  int ofs_i1,
  int width, // of the working area, vs image pitch below
  int height, // of the working area
  int pitch,
  int c0,
  int c1)
{
  // no tiling on height dimension
  // assert(height==get_global_size(0));
  int j = get_global_id(0);

  // loop i upper bound
  int Wtile = (width+get_global_size(1)-1)/get_global_size(1);
  int Wlast = Wtile*(get_global_id(1)+1);
  if (Wlast>width) Wlast = width;

  // N & S boundaries, one thread on first dimension per row
  int is_N = (get_global_id(0)==0);
  int is_S = (get_global_id(0)==(height-1));

  // get input & output image pointers
  int shift = pitch*j;
  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;
  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;
  GLOBAL PIXEL * j1 = i1 + ofs_i1 + shift;

  int i;
  for (i=Wtile*get_global_id(1); i<Wlast; i++)
  {
    // W & E boundaries, assuming i global index
    int is_W = (i==0);
    int is_E = (i==(width-1));

    // get input pixels
    PIXEL t37 = PIXEL_MIN;
    PIXEL in0NW = (is_N|is_W)? 0: j0[i-pitch-1];
    PIXEL in0N = (is_N)? 0: j0[i-pitch];
    PIXEL in0NE = (is_N|is_E)? 0: j0[i-pitch+1];
    PIXEL in0W = (is_W)? 0: j0[i-1];
    PIXEL in0 = j0[i];
    PIXEL in0E = (is_E)? 0: j0[i+1];
    PIXEL in0SW = (is_S|is_W)? 0: j0[i+pitch-1];
    PIXEL in0S = (is_S)? 0: j0[i+pitch];
    PIXEL in0SE = (is_S|is_E)? 0: j0[i+pitch+1];
    PIXEL t38 = PIXEL_MAX;
    PIXEL in1 = j1[i];

    // pixel computations
    t37 = PIXEL_SUP(t37, (is_N|is_W)? PIXEL_MIN: in0NW);
    t37 = PIXEL_SUP(t37, (is_N)? PIXEL_MIN: in0N);
    t37 = PIXEL_SUP(t37, (is_N|is_E)? PIXEL_MIN: in0NE);
    t37 = PIXEL_SUP(t37, (is_W)? PIXEL_MIN: in0W);
    t37 = PIXEL_SUP(t37, in0);
    t37 = PIXEL_SUP(t37, (is_E)? PIXEL_MIN: in0E);
    t37 = PIXEL_SUP(t37, (is_S|is_W)? PIXEL_MIN: in0SW);
    t37 = PIXEL_SUP(t37, (is_S)? PIXEL_MIN: in0S);
    t37 = PIXEL_SUP(t37, (is_S|is_E)? PIXEL_MIN: in0SE);
    t38 = PIXEL_INF(t38, (is_N|is_W)? PIXEL_MAX: in0NW);
    t38 = PIXEL_INF(t38, (is_N)? PIXEL_MAX: in0N);
    t38 = PIXEL_INF(t38, (is_N|is_E)? PIXEL_MAX: in0NE);
    t38 = PIXEL_INF(t38, (is_W)? PIXEL_MAX: in0W);
    t38 = PIXEL_INF(t38, in0);
    t38 = PIXEL_INF(t38, (is_E)? PIXEL_MAX: in0E);
    t38 = PIXEL_INF(t38, (is_S|is_W)? PIXEL_MAX: in0SW);
    t38 = PIXEL_INF(t38, (is_S)? PIXEL_MAX: in0S);
    t38 = PIXEL_INF(t38, (is_S|is_E)? PIXEL_MAX: in0SE);
    PIXEL t39 = PIXEL_SUB(t37, t38);
    PIXEL t41 = PIXEL_ADD(t39, c0);
    PIXEL t42 = PIXEL_AND(t41, c1);
    PIXEL t43 = PIXEL_MUL(in1, t42);

    // set output pixels
    p0[i] = t43;
  }
}

! updated function

freia_status vs_core(freia_data2d *bg, freia_data2d *mv, freia_data2d *in, freia_data2d *stab, int motion_a, int motion_b, int motion_trig, int motion_th)
{
   freia_data2d *t1 = freia_common_create_data(16, 128, 128);           /*0011*/
   int binvalue, maxmotion, minmotion;                                  /*0012*/

   vs_core_opencl_helper_0_0_0(mv, bg, bg, in, motion_a, motion_b, &maxmotion, &minmotion, motion_a+motion_b);       /*0014*/
   
   // measure
   
   // bg update
   
   if (maxmotion-minmotion>motion_trig)                                 /*0027*/
      binvalue = maxmotion*motion_th/100;                               /*0028*/

   freia_aipo_threshold(mv, mv, binvalue, 255, true);                   /*0030*/
   
   // open
   vs_core_opencl_helper_E8_511(t1, mv);                                /*0033*/
   vs_core_opencl_helper_D8_511(t1, t1);                                /*0034*/
   
   // gradient
   vs_core_opencl_helper_0_1_2(mv, t1, stab, -1, 1);                    /*0037*/
   
   
   freia_common_destruct_data(t1);                                      /*0046*/
   return FREIA_OK;                                                     /*0048*/
}

! cleaned up function

freia_status vs_core(freia_data2d *bg, freia_data2d *mv, freia_data2d *in, freia_data2d *stab, int motion_a, int motion_b, int motion_trig, int motion_th)
{
   freia_data2d *t1 = freia_common_create_data(16, 128, 128);           /*0011*/
   int binvalue, maxmotion, minmotion;                                  /*0012*/

   vs_core_opencl_helper_0_0_0(mv, bg, bg, in, motion_a, motion_b, &maxmotion, &minmotion, motion_a+motion_b);       /*0014*/
   
   // measure
   
   // bg update
   
   if (maxmotion-minmotion>motion_trig)                                 /*0027*/
      binvalue = maxmotion*motion_th/100;                               /*0028*/

   freia_aipo_threshold(mv, mv, binvalue, 255, 1);                      /*0030*/
   
   // open
   vs_core_opencl_helper_E8_511(t1, mv);                                /*0033*/
   vs_core_opencl_helper_D8_511(t1, t1);                                /*0034*/
   
   // gradient
   vs_core_opencl_helper_0_1_2(mv, t1, stab, -1, 1);                    /*0037*/
   
   
   freia_common_destruct_data(t1);                                      /*0046*/
   return 0;                                                            /*0048*/
}
#
# SIGMAC
#
#
# compile generated vs_core
#

! initial function

freia_status vs_core(freia_data2d *bg, freia_data2d *mv, freia_data2d *in, freia_data2d *stab, int motion_a, int motion_b, int motion_trig, int motion_th)
{
   freia_data2d *t0 = freia_common_create_data(16, 128, 128), *t1 = freia_common_create_data(16, 128, 128), *t2 = freia_common_create_data(16, 128, 128);         /*0011*/
   int binvalue, maxmotion, minmotion;                                  /*0012*/

   freia_aipo_absdiff(mv, in, bg);                                      /*0014*/
   
   // bg update
   freia_aipo_copy(t0, in);                                             /*0017*/
   freia_aipo_mul_const(t0, t0, motion_a);                              /*0018*/
   freia_aipo_mul_const(bg, bg, motion_b);                              /*0019*/
   freia_aipo_add(bg, bg, t0);                                          /*0020*/
   freia_aipo_div_const(bg, bg, motion_a+motion_b);                     /*0021*/
   
   // measure
   freia_aipo_global_max(mv, &maxmotion);                               /*0024*/
   freia_aipo_global_min(mv, &minmotion);                               /*0025*/

   if (maxmotion-minmotion>motion_trig)                                 /*0027*/
      binvalue = maxmotion*motion_th/100;                               /*0028*/

   freia_aipo_threshold(mv, mv, binvalue, 255, true);                   /*0030*/
   
   // open
   freia_aipo_erode_8c(t1, mv, freia_morpho_kernel_8c);                 /*0033*/
   freia_aipo_dilate_8c(t1, t1, freia_morpho_kernel_8c);                /*0034*/
   
   // gradient
   freia_aipo_dilate_8c(t2, t1, freia_morpho_kernel_8c);                /*0037*/
   freia_aipo_erode_8c(mv, t1, freia_morpho_kernel_8c);                 /*0038*/
   freia_aipo_sub(mv, t2, mv);                                          /*0039*/

   freia_aipo_sub_const(mv, mv, 1);                                     /*0041*/
   freia_aipo_and_const(mv, mv, 1);                                     /*0042*/
   freia_aipo_mul(mv, stab, mv);                                        /*0043*/

   freia_common_destruct_data(t0);                                      /*0045*/
   freia_common_destruct_data(t1);                                      /*0046*/
   freia_common_destruct_data(t2);                                      /*0047*/
   return FREIA_OK;                                                     /*0048*/
}

