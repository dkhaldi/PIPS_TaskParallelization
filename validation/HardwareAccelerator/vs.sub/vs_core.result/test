#include "sigmacAgents.h"


// code module=vs_core fname=vs_core_sigmac_helper_0 split=0


/*#####################################################################*/
/*################ vs_core_sigmac_helper_0_0 Subgraph #################*/
/*#####################################################################*/

subgraph
vs_core_sigmac_helper_0_0
(char pipein[255],
 char pipeout[255],
 int width,
 int height,
 size_t sizeMax,
 char pipeparin[255],
 char pipeparout[255])
{

  map {
    agent lhin = new Launcher_In(pipein, 2, width, height);
    agent strin = new Streamer_In(width, height, 2);
    connect (lhin.param, strin.input);
    SigmaC_agent_setUnitType(strin,"k1-I/O");
    agent lhout = new Launcher_Out(pipeout, 2, width, height);
    agent strout = new Streamer_Out(width, height, 2);
    connect (strout.output, lhout.result);
    SigmaC_agent_setUnitType(strout,"k1-I/O");

    agent pp = new Params_Provider(pipeparin, 3);
    agent sp = new Split<int32_t>(3, 1);
    SigmaC_agent_setUnitType(sp,"k1-I/O");
    connect (pp.output, sp.input);

    agent mr = new Measures_Retriever(pipeparout, 2);
    agent jo = new Join<int32_t>(2, 1);
    SigmaC_agent_setUnitType(jo,"k1-I/O");
    connect (jo.output, mr.input);

    // parameters declaration...

    // instantiations...
    agent dup2 = new rep(2, width);
    agent dup1 = new rep(2, width);
    agent dup0 = new rep(3, width);
    agent absd0 = new img_absdiff_img_16(width);
    agent mulcst1 = new img_mul_const_16_dyn(width, height);
    agent mulcst0 = new img_mul_const_16_dyn(width, height);
    agent add0 = new img_add_img_16(width);
    agent divc0 = new img_div_const_16_dyn(width, height);
    agent max0 = new img_global_max(width, height);
    agent min0 = new img_global_min(width, height);

    // connections...
    connect (strin.output[1] /* in */, dup2.input);
    connect (dup2.output[1], mulcst1.input);
    connect (dup2.output[0], absd0.input[0]);
    connect (strin.output[0] /* bg */, dup1.input);
    connect (dup1.output[1], mulcst0.input);
    connect (dup1.output[0], absd0.input[1]);
    connect (absd0.output, dup0.input);
    connect (dup0.output[2], min0.input);
    connect (dup0.output[1], max0.input);
    connect (dup0.output[0], strout.input[0] /* mv_6 */);
    connect (mulcst1.output, add0.input[1]);
    connect (sp.output[2], mulcst1.param);
    connect (mulcst0.output, add0.input[0]);
    connect (sp.output[1], mulcst0.param);
    connect (add0.output, divc0.input);
    connect (divc0.output, strout.input[1] /* bg */);
    connect (sp.output[0], divc0.param);
    connect (max0.output, jo.input[1]);
    connect (min0.output, jo.input[0]);
  }
}

