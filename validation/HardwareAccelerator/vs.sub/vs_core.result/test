  "20 +" [shape=trapezium,orientation=270];
  "20 +" -> "21 /_" [label="bg"];
  "19 *_" [shape=trapezium,orientation=270];
  "19 *_" -> "20 +" [label="bg"];
  "18 *_" [shape=trapezium,orientation=270];
  "18 *_" -> "20 +" [label="t0"];
  "17 =" [shape=circle];
  "17 =" -> "18 *_" [label="t0"];
  "14 -|" [shape=trapezium,orientation=270];
  "14 -|" -> "30 thr" [label="mv"];
  "14 -|" -> "25 min" [label="mv"];
  "14 -|" -> "24 max" [label="mv"];
  "bg" -> "19 *_";
  "bg" -> "14 -|";
