   freia_aipo_addsat_const(o, o, 1);                                    /*0006*/
   freia_aipo_absdiff_const(o, o, 2);                                   /*0007*/
   freia_aipo_subsat_const(o, o, 3);                                    /*0008*/
   return FREIA_OK;                                                     /*0009*/
}

