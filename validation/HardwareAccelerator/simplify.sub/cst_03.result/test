! acceleration function

// FREIA OpenCL includes
#include <CL/opencl.h>
#include "freia.h"
#include "private/freia_opencl_runtime.h"


// dag 0: 4 ops and 0 copies, optimized to 4 ops and 0+0+0 copies

// hold kernels for cst_03_opencl_helper_0_0_0
static cl_kernel cst_03_opencl_helper_0_0_0_kernel[2];

// compile kernels for cst_03_opencl_helper_0_0_0
static freia_status cst_03_opencl_helper_0_0_0_compile(void)
{
  // OpenCL source for cst_03_opencl_helper_0_0_0
  const char * cst_03_opencl_helper_0_0_0_source =
    "#include <freia_opencl_runtime.hcl>\n"
    "\n"
    "// opencl function cst_03_opencl_helper_0_0_0\n"
    "KERNEL void cst_03_opencl_helper_0_0_0(\n"
    "  GLOBAL PIXEL * o0,\n"
    "  int ofs_o0,\n"
    "  int width, // of the working area, vs image pitch below\n"
    "  int height, // of the working area\n"
    "  int pitch,\n"
    "  int c0,\n"
    "  int c1,\n"
    "  int c2,\n"
    "  int c3)\n"
    "{\n"
    "  // no tiling on height dimension\n"
    "  // assert(height==get_global_size(0));\n"
    "  int j = get_global_id(0);\n"
    "\n"
    "  // loop i upper bound\n"
    "  int Wtile = (width+get_global_size(1)-1)/get_global_size(1);\n"
    "  int Wlast = Wtile*(get_global_id(1)+1);\n"
    "  if (Wlast>width) Wlast = width;\n"
    "\n"
    "  // get input & output image pointers\n"
    "  int shift = pitch*j;\n"
    "  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;\n"
    "\n"
    "  int i;\n"
    "  for (i=Wtile*get_global_id(1); i<Wlast; i++)\n"
    "  {\n"
    "\n"
    "    // pixel computations\n"
    "    PIXEL t5 = PIXEL_SET(c0);\n"
    "    PIXEL t6 = PIXEL_ADDSAT(t5, c1);\n"
    "    PIXEL t7 = PIXEL_ABSDIFF(t6, c2);\n"
    "    PIXEL t8 = PIXEL_SUBSAT(t7, c3);\n"
    "\n"
    "    // set output pixels\n"
    "    p0[i] = t8;\n"
    "  }\n"
    "}\n";
  freia_status err = FREIA_OK;
  err |= freia_op_compile_kernel(cst_03_opencl_helper_0_0_0_source, "cst_03_opencl_helper_0_0_0", "-DPIXEL8", &cst_03_opencl_helper_0_0_0_kernel[0]);
  err |= freia_op_compile_kernel(cst_03_opencl_helper_0_0_0_source, "cst_03_opencl_helper_0_0_0", "-DPIXEL16", &cst_03_opencl_helper_0_0_0_kernel[1]);
  return err;
}

// helper function cst_03_opencl_helper_0_0_0
freia_status cst_03_opencl_helper_0_0_0(
  freia_data2d * o0,
  int32_t c0,
  int32_t c1,
  int32_t c2,
  int32_t c3)
{
  freia_status err = FREIA_OK;

  // handle on the fly compilation...
  static int to_compile = 1;
  if (to_compile) {
    err |= cst_03_opencl_helper_0_0_0_compile();
    // compilation may have failed
    if (err) return err;
    to_compile = 0;
  }

  // now get kernel, which must be have be compiled
  uint32_t bpp = o0->bpp>>4;
  cl_kernel kernel = cst_03_opencl_helper_0_0_0_kernel[bpp];

  // call kernel cst_03_opencl_helper_0_0_0
  err |= freia_op_call_kernel(kernel, 1, 0, 4, 0, o0, c0, c1, c2, c3);

  return err;
}
#include <freia_opencl_runtime.hcl>
// generated OpenCL kernels for function cst_03

// opencl for dag 0

// opencl function cst_03_opencl_helper_0_0_0
KERNEL void cst_03_opencl_helper_0_0_0(
  GLOBAL PIXEL * o0,
  int ofs_o0,
  int width, // of the working area, vs image pitch below
  int height, // of the working area
  int pitch,
  int c0,
  int c1,
  int c2,
  int c3)
{
  // no tiling on height dimension
  // assert(height==get_global_size(0));
  int j = get_global_id(0);

  // loop i upper bound
  int Wtile = (width+get_global_size(1)-1)/get_global_size(1);
  int Wlast = Wtile*(get_global_id(1)+1);
  if (Wlast>width) Wlast = width;

  // get input & output image pointers
  int shift = pitch*j;
  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;

  int i;
  for (i=Wtile*get_global_id(1); i<Wlast; i++)
  {

    // pixel computations
    PIXEL t5 = PIXEL_SET(c0);
    PIXEL t6 = PIXEL_ADDSAT(t5, c1);
    PIXEL t7 = PIXEL_ABSDIFF(t6, c2);
    PIXEL t8 = PIXEL_SUBSAT(t7, c3);

    // set output pixels
    p0[i] = t8;
  }
}

