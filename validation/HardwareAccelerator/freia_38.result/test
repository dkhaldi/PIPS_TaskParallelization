   // preschedule example for terapix tests
   
   freia_aipo_erode_8c(e, i, kern);                                     /*0010*/
   freia_aipo_dilate_8c(d, i, kern);                                    /*0011*/

   freia_aipo_erode_8c(e, e, kern);                                     /*0013*/
