// OMP style
      PROGRAM PARALLEL_DO

      INTEGER N, I
      PARAMETER (N=100)
      REAL A(N)


!$OMP PARALLEL DO 
      DO I = 1, N
         A(I) = I*1.0
      ENDDO
      END
      PROGRAM PARALLEL_DO

      INTEGER N, I
      PARAMETER (N=100)
      REAL A(N)

!$OMP PARALLEL DO 
      DO I = 1, N
         A(I) = I*1.0
      ENDDO
      END
