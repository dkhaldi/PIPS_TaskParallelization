#
# Code after openmp for module fct_lib
#
void fct_lib(const long int size, t_real x[size])
{
   long int i;
   
   // compare loop
   for(i = 1; i <= size-1-1; i += 1)
      x[i] = x[i-1]+x[i]+x[i+1];
   return;
}
void fct_lib(const long int size, t_real x[size])
{
   long int i;
   
   // compare loop
   for(i = 1; i <= size-1-1; i += 1)
      x[i] = x[i-1]+x[i]+x[i+1];
   return;
