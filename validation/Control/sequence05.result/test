
Parsed printed file

void sequence05()
{
   int i = 4;

   if (i%2==0)
      goto l100;
   i = i+10;
l100:   i = i+20;
   {
      int j = 3;
      i += j;
      goto here;
here:      ;
   }
   i += 30;
   return;
}

Printed file


Output of controlizer

void sequence05()
{
   int i = 4;

   if (i%2==0) goto l100;
   i = i+10;
l100:   ;
   i = i+20;
   {
      int j = 3;
      i += j;
here:      ;
   }
   i += 30;
   return;
}

Output of control restructurer

void sequence05()
{
   int i = 4;

   if (i%2==0)
      ;
   else
      i = i+10;
l100:   ;
   i = i+20;
   {
      int j = 3;
      i += j;
here:      ;
   }
   i += 30;
   return;
}
