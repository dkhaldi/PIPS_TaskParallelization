Try to restructure the code for more precision:
// (0,1)
// BEGIN BLOCK
{{
// (0,2)
   extern int main(int argc, char *argv[]);                             /*0001*/
}}
// END BLOCK
int main(int argc, char *argv[])
{
// (0,1)
   // BEGIN BLOCK
{{
   {
// (0,2)
      // BEGIN BLOCK
   {{
// (0,3)
      int i;                                                            /*0004*/
// (0,4)
      i = 3;                                                            /*0005*/
   }}
      // END BLOCK
   }
   {
// (0,5)
      // BEGIN BLOCK
   {{
// (0,6)
      int i;                                                            /*0008*/
// (0,7)
      i = 4;                                                            /*0009*/
   }}
      // END BLOCK
   }
// (0,8)
   return 0;                                                            /*0012*/
}}
   // END BLOCK
}
