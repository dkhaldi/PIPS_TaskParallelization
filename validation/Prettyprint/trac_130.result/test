
Proper effects for "main"

int main()
{
   float a[10];                                                         /*0004*/
   float b[10];                                                         /*0005*/
   int i, j;                                                            /*0006*/
//             <    is written>: x
   float x = 2.12;                                                      /*0007*/
//             <    is written>: z
   float z = 3.0;                                                       /*0008*/
//             <    is read   >: x z
//             <    is written>: x
   //float y;
   
   x += z;                                                              /*0011*/
//             <    is written>: i

   for(i = 0; i <= 9; i += 1) {                                         /*0013*/
//             <    is written>: y
      float y = 2.0;                                                    /*0014*/
//             <    is read   >: i x y
//             <    is written>: a[i]
      a[i] = x*y;                                                       /*0015*/
//             <    is read   >: i y z
//             <    is written>: b[i]
      b[i] = z*y;                                                       /*0016*/
   }
}

Privatization


Preconditions for "main" after privatization


//  P() {}

int main()
{

//  P() {}

   float a[10];                                                         /*0004*/

//  P() {}

   float b[10];                                                         /*0005*/

//  P() {}

   int i, j;                                                            /*0006*/

//  P() {}

   float x = 2.12;                                                      /*0007*/

//  P() {}

   float z = 3.0;                                                       /*0008*/

//  P() {}

   //float y;
   
   x += z;                                                              /*0011*/

//  P() {}


   for(i = 0; i <= 9; i += 1) {                                         /*0013*/

//  P(i) {0<=i, i<=9}

      float y = 2.0;                                                    /*0014*/

//  P(i) {0<=i, i<=9}

      a[i] = x*y;                                                       /*0015*/

//  P(i) {0<=i, i<=9}

      b[i] = z*y;                                                       /*0016*/
   }
}

Dependence graph for "main"


 **************** Effective Dependence Graph ****************

	14 --> 16 with conflicts
		from <y-W-EXACT> to <y-R-EXACT> at levels  2
	14 --> 16 with conflicts
		from <y-WE-EXACT> to <y-RE-EXACT> at levels  2
	14 --> 15 with conflicts
		from <y-W-EXACT> to <y-R-EXACT> at levels  2
	14 --> 15 with conflicts
		from <y-WE-EXACT> to <y-RE-EXACT> at levels  2

****************** End of Dependence Graph ******************

Parallelization of "main" after privatization

int main()
{
   float a[10];                                                         /*0004*/
   float b[10];                                                         /*0005*/
   int i, j;                                                            /*0006*/
   float x = 2.12;                                                      /*0007*/
   float z = 3.0;                                                       /*0008*/
   //float y;
   
   x += z;                                                              /*0011*/

#pragma omp parallel for 
   for(i = 0; i <= 9; i += 1) {                                         /*0013*/
      float y = 2.0;                                                    /*0014*/
      a[i] = x*y;                                                       /*0015*/
      b[i] = z*y;                                                       /*0016*/
   }
}
