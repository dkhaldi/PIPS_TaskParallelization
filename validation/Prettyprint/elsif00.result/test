int elsif00(int i)
{
   // direct
   if (i==1)                                                            /*0004*/
      i++;                                                              /*0005*/
   else if (i==2)                                                       /*0004*/
      i--;                                                              /*0007*/
   else
      i -= 3;                                                           /*0009*/
   // blocks
   if (i==4)                                                            /*0011*/
      i++;                                                              /*0012*/
   else if (i==5)                                                       /*0011*/
      i--;                                                              /*0014*/
   else
      i -= 6;                                                           /*0016*/
   // sub blocks
   if (i==2)                                                            /*0019*/
      i++;                                                              /*0020*/
   else if (i==5)                                                       /*0019*/
      i--;                                                              /*0023*/
   else
      i -= 3;                                                           /*0025*/
   // block sub blocks
   if (i==2)                                                            /*0028*/
      i++;                                                              /*0029*/
   else if (i==5)                                                       /*0028*/
      i--;                                                              /*0032*/
   else
      i -= 3;                                                           /*0034*/
}
