Transformers for "main"

//  T() {}
void func1(int n, int m, float a[n][m], float b[n][m], float h)
{
//  T() {}
   float x;
//  T() {}
   int i, j;
//  T(i,j,x) {1<=i, i<=n}
   for(i = 1; i <= n; i += 1)
//  T(j,x) {1<=i, i<=n, 1<=j, j<=m}
      for(j = 1; j <= m; j += 1) {
//  T(x) {1<=i, i<=n, 1<=j, j<=m}
         x = i*h+j;
//  T() {1<=i, i<=n, 1<=j, j<=m}
         a[i][j] = b[i][j]*x;
      }
}
//  T() {}
int main()
{
//  T() {}
   float a[10][10], b[10][10], h;
//  T() {}
   int i, j;
//  T(i,j) {1<=i, i<=10}
   for(i = 1; i <= 10; i += 1)
//  T(j) {1<=i, i<=10, 1<=j, j<=10}
      for(j = 1; j <= 10; j += 1)
//  T() {1<=i, i<=10, 1<=j, j<=10}
         b[i][j] = 1.0;
//  T(h) {h==2., i==11, j==11}
   h = 2.0;
//  T() {h==2., i==11, j==11}
   func1(10, 10, a, b, h);
//  T(i,j) {h==2., i#init==11, j#init==11, 1<=i, i<=10}
   for(i = 1; i <= 10; i += 1)
//  T(j) {h==2., 1<=i, i<=10, 1<=j, j<=10}
      for(j = 1; j <= 10; j += 1)
//  T() {h==2., 1<=i, i<=10, 1<=j, j<=10}
         fprintf(stderr, "a[%d] = %f \n", i, a[i][j]);
}

Preconditions for "main"

//  P() {}
int main()
{
//  P() {}
   float a[10][10], b[10][10], h;
//  P() {}
   int i, j;
//  P() {}
   for(i = 1; i <= 10; i += 1)
//  P(i,j) {1<=i, i<=10}
      for(j = 1; j <= 10; j += 1)
//  P(i,j) {1<=i, i<=10, 1<=j, j<=10}
         b[i][j] = 1.0;
//  P(i,j) {i==11, j==11}
   h = 2.0;
//  P(h,i,j) {h==2., i==11, j==11}
   func1(10, 10, a, b, h);
//  P(h,i,j) {h==2., i==11, j==11}
   for(i = 1; i <= 10; i += 1)
//  P(h,i,j) {h==2., 1<=i, i<=10}
      for(j = 1; j <= 10; j += 1)
//  P(h,i,j) {h==2., 1<=i, i<=10, 1<=j, j<=10}
         fprintf(stderr, "a[%d] = %f \n", i, a[i][j]);
}
//  P() {h==2., m==10, n==10}
void func1(int n, int m, float a[n][m], float b[n][m], float h)
{
//  P() {h==2., m==10, n==10}
   float x;
//  P() {h==2., m==10, n==10}
   int i, j;
//  P() {h==2., m==10, n==10}
   for(i = 1; i <= n; i += 1)
//  P(i,j,x) {h==2., m==10, n==10, 1<=i, i<=10}
      for(j = 1; j <= m; j += 1) {
//  P(i,j,x) {h==2., m==10, n==10, 1<=i, i<=10, 1<=j, j<=10}
         x = i*h+j;
//  P(i,j,x) {h==2., m==10, n==10, 1<=i, i<=10, 1<=j, j<=10}
         a[i][j] = b[i][j]*x;
      }
}

Printed files with regions


//  <a[PHI1][PHI2]-W-EXACT-{1<=PHI1, PHI1<=10, 1<=PHI2, PHI2<=10,
//    h==2., m==10, n==10}>
//  <b[PHI1][PHI2]-R-EXACT-{1<=PHI1, PHI1<=10, 1<=PHI2, PHI2<=10,
//    h==2., m==10, n==10}>

void func1(int n, int m, float a[n][m], float b[n][m], float h)
{
   float x;
   int i, j;

//  <a[PHI1][PHI2]-W-EXACT-{1<=PHI1, PHI1<=10, 1<=PHI2, PHI2<=10,
//    h==2., m==10, n==10}>
//  <b[PHI1][PHI2]-R-EXACT-{1<=PHI1, PHI1<=10, 1<=PHI2, PHI2<=10,
//    h==2., m==10, n==10}>

   for(i = 1; i <= n; i += 1)

//  <a[PHI1][PHI2]-W-EXACT-{PHI1==i, 1<=PHI2, PHI2<=10, h==2., m==10,
//    n==10, 1<=i, i<=10}>
//  <b[PHI1][PHI2]-R-EXACT-{PHI1==i, 1<=PHI2, PHI2<=10, h==2., m==10,
//    n==10, 1<=i, i<=10}>

      for(j = 1; j <= m; j += 1) {
         x = i*h+j;

//  <a[PHI1][PHI2]-W-EXACT-{PHI1==i, PHI2==j, h==2., m==10, n==10,
//    1<=i, i<=10, 1<=j, j<=10}>
//  <b[PHI1][PHI2]-R-EXACT-{PHI1==i, PHI2==j, h==2., m==10, n==10,
//    1<=i, i<=10, 1<=j, j<=10}>

         a[i][j] = b[i][j]*x;
      }
}

//  <b[PHI1][PHI2]-IN-EXACT-{1<=PHI1, PHI1<=10, 1<=PHI2, PHI2<=10,
//    h==2., m==10, n==10}>

void func1(int n, int m, float a[n][m], float b[n][m], float h)
{
   float x;
   int i, j;

//  <b[PHI1][PHI2]-IN-EXACT-{1<=PHI1, PHI1<=10, 1<=PHI2, PHI2<=10,
//    h==2., m==10, n==10}>

   for(i = 1; i <= n; i += 1)

//  <b[PHI1][PHI2]-IN-EXACT-{PHI1==i, 1<=PHI2, PHI2<=10, h==2., m==10,
//    n==10, 1<=i, i<=10}>

      for(j = 1; j <= m; j += 1) {
         x = i*h+j;

//  <b[PHI1][PHI2]-IN-EXACT-{PHI1==i, PHI2==j, h==2., m==10, n==10,
//    1<=i, i<=10, 1<=j, j<=10}>

         a[i][j] = b[i][j]*x;
      }
}

//  <stderr-IN-EXACT-{}>
//  <stderr[PHI1]-IN-MAY-{PHI1==0}>
//  <__IO_EFFECTS:LUNS[PHI1]-IN-EXACT-{PHI1==2}>

int main()
{
   float a[10][10], b[10][10], h;
   int i, j;
   for(i = 1; i <= 10; i += 1)
      for(j = 1; j <= 10; j += 1)
         b[i][j] = 1.0;
   h = 2.0;

//  <b[PHI1][PHI2]-IN-EXACT-{1<=PHI1, PHI1<=10, 1<=PHI2, PHI2<=10,
//    h==2., i==11, j==11}>

   func1(10, 10, a, b, h);

//  <a[PHI1][PHI2]-IN-EXACT-{1<=PHI1, PHI1<=10, 1<=PHI2, PHI2<=10,
//    h==2., j==11}>
//  <stderr-IN-EXACT-{}>
//  <stderr[PHI1]-IN-MAY-{PHI1==0, h==2., j==11}>
//  <__IO_EFFECTS:LUNS[PHI1]-IN-EXACT-{PHI1==2, h==2., j==11}>

   for(i = 1; i <= 10; i += 1)

//  <a[PHI1][PHI2]-IN-EXACT-{PHI1==i, 1<=PHI2, PHI2<=10, h==2., 1<=i,
//    i<=10}>
//  <stderr-IN-EXACT-{}>
//  <stderr[PHI1]-IN-MAY-{PHI1==0}>
//  <__IO_EFFECTS:LUNS[PHI1]-IN-EXACT-{PHI1==2, h==2., 1<=i, i<=10}>

      for(j = 1; j <= 10; j += 1)

//  <a[PHI1][PHI2]-IN-EXACT-{PHI1==i, PHI2==j, h==2., 1<=i, i<=10,
//    1<=j, j<=10}>
//  <stderr-IN-EXACT-{}>
//  <stderr[PHI1]-IN-MAY-{PHI1==0}>
//  <__IO_EFFECTS:LUNS[PHI1]-IN-EXACT-{PHI1==2, h==2., 1<=i, i<=10,
//    1<=j, j<=10}>

         fprintf(stderr, "a[%d] = %f \n", i, a[i][j]);
}

//  <a[PHI1][PHI2]-OUT-EXACT-{1<=PHI1, PHI1<=10, 1<=PHI2, PHI2<=10,
//    h==2., m==10, n==10}>

void func1(int n, int m, float a[n][m], float b[n][m], float h)
{
   float x;
   int i, j;

//  <a[PHI1][PHI2]-OUT-EXACT-{1<=PHI1, PHI1<=10, 1<=PHI2, PHI2<=10,
//    h==2., m==10, n==10}>

   for(i = 1; i <= n; i += 1)

//  <a[PHI1][PHI2]-OUT-EXACT-{PHI1==i, 1<=PHI2, PHI2<=10, h==2.,
//    m==10, n==10, 1<=i, i<=10}>

      for(j = 1; j <= m; j += 1) {
         x = i*h+j;

//  <a[PHI1][PHI2]-OUT-EXACT-{PHI1==i, PHI2==j, h==2., m==10, n==10,
//    1<=i, i<=10, 1<=j, j<=10}>

         a[i][j] = b[i][j]*x;
      }
}
