
Parsed printed file for "func1"

void func1(int n, int m, float a[n][m], float b[n][m], float h)
{
   float x;
   int i, j;
   for (i = 1;i<=n;i++)
      for (j = 1;j<=m;j++) {
         x = i*h+j;
         a[i][j] = b[i][j]*x;
      }
}

Proper effetcs for "func1"

void func1(int n, int m, float a[n][m], float b[n][m], float h)
{
   float x;
   int i, j;
//             <    is read   >: n
//             <    is written>: i
   for(i = 1; i <= n; i += 1)
//             <    is read   >: m n
//             <    is written>: j
      for(j = 1; j <= m; j += 1) {
//             <    is read   >: h i j m n
//             <    is written>: x
         x = i*h+j;
//             <    is read   >: b[i][j] i j m n x
//             <    is written>: a[i][j]
         a[i][j] = b[i][j]*x;
      }
}

Cumulated effetcs for "func1"

//             <may be read   >: b[*][*] h
//             <may be written>: a[*][*]
//             <    is read   >: m n
void func1(int n, int m, float a[n][m], float b[n][m], float h)
{
   float x;
   int i, j;
//             <may be read   >: b[*][*] h i j m x
//             <may be written>: a[*][*] j x
//             <    is read   >: n
//             <    is written>: i
   for(i = 1; i <= n; i += 1)
//             <may be read   >: b[*][*] h i j x
//             <may be written>: a[*][*] x
//             <    is read   >: m n
//             <    is written>: j
      for(j = 1; j <= m; j += 1) {
//             <    is read   >: h i j m n
//             <    is written>: x
         x = i*h+j;
//             <may be read   >: b[*][*]
//             <may be written>: a[*][*]
//             <    is read   >: i j m n x
         a[i][j] = b[i][j]*x;
      }
}

Proper effetcs for "main"

int main()
{
   float a[10][10], b[10][10], h;
   int i, j;
//             <    is written>: i
   for(i = 1; i <= 10; i += 1)
//             <    is written>: j
      for(j = 1; j <= 10; j += 1)
//             <    is read   >: i j
//             <    is written>: b[i][j]
         b[i][j] = 1.0;
//             <    is written>: h
   h = 2.0;
//             <may be read   >: b[*][*]
//             <may be written>: a[*][*]
//             <    is read   >: h
   func1(10, 10, a, b, h);
//             <    is written>: i
   for(i = 1; i <= 10; i += 1)
//             <    is written>: j
      for(j = 1; j <= 10; j += 1)
//             <may be read   >: stderr[0]
//             <may be written>: stderr[0]
//             <    is read   >: a[i][j] i j stderr 
//                               __IO_EFFECTS:LUNS[2]
//             <    is written>: __IO_EFFECTS:LUNS[2]
         fprintf(stderr, "a[%d] = %f \n", i, a[i][j]);
}

