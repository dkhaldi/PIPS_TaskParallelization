}
//                                                     17*m.n + 3*n + 2 (SUMMARY)
void func1(int n, int m, float a[n][m], float b[n][m], float h)
{
//                                                                    0 (STMT)
   float x;
//                                                                    0 (STMT)
   int i, j;
//                                                     17*m.n + 3*n + 2 (DO)
   for(i = 1; i <= n; i += 1)
//                                                             17*m + 3 (DO)
      for(j = 1; j <= m; j += 1) {
//                                                                    6 (STMT)
         x = i*h+j;
//                                                                   10 (STMT)
         a[i][j] = b[i][j]*x;
      }
}
