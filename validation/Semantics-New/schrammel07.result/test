
TRANSFORMERS


//  T() {}

int main()
{

//  T(j) {j==2}

   int i, j = 2, k;

//  T(j,k) {j#init==2, k<=k#init}


   while (i*i>1)

//  T(j,k) {3<=k, k<=k#init, 3<=k#init}

      while (k>=3) {

//  T(j) {j==3, 3<=k}

         j = 3;

//  T(k) {j==3, k==k#init-1, 2<=k}

         k--;
      }

//  T(j) {j==j#init, 0<=i+1, i<=1}

   j = j;
}

PRECONDITIONS


//  P() {}

int main()
{

//  P() {}

   int i, j = 2, k;

//  P(j) {j==2}


   while (i*i>1)

//  P(j,k) {2<=j, j<=3}

      while (k>=3) {

//  P(j,k) {2<=j, j<=3, 3<=k}

         j = 3;

//  P(j,k) {j==3, 3<=k}

         k--;
      }

//  P(j,k) {0<=i+1, i<=1, 2<=j, j<=3}

   j = j;
}
