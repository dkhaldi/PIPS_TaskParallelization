
Source code with transformers


//  T() {}

int main()
{

//  T(i) {i==0}

   int i = 0;

//  T() {i==0}

   int a[20];

//  T() {i==0}

   int n;

//  T() {i==0}

   int m;

//  T(k) {i==0, k<=m, k<=n}

   int k = m<=n?m:n;

//  T(n) {i==0, k<=m, k<=n#init}


   scanf("%d", &n);

//  T(m) {i==0, k<=m#init}

   scanf("%d", &m);

//  T(i) {i#init==0, 1<=i, i<=m+1, i<=n+1, 0<=m, 0<=n}


   while (i++<=(n<=m?n:m))

//  T() {i<=m+1, i<=n+1, 0<=m, 0<=n}

      a[i] = i;

//  T() {1<=i}

   printf("%d\n", i);
}

Source code with preconditions


//  P() {}

int main()
{

//  P() {}

   int i = 0;

//  P(i) {i==0}

   int a[20];

//  P(i) {i==0}

   int n;

//  P(i) {i==0}

   int m;

//  P(i) {i==0}

   int k = m<=n?m:n;

//  P(i,k) {i==0, k<=m, k<=n}


   scanf("%d", &n);

//  P(i,k,n) {i==0, k<=m}

   scanf("%d", &m);

//  P(i,k,m,n) {i==0}


   while (i++<=(n<=m?n:m))

//  P(i,k,m,n) {1<=i, i<=m+1, i<=n+1}

      a[i] = i;

//  P(i,k,m,n) {1<=i}

   printf("%d\n", i);
}
