Proper effects with points-to for "main"

int main(int argc, char *argv[])
{
//             <    is referenced>: content
//             <    is declared>: a
   content a;
//             <    is declared>: b
   int b[100];
//             <    is declared>: c
   int c[100];
//             <    is written>: a.var1
//             <    is referenced>: a

   update1(&a);
//             <may be written>: b[*]
//             <    is read   >: a.var1
//             <    is referenced>: a b
   compute1(&a, b);
//             <    is written>: a.s[1].var2
//             <    is referenced>: a
   update2(&a);
//             <may be read   >: b[*]
//             <may be written>: c[*]
//             <    is read   >: a.s[1].var2
//             <    is referenced>: a b c
   compute2(&a, b, c);

   return 0;
}

