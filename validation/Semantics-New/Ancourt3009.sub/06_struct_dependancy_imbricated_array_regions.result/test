Regions with point-to for "compute2"


//  <_a_1[PHI1].s[PHI3].var2-R-EXACT-{PHI1==0, PHI3==1,
//    _a_1[0][s][1][var2]==9}>
//  <a-R-EXACT-{}>
//  <b[PHI1]-R-EXACT-{0<=PHI1, PHI1<=99, _a_1[0][s][1][var2]==9}>
//  <c[PHI1]-W-EXACT-{0<=PHI1, PHI1<=99, _a_1[0][s][1][var2]==9}>

void compute2(content *a, int b[100], int c[100])
{
#pragma MOTIF
   {
      int i;

//  <_a_1[PHI1].s[PHI3].var2-R-EXACT-{PHI1==0, PHI3==1,
//    _a_1[0][s][1][var2]==9}>
//  <a-R-EXACT-{}>
//  <a-RE-EXACT-{}>
//  <b-RE-EXACT-{}>
//  <b[PHI1]-R-EXACT-{0<=PHI1, PHI1<=99, _a_1[0][s][1][var2]==9}>
//  <c-RE-EXACT-{}>
//  <c[PHI1]-W-EXACT-{0<=PHI1, PHI1<=99, _a_1[0][s][1][var2]==9}>

      for(i = 0; i <= 99; i += 1)

//  <_a_1[PHI1].s[PHI3].var2-R-EXACT-{PHI1==0, PHI3==1,
//    _a_1[0][s][1][var2]==9, 0<=i, i<=99}>
//  <a-R-EXACT-{}>
//  <a-RE-EXACT-{}>
//  <b-RE-EXACT-{}>
//  <b[PHI1]-R-EXACT-{PHI1==i, _a_1[0][s][1][var2]==9, 0<=i, i<=99}>
//  <c-RE-EXACT-{}>
//  <c[PHI1]-W-EXACT-{PHI1==i, _a_1[0][s][1][var2]==9, 0<=i, i<=99}>

         c[i] = b[i]+(a->s)[1].var2;
   }
   return;
}

Regions with point-to for "main"

int main(int argc, char *argv[])
{

//  <a-WE-EXACT-{}>
//  <content-RT-EXACT-{}>

   content a;

//  <b-WE-EXACT-{}>

   int b[100];

//  <c-WE-EXACT-{}>

   int c[100];

//  <a-RE-EXACT-{}>
//  <a.var1-W-EXACT-{}>


   update1(&a);

//  <a-RE-EXACT-{}>
//  <a.var1-R-EXACT-{a[var1]==8}>
//  <b-RE-EXACT-{}>
//  <b[PHI1]-W-EXACT-{0<=PHI1, PHI1<=99, a[var1]==8}>

   compute1(&a, b);

//  <a-RE-EXACT-{}>
//  <a.s[PHI2].var2-W-EXACT-{PHI2==1}>

   update2(&a);

//  <a-RE-EXACT-{}>
//  <a.s[PHI2].var2-R-EXACT-{PHI2==1,
//    compute2:_a_1[0][s][1][var2]==9}>
//  <b-RE-EXACT-{}>
//  <b[PHI1]-R-EXACT-{0<=PHI1, PHI1<=99,
//    compute2:_a_1[0][s][1][var2]==9, a[s][1][var2]==9, a[var1]==8}>
//  <c-RE-EXACT-{}>
//  <c[PHI1]-W-EXACT-{0<=PHI1, PHI1<=99,
//    compute2:_a_1[0][s][1][var2]==9, a[s][1][var2]==9, a[var1]==8}>

   compute2(&a, b, c);

   return 0;
}
