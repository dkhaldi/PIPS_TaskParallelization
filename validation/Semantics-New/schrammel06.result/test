
TRANSFORMERS


//  T() {}

int main()
{

//  T(j) {j==2}

   int i, j = 2, k;

//  T(j) {j#init==2}


   while (i*i>1)

//  T(j) {}

      if (k>=3)

//  T(j) {j==3, 3<=k}

         j = 3;

//  T(j) {j==j#init, 0<=i+1, i<=1}

   j = j;
}

PRECONDITIONS


//  P() {}

int main()
{

//  P() {}

   int i, j = 2, k;

//  P(j) {j==2}


   while (i*i>1)

//  P(j) {2<=j, j<=3}

      if (k>=3)

//  P(j) {2<=j, j<=3, 3<=k}

         j = 3;

//  P(j) {0<=i+1, i<=1, 2<=j, j<=3}

   j = j;
}
