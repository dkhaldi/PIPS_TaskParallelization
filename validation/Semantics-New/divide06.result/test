
Transformers for divide06


//  T(divide06) {0<=divide06+200, divide06<=200}

int divide06(int v1, int v2)
{

//  T() {}

   int v;

//  T(v1) {}

   scanf("%d", &v1);

//  T(v2) {}

   scanf("%d", &v2);

//  T() {0<=v1+100, v1<=200, 0<=v2+3, v2<=2}

   if (!(-100<=v1&&v1<=200&&-3<=v2&&v2<=2))

//  T() {0==-1}

      abort();

//  T(v) {0<=v+200, v<=200, 0<=v1+100, v1<=200, 0<=v2+3, v2<=2}

   v = v1/v2;

//  T() {0<=v+200, v<=200, 0<=v1+100, v1<=200, 0<=v2+3, v2<=2}

   printf("%d\n", v);

//  T(divide06) {divide06==v, 0<=divide06+200, divide06<=200,
//    0<=v1+100, v1<=200, 0<=v2+3, v2<=2}

   return v;
}

Preconditions for divide06


//  P() {}

int divide06(int v1, int v2)
{

//  P() {}

   int v;

//  P() {}

   scanf("%d", &v1);

//  P(v1) {}

   scanf("%d", &v2);

//  P(v1,v2) {}

   if (!(-100<=v1&&v1<=200&&-3<=v2&&v2<=2))

//  P(v1,v2) {}

      abort();

//  P(v1,v2) {0<=v1+100, v1<=200, 0<=v2+3, v2<=2}

   v = v1/v2;

//  P(v,v1,v2) {0<=v+200, v<=200, 0<=v1+100, v1<=200, 0<=v2+3, v2<=2}

   printf("%d\n", v);

//  P(v,v1,v2) {0<=v+200, v<=200, 0<=v1+100, v1<=200, 0<=v2+3, v2<=2}

   return v;
}
