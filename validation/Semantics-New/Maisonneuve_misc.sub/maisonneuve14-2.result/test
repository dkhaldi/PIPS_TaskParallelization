
TRANSFORMERS


//  T() {0==-1}

void run(void)
{


//  T(n) {n==0}

   int n = 0;

//  T(n) {n==0, n#init==0}


   while (1) {

//  T(n) {n==2, n#init==0}

      if (n==0) {

//  T(n) {n==2, n#init==0}

         n = 2;

//  T() {n==2}

         if (n==1)

//  T() {0==-1}

            checking_error();
      }
      else

//  T() {}

         while (1)

//  T() {}

            ;

//  T(n) {n==0, n#init==2}

      if (n==2) {

//  T(n) {n==0, n#init==2}

         n = 0;

//  T() {n==0}

         if (n==1)

//  T() {0==-1}

            checking_error();
      }
      else

//  T() {0==-1}

         while (1)

//  T() {0==-1}

            ;
   }
}

PRECONDITIONS


//  P() {}

void run(void)
{


//  P() {}

   int n = 0;

//  P(n) {n==0}


   while (1) {

//  P(n) {n==0}

      if (n==0) {

//  P(n) {n==0}

         n = 2;

//  P(n) {n==2}

         if (n==1)

//  P() {0==-1}

            checking_error();
      }
      else

//  P() {0==-1}

         while (1)

//  P() {0==-1}

            ;

//  P(n) {n==2}

      if (n==2) {

//  P(n) {n==2}

         n = 0;

//  P(n) {n==0}

         if (n==1)

//  P() {0==-1}

            checking_error();
      }
      else

//  P() {0==-1}

         while (1)

//  P() {0==-1}

            ;
   }
}
