
TRANSFORMERS


//  T() {0==-1}

void run(void)
{


//  T() {}

   int x, y;

//  T(x,y) {x==0, y==0}

   x = y = 0;

//  T() {x==0, y==0}

   if (x>=3&&y>=3)

//  T() {0==-1}

      checking_error();

//  T(x,y) {x#init==0, y#init==0, 0<=x, y<=x+1, 0<=y, y<=2}

   while (1)

//  T(x,y) {x#init+2y<=x+3, x#init+y#init+1<=x+y,
//    x#init+2y<=x+2y#init+2, x#init+4y<=x+2y#init+5, y<=2, y#init<=y}

      if (flip())

//  T(x) {x==x#init+1, y<=2}

         if (y<=2) {

//  T(x) {x==x#init+1, y<=2}

            x++;

//  T() {y<=2}

            if (x>=3&&y>=3)

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {3<=y}

            while (1)

//  T() {3<=y}

               ;
      else if (flip())

//  T(y) {y==y#init+1, x<=2, 2y<=x+1}

         if (x<=2&&2*y<x) {

//  T(y) {y==y#init+1, x<=2, 2y<=x+1}

            y++;

//  T() {x<=2, 2y<=x+1}

            if (x>=3&&y>=3)

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {}

            while (1)

//  T() {}

               ;
      else {

//  T(y) {y==y#init+1, x<=2, x+2<=2y}

         if (x<=2&&2*y>=x) {

//  T(y) {y==y#init+1, x<=2, x+2<=2y}

            y++;

//  T() {x<=2, x+2<=2y}

            if (x>=3&&y>=3)

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {}

            while (1)

//  T() {}

               ;

//  T(x,y) {x<=2, x#init<=x, y#init<=y}

         while (flip())

//  T(x,y) {x+y==x#init+y#init+1, x<=2, x#init<=x, x<=x#init+1}

            if (flip())

//  T(x) {x==x#init+1, x+2<=2y, y<=2}

               if (y<=2&&2*y>=x+3) {

//  T(x) {x==x#init+1, x+2<=2y, y<=2}

                  x++;

//  T() {x+2<=2y, y<=2}

                  if (x>=3&&y>=3)

//  T() {0==-1}

                     checking_error();
               }
               else

//  T() {}

                  while (1)

//  T() {}

                     ;
            else if (x<=2) {

//  T(y) {y==y#init+1, x<=2}

               y++;

//  T() {x<=2}

               if (x>=3&&y>=3)

//  T() {0==-1}

                  checking_error();
            }
            else

//  T() {3<=x}

               while (1)

//  T() {3<=x}

                  ;

//  T(x) {x==x#init+1, 2y<=x+1, y<=2}

         if (y<=2&&2*y<x+3) {

//  T(x) {x==x#init+1, 2y<=x+1, y<=2}

            x++;

//  T() {2y<=x+1, y<=2}

            if (x>=3&&y>=3)

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {}

            while (1)

//  T() {}

               ;
      }
}

PRECONDITIONS


//  P() {}

void run(void)
{


//  P() {}

   int x, y;

//  P() {}

   x = y = 0;

//  P(x,y) {x==0, y==0}

   if (x>=3&&y>=3)

//  P() {0==-1}

      checking_error();

//  P(x,y) {x==0, y==0}

   while (1)

//  P(x,y) {0<=x, y<=x+1, 0<=y, y<=2}

      if (flip())

//  P(x,y) {0<=x, y<=x+1, 0<=y, y<=2}

         if (y<=2) {

//  P(x,y) {0<=x, y<=x+1, 0<=y, y<=2}

            x++;

//  P(x,y) {1<=x, y<=x, 0<=y, y<=2}

            if (x>=3&&y>=3)

//  P() {0==-1}

               checking_error();
         }
         else

//  P() {0==-1}

            while (1)

//  P() {0==-1}

               ;
      else if (flip())

//  P(x,y) {0<=x, y<=x+1, 0<=y, y<=2}

         if (x<=2&&2*y<x) {

//  P(x,y) {0<=x, x<=2, y<=x+1, 2y+1<=x, 0<=y, y<=2}

            y++;

//  P(x,y) {0<=x, x<=2, y<=x+2, 2y<=x+1, 1<=y, y<=3}

            if (x>=3&&y>=3)

//  P() {0==-1}

               checking_error();
         }
         else

//  P(x,y) {0<=x, y<=x+1, 0<=y, y<=2}

            while (1)

//  P(x,y) {0<=x, y<=x+1, 0<=y, y<=2}

               ;
      else {

//  P(x,y) {0<=x, y<=x+1, 0<=y, y<=2}

         if (x<=2&&2*y>=x) {

//  P(x,y) {0<=x, x<=2, x<=2y, y<=x+1, 0<=y, y<=2}

            y++;

//  P(x,y) {0<=x, x<=2, y<=x+2, x+2<=2y, 1<=y, y<=3}

            if (x>=3&&y>=3)

//  P() {0==-1}

               checking_error();
         }
         else

//  P(x,y) {0<=x, y+1<=x, 0<=y, y<=2}

            while (1)

//  P(x,y) {0<=x, y+1<=x, 0<=y, y<=2}

               ;

//  P(x,y) {0<=x, x<=2, y<=x+2, x+2<=2y, 1<=y, y<=3}

         while (flip())

//  P(x,y) {0<=x, x<=2, 1<=y}

            if (flip())

//  P(x,y) {0<=x, x<=2, 1<=y}

               if (y<=2&&2*y>=x+3) {

//  P(x,y) {0<=x, x<=2, x+3<=2y, 1<=y, y<=2}

                  x++;

//  P(x,y) {1<=x, x<=3, x+2<=2y, 1<=y, y<=2}

                  if (x>=3&&y>=3)

//  P() {0==-1}

                     checking_error();
               }
               else

//  P(x,y) {0<=x, x<=2, 1<=y}

                  while (1)

//  P(x,y) {0<=x, x<=2, 1<=y}

                     ;
            else if (x<=2) {

//  P(x,y) {0<=x, x<=2, 1<=y}

               y++;

//  P(x,y) {0<=x, x<=2, 2<=y}

               if (x>=3&&y>=3)

//  P() {0==-1}

                  checking_error();
            }
            else

//  P() {0==-1}

               while (1)

//  P() {0==-1}

                  ;

//  P(x,y) {0<=x, x<=2, x+2<=2y}

         if (y<=2&&2*y<x+3) {

//  P(x,y) {x==2y-2, 1<=y, y<=2}

            x++;

//  P(x,y) {x==2y-1, 1<=y, y<=2}

            if (x>=3&&y>=3)

//  P() {0==-1}

               checking_error();
         }
         else

//  P(x,y) {0<=x, x<=2, x+3<=2y}

            while (1)

//  P(x,y) {0<=x, x<=2, x+3<=2y}

               ;
      }
}
