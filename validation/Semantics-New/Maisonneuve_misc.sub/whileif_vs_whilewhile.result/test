
Transformers for "whileif"


//  T() {}

void whileif(void)
{

//  T() {}

   int x, y;

//  T(x,y) {x==0, y==0}

   x = y = 0;

//  T(x,y) {x#init==0, y#init==0, x<=58, y<=x, 10y<=9x+9, 6y<=x+50,
//    0<=y}

   while (x<=58)

//  T(x,y) {x==x#init+1, x<=59, 6y<=x+50, y#init<=y, y<=y#init+1,
//    61y<=55y#init+109}

      if (y<=8) {

//  T(x) {x==x#init+1, x<=59, y<=8}

         x++;

//  T(y) {y==y#init+1, x<=59, y<=9}

         y++;

//  T() {x<=59, 6y<=x+50, y<=9}

         if (6*y>x+50)

//  T() {0==-1}

            error();
      }
      else {

//  T(x) {x==x#init+1, x<=59, 9<=y}

         x++;

//  T() {x<=59, 6y<=x+50, 9<=y}

         if (6*y>x+50)

//  T() {0==-1}

            error();
      }
}

Transformers for "whilewhile"


//  T() {}

void whilewhile(void)
{

//  T() {}

   int x, y;

//  T(x,y) {x==0, y==0}

   x = y = 0;

//  T(x,y) {x#init==0, y#init==0, x<=58, y<=x, 0<=y}

   while (x<=58) {

//  T(x,y) {x+y#init==x#init+y, x<=58, x#init<=x, x#init<=58, y<=8,
//    y#init<=8}

      while (x<=58&&y<=8) {

//  T(x) {x==x#init+1, x<=59, y<=8}

         x++;

//  T(y) {y==y#init+1, x<=59, y<=9}

         y++;

//  T() {x<=59, 6y<=x+50, y<=9}

         if (6*y>x+50)

//  T() {0==-1}

            error();
      }

//  T(x) {x<=58, x#init<=x, x#init<=58, 9<=y}

      while (x<=58&&y>8) {

//  T(x) {x==x#init+1, x<=59, 9<=y}

         x++;

//  T() {x<=59, 6y<=x+50, 9<=y}

         if (6*y>x+50)

//  T() {0==-1}

            error();
      }
   }
}

Preconditions for "whileif"


//  P() {}

void whileif(void)
{

//  P() {}

   int x, y;

//  P() {}

   x = y = 0;

//  P(x,y) {x==0, y==0}

   while (x<=58)

//  P(x,y) {y<=x, 9x<=58y, 0<=y, y<=9}

      if (y<=8) {

//  P(x,y) {y<=x, 7x<=45y, x<=6y+3, 0<=y, y<=8}

         x++;

//  P(x,y) {x<=59, y+1<=x, x<=6y+4, 7x<=45y+7, 0<=y, y<=8}

         y++;

//  P(x,y) {x<=59, y<=x, x+2<=6y, 7x+38<=45y, 1<=y, y<=9}

         if (6*y>x+50)

//  P() {0==-1}

            error();
      }
      else {

//  P(x,y) {y==9, 9<=x, x<=58}

         x++;

//  P(x,y) {y==9, 10<=x, x<=59}

         if (6*y>x+50)

//  P() {0==-1}

            error();
      }
}

Preconditions for "whilewhile"


//  P() {}

void whilewhile(void)
{

//  P() {}

   int x, y;

//  P() {}

   x = y = 0;

//  P(x,y) {x==0, y==0}

   while (x<=58) {

//  P(x,y) {x==0, y==0}

      while (x<=58&&y<=8) {

//  P(x,y) {x==y, 0<=x, x<=8}

         x++;

//  P(x,y) {x==y+1, 1<=x, x<=9}

         y++;

//  P(x,y) {x==y, 1<=x, x<=9}

         if (6*y>x+50)

//  P() {0==-1}

            error();
      }

//  P(x,y) {x==9, y==9}

      while (x<=58&&y>8) {

//  P(x,y) {y==9, 9<=x, x<=58}

         x++;

//  P(x,y) {y==9, 10<=x, x<=59}

         if (6*y>x+50)

//  P() {0==-1}

            error();
      }
   }
}
