
TRANSFORMERS


//  T() {}

int main()
{

//  T(j) {j==2}

   int i, j = 2, k;

//  T() {j==2}

   float x;

//  T(j) {j#init==2}


   while (i*i>1) {

//  T(j) {}

      while (k*k>1)

//  T(j) {j==1}

         j = 1;

//  T() {0<=k+1, k<=1}

      // Oops, this generate an indentity transformer that is not identified
      //    j = j;
      x = 0.;
   }

//  T(j) {j==j#init, 0<=i+1, i<=1}

   j = j;
}

PRECONDITIONS


//  P() {}

int main()
{

//  P() {}

   int i, j = 2, k;

//  P(j) {j==2}

   float x;

//  P(j) {j==2}


   while (i*i>1) {

//  P(j) {1<=j, j<=2}

      while (k*k>1)

//  P(j) {1<=j, j<=2}

         j = 1;

//  P(j) {1<=j, j<=2, 0<=k+1, k<=1}

      // Oops, this generate an indentity transformer that is not identified
      //    j = j;
      x = 0.;
   }

//  P(j) {0<=i+1, i<=1, 1<=j, j<=2}

   j = j;
}
