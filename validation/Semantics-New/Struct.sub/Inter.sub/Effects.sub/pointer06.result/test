
//  T(main) {main==0}

int main()
{

//  T(a,b) {a==0, b==0}

   int a = 0, b = 0;

//  T() {}

   int *p = &b;

//  T(b) {b==12}

   *p = 12;

//  T(main) {main==0}

   return 0;
}

//  P() {}

int main()
{

//  P() {}

   int a = 0, b = 0;

//  P(a,b) {a==0, b==0}

   int *p = &b;

//  P(a,b) {a==0, b==0}

   *p = 12;

//  P(a,b) {a==0, b==12}

   return 0;
}
