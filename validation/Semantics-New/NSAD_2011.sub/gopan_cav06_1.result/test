
TRANSFORMERS


//  T() {}

void ts_singlestate(void)
{

//  T() {}

   int s, x, y;

//  T(s) {s==1}

   s = 1;

//  T(x,y) {s==1, x==0, y==0}

   x = y = 0;

//  T() {s==1, x==0, y==0}

   if (!(s==2||y<=x&&y<=-x+102))

//  T() {0==-1}

      checking_error();

//  T(s,x,y) {1<=s, s<=2, s#init+x#init+y#init<=s+x+y,
//    s#init+x#init+y<=s+x+y#init, 1<=s+x+y, y+1<=s+x, 0<=x,
//    x#init<=x}

   while (rand_b())

//  T(s,x,y) {s+s#init==3, s+x==x#init+2, s<=2, y#init+1<=s+y,
//    y+1<=s+y#init}

      if (rand_b()) {

//  T() {s==1, x<=50}

         if (!(s==1&&x<=50))

//  T() {0==-1}

            deadlock();

//  T(s) {s==2, s#init==1, x<=50}

         s = 2;

//  T(y) {s==2, y==y#init+1, x<=50}

         y++;

//  T() {s==2, x<=50}

         if (!(s==2||y<=x&&y<=-x+102))

//  T() {0==-1}

            checking_error();
      }
      else if (rand_b()) {

//  T() {s==1, 51<=x}

         if (!(s==1&&x>50))

//  T() {0==-1}

            deadlock();

//  T(s) {s==2, s#init==1, 51<=x}

         s = 2;

//  T(y) {s==2, y==y#init-1, 51<=x}

         y--;

//  T() {s==2, 51<=x}

         if (!(s==2||y<=x&&y<=-x+102))

//  T() {0==-1}

            checking_error();
      }
      else {

//  T() {s==2, 0<=y}

         if (!(s==2&&y>=0))

//  T() {0==-1}

            deadlock();

//  T(s) {s==1, s#init==2, 0<=y}

         s = 1;

//  T(x) {s==1, x==x#init+1, 0<=y}

         x++;

//  T() {s==1, y<=x, x+y<=102, 0<=y}

         if (!(s==2||y<=x&&y<=-x+102))

//  T() {0==-1}

            checking_error();
      }
}

//  T() {}

void ts_restructured(void)
{

//  T() {}

   int s, x, y;

//  T(s) {s==1}

   s = 1;

//  T(x,y) {s==1, x==0, y==0}

   x = y = 0;

//  T() {s==1, x==0, y==0}

   if (!(s==2||y<=x&&y<=-x+102))

//  T() {0==-1}

      checking_error();

//  T(s,x,y) {s==1, s#init==1, x==y, x#init==y#init, 0<=x, x<=50,
//    y#init<=x}

   while (rand_b()) {

//  T() {s==1, x<=50}

      if (!(s==1&&x<=50))

//  T() {0==-1}

         deadlock();

//  T(s) {s==2, s#init==1, x<=50}

      s = 2;

//  T(y) {s==2, y==y#init+1, x<=50}

      y++;

//  T() {s==2, x<=50}

      if (!(s==2||y<=x&&y<=-x+102))

//  T() {0==-1}

         checking_error();

//  T() {s==2, x<=49, 0<=y}

      if (!(s==2&&y>=0&&x<=49))

//  T() {0==-1}

         deadlock();

//  T(s) {s==1, s#init==2, x<=49, 0<=y}

      s = 1;

//  T(x) {s==1, x==x#init+1, x<=50, 0<=y}

      x++;

//  T() {s==1, x<=50, y<=x, x+y<=102, 0<=y}

      if (!(s==2||y<=x&&y<=-x+102))

//  T() {0==-1}

         checking_error();
   }

//  T() {s==1, x==y, 0<=x, x<=50}

   if (!(s==1&&x<=50))

//  T() {0==-1}

      deadlock();

//  T(s) {s==2, s#init==1, x==y, 0<=x, x<=50}

   s = 2;

//  T(y) {s==2, x==y-1, x==y#init, 0<=x, x<=50}

   y++;

//  T() {s==2, x==y-1, 0<=x, x<=50}

   if (!(s==2||y<=x&&y<=-x+102))

//  T() {0==-1}

      checking_error();

//  T() {s==2, x==50, y==51}

   if (!(s==2&&y>=0&&x==50))

//  T() {0==-1}

      deadlock();

//  T(s) {s==1, s#init==2, x==50, y==51}

   s = 1;

//  T(x) {s==1, x==51, x#init==50, y==51}

   x++;

//  T() {s==1, x==51, y==51}

   if (!(s==2||y<=x&&y<=-x+102))

//  T() {0==-1}

      checking_error();

//  T(s,x,y) {s==1, s#init==1, x+y==102, x#init+y#init==102, 51<=x,
//    x<=102, 102<=x+y#init}

   while (rand_b()) {

//  T() {s==1, 51<=x, 1<=y}

      if (!(s==1&&x>50&&y>=1))

//  T() {0==-1}

         deadlock();

//  T(s) {s==2, s#init==1, 51<=x, 1<=y}

      s = 2;

//  T(y) {s==2, y==y#init-1, 51<=x, 0<=y}

      y--;

//  T() {s==2, 51<=x, 0<=y}

      if (!(s==2||y<=x&&y<=-x+102))

//  T() {0==-1}

         checking_error();

//  T() {s==2, 51<=x, 0<=y}

      if (!(s==2&&y>=0))

//  T() {0==-1}

         deadlock();

//  T(s) {s==1, s#init==2, 51<=x, 0<=y}

      s = 1;

//  T(x) {s==1, x==x#init+1, 52<=x, 0<=y}

      x++;

//  T() {s==1, 52<=x, y<=x, x+y<=102, 0<=y}

      if (!(s==2||y<=x&&y<=-x+102))

//  T() {0==-1}

         checking_error();
   }

//  T() {s==1, x==102, y==0}

   if (!(s==1&&x>50&&y==0))

//  T() {0==-1}

      deadlock();

//  T(s) {s==2, s#init==1, x==102, y==0}

   s = 2;

//  T(y) {s==2, x==102, y==-1, y#init==0}

   y--;

//  T() {s==2, x==102, y==-1}

   if (!(s==2||y<=x&&y<=-x+102))

//  T() {0==-1}

      checking_error();
}

PRECONDITIONS


//  P() {}

void ts_singlestate(void)
{

//  P() {}

   int s, x, y;

//  P() {}

   s = 1;

//  P(s) {s==1}

   x = y = 0;

//  P(s,x,y) {s==1, x==0, y==0}

   if (!(s==2||y<=x&&y<=-x+102))

//  P() {0==-1}

      checking_error();

//  P(s,x,y) {s==1, x==0, y==0}

   while (rand_b())

//  P(s,x,y) {1<=s, s<=2, 1<=s+x+y, y+1<=s+x, 0<=x}

      if (rand_b()) {

//  P(s,x,y) {1<=s, s<=2, 1<=s+x+y, y+1<=s+x, 0<=x}

         if (!(s==1&&x<=50))

//  P(s,x,y) {1<=s, s<=2, 102<=51s+x, 1<=s+x+y, y+1<=s+x, 0<=x}

            deadlock();

//  P(s,x,y) {s==1, 0<=x, x<=50, 0<=x+y, y<=x}

         s = 2;

//  P(s,x,y) {s==2, 0<=x, x<=50, 0<=x+y, y<=x}

         y++;

//  P(s,x,y) {s==2, 0<=x, x<=50, 1<=x+y, y<=x+1}

         if (!(s==2||y<=x&&y<=-x+102))

//  P() {0==-1}

            checking_error();
      }
      else if (rand_b()) {

//  P(s,x,y) {1<=s, s<=2, 1<=s+x+y, y+1<=s+x, 0<=x}

         if (!(s==1&&x>50))

//  P(s,x,y) {1<=s, s<=2, 1<=s+x+y, y+1<=s+x, 0<=x}

            deadlock();

//  P(s,x,y) {s==1, 51<=x, 0<=x+y, y<=x}

         s = 2;

//  P(s,x,y) {s==2, 51<=x, 0<=x+y, y<=x}

         y--;

//  P(s,x,y) {s==2, 51<=x, 0<=x+y+1, y+1<=x}

         if (!(s==2||y<=x&&y<=-x+102))

//  P() {0==-1}

            checking_error();
      }
      else {

//  P(s,x,y) {1<=s, s<=2, 1<=s+x+y, y+1<=s+x, 0<=x}

         if (!(s==2&&y>=0))

//  P(s,x,y) {1<=s, s<=2, 1<=s+x+y, y+1<=s+x, s+y<=x+1, 0<=x}

            deadlock();

//  P(s,x,y) {s==2, 0<=x, y<=x+1, 0<=y}

         s = 1;

//  P(s,x,y) {s==1, 0<=x, y<=x+1, 0<=y}

         x++;

//  P(s,x,y) {s==1, 1<=x, y<=x, 0<=y}

         if (!(s==2||y<=x&&y<=-x+102))

//  P(s,x,y) {s==1, 1<=x, y<=x, 103<=x+y, 0<=y}

            checking_error();
      }
}

//  P() {}

void ts_restructured(void)
{

//  P() {}

   int s, x, y;

//  P() {}

   s = 1;

//  P(s) {s==1}

   x = y = 0;

//  P(s,x,y) {s==1, x==0, y==0}

   if (!(s==2||y<=x&&y<=-x+102))

//  P() {0==-1}

      checking_error();

//  P(s,x,y) {s==1, x==0, y==0}

   while (rand_b()) {

//  P(s,x,y) {s==1, x==y, 0<=x, x<=50}

      if (!(s==1&&x<=50))

//  P() {0==-1}

         deadlock();

//  P(s,x,y) {s==1, x==y, 0<=x, x<=50}

      s = 2;

//  P(s,x,y) {s==2, x==y, 0<=x, x<=50}

      y++;

//  P(s,x,y) {s==2, x==y-1, 0<=x, x<=50}

      if (!(s==2||y<=x&&y<=-x+102))

//  P() {0==-1}

         checking_error();

//  P(s,x,y) {s==2, x==y-1, 0<=x, x<=50}

      if (!(s==2&&y>=0&&x<=49))

//  P(s,x,y) {s==2, x==50, y==51}

         deadlock();

//  P(s,x,y) {s==2, x==y-1, 0<=x, x<=49}

      s = 1;

//  P(s,x,y) {s==1, x==y-1, 0<=x, x<=49}

      x++;

//  P(s,x,y) {s==1, x==y, 1<=x, x<=50}

      if (!(s==2||y<=x&&y<=-x+102))

//  P() {0==-1}

         checking_error();
   }

//  P(s,x,y) {s==1, x==y, 0<=x, x<=50}

   if (!(s==1&&x<=50))

//  P() {0==-1}

      deadlock();

//  P(s,x,y) {s==1, x==y, 0<=x, x<=50}

   s = 2;

//  P(s,x,y) {s==2, x==y, 0<=x, x<=50}

   y++;

//  P(s,x,y) {s==2, x==y-1, 0<=x, x<=50}

   if (!(s==2||y<=x&&y<=-x+102))

//  P() {0==-1}

      checking_error();

//  P(s,x,y) {s==2, x==y-1, 0<=x, x<=50}

   if (!(s==2&&y>=0&&x==50))

//  P(s,x,y) {s==2, x==y-1, 0<=x, x<=49}

      deadlock();

//  P(s,x,y) {s==2, x==50, y==51}

   s = 1;

//  P(s,x,y) {s==1, x==50, y==51}

   x++;

//  P(s,x,y) {s==1, x==51, y==51}

   if (!(s==2||y<=x&&y<=-x+102))

//  P() {0==-1}

      checking_error();

//  P(s,x,y) {s==1, x==51, y==51}

   while (rand_b()) {

//  P(s,x,y) {s==1, x+y==102, 51<=x, x<=102}

      if (!(s==1&&x>50&&y>=1))

//  P(s,x,y) {s==1, x==102, y==0}

         deadlock();

//  P(s,x,y) {s==1, x+y==102, 51<=x, x<=101}

      s = 2;

//  P(s,x,y) {s==2, x+y==102, 51<=x, x<=101}

      y--;

//  P(s,x,y) {s==2, x+y==101, 51<=x, x<=101}

      if (!(s==2||y<=x&&y<=-x+102))

//  P() {0==-1}

         checking_error();

//  P(s,x,y) {s==2, x+y==101, 51<=x, x<=101}

      if (!(s==2&&y>=0))

//  P() {0==-1}

         deadlock();

//  P(s,x,y) {s==2, x+y==101, 51<=x, x<=101}

      s = 1;

//  P(s,x,y) {s==1, x+y==101, 51<=x, x<=101}

      x++;

//  P(s,x,y) {s==1, x+y==102, 52<=x, x<=102}

      if (!(s==2||y<=x&&y<=-x+102))

//  P() {0==-1}

         checking_error();
   }

//  P(s,x,y) {s==1, x+y==102, 51<=x, x<=102}

   if (!(s==1&&x>50&&y==0))

//  P(s,x,y) {s==1, x+y==102, 51<=x, x<=101}

      deadlock();

//  P(s,x,y) {s==1, x==102, y==0}

   s = 2;

//  P(s,x,y) {s==2, x==102, y==0}

   y--;

//  P(s,x,y) {s==2, x==102, y==-1}

   if (!(s==2||y<=x&&y<=-x+102))

//  P() {0==-1}

      checking_error();
}
