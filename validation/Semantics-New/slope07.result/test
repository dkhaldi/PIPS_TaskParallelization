
Transformers for slope07


//  T() {}

void slope07()
{

//  T() {}

   int x, y, k;

//  T() {0<=x, x<=58}

   
   /* create a bounding box */
   // assert(0<=x && x<=10 && 0<=y && 0<=10);
   if (!(0<=x&&x<=58))

//  T() {0==-1}

      abort();

//  T(k) {x+y==k}


   if (61*y<=55*x+54)

//  T(k) {x+y==k}

      /* x=9, y=9 is reachable */
      /* The new constraints are derived from points (0,0), (9,9),
             (49,45) and (58,53) with slopes 1, 9/10 and 8/9 */
      k = x+y;
   else

//  T(k) {x+y==k}

      k = x+y;

//  T(x) {}


   scanf("%d", &x);

//  T() {0<=y, y<=9}

   /* create another bounding box */
   if (!(0<=y&&y<=9))

//  T() {0==-1}

      abort();

//  T(k) {x+y==k}


   if (61*y<=55*x+54)

//  T(k) {x+y==k}

      k = x+y;
   else

//  T(k) {x+y==k}

      k = x+y;

//  T() {}


   return;
}

Preconditions for slope07


//  P() {}

void slope07()
{

//  P() {}

   int x, y, k;

//  P() {}

   
   /* create a bounding box */
   // assert(0<=x && x<=10 && 0<=y && 0<=10);
   if (!(0<=x&&x<=58))

//  P() {}

      abort();

//  P() {0<=x, x<=58}


   if (61*y<=55*x+54)

//  P() {0<=x, x<=58, y<=x, 10y<=9x+9, 9y<=8x+13}

      /* x=9, y=9 is reachable */
      /* The new constraints are derived from points (0,0), (9,9),
             (49,45) and (58,53) with slopes 1, 9/10 and 8/9 */
      k = x+y;
   else

//  P() {0<=x, x<=58, x<=y+4, 9x+10<=10y}

      k = x+y;

//  P(k) {x+y==k, 0<=x, x<=58}


   scanf("%d", &x);

//  P(k,x) {y<=k, k<=y+58}

   /* create another bounding box */
   if (!(0<=y&&y<=9))

//  P(k,x) {y<=k, k<=y+58}

      abort();

//  P(k,x) {y<=k, k<=y+58, 0<=y, y<=9}


   if (61*y<=55*x+54)

//  P(k,x) {y<=k, k<=y+58, y<=x, 0<=y, y<=9}

      k = x+y;
   else

//  P(k,x) {y<=k, k<=y+58, x+1<=y, 0<=y, y<=9}

      k = x+y;

//  P(k,x) {x+y==k, x<=k, k<=x+9}


   return;
}
