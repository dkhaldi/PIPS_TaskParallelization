
Source code with transformers


//  T() {}

void modulo15(void)
{

//  T(l) {l==1}

   int i, j, k, l = 1, mi, mj, mk;

//  T() {l==1}

   if (3*i-2*j==-1&&l==1&&k==j+1&&l==1&&3*i-2*k==-2)

//  T() {0==-1}

      if (l==1) {

//  T() {0==-1}

         mi = i%2;

//  T() {0==-1}

         mj = j%3;

//  T() {0==-1}

         mk = k%3;

//  T() {0==-1}

         l = 1;
      }

//  T() {l==1}

   return;
}

//  P() {}

void modulo15(void)
{

//  P() {}

   int i, j, k, l = 1, mi, mj, mk;

//  P(l) {l==1}

   if (3*i-2*j==-1&&l==1&&k==j+1&&l==1&&3*i-2*k==-2)

//  P() {0==-1}

      if (l==1) {

//  P() {0==-1}

         mi = i%2;

//  P() {0==-1}

         mj = j%3;

//  P() {0==-1}

         mk = k%3;

//  P() {0==-1}

         l = 1;
      }

//  P(l) {l==1}

   return;
}
