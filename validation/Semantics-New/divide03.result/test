
Transformers for divide03


//  T(divide03) {0<=divide03+50, divide03+3<=0}

int divide03(int v1, int v2)
{

//  T() {}

   int v;

//  T(v1) {}

   scanf("%d", &v1);

//  T(v2) {}

   scanf("%d", &v2);

//  T() {10<=v1, v1<=100, 0<=v2+3, v2+2<=0}

   if (!(10<=v1&&v1<=100&&-3<=v2&&v2<=-2))

//  T() {0==-1}

      abort();

//  T(v) {0<=2v+v1, 3v+v1+v2+1<=0, 10<=v1, v1<=100, 0<=v2+3, v2+2<=0}

   // -50 <= v <= -3
   v = v1/v2;

//  T() {0<=2v+v1, 3v+v1+v2+1<=0, 10<=v1, v1<=100, 0<=v2+3, v2+2<=0}

   printf("%d\n", v);

//  T(divide03) {divide03==v, 0<=2divide03+v1, 3divide03+v1+v2+1<=0,
//    10<=v1, v1<=100, 0<=v2+3, v2+2<=0}

   return v;
}

Preconditions for divide03


//  P() {}

int divide03(int v1, int v2)
{

//  P() {}

   int v;

//  P() {}

   scanf("%d", &v1);

//  P(v1) {}

   scanf("%d", &v2);

//  P(v1,v2) {}

   if (!(10<=v1&&v1<=100&&-3<=v2&&v2<=-2))

//  P(v1,v2) {}

      abort();

//  P(v1,v2) {10<=v1, v1<=100, 0<=v2+3, v2+2<=0}

   // -50 <= v <= -3
   v = v1/v2;

//  P(v,v1,v2) {0<=2v+v1, 3v+v1+v2+1<=0, 10<=v1, v1<=100, 0<=v2+3,
//    v2+2<=0}

   printf("%d\n", v);

//  P(v,v1,v2) {0<=2v+v1, 3v+v1+v2+1<=0, 10<=v1, v1<=100, 0<=v2+3,
//    v2+2<=0}

   return v;
}
