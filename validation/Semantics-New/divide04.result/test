
Transformers for divide04


//  T(divide04) {3<=divide04, divide04<=50}

int divide04(int v1, int v2)
{

//  T() {}

   int v;

//  T(v1) {}

   scanf("%d", &v1);

//  T(v2) {}

   scanf("%d", &v2);

//  T() {0<=v1+100, v1+10<=0, 0<=v2+3, v2+2<=0}

   if (!(-100<=v1&&v1<=-10&&-3<=v2&&v2<=-2))

//  T() {0==-1}

      abort();

//  T(v) {2v+v1<=0, v2+1<=3v+v1, 0<=v1+100, v1+10<=0, 0<=v2+3,
//    v2+2<=0}

   v = v1/v2;

//  T() {2v+v1<=0, v2+1<=3v+v1, 0<=v1+100, v1+10<=0, 0<=v2+3, v2+2<=0}

   printf("%d\n", v);

//  T(divide04) {divide04==v, 2divide04+v1<=0, v2+1<=3divide04+v1,
//    0<=v1+100, v1+10<=0, 0<=v2+3, v2+2<=0}

   return v;
}

Preconditions for divide04


//  P() {}

int divide04(int v1, int v2)
{

//  P() {}

   int v;

//  P() {}

   scanf("%d", &v1);

//  P(v1) {}

   scanf("%d", &v2);

//  P(v1,v2) {}

   if (!(-100<=v1&&v1<=-10&&-3<=v2&&v2<=-2))

//  P(v1,v2) {}

      abort();

//  P(v1,v2) {0<=v1+100, v1+10<=0, 0<=v2+3, v2+2<=0}

   v = v1/v2;

//  P(v,v1,v2) {2v+v1<=0, v2+1<=3v+v1, 0<=v1+100, v1+10<=0, 0<=v2+3,
//    v2+2<=0}

   printf("%d\n", v);

//  P(v,v1,v2) {2v+v1<=0, v2+1<=3v+v1, 0<=v1+100, v1+10<=0, 0<=v2+3,
//    v2+2<=0}

   return v;
}
