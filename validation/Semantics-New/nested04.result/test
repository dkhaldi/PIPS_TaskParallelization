
TRANSFORMERS


//  T(main) {main==0}

int main()
{
   /*=10*/

//  T(i,l) {i==0, l==0}

   int i = 0, j, k, l = 0, n;

//  T(i,j,k,l) {i#init==0, l#init==0, 0<=i, i<=l, i+1<=n}


   for(i = 0; i <= n-1; i += 1)

//  T(j,k,l) {0<=i, i+1<=n, 0<=j, j+l#init<=l, j+1<=n}

      for(j = 0; j <= n-1; j += 1)

//  T(k,l) {k+l#init==l, 0<=i, i+1<=n, 0<=j, j+1<=n, 0<=k, k+1<=n}

         for(k = 0; k <= n-1; k += 1)

//  T(l) {l==l#init+1, 0<=i, i+1<=n, 0<=j, j+1<=n, 0<=k, k+1<=n}

            l++;

//  T() {0<=i, i<=l, 3i<=l+2, n<=i}


   printf("l=%d\n", l);

//  T(main) {main==0, 0<=i, i<=l, 3i<=l+2, n<=i}

   return 0;
}

PRECONDITIONS


//  P() {}

int main()
{
   /*=10*/

//  P() {}

   int i = 0, j, k, l = 0, n;

//  P(i,l) {i==0, l==0}


   for(i = 0; i <= n-1; i += 1)

//  P(i,j,k,l) {0<=i, i<=l, i+1<=n}

      for(j = 0; j <= n-1; j += 1)

//  P(i,j,k,l) {0<=i, i+j<=l, i+1<=n, 0<=j, j+1<=n}

         for(k = 0; k <= n-1; k += 1)

//  P(i,j,k,l) {0<=i, i+j+k<=l, i+1<=n, 0<=j, j+1<=n, 0<=k, k+1<=n}

            l++;

//  P(i,j,k,l) {0<=i, i<=l, 3i<=l+2, n<=i}


   printf("l=%d\n", l);

//  P(i,j,k,l) {0<=i, i<=l, 3i<=l+2, n<=i}

   return 0;
}
