
Transformers for "main"


//  T(main) {main==3}

int main(void)
{

//  T(stabilize) {stabilize==1}

   _Bool stabilize = 1;

//  T() {stabilize==1}

   int k;

//  T(k) {k==3, stabilize==1}


   k = 3*stabilize;

//  T() {k==3, stabilize==1}


   printf("k=%d\n", k);

//  T(main) {k==3, main==3, stabilize==1}


   return k;
}

Preconditions for "main"


//  P() {}

int main(void)
{

//  P() {}

   _Bool stabilize = 1;

//  P(stabilize) {stabilize==1}

   int k;

//  P(stabilize) {stabilize==1}


   k = 3*stabilize;

//  P(k,stabilize) {k==3, stabilize==1}


   printf("k=%d\n", k);

//  P(k,stabilize) {k==3, stabilize==1}


   return k;
}

Code after control simplification

int main(void)
{
   _Bool stabilize = 1;
   int k;

   k = 3*stabilize;

   printf("k=%d\n", k);

   return k;
}
