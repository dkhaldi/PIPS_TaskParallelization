
//  P() {}

int main(void)
{

//  P() {}

   _Bool b1, b2;

//  P() {}

   int i = 1, j = 2, k;

//  P(i,j) {i==1, j==2}


   b1 = i<=j;

//  P(b1,i,j) {b1==1, i==1, j==2}

   b2 = i>j;

//  P(b1,b2,i,j) {b1==1, b2==0, i==1, j==2}


   if (k)

//  P(b1,b2,i,j) {b1==1, b2==0, i==1, j==2}

      i = 2, j = 1;

//  P(b1,b2,i,j) {b1==1, b2==0, i+j==3, 1<=i, i<=2}


   b1 = i<=j;

//  P(b1,b2,i,j) {b2==0, i+j==3, 1<=i, i<=2}

   b2 = i>j;

//  P(b1,b2,i,j) {i+j==3, 1<=i, i<=2}


   return 0;
}
