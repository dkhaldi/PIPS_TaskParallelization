
C  T() {}

      PROGRAM TYPE12
      REAL A, B

C  T(A,B) {}


      READ *, A, B                                                      0004

C  T() {}


      IF (A+B.EQ.0.0) THEN                                              0006

C  T() {}

         PRINT *, 'a+b = 0'                                             0007

C  T() {}

         PRINT *, A+B                                                   0008
      ENDIF

      END

C  P() {}

      PROGRAM TYPE12
      REAL A, B

C  P() {}


      READ *, A, B                                                      0004

C  P(A,B) {}


      IF (A+B.EQ.0.0) THEN                                              0006

C  P(A,B) {A+B==0}

         PRINT *, 'a+b = 0'                                             0007

C  P(A,B) {A+B==0}

         PRINT *, A+B                                                   0008
      ENDIF

      END

