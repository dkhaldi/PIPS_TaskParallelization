
Transformers for "while05"


//  T(while05) {while05==9}

int while05()
{

//  T() {}

   int i, n;

//  T(i) {i==0}


   i = 0;

//  T(n) {n==10}

   n = 10;

//  T(i,n) {i+n==i#init+n#init-1}


   while (--n) {

//  T() {}

      printf("loop: i=%d, n=%d, i+n=%d\n", i, n, i+n);

//  T(i) {i==i#init+1}

      i++;
   }

//  T() {}


   printf("exit: i=%d, n=%d, i+n=%d\n", i, n, i+n);

//  T(while05) {i==while05}


   return i;
}

Preconditions for "while05"


//  P() {}

int while05()
{

//  P() {}

   int i, n;

//  P() {}


   i = 0;

//  P(i) {i==0}

   n = 10;

//  P(i,n) {i==0, n==10}


   while (--n) {

//  P(i,n) {8<=i+n, i+n<=9}

      printf("loop: i=%d, n=%d, i+n=%d\n", i, n, i+n);

//  P(i,n) {8<=i+n, i+n<=9}

      i++;
   }

//  P(i,n) {n==0, 8<=i, i<=9}


   printf("exit: i=%d, n=%d, i+n=%d\n", i, n, i+n);

//  P(i,n) {n==0, 8<=i, i<=9}


   return i;
}
