
C  T() {}

      PROGRAM NON_LINEAR03

C  T(K) {2<=I, 3<=J}

C (0,1)
C     BEGIN BLOCK

C  T() {2<=I, 3<=J}

C (0,2)

C     Check non-linear expression handling

      IF (I.LT.2.OR.J.LT.3) STOP                                        0005

C  T(K) {}

C (0,5)

      K = I*(J-1)                                                       0007

C     END BLOCK
      END

