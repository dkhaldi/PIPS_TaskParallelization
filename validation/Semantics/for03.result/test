
Initial source code

int for03()
{
   int x[512];
   int i;
   int j;

   for (i = 0, j = 0;i<500;i += 128, j++)
      x[i] = 0;
   
   //printf("%d, %d\n", i, j);
   
   return 0;
}

Check transformers on strip-mined loop


//  T(for03) {for03==0}

int for03()
{

//  T() {}

   int x[512];

//  T() {}

   int i;

//  T() {}

   int j;

//  T(i,j) {i==128j, 0<=j, j<=3}


   for (i = 0, j = 0;i<500;i += 128, j++)

//  T() {i<=499}

      x[i] = 0;

//  T(for03) {for03==0, i==128j, j==4}

   
   //printf("%d, %d\n", i, j);
   
   return 0;
}
