
//  P() {}

int main()
{

//  P() {}

   int loc_0000;

//  P() {}

   int loc_0001[500];

//  P() {}

   int loc_0002[500];

//  P() {}

   int loc_0003;

//  P() {}

   int loc_0004[500];

//  P() {}


   loc_0000 = 0;

//  P(loc_0000) {loc_0000==0}


   while (loc_0000<=499) {

//  P(loc_0000) {loc_0000<=499}

L0000:      loc_0001[loc_0000] = loc_0000;

//  P(loc_0000) {loc_0000<=499}

      loc_0002[loc_0000] = loc_0000;

//  P(loc_0000) {loc_0000<=499}

      // The transformer here should not be constant...
      loc_0000 = loc_0000+1;
   }

//  P(loc_0000) {loc_0000==500}

   // Code should not be dead here...
   loc_0003 = 2;

//  P(loc_0000,loc_0003) {loc_0000==500, loc_0003==2}


   return loc_0003;
}

//  T(main) {main==2}

int main()
{

//  T() {}

   int loc_0000;

//  T() {}

   int loc_0001[500];

//  T() {}

   int loc_0002[500];

//  T() {}

   int loc_0003;

//  T() {}

   int loc_0004[500];

//  T(loc_0000) {loc_0000==0}


   loc_0000 = 0;

//  T(loc_0000) {loc_0000#init==0, loc_0000<=499}


   while (loc_0000<=499) {

//  T() {loc_0000<=499}

L0000:      loc_0001[loc_0000] = loc_0000;

//  T() {loc_0000<=499}

      loc_0002[loc_0000] = loc_0000;

//  T(loc_0000) {loc_0000==loc_0000#init+1, loc_0000<=500}

      // The transformer here should not be constant...
      loc_0000 = loc_0000+1;
   }

//  T(loc_0003) {loc_0000==500, loc_0003==2}

   // Code should not be dead here...
   loc_0003 = 2;

//  T(main) {loc_0000==500, loc_0003==2, main==2}


   return loc_0003;
}
