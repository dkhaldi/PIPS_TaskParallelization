
Proper effects

int main()
{
   int *a, *b, c, d, i;
//             <    is written>: c
   c = 0;
//             <    is written>: d
   d = 1;
//             <    is written>: b
   b = &c;
//             <    is written>: i

   for(i = 1; i <= 4; i += 1) {
//             <    is written>: c
      c = 5;
//             <    is read   >: b
//             <    is written>: a
      a = b;
//             <    is written>: b
      b = &d;
   }

   return 0;
}

Points-to


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int *a, *b, c, d, i;

// Points To:
// a -> undefined , EXACT
// b -> undefined , EXACT

   c = 0;

// Points To:
// a -> undefined , EXACT
// b -> undefined , EXACT

   d = 1;

// Points To:
// a -> undefined , EXACT
// b -> undefined , EXACT

   b = &c;

// Points To:
// a -> undefined , EXACT
// b -> c , EXACT


   for(i = 1; i <= 4; i += 1) {

// Points To:
// a -> undefined , MAY
// a -> c , MAY
// a -> d , MAY
// b -> c , MAY
// b -> d , MAY

      c = 5;

// Points To:
// a -> undefined , MAY
// a -> c , MAY
// a -> d , MAY
// b -> c , MAY
// b -> d , MAY

      a = b;

// Points To:
// a -> c , MAY
// a -> d , MAY
// b -> c , MAY
// b -> d , MAY

      b = &d;
   }

// Points To:
// a -> undefined , MAY
// a -> c , MAY
// a -> d , MAY
// b -> c , MAY
// b -> d , MAY


   return 0;
}
