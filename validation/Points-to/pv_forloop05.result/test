
Proper effects

int main()
{
   float *p[10];
   float a[10], t;
   int i;
//             <    is written>: t

   t = 1.0;
//             <    is read   >: t
   while (t<2.0) {
//             <    is written>: i
      for(i = 0; i <= 9; i += 1)
//             <    is read   >: i
//             <    is written>: p[i]
         p[i] = &a[i];
//             <    is read   >: t
//             <    is written>: t
      t += 0, 01;
   }

   return 0;
}

Points-to


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   float *p[10];

// Points To:
// p[*] -> undefined , EXACT

   float a[10], t;

// Points To:
// p[*] -> undefined , EXACT

   int i;

// Points To:
// p[*] -> undefined , EXACT


   t = 1.0;

// Points To:
// p[*] -> undefined , EXACT

   while (t<2.0) {

// Points To:
// p[*] -> undefined , MAY
// p[*] -> a[*] , MAY

      for(i = 0; i <= 9; i += 1)

// Points To:
// p[*] -> undefined , MAY
// p[*] -> a[*] , MAY

         p[i] = &a[i];

// Points To:
// p[*] -> undefined , MAY
// p[*] -> a[*] , MAY

      t += 0, 01;
   }

// Points To:
// p[*] -> undefined , MAY
// p[*] -> a[*] , MAY


   return 0;
}
