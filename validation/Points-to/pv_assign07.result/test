
Proper effects

int main()
{
   mystruct s1, s2, *s1p, *s2p;
//             <    is written>: i
   int i = 1;
//             <    is written>: j
   int j = 2;
//             <    is written>: k
   int k = 3;
//             <    is written>: s1p

   s1p = &s1;
//             <    is written>: s2p
   s2p = &s1;
//             <    is written>: s1.a

   s1.a = &i;
//             <    is written>: s2.a
   s2.a = &j;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*_b0
//             <    is read   >: s1p s2.a

   s1p->a = s2.a;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*_b0
//             <    is read   >: s2p
   s2p->a = &k;
//             <    is written>: s2p

   s2p = &s2;
   
   
   //  printf("s1.a = %d, s1p->a = %d, s2.a = %d, s2p->a = %d\n", *s1.a, *s1p->a, *s2.a , *s2p->a);
   return 0;
}

Points-to


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   mystruct s1, s2, *s1p, *s2p;

// Points To:
// s1.a -> undefined , EXACT
// s1p -> undefined , EXACT
// s2.a -> undefined , EXACT
// s2p -> undefined , EXACT

   int i = 1;

// Points To:
// s1.a -> undefined , EXACT
// s1p -> undefined , EXACT
// s2.a -> undefined , EXACT
// s2p -> undefined , EXACT

   int j = 2;

// Points To:
// s1.a -> undefined , EXACT
// s1p -> undefined , EXACT
// s2.a -> undefined , EXACT
// s2p -> undefined , EXACT

   int k = 3;

// Points To:
// s1.a -> undefined , EXACT
// s1p -> undefined , EXACT
// s2.a -> undefined , EXACT
// s2p -> undefined , EXACT


   s1p = &s1;

// Points To:
// s1.a -> undefined , EXACT
// s1p -> s1 , EXACT
// s2.a -> undefined , EXACT
// s2p -> undefined , EXACT

   s2p = &s1;

// Points To:
// s1.a -> undefined , EXACT
// s1p -> s1 , EXACT
// s2.a -> undefined , EXACT
// s2p -> s1 , EXACT


   s1.a = &i;

// Points To:
// s1.a -> i , EXACT
// s1p -> s1 , EXACT
// s2.a -> undefined , EXACT
// s2p -> s1 , EXACT

   s2.a = &j;

// Points To:
// s1.a -> i , EXACT
// s1p -> s1 , EXACT
// s2.a -> j , EXACT
// s2p -> s1 , EXACT


   s1p->a = s2.a;

// Points To:
// s1.a -> j , EXACT
// s1p -> s1 , EXACT
// s2.a -> j , EXACT
// s2p -> s1 , EXACT

   s2p->a = &k;

// Points To:
// s1.a -> k , EXACT
// s1p -> s1 , EXACT
// s2.a -> j , EXACT
// s2p -> s1 , EXACT


   s2p = &s2;

// Points To:
// s1.a -> k , EXACT
// s1p -> s1 , EXACT
// s2.a -> j , EXACT
// s2p -> s2 , EXACT

   
   
   //  printf("s1.a = %d, s1p->a = %d, s2.a = %d, s2p->a = %d\n", *s1.a, *s1p->a, *s2.a , *s2p->a);
   return 0;
}
