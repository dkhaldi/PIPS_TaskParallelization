
POINTS TO


// Points To IN: none


// Points To OUT: none

int main(int argc, char *argv[])
{

// Points To: none

   int *pi;

// Points To:
// pi -> undefined , EXACT

   float *px;

// Points To:
// pi -> undefined , EXACT
// px -> undefined , EXACT

   float *pz;

// Points To:
// pi -> undefined , EXACT
// px -> undefined , EXACT
// pz -> undefined , EXACT

   int (*pt)[30];

// Points To:
// pi -> undefined , EXACT
// pt -> undefined , EXACT
// px -> undefined , EXACT
// pz -> undefined , EXACT


   pi = (int *) malloc(sizeof(int));

// Points To:
// pi -> *HEAP*_l_21 , MAY
// pt -> undefined , EXACT
// px -> undefined , EXACT
// pz -> undefined , EXACT

   px = (float *) malloc(sizeof(float));

// Points To:
// pi -> *HEAP*_l_21 , MAY
// pt -> undefined , EXACT
// px -> *HEAP*_l_22 , MAY
// pz -> undefined , EXACT

   pz = (float *) malloc(sizeof(float));

// Points To:
// pi -> *HEAP*_l_21 , MAY
// pt -> undefined , EXACT
// px -> *HEAP*_l_22 , MAY
// pz -> *HEAP*_l_23 , MAY

   pt = (int (*)[30]) malloc(sizeof(*pt));

// Points To:
// pi -> *HEAP*_l_21 , MAY
// pt -> *HEAP*_l_24 , MAY
// px -> *HEAP*_l_22 , MAY
// pz -> *HEAP*_l_23 , MAY


   printf("%p\n", pt);

// Points To:
// pi -> *HEAP*_l_21 , MAY
// pt -> *HEAP*_l_24 , MAY
// px -> *HEAP*_l_22 , MAY
// pz -> *HEAP*_l_23 , MAY

   
   // pt++; Let's avoid this bug to preserve the remainder of the test case
   
   printf("%p\n", pt);

// Points To:
// pi -> *HEAP*_l_21 , MAY
// pt -> *HEAP*_l_24 , MAY
// px -> *HEAP*_l_22 , MAY
// pz -> *HEAP*_l_23 , MAY


   return 0;
}
