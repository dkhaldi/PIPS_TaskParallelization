
PROPER  EFFECTS

int main()
{
//             <    is written>: i
   int i = 0;
   int *p;
   int a[10];
//             <    is read   >: i

   while (i<10) {
//             <    is read   >: i
//             <    is written>: p
      p = &a[i];
//             <    is read   >: i
//             <    is written>: i
      i++;
   }

   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int i = 0;

// Points To: none

   int *p;

// Points To:
// p -> undefined , EXACT

   int a[10];

// Points To:
// p -> undefined , EXACT


   while (i<10) {

// Points To:
// p -> undefined , MAY
// p -> a[*] , MAY

      p = &a[i];

// Points To:
// p -> a[*] , MAY

      i++;
   }

// Points To:
// p -> undefined , MAY
// p -> a[*] , MAY


   return 0;
}
