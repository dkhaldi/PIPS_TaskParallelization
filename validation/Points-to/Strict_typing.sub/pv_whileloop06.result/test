
PROPER  EFFECTS

int main()
{
   int i;
   int *p, *q;
   int a[10];
   int b[10];
//             <    is written>: p

   p = &a[0];
//             <    is written>: q
   q = &b[0];
//             <    is written>: i
   i = 0;
//             <    is read   >: i
   while (i<10) {
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*_b0
//             <may be written>: *ANY_MODULE*:*ANYWHERE*_b0
//             <    is read   >: p q
//             <    is written>: p q
      *p++ = 2**q++;
//             <    is read   >: i
//             <    is written>: i
      i++;
   }

   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int i;

// Points To: none

   int *p, *q;

// Points To:
// p -> undefined , EXACT
// q -> undefined , EXACT

   int a[10];

// Points To:
// p -> undefined , EXACT
// q -> undefined , EXACT

   int b[10];

// Points To:
// p -> undefined , EXACT
// q -> undefined , EXACT


   p = &a[0];

// Points To:
// p -> a[0] , EXACT
// q -> undefined , EXACT

   q = &b[0];

// Points To:
// p -> a[0] , EXACT
// q -> b[0] , EXACT

   i = 0;

// Points To:
// p -> a[0] , EXACT
// q -> b[0] , EXACT

   while (i<10) {

// Points To:
// p -> a[*] , MAY
// p -> a[0] , MAY
// p -> a[1] , MAY
// p -> a[2] , MAY
// p -> a[3] , MAY
// q -> b[*] , MAY
// q -> b[0] , MAY
// q -> b[1] , MAY
// q -> b[2] , MAY
// q -> b[3] , MAY

      *p++ = 2**q++;

// Points To:
// p -> a[*] , MAY
// p -> a[1] , MAY
// p -> a[2] , MAY
// p -> a[3] , MAY
// q -> b[*] , MAY
// q -> b[1] , MAY
// q -> b[2] , MAY
// q -> b[3] , MAY

      i++;
   }

// Points To:
// p -> a[*] , MAY
// p -> a[0] , MAY
// p -> a[1] , MAY
// p -> a[2] , MAY
// p -> a[3] , MAY
// q -> b[*] , MAY
// q -> b[0] , MAY
// q -> b[1] , MAY
// q -> b[2] , MAY
// q -> b[3] , MAY


   return 0;
}
