
PROPER  EFFECTS

int assignment09()
{
   int a[10];
//             <    is written>: q
   int *q = a;
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*_b0
//             <    is read   >: q

   return *q;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int assignment09()
{

// Points To: none

   int a[10];

// Points To: none

   int *q = a;

// Points To:
// q -> a[0] , EXACT


   return *q;
}
