
POINTS-TO


// Points To IN:
// _p_1[3] -> *NULL* , MAY
// _p_1[3] -> _p_1_1[3][0] , MAY
// p -> *NULL* , MAY
// p -> _p_1[0] , MAY


// Points To OUT:
// _p_1[3] -> _p_1_1[3][0] , EXACT
// p -> _p_1[0] , EXACT

int pointer_reference02(char **p)
{

// Points To:
// _p_1[3] -> *NULL* , MAY
// _p_1[3] -> _p_1_1[3][0] , MAY
// p -> *NULL* , MAY
// p -> _p_1[0] , MAY

   char *q = p[3];

// Points To:
// _p_1[3] -> _p_1_1[3][0] , EXACT
// p -> _p_1[0] , EXACT
// q -> _p_1_1[3][0] , EXACT

   char *r;

// Points To:
// _p_1[3] -> _p_1_1[3][0] , EXACT
// p -> _p_1[0] , EXACT
// q -> _p_1_1[3][0] , EXACT
// r -> undefined , EXACT

   r = p[3];

// Points To:
// _p_1[3] -> _p_1_1[3][0] , EXACT
// p -> _p_1[0] , EXACT
// q -> _p_1_1[3][0] , EXACT
// r -> _p_1_1[3][0] , EXACT

   return q-p[3];
}
