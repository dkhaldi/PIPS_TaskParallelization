// Points To OUT: none

int decl01()
{

// Points To: none

   int i = 2;

// Points To: none

   int j = i;

// Points To: none

   int a[sizeof(i)];

// Points To: none


   i = 2;

// Points To: none

   j = i;

// Points To: none

   j = sizeof(i);

// Points To: none

   return 0;
}
