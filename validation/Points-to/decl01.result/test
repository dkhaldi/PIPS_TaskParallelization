
Proper effects

int decl01()
{
//             <    is written>: i
   int i = 2;
//             <    is read   >: i
//             <    is written>: j
   int j = i;
   int a[sizeof(i)];
//             <    is written>: i

   i = 2;
//             <    is read   >: i
//             <    is written>: j
   j = i;
//             <    is written>: j
   j = sizeof(i);
   return 0;
}

