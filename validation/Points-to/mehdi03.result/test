
POINTS TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   struct p_s {
      struct q_s {int r;} *q;
   };


// Points To: none

   struct q_s;

// Points To: none

   struct p_s *p;

// Points To:
// p -> undefined , EXACT

   struct q_s *x;

// Points To:
// p -> undefined , EXACT
// x -> undefined , EXACT


   p = (struct p_s *) malloc(sizeof(struct p_s ));

// Points To:
// p -> *ANY_MODULE*:*HEAP**ANYWHERE* , MAY
// x -> undefined , EXACT

   p->q = x;

// Points To:
// *ANY_MODULE*:*HEAP**ANYWHERE* -> undefined , MAY
// p -> *ANY_MODULE*:*HEAP**ANYWHERE* , MAY
// x -> undefined , EXACT


   return 0;
}
