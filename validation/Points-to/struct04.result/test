
Proper effects

int struct04()
{
   struct one {
      int first;
      int second;
   } x;
   int y[10];
//             <    is read   >: x.second
//             <    is written>: x.first
   
   //  x.first = 1;
   //x.second = 2;
   
   //y[0] = 1;
   
   x.first = x.second+1;
   return 0;
}

Points-to


// Points To IN: none


// Points To OUT: none

int struct04()
{

// Points To: none

   struct one {
      int first;
      int second;
   } x;

// Points To: none

   int y[10];

// Points To: none

   
   //  x.first = 1;
   //x.second = 2;
   
   //y[0] = 1;
   
   x.first = x.second+1;

// Points To: none

   return 0;
}
