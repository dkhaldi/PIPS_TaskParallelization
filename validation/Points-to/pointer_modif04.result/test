
Proper effects

int main()
{
   s_t s;
   int j;
//             <    is read   >: s.max s.tab _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: _MALLOC_EFFECTS:_MALLOC_

   init_s(s, 10);
//             <may be written>: *ANY_MODULE*:*ANYWHERE*_b0
//             <    is read   >: s.max s.tab

   compute_s(s, 10);
   
   
   return 0;
}

Points-to


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   s_t s;

// Points To:
// s.tab -> undefined , EXACT

   int j;

// Points To:
// s.tab -> undefined , EXACT


   init_s(s, 10);

// Points To:
// s.tab -> undefined , EXACT


   compute_s(s, 10);

// Points To:
// s.tab -> undefined , EXACT

   
   
   return 0;
}
