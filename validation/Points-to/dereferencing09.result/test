
PROPER  EFFECTS

int main()
{
   int a, aa, *b, *bb, **c;
//             <    is written>: a

   a = 1;
//             <    is written>: b
   b = &a;
//             <    is written>: c
   c = &b;
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*_b0
//             <    is read   >: c
//             <    is written>: bb
   
   // Here c->b->a
   
   bb = *c;
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*_b0
//             <    is read   >: c
//             <    is written>: aa
   
   // bb has the value of b, hence bb->a
   
   aa = **c;
   
   // aa is a copy of a
   
   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int a, aa, *b, *bb, **c;

// Points To:
// b -> undefined , EXACT
// bb -> undefined , EXACT
// c -> undefined , EXACT


   a = 1;

// Points To:
// b -> undefined , EXACT
// bb -> undefined , EXACT
// c -> undefined , EXACT

   b = &a;

// Points To:
// b -> a , EXACT
// bb -> undefined , EXACT
// c -> undefined , EXACT

   c = &b;

// Points To:
// b -> a , EXACT
// bb -> undefined , EXACT
// c -> b , EXACT

   
   // Here c->b->a
   
   bb = *c;

// Points To:
// b -> a , EXACT
// bb -> a , EXACT
// c -> b , EXACT

   
   // bb has the value of b, hence bb->a
   
   aa = **c;

// Points To:
// b -> a , EXACT
// bb -> a , EXACT
// c -> b , EXACT

   
   // aa is a copy of a
   
   return 0;
}
