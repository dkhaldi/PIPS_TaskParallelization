
PROPER  EFFECTS

int main()
{
   char *s;
//             <    is written>: s
   s = "hello\n";
   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   char *s;

// Points To:
// s -> undefined , EXACT

   s = "hello\n";

// Points To:
// s -> "hello\n" , EXACT

   return 0;
}
