{

// Points To: none

   int a;

// Points To:
// _p_1[0][0] -> *NULL* , MAY
// _p_1[0][0] -> _p_1_1[0][0][0] , MAY
// p -> *NULL* , MAY
// p -> _p_1[0][0] , MAY


   (*p)[3] = &a;

// Points To:
