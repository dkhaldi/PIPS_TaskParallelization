
PROPER  EFFECTS

int main()
{
   mystruct s1, *s2;
//             <    is written>: i
   int i = 1;
//             <    is written>: j
   int j = 2;
//             <    is written>: s2
   s2 = &s1;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*_b0
//             <    is read   >: s2
   s2->a = &i;
//             <    is written>: s1.a
   s1.a = &j;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*_b0
//             <    is read   >: s2
   (s2->b)[0] = &i;
   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   mystruct s1, *s2;

// Points To:
// s1.b[*] -> undefined , EXACT
// s1.a -> undefined , EXACT
// s1.c -> undefined , EXACT
// s2 -> undefined , EXACT

   int i = 1;

// Points To:
// s1.b[*] -> undefined , EXACT
// s1.a -> undefined , EXACT
// s1.c -> undefined , EXACT
// s2 -> undefined , EXACT

   int j = 2;

// Points To:
// s1.b[*] -> undefined , EXACT
// s1.a -> undefined , EXACT
// s1.c -> undefined , EXACT
// s2 -> undefined , EXACT

   s2 = &s1;

// Points To:
// s1.b[*] -> undefined , EXACT
// s1.a -> undefined , EXACT
// s1.c -> undefined , EXACT
// s2 -> s1 , EXACT

   s2->a = &i;

// Points To:
// s1.b[*] -> undefined , EXACT
// s1.c -> undefined , EXACT
// s1.a -> i , EXACT
// s2 -> s1 , EXACT

   s1.a = &j;

// Points To:
// s1.b[*] -> undefined , EXACT
// s1.c -> undefined , EXACT
// s1.a -> j , EXACT
// s2 -> s1 , EXACT

   (s2->b)[0] = &i;

// Points To:
// s1.b[*] -> undefined , MAY
// s1.c -> undefined , EXACT
// s1.b[0] -> i , EXACT
// s1.a -> j , EXACT
// s2 -> s1 , EXACT

   return 0;
}
