
PROPER  EFFECTS

int main()
{
//             <    is written>: a b x1 y
   int a = 2, b = 1, **x, *y = &a, *x1 = &b;
//             <    is written>: x
   x = &x1;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*_b0
//             <    is read   >: x y
   *x = y;

   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int a = 2, b = 1, **x, *y = &a, *x1 = &b;

// Points To:
// x -> undefined , EXACT
// x1 -> b , EXACT
// y -> a , EXACT

   x = &x1;

// Points To:
// x -> x1 , EXACT
// x1 -> b , EXACT
// y -> a , EXACT

   *x = y;

// Points To:
// x -> x1 , EXACT
// x1 -> a , EXACT
// y -> a , EXACT


   return 0;
}
