
PROPER  EFFECTS

int main()
{
   int *a, *b, c, d, i;
//             <    is written>: c
   c = 0;
//             <    is written>: d
   d = 1;
//             <    is written>: i
   i = 2;
//             <    is written>: b
   b = &c;
//             <    is read   >: i
   do {
//             <    is read   >: b
//             <    is written>: a
      a = b;
//             <    is written>: b
      b = &d;
//             <    is read   >: i
//             <    is written>: i
      i = i+1;
   }
   while (i<5);

   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int *a, *b, c, d, i;

// Points To:
// a -> undefined , EXACT
// b -> undefined , EXACT

   c = 0;

// Points To:
// a -> undefined , EXACT
// b -> undefined , EXACT

   d = 1;

// Points To:
// a -> undefined , EXACT
// b -> undefined , EXACT

   i = 2;

// Points To:
// a -> undefined , EXACT
// b -> undefined , EXACT

   b = &c;

// Points To:
// a -> undefined , EXACT
// b -> c , EXACT

   do {

// Points To:
// a -> undefined , MAY
// a -> c , MAY
// a -> d , MAY
// b -> c , MAY
// b -> d , MAY

      a = b;

// Points To:
// a -> c , MAY
// a -> d , MAY
// b -> c , MAY
// b -> d , MAY

      b = &d;

// Points To:
// a -> c , MAY
// a -> d , MAY
// b -> d , EXACT

      i = i+1;
   }
   while (i<5);

// Points To:
// a -> c , MAY
// a -> d , MAY
// b -> d , EXACT


   return 0;
}
