
Proper effects

int main()
{
   int i;
   int *p[10];
   int a[10];
//             <    is written>: i

   for(i = 0; i <= 9; i += 1)
//             <    is read   >: i
//             <    is written>: p[i]
      p[i] = &a[i];

   return 0;
}

Points-to


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int i;

// Points To: none

   int *p[10];

// Points To:
// p[*] -> undefined , EXACT

   int a[10];

// Points To:
// p[*] -> undefined , EXACT


   for(i = 0; i <= 9; i += 1)

// Points To:
// p[*] -> undefined , MAY
// p[*] -> a[*] , MAY

      p[i] = &a[i];

// Points To:
// p[*] -> undefined , MAY
// p[*] -> a[*] , MAY


   return 0;
}
