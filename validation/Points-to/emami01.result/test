
PROPER  EFFECTS

int main()
{
//             <    is written>: a
   int *x, a = 2;
//             <    is written>: x
   x = &a;

   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int *x, a = 2;

// Points To:
// x -> undefined , EXACT

   x = &a;

// Points To:
// x -> a , EXACT


   return 0;
}
