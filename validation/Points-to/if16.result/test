
POINTS TO for "main"


// Points To IN:
// p -> *NULL* , MAY
// p -> _p_1[0] , MAY


// Points To OUT:
// p -> *NULL* , MAY
// p -> _p_1[0] , MAY

int if16(int *p)
{

// Points To: none

   int *q, i;

// Points To:
// p -> *NULL* , MAY
// p -> _p_1[0] , MAY
// q -> undefined , EXACT


   if (p!=(void *) 0)

// Points To:
// p -> _p_1[0] , EXACT
// q -> undefined , EXACT

      q = p;
   else

// Points To:
// p -> *NULL* , EXACT
// q -> undefined , EXACT

      q = &i;

// Points To:
// p -> *NULL* , MAY
// p -> _p_1[0] , MAY
// q -> _p_1[0] , MAY
// q -> i , MAY


   return 0;
}
